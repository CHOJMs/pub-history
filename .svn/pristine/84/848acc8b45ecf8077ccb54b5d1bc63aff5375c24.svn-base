<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="ko" lang="ko">
<head>
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<%@ page contentType="text/html; charset=euc-kr" language="java" %>

<title>DGB 캐피탈 관리자</title>
<link type="text/css" rel="stylesheet" href="/static/admin/css/admin_new.css">
<link rel="shortcut icon" type="image/x-icon" href="/static/admin/img/common/dgb.ico" />

<!-- SecureFrame Start-->

<!-- SecureFrame End-->

<script type="text/javascript" src="/static/admin/js/jquery-1.9.1.min.js"></script>
<script type="text/javascript" src="/static/admin/js/jquery.easing.js"></script>
<script type="text/javascript" src="/static/admin/js/css_browser_selector.min.js"></script>
<script type="text/javascript" src="/static/admin/js/commonEncode.js"></script>
<script type="text/javascript" src="/static/admin/js/common.js"></script>


<%
			
		// CODE
		String CODE = "SF080083";
		// CODE_NAME
		String CODE_NAME = "CODE_NAME";
		// CODE_NAME_ENG
		String CODE_NAME_ENG = "CODE_NAME_ENG";
		// UP_CODE
		String UP_CODE = "UP_CODE";
		// UP_CODE_NAME
		String UP_CODE_NAME = "헝가리";
		// DIVISION
		String DIVISION = "브랜드";

		
%>


<script type="text/javascript">

	$(document).ready(function(){

	});


	function goUpdate(){
		if(validate()){

			var frm  = document.frm;
			frm.METHOD.value = "AdminMtCodeUpd";
			frm.action = "/admin/AdminCodeAction.do";
			EncForm(frm);
			frm.submit();
		}
	}


	function validate(){
		var CODE_NAME = $("input[name='CODE_NAME']").val();
		var DIVISION = $("input[name='DIVISION']").val();

		if( CODE_NAME == '' ){
			alert("코드명을 입력하세요.");
			$("input[name='CODE_NAME']").focus();
			return false;
		}

		if( DIVISION == '' ){
			alert("구분명을 입력하세요.");
			$("input[name='DIVISION']").focus();
			return false;
		}

		return true;	
	}


	// 목록
	function goSearch(){

		var f = document.frm;
		f.METHOD.value = "AdminMtCodeList";
		f.action = "/admin/AdminCodeAction.do";
		EncForm(f);
		f.submit();
	}

</script>
</head>




<body>
	<div class="wrapper">
		<div class="header">
			<jsp:include page="/admin/inc/header_new.jsp" flush="false"> 
				<jsp:param name="menuNo" value="2"/> 
			</jsp:include>
		</div> <!--// header -->

		<div class="container">
			<div class="local-menu">
				<jsp:include page="/admin/inc/leftMenu.jsp" flush="false"> 
					<jsp:param name="menuNo" value="2"/>
					<jsp:param name="subMenuNo" value="4"/>
				</jsp:include>
			</div> <!--// leftarea -->


		<form name="frm" method="post">
			<input type="hidden" name="METHOD" />
			<input type="hidden" name="INIpluginData" />
			<input type="hidden" name="LOGIN_SVC"  value="N"/>
			<input type="hidden" name="menuNo"	value="2" />
			<input type="hidden" name="subMenuNo"	value="4" />
			<input type="hidden" name="CODE"  value="<%=CODE %>" />

			<!-- 조회 조건 -->
			<input type="hidden" name="CODEMENU_SELECT"	value="" />
			<input type="hidden" name="CODE_SELECT"	value="" />
			<input type="hidden" name="PGE_NO"	value="" />

			<div class="content-wrap">
				<h3>코드 관리(미트론)</h3>
				<div class="contents">
					<h4>코드 정보 변경</h4>
					<p class="align-right"><em>* 필수항목입니다.</em></p>
					<br>
					<fieldset class="form-type bbs-form">
						<div class="dl-type tb">
							<dl>
								<dt>
									<label>코드 </label>
								</dt>
								<dd><div><%=CODE %></div></dd>
							</dl>
							<dl>
								<dt>
									<label for="CODE_NAME">코드명 <em>*</em></label>
								</dt>
								<dd><div><input type="text" id="CODE_NAME" name="CODE_NAME" value="<%=CODE_NAME %>" class="wd2"></div></dd>
							</dl>
							<dl>
								<dt>
									<label for="CODE_NAME_ENG">코드영문명 <em>*</em></label>
								</dt>
								<dd><div><input type="text" id="CODE_NAME_ENG" name="CODE_NAME_ENG" value="<%=CODE_NAME_ENG %>" class="wd2"></div></dd>
							</dl>
							<dl>
								<dt>
									<label for="UP_CODE">상위코드 <em>*</em></label>
								</dt>
								<dd><div><input type="text" id="UP_CODE" name="UP_CODE" value="<%=UP_CODE %>" class="wd2"></div></dd>
							</dl>
							<dl>
								<dt>
									<label for="UP_CODE_NAME">상위코드명 <em>*</em></label>
								</dt>
								<dd><div><input type="text" id="UP_CODE_NAME" name="UP_CODE_NAME" value="<%=UP_CODE_NAME %>" class="wd2"></div></dd>
							</dl>
							<dl>
								<dt>
									<label for="DIVISION">구분명 <em>*</em></label>
								</dt>
								<dd><div><input type="text" id="DIVISION" name="DIVISION" value="<%=DIVISION %>" class="wd2"></div></dd>
							</dl>

						</div>
						<br>
						<br>
						<p class="align-center">
							<a href="#none" class="btn h42" onClick="javascript:goUpdate();">수정</a>
							<a href="#none" class="btn h42 neg" onClick="javascript:goSearch();">목록</a>
						</p>
					</fieldset>
				</div><!--// contents -->
			</div><!--// content-wrap -->
			</form>

		</div><!--// container -->

		<div class="footer">
			<jsp:include page="/admin/inc/footer.jsp" flush="false"></jsp:include>
		</div><!--// footer -->
	</div><!--// wrapper -->

<script type="text/javascript" src="/static/admin/js/nProtect.js"></script>

</body>
</html>