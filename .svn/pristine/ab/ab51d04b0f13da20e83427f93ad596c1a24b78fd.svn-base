	$(document).ready(function(){

	});


	function goInsert(){
		if(validate()){

			var frm  = document.frm;

			frm.METHOD.value = "AdminAutoLineupCodeIns";
			frm.action = "/admin/AdminCodeAction.do";
			EncForm(frm);
			frm.submit();
		}
	}


	function validate(){
		var LINE_UP_CD = $("input[name='LINE_UP_CD1']").val();

		if( LINE_UP_CD == '' ){
			alert("라인업코드를 입력하세요.");
			$("input[name='LINE_UP_CD1']").focus();
			return false;
		}

		return true;	
	}

	// 목록
	function goSearch(){

		var f = document.frm;
		f.METHOD.value = "AdminMtCodeList";
		f.action = "/admin/AdminCodeAction.do";
		EncForm(f);
		f.submit();
	}

