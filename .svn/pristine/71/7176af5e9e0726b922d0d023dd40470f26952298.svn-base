textarea:-ms-input-placeholder {
  color: #666;
} /*Internet Explorer 10 이상에 적용*/
textarea::-webkit-input-placeholder {
  color: #666;
} /* Webkit, Blink, Edge에 적용*/
textarea::-moz-placeholder {
  color: #666;
} /* Mozilla Firefox 4 부터 18에 적용*/
textarea::-moz-placeholder {
  color: #666;
} /* Mozilla Firefox 19 이상에 적용*/

input:-ms-input-placeholder {
  color: #666;
} /*Internet Explorer 10 이상에 적용*/
input::-webkit-input-placeholder {
  color: #666;
} /* Webkit, Blink, Edge에 적용*/
input::-moz-placeholder {
  color: #666;
} /* Mozilla Firefox 4 부터 18에 적용*/
input::-moz-placeholder {
  color: #666;
} /* Mozilla Firefox 19 이상에 적용*/

/* Hide HTML5 number type Up and Down arrows. */
input[type="number"]::-webkit-outer-spin-button,
input[type="number"]::-webkit-inner-spin-button {
  -webkit-appearance: none;
  margin: 0;
}

input[type="number"] {
  -moz-appearance: textfield;
}

/*2016-09-30 접근성 관련 수정*/
input[type="radio"]:focus + label {
  outline: 1px dotted #333;
}
input[type="checkbox"]:focus + label {
  outline: 1px dotted #333;
}

input[type="radio"] + label {outline: 1px dotted transparent !important;}
input[type="checkbox"] + label {outline: 1px dotted transparent !important;}

.focus {
  border-color: #77a9fc;
  -webkit-box-shadow: inset 0 0 3px rgba(0, 0, 0, 0.7),
    0 0 3px rgba(0, 144, 255, 0.9);
  box-shadow: inset 0 0 3px rgba(0, 0, 0, 0.7), 0 0 3px rgba(0, 144, 255, 0.9);
}

button {
  border-radius: 4px; border: none;
}

em { font-style: normal;}

