/* End-of 웹접근성 */

/* hidden */
.hidden {
  /* 251030 접근성에 맞지 않아 hidden 속성 변경 */
  position: absolute !important;
  left:0;
  top:0;
  width: 1px;
  height: 1px;
  margin: -1px;
  padding: 0;
  border: 0;
  overflow: hidden;
  clip-path: inset(50%);
  white-space: nowrap;
}
