/* caption */
caption {
  position: relative;
  width: 0;
  height: 0;
  overflow: hidden;
  line-height: 0;
  text-indent: -5000px;
}

/* 테이블 */
table,
th,
td {
  border: none;
  border-spacing: 0;
  border-collapse: collapse;
  word-break: keep-all;
}
td img {
  vertical-align: middle;
}

/* margin */
.mr10 {
  margin-right: 10px;
}

