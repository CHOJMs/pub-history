/*토스트팝업*/
/*20250723 auto에만 사용하던 토스트팝업 전체 적용을 위해 common.css에 옮김 혹시나 주석으로 남김 */
.fixed-guide-area.toast{z-index:100;animation-name: alertShow, alertHide, zIndex-1;animation-delay: 0s, 3s, 4.2s;animation-duration: 0.8s, 0.8s, 0.1s;animation-fill-mode:forwards;}
.toast-msg{position:relative;display:flex;align-items:center;width:100%;min-height:56px;padding:16px 16px 16px 48px;border-radius:8px;background-color:#373a3c;}
.toast-msg:before{position:absolute;top:16px;left:16px;width:24px;height:24px;margin-right:7px;background:url('../img/ui_common/ico_check_on.png') no-repeat;background-size:24px auto;content:''}
.toast-msg.ico-error:before{background-image:url('../img/ui_common/ico_error02.png');}
.toast-msg p{font-size:16px;font-weight:500;text-align:left;color:#fff;}
@media screen and (max-width: 1024px){  
   .toast-msg p{font-size:15px}
}