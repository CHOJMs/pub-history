    <script>
        $('.qna-list > div > ul > li').each(function(){
            $(this).children('a').attr('title','메뉴 열기');
            $(this).children('a').attr('href','javascript:;');
        })
        $('.qna-list > div > ul > li > a').click(function(){

            if(!$(this).parent('li').hasClass('active')){
                $('.qna-list > div > ul > li').each(function(){
                    $(this).removeClass('active');
                    $(this).children('div').removeClass('active').slideUp();
                })
            }

            $(this).parent('.qna-list > div > ul > li').toggleClass('active');
            $(this).siblings('div').slideToggle();

            $.each($(".qna-list > div > ul > li"), function(){
                if($(this).hasClass('active')){
                    $(this).children('a').attr('title','내용 닫기');
                }else{
                    $(this).children('a').attr('title','내용 열기');
                }
            });
            return false;
	    });
    </script>
   <script>
        // 모바일 브라우저 가로크기 체크
        if (document.body.clientWidth < 767) {

            var subNavSwiper = new Swiper('.swipe_tab_box', {
            slidesPerView: 'auto',
            direction: 'horizontal',
            freeMode: true,
            slidesOffsetAfter: 12,
            preventClicks: true,
            preventClicksPropagation: false,
            observer: true,
            observeParents: true
            });

            var $snbSwiperItem = $('.swipe_tab_box .swiper-wrapper .swiper-slide a');

                $snbSwiperItem.click(function(){
                    var target = $(this);
                    $snbSwiperItem.removeClass('on')
                    target.addClass('on');
                    muCenter(target);
                });

            function muCenter(target){
                var snbwrap = $('.swipe_tab_box .swiper-wrapper');
                var targetPos = target.parent("li").position();
                var box = $('.swipe_tab_box');
                var boxHarf = box.width()/2;
                var pos;
                var listWidth=0;

                snbwrap.find('.swiper-slide').each(function(){ listWidth += $(this).outerWidth(); })

                var selectTargetPos = targetPos.left + target.outerWidth()/2;
                if (selectTargetPos <= boxHarf) { // left
                    pos = 0;
                }else if ((listWidth - selectTargetPos) <= boxHarf) { //right : target 절반 이후 영역이 boxHarf 보다 작을경우 right 정렬
                    pos = listWidth-box.width();
                }else {
                    pos = selectTargetPos - boxHarf; // 중앙정렬
                }

                setTimeout(function(){snbwrap.css({
                    "transform": "translate3d("+ (pos*-1) +"px, 0, 0)",
                    "transition-duration": "500ms"
                })}, 100);
            }
        }
    </script>
