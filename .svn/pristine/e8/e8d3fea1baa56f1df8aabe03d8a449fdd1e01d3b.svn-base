    .mob-wrap .section.section-01 .slide-text > div > div:nth-child(3) {right: -122px; top: 0;}
    .mob-wrap .section.section-02 .slide-text > div > div:nth-child(3) {right: -108px; top: 0;}
    .mob-wrap .section.section-03 .slide-text > div > div:nth-child(3) {right: -93px; top: 0;}
    .mob-wrap .section figure {padding-top: 24px;}
    .mob-contents {background: rgb(245, 245, 245);}
    .mob-contents .main-content-01 {padding: 0px 20px 0;}
    .mob-wrap .mob-visual-Swiper {height: calc(100% - 95px); overflow: hidden; position: relative;}
    .mob-wrap .mob-visual-Swiper .panel-01 {height: 100%; background: rgb(245, 245, 245) url(../img/mobile/bg_01.png) center top / 100% no-repeat;}
    .mob-wrap .mob-visual-Swiper .panel-02 {height: 100%; background: rgb(245, 245, 245) url(../img/mobile/bg_02.png) center top / 100% no-repeat; overflow-y: initial;}
    .mob-wrap .mob-visual-Swiper .panel-03 {height: 100%; background: rgb(245, 245, 245) url(../img/mobile/bg_03.png) center top / 100% no-repeat;}

    .mob-wrap .mob-visual-Swiper .panel-02 .section figure img {float: right;}

    .mob-visual-Swiper .swiper-pagination {bottom: 42px !important;}
    .mob-visual-Swiper .swiper-pagination .swiper-pagination-bullet {width: 6px; height: 6px; background: rgb(188, 192, 197); opacity: 1; margin: 0 3px !important;}
    .mob-visual-Swiper .swiper-pagination .swiper-pagination-bullet-active {width: 20px; height: 6px; border-radius: 5px; border: 1px solid rgb(46, 46, 46); background: rgb(46, 46, 46);}

    .eg-flick-viewport {height: initial !important; z-index: 2002 !important;}

