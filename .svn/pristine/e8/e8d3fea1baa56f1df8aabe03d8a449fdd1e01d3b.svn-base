    .small-banner .mobile_btn button.auto{background-repeat:no-repeat; background-position:center center;}
    .small-banner .mobile_btn button.play{ width:12px; height:11px; background:url('../img/main/ico_stop.png') no-repeat 0 0; background-size: cover;}
    .small-banner .mobile_btn button.pause{ width:12px; height:11px; background:url('../img/main/ico_play.png') no-repeat 0 0; background-size: cover;}
    .small-banner .mobile_btn ul.slick-dots li.slick-active button { width: 20px; border: 1px solid rgb(46, 46, 46); background: rgb(46, 46, 46); border-radius: 5px;}
    .small-banner .mobile_btn .slick-slide a{display:block; color:#fff; font-size:18px;}


