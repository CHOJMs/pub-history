.swiper-container.main-visual-bg {position: absolute; left: 0; top: 0; width: 100%; height: 700px; z-index: 1;}
.swiper-container.main-visual-bg .swiper-slide {width: 100%; height: 100%;}
.swiper-container.main-visual-bg .swiper-slide.bg-01 {background: url(../img/main/bg_01.png) center center / cover no-repeat;}
.swiper-container.main-visual-bg .swiper-slide.bg-02 {background: url(../img/main/bg_02.png) center center / cover no-repeat;}
.swiper-container.main-visual-bg .swiper-slide.bg-03 {background: url(../img/main/bg_03.png) center center / cover no-repeat;}

.main-contents {max-width: 1920px; width: 100%; display: flex; flex-wrap: wrap; justify-content: center; background: rgb(245, 245, 245);}
.main-contents > div {width: calc(33.333% - 31.333px); flex: 1;}
.main-contents > div + div {margin-left: 47px;}
.main-content-01 {display: flex; flex-direction: column;}
.btn-color a {display: inline-flex; justify-content: space-between; align-items: center; width: 100%; padding: 0 40px; height: 100px; background: rgb(88, 102, 116) url(../img/main/ico_move2.png) right 40px center / 28px no-repeat; font-size: 24px; font-weight: bold; letter-spacing: -0.05em; color: #fff; border-radius: 8px;}

.list-wrap {margin-top: 30px;}
.list-wrap ul {position: relative; padding: 0 40px; display: flex; flex-wrap: wrap; background: #fff; box-shadow: 0 0 12px rgba(0, 0, 0, 0.05); border-radius: 8px;}
.list-wrap ul::before {content: ""; display: block; position: absolute; left: 0; top: 50%; transform: translateY(-50%); width: 100%; height: 1px; background: rgb(232, 232, 232);}
.list-wrap ul li {display: inline-flex; justify-content: flex-start; align-items: center; width: 50%; height: 93.5px;}
.list-wrap ul li:nth-of-type(2n) {padding-left: 40px;}
.list-wrap ul li a {display: inline-flex; align-items: center; line-height: 34px; font-size: 22px; font-weight: bold; color:rgb(46, 46, 46);}
.list-wrap ul li a .list-count {margin-left: 8px; display: inline-flex; justify-content: center; align-items: center; width: 34px; height: 34px; line-height: 1; font-size: 20px; font-weight: bold; color: #fff; background: rgb(18, 151, 239); border-radius: 100%;}

.small-banner-wrap {margin-top: 30px; width: 100%; padding-bottom: 26px !important;}
.small-banner-wrap .swiper-slide a { display: inline-block; width: 100%; margin-left: 1px; padding: 36px 44px; background: rgb(231, 227, 222); border-radius: 8px;}
.small-banner-wrap .swiper-slide p {font-size: 24px; font-weight: bold; color: rgb(46, 46, 46); letter-spacing: -0.05em;}
.small-banner-wrap .swiper-slide span {display: block; margin-top: 8px; font-size: 20px; color: rgb(46, 46, 46); letter-spacing: -0.05em;}

.small-banner-wrap .swiper-slide a:focus {outline: 1px dotted #333;}

.small-banner-wrap.swiper-container .swiper-pagination {bottom: 0;}
.small-banner-wrap.swiper-container .swiper-pagination span {margin: 0 4px; width: 10px; height: 10px; background: none; border: 1px solid rgb(188, 192, 197); border-radius: 100%; opacity: 1;}

.small-banner-wrap.swiper-container .swiper-pagination span.swiper-pagination-bullet-active {width: 30px; border-radius: 5px; background: rgb(46, 46, 46); border-color: rgb(46, 46, 46);}

.main-content-02 > div {display: flex; flex-direction: column; justify-content: space-between; padding: 40px; width: 100%; height: 232px; box-shadow: 0 0 12px rgba(0, 0, 0, 0.05); background: #fff;}
.main-content-02 > div + div {margin-top: 30px;}
.main-content-02 > div:nth-of-type(1) {background: rgb(255, 255, 255) url(../img/main/main_img_01.png) right 33px bottom / 165px no-repeat; border-radius: 8px;}
.main-content-02 > div:nth-of-type(2) {background: rgb(255, 255, 255) url(../img/main/main_img_02.png) right 46.5px bottom 40px / 198px no-repeat; border-radius: 8px;}
.main-content-02 > div p {font-size: 24px; font-weight: bold; letter-spacing: -0.05em; color: rgb(34, 36, 38);}
.main-content-02 > div span {display: block; margin-top: 10px; font-size: 20px; letter-spacing: -0.05em; color: rgb(34, 36, 38);}
.main-content-02 > div a {display: inline-flex; align-items: center; font-size: 20px; letter-spacing: -0.05em; font-weight: 500; color:rgb(18, 151, 239);}
.main-content-02 > div a::after {content: ""; display: inline-flex; margin-top: 1px; width: 24px; height: 24px; background: url(../img/main/ico_link.png) center center / cover no-repeat;}

.main-box-03 {position: relative; padding: 40px; height: 380px; box-shadow: 0 0 12px rgba(0, 0, 0, 0.05); background: #fff; border-radius: 8px;}
.main-box-03 p {line-height: 1.46; font-size: 26px; font-weight: bold; letter-spacing: -0.05em;}
.main-box-03 span {position: absolute; right: 40px; bottom: 40px;}
.main-box-03 span a {font-size: 20px; font-weight: 500; letter-spacing: -0.05em; color:rgb(18, 151, 239);}

.flip-count ul {margin: 33.5px auto 0; display: flex; justify-content: center; flex-wrap: wrap;}
.flip-count ul li {position: relative; display: inline-flex; justify-content: center; align-items: center; width: 70px; height: 104px; border: 1px solid rgb(34, 36, 38); transition: transform 0.6s; transform-style: preserve-3d; cursor: default; animation: 0.5s ease-out infinite alternate filpCount; box-shadow: 0 4px 8px rgba(0, 0, 0, 0.16); -webkit-backface-visibility: visible; backface-visibility: visible;}
.flip-count ul li {position: relative; display: inline-flex; justify-content: center; align-items: center; width: 70px; height: 104px; border: 1px solid rgb(34, 36, 38); transition: transform 0.6s; transform-style: preserve-3d; cursor: default; /*animation: 0.5s ease-out infinite alternate filpCount;*/ box-shadow: 0 4px 8px rgba(0, 0, 0, 0.16); -webkit-backface-visibility: visible; backface-visibility: visible;}/* WA */
.flip-count ul li:nth-of-type(1) {animation-delay: 0.1s;}
.flip-count ul li:nth-of-type(2) {animation-delay: 0.4s;}
.flip-count ul li:nth-of-type(3) {animation-delay: 0.2s;}
.flip-count ul li:nth-of-type(4) {animation-delay: 0.3s;}
.flip-count ul li + li {margin-left: 10px;}
.flip-count ul li span::after {content: ""; display: block; position: absolute; top: 50%; transform: translateY(-50%); left: 0; width: 100%; height: 1px; background: rgb(34, 36, 38); z-index: 3;}
.flip-count ul li span {font-family: 'Play'; font-size: 83px; font-weight: bold; text-align: center;}
.flip-count ul li span.front {position: absolute; left: 0; top: 0; width: 100%; height: 100%; background: rgb(255, 255, 255); line-height: 1.25;}
.flip-count ul li span.back {transform: rotateX(180deg) perspective(1000px); position: absolute; left: 0; top: 0; width: 100%; height: 100%; color: rgb(255, 255, 255); background: rgb(90, 94, 99); z-index: 1; line-height: 1.25;}
.flip-count ul li span.back {/*transform: rotateX(180deg) perspective(1000px);*/ position: absolute; left: 0; top: 0; width: 100%; height: 100%; color: rgb(255, 255, 255); background: rgb(90, 94, 99); z-index: 1; line-height: 1.25;}/* WA */
.flip-count ul li span.back::before, .flip-count ul li span.front::before { display: inline-flex; content: "?";}

.main-content-03 figure {position: relative; width: 100%; height: 320px; background: none; overflow: hidden;}
.main-content-03 figure span {position: absolute; width: auto; /*top: -50%; animation: 1s ease-in infinite coinDown; animation-play-state: running;*/} /* running*//* WA */
.main-content-03 figure span {position: absolute; width: auto; top: -50%; animation: 1s ease-in infinite coinDown; animation-play-state: running;} /* running*/
.main-content-03 figure span:nth-child(1) {left: 10%; animation-delay: 0.1s;}
.main-content-03 figure span:nth-child(2) {left: 35%; animation-delay: 0.1s;}
.main-content-03 figure span:nth-child(3) {left: 80%; animation-delay: 0.3s;}
.main-content-03 figure span:nth-child(4) {left: 65%; animation-delay: 0.3s;}
.main-content-03 figure span:nth-child(5) {left: 20%; animation-delay: 0.6s;}
.main-content-03 figure span:nth-child(6) {left: 50%; animation-delay: 0.6s;}
.main-content-03 figure span:nth-child(7) {left: 45%; animation-delay: 0.4s;}

.main-content-03 figure span:nth-child(1) {left: 10%; animation-delay: 0.1s; top: 10%;}/* WA */
.main-content-03 figure span:nth-child(2) {left: 35%; animation-delay: 0.1s; top: 35%;}/* WA */
.main-content-03 figure span:nth-child(3) {left: 80%; animation-delay: 0.3s; top: 48%;}/* WA */
.main-content-03 figure span:nth-child(4) {left: 65%; animation-delay: 0.3s; top: 5%;}/* WA */
.main-content-03 figure span:nth-child(5) {left: 20%; animation-delay: 0.6s; top: 65%;}/* WA */
.main-content-03 figure span:nth-child(6) {left: 50%; animation-delay: 0.6s; top: 15%;}/* WA */
.main-content-03 figure span:nth-child(7) {left: 45%; animation-delay: 0.4s; top: 60%;}/* WA */
.main-content-03 figure img {width: 50%;}

@keyframes filpCount {
    from {
        transform: rotateX(0);
    }
    to {
        transform: rotateX(180deg);
    }
}

@keyframes coinDown {
    from {
        top: -50%
    }
    to {
        top: 100%;
    }
}

/* mydgb */

/* body {overflow: hidden;} */

body {transition: all 0.5s;}

.mydgb-wrap.active {left: 60px; overflow-y: auto;}
.mydgb-wrap {position: fixed; left: 100%; top: 0; padding: 100px 0 20px 120px; width: calc(100% - 60px); height: 100vh; height: calc(var(--vh, 1vh) * 100); background: rgb(34, 36, 38); color: rgb(255, 255, 255); z-index: 999; transition: all 0.5s;}
.mydgb-wrap::after {visibility: hidden; content: ""; display: block; position: fixed; left: -100%; top: 0; width: 60px; height: 100%; z-index: 99; background: rgb(245, 245, 245); transition: 0.5s ease;}
.mydgb-wrap.active::after {visibility: visible; height: 100%; left: 0;}
.mydgb-close.active {left: 0; display: block;}
.mydgb-close.active a { display: block;}
.mydgb-close.active a:focus img {border:1px dotted rgb(255, 255, 255)}
.mydgb-close {position: fixed; left: -100%; top: 0; z-index: 998; transition: all 0.5s; display: none;}
.mydgb-close img {max-width: 60px;}

/* mydgb 타이틀 */
.mydgb-wrap h2 {line-height: 1; font-size: 48px; font-weight: bold; color: rgb(255, 255, 255);}
.mydgb-wrap h2 + p {margin-top: 16px; line-height: 1.5; font-size: 28px; letter-spacing: -0.05em; color: rgb(255, 255, 255);}
.mydgb-wrap h3 {line-height: 1; font-size: 38px; font-weight: bold; color: rgb(255, 255, 255);}
.mydgb-wrap h3 + p {margin-top: 24px; line-height: 1.5; font-size: 20px; letter-spacing: -0.05em; color: rgb(255, 255, 255);}
.mydgb-wrap p {}

/* mydgb 버튼 */
.mydgb-wrap .my-btn {margin-top: 40px; display: inline-flex; justify-content: center; align-items: center; width: 160px; height: 54px; border-radius: 27px; line-height: 1; font-size: 20px; font-weight: bold; letter-spacing: -0.05em;}
.mydgb-wrap .my-btn.my-btn-01 {color: rgb(255, 255, 255); background: rgb(18, 151, 239);}
.mydgb-wrap .my-btn.my-btn-02 {color: rgb(255, 255, 255); background: none; border: 1px solid rgb(255, 255, 255);}
.mydgb-wrap .my-btn.my-btn-03 {width: auto; padding: 0 24px; height: 49px; color: rgb(255, 255, 255); background: rgb(18, 151, 239); border-radius: 0;}
.mydgb-wrap a:focus {outline: 2px dotted #fff;}
/* mydgb 컨텐츠 */


.mydgb-section-01 {}
.mydgb-section-01::after {content: ""; display: block; margin: 80px auto; width: 100%; height: 5px; background: rgb(255, 255, 255);}

.mydgb-section-01 {display: flex; flex-wrap: wrap; justify-content: space-between;}
.mydgb-section-01 .section-title {line-height: 1.5; font-size: 28px; letter-spacing: -0.05em;}
.mydgb-section-01 .left {max-width: 840px; width: 100%;}
.mydgb-section-01 .left ul.btn-list {margin-top: 48px; display: flex; flex-wrap: wrap;}
.mydgb-section-01 .left ul.btn-list li + li {margin-left: 30px;}
.mydgb-section-01 .left ul.btn-list li a {display: inline-flex; justify-content: center; align-items: center; padding: 27px 40px; line-height: 1; font-size: 20px; font-weight: 500; color: rgb(255, 255, 255); background: rgb(18, 151, 239);}
.mydgb-section-01 .left ul.btn-list li a::after {margin-left: 10px; margin-top: 1px; display: inline-flex; content: ""; width: 20px; height: 20px; background: url(../img/main/ico_small_arrow.png) center center / cover no-repeat;}

.mydgb-section-01 .mydgb-small-banner .swiper-slide {}
.mydgb-section-01 .mydgb-small-banner .swiper-slide a {padding: 20px 24px; display: flex; flex-direction: column; background: rgb(224, 229, 216); border-radius: 4px 0 0 4px;}
.mydgb-section-01 .mydgb-small-banner .swiper-slide a .stit {line-height: 1.5; font-size: 16px; font-weight: 700; color: rgb(34, 36, 38);}
.mydgb-section-01 .mydgb-small-banner .swiper-slide a .stxt {margin-top: 6px; line-height: 1.5; font-size: 13px; font-weight: 400; color: rgb(34, 36, 38);}

.mydgb-section-01 .right {max-width: 395px; width: 50%;}
.mydgb-section-01 .right ul {margin-bottom: 30px; display: flex; flex-wrap: wrap; align-items: center;}
.mydgb-section-01 .right ul li {width: 50%; border-bottom: 1px solid rgb(112, 116, 120);}
.mydgb-section-01 .right ul li a {display: inline-flex; align-items: center; height: 62px; line-height: 1; font-size: 18px; font-weight: 400; letter-spacing: -0.05em; color: rgb(255, 255, 255);}
.mydgb-section-01 .right ul li a::before {content: ""; display: inline-block; margin-right: 12px; width: 20px; height: 20px;}
.mydgb-section-01 .right ul li:nth-of-type(1) a::before {background: url(../img/main/ico_calculator.png) center center / 100% no-repeat;}
.mydgb-section-01 .right ul li:nth-of-type(2) a::before {background: url(../img/main/ico_document.png) center center / 100% no-repeat;}
.mydgb-section-01 .right ul li:nth-of-type(3) a::before {background: url(../img/main/ico_shield.png) center center / 100% no-repeat;}
.mydgb-section-01 .right ul li:nth-of-type(4) a::before {background: url(../img/main/ico_percent.png) center center / 100% no-repeat;}

.mydgb-section-01 .mydgb-small-banner {padding-bottom: 16px !important;}
.mydgb-section-01 .swiper-pagination.swiper-pagination-bullets {bottom: 0; text-align: left;}
.mydgb-section-01 .swiper-pagination.swiper-pagination-bullets span {margin: 0 0; width: 14px; height: 4px; border: 1px solid rgb(112, 116, 120); background: none; opacity: 1; border-radius: 1px;}

.mydgb-section-01 .swiper-pagination.swiper-pagination-bullets span.swiper-pagination-bullet-active {width: 20px; background: rgb(255, 255, 255); border-color: rgb(255, 255, 255);}
.mydgb-section-01 .swiper-pagination.swiper-pagination-bullets span + span {margin-left: 4px;}

.mydgb-section-01 .line-text {margin-bottom: 50px; display: inline-flex; line-height: 1.2; font-size: 24px; font-weight: bold; letter-spacing: -0.05em; border-bottom: 1px solid rgb(255, 255, 255);}
.mydgb-section-01 .input-title {display: flex; align-items: center; line-height: 1; font-size: 30px; font-weight: 500; letter-spacing: -0.05em; color: rgb(255, 255, 255);}
.mydgb-section-01 .input-title span {margin-left: 10px; display: inline-flex; align-items: center; height: 36px; padding: 0 22px; line-height: 1; font-size: 19px; font-weight: 500; letter-spacing: -0.05em; color: rgb(34, 36, 38); background: rgb(255, 243, 165); border-radius: 18px;}

.amount-wrap + .amount-wrap::before {content: ""; display: block; margin: 40px auto; width: 100%; height: 1px; background: rgb(112, 116, 120);}
.amount-detail {display: flex; align-items: center; justify-content: space-between; margin-top: 24px;}
.amount-detail p {line-height: 1; font-size: 23px; letter-spacing: -0.05em; color: rgb(255, 255, 255);}
.amount-detail a {display: inline-flex; line-height: 1.2; font-size: 16px; font-weight: 500; letter-spacing: -0.05em; color: rgb(255, 255, 255); border-bottom: 1px solid rgb(255, 255, 255);}
.amount-field {margin-top: 16px; position: relative; display: inline-flex; align-items: center; max-width: 840px; width: 100%; padding: 0 50px; height: 84px; text-align: right; background: rgb(58, 61, 64); border-radius: 8px;}
.amount-field input[type="text"] {border: 0; color: rgb(255, 255, 255); line-height: 1; font-size: 40px; font-weight: bold; text-align: right;}
.amount-field span {display: block; position: absolute; right: 50px; top: calc(50% + 5px); transform: translate(120%, -50%); line-height: 1; font-size: 26px; font-weight: bold; color: rgb(255, 255,255); letter-spacing: -0.05em;}
.amount-date {display: flex; align-items: center; margin-top: 20px;}
.amount-date p {font-size: 23px; letter-spacing: -0.05em;}
.amount-date p + p {margin-left: 60px;}
.amount-date p span {font-size: 25px;}
.amount-btn a + a {margin-left: 16px;}

.mydgb-section-01.line-01::after {display: block; content: ""; margin: 60px auto; width: 100%; height: 8px; background: url(../img/main/line_01.png) center center / 100% repeat;}

