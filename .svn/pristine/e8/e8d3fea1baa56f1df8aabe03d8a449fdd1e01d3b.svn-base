@charset "UTF-8";

.wrap { padding-bottom: 0;}
#common_body {padding-bottom: 0px;}
h1.h1_tl { font-size: 0;}

/*sut_layout*/
.container {position: relative; width: 100%; padding: 95px 140px 0 60px; margin: 0 auto;}

#header::after {display: none;}
#header.sub { border-bottom:none; width: calc(100% - 80px);}
#header.main h1.logo { display: none;}
#header.sub h1.logo {display: none;}
#header.sub .header-inner > div.right { margin-right: 48px;}
#header.main.active  .header-inner::after {content: ""; display: inline-block; position: absolute; top: 92px; left: 60px; width: calc(100% - 120px); height: 4px; background-color: #000;}

.header-inner > div > ul > li > a::after {content: ""; display: block; position: absolute; bottom: -4px; left: 0; width: 100%; transform-origin: 50% 0; transform: scaleX(0); height: 4px; background: rgba(71, 179, 251, 0); transition: all 0.3s; border-radius: 2px; z-index: 2;}

.main-line, .sub-line { width: calc(100% - 200px);}

.header-inner { width: calc(100% - 80px);}

.container.bg-gray {padding-bottom: 0;}
.flex-wrap {display: flex; flex-wrap: wrap;}

.side-bar.active {left: 0;}
.side-bar {position: absolute; top: 0; left: -80px; padding-top: 40px; width: 80px; height: 100vh; height: calc(var(--vh, 1vh) * 100); background: rgb(34, 36, 38); z-index: 1000; cursor: pointer; transition: all 0.5s;}
.side-bar div {width: 100%; display: flex; align-items: center; transform-origin: 0 0; transform: rotate(90deg) translateY(-59px);}
.side-bar div a { display: flex;}
.side-bar div a:focus {outline: 1px dotted rgb(255, 255, 255);}/* WA */
.side-bar div a p {font-size: 22px; font-weight: bold; letter-spacing: 0.05em; color: #fff; white-space: nowrap; text-transform: uppercase;}
.side-bar div a span {display: inline-flex; margin-left: 24px; font-size: 18px; letter-spacing: -0.05em; color:rgb(191, 200, 201);}
.side-bar + div { display: none;}
.side-bar.active + div { display: block;}
.mydgb-section-01 .left ul.btn-list li a:focus {outline: 1px dotted rgb(255, 255, 255);}

.slide-arrow {display: none;}

.main-visual-wrap {max-width: 1720px; width: 100%;}
.main-visual-wrap .swiper-slide {position: relative; width: 100%; height: 700px; text-align: center; visibility: hidden;}
.main-visual-wrap .swiper-slide figure {display: inline-flex; height: 100%; margin: 0 auto;}
.main-visual-wrap .swiper-slide figure img {width: auto; height: 100%;}
.main-visual-wrap .swiper-slide.swiper-slide-active { visibility: visible;}
/* .main-visual-wrap .swiper-slide::after {content: ''; position: absolute; left: 0; top: 88px; width: 100%; height: 1px; background: red;} */
.main-visual-wrap .slide-text {position: absolute; left: 0; top: 50px; color: rgb(34, 36, 38); text-align: left;}
.main-visual-wrap .slide-text h2 {line-height: 68px; font-size: 80px; font-weight: bold; font-family: "KOHO";}
.main-visual-wrap .slide-text p.tit {margin-top: 18px; font-size: 40px; font-weight: bold; letter-spacing: -0.05em;}
.main-visual-wrap .slide-text p.txt {margin-top: 8px; font-size: 26px; font-weight: 400; letter-spacing: -0.05em; color: #222426;}
.main-visual-wrap .slide-text a, .main-visual-wrap .slide-text button {margin-top: 40px; display: inline-flex; justify-content: center; align-items: center; width: 64px; height: 64px; background: rgb(34, 36, 38) url(../img/main/ico_move.png) center center / 50% no-repeat; border-radius: 100%; font-size: 0;}

.main-visual-wrap.swiper-container {z-index: 10;}
.main-visual-wrap.swiper-container .swiper-pagination {display: inline-flex;flex-direction: column;bottom: auto;left: auto;top: 50px;right: 0;width: auto;text-align: left;}
.main-visual-wrap.swiper-container .swiper-pagination .swiper-pagination-bullet.swiper-pagination-bullet-active {opacity: 1;}
.main-visual-wrap.swiper-container .swiper-pagination .swiper-pagination-bullet {margin: 0 0; margin-top: 16px; padding: 16px 0 16px; width: 290px; height: auto; background: none; border-radius: 0; font-size: 24px; font-weight: bold; letter-spacing: -0.05em; color:rgb(34, 36, 38); border-bottom: 2px solid rgb(34, 36, 38); opacity: .4;}
.main-visual-wrap.swiper-container .swiper-pagination .swiper-pagination-bullet {margin: 0 0; margin-top: 16px; padding: 16px 0 16px; width: 290px; height: auto; background: none; border-radius: 0; font-size: 24px; font-weight: bold; letter-spacing: -0.05em; color:rgb(34, 36, 38); border-bottom: 2px solid rgb(34, 36, 38); opacity: .7;}/* WA */
.main-visual-wrap.swiper-container .swiper-pagination .swiper-pagination-bullet span {display: block; margin-top: 5px; font-size: 20px; font-weight: 400; letter-spacing: -0.05em;}
.main-visual-wrap.swiper-container .swiper-pagination .swiper-pagination-bullet h3 {font-size: 24px; margin-top: 0;}

