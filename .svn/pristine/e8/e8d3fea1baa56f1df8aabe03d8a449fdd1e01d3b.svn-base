    /* ----------------------------------------------------------
    2024-10 리뉴얼
    ---------------------------------------------------------- */

    /* main */
    .main-body{background:#f2f5f8}
    .main-body header .header-mobile{background:#f2f5f8}
    .main-content{padding:64px 0 48px}
    .main-content .main-inner{width:auto;padding:0 20px}
    .main-content .main-top{display:block;}
    .main-content .main-top > li:nth-child(1){width:auto}
    .main-content .main-top > li:nth-child(2){width:auto}
    .main-content .main-banner-wrap{width:100%;height:auto}
    .main-banner-wrap{width:100%;height:80px}
    .main-banner .mb-li a{padding:15px 24px;width:100%;height:80px;background-color:#fff}
    .main-banner .mb-li.page2 a{background:#fff}
    .main-banner .tx01,
    .main-banner .tx03{display:none}
    .main-banner .tx02{margin-top:7px;font-size:inherit;line-height:24px}
    .main-banner .tx02 .t1{display:block;font-size:12px;color:#5c6773;font-weight:500;line-height:1.2}
    .main-banner .tx02 .t2{display:block;margin-top:4px;font-size:15px;font-weight:600;line-height:1.2}
    .main-banner .img01{top:8px;right:24px}
    .main-banner .img01 img{width:64px}
    .main-banner .slick-dots{top:12px;right:12px}
    .main-banner .slick-dots > li{width:4px;height:4px}
    .main-banner .slick-dots .slick-active{width:11px}
    .main-banner-wrap .controler{display:none}
    .top-sub-bn{display:flex;justify-content:space-between;margin-top:16px}
    .top-sub-bn > li{width:calc(50% - 7px)}
    .top-sub-bn > li + li{margin-top:0}
    .top-sub-bn > li > a{width:auto;height:204px;padding:20px;box-shadow: 0 4px 8px 0 rgba(13, 31, 54, 0.16);}
    .top-sub-bn > li:nth-child(2) > a:after{bottom:56px;right:20px;width:72px;height:56px;background-image:url('../img/main/img_main_top02_mo.png');background-size:72px auto}
    .top-sub-bn > li:nth-child(1) .img{bottom:56px;right:20px;width:64px;height:39px;}
    .top-sub-bn > li:nth-child(1) .img .money{width:64px;height:39px;background-size:64px auto}
    .top-sub-bn > li:nth-child(1) .img .coin01{bottom:26px;right:5px;width:20px;height:18px;background-size:18px auto}
    .top-sub-bn > li:nth-child(1) .img .coin02{bottom:40px;right:20px;width:17px;height:15px;background-size:15px auto}
    .top-sub-bn > li:nth-child(1) .img .coin03{bottom:52px;right:3px;width:13px;height:10px;background-size:11px auto;}
    .top-sub-bn > li > a p{font-size:18px;line-height:26px}
    .top-sub-bn > li > a .tx{height:48px;line-height:48px;font-size:15px;font-weight:600}

    .main-menu-slide{margin-top:0;padding:22px 0;}
    .main-menu-slide .slick-list{padding:10px 20px}
    .main-menu-slide .m-s-ul .m-s-li a{font-size:14px}
    .main-menu-slide .m-s-ul .m-s-li a:before{margin-bottom:0;border:0;background-color:transparent;width:48px;height:48px;box-shadow:none;}
    .main-menu-slide .m-s-ul .m-s-li a span{top:0;padding:0 2px 0 6px;height:18px;font-size:10px;line-height:18px;border-radius:9px;}

    /* main - 내게 맞는 대출 찾기 */
    .main-loan-product{padding:0 20px;background:transparent}
    .main-loan-product .main-inner{padding:24px;background:#fff;box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.04);border-radius:12px;}
    .main-loan-tit{display:block;}
    .main-sub-tit{font-size:20px;}
    .main-loan-btn{margin-top:24px}
    .main-loan-btn > button{background:#efefef;line-height:34px;font-weight:500;font-size:14px}
    .main-loan-btn > button:before{display:none}
    .main-loan-btn > button + button{margin-left:6px}
    .loan-prod-arrow{display:none}
    .loan-prod-ul .loan-prod-li + .loan-prod-li{margin-top:24px}
    .loan-prod-ul .loan-prod-li > a{position:relative;padding:0 0 0 64px;height:auto;background:transparent}
    .loan-prod-ul .loan-prod-li > a:before{position:absolute;top:0;left:0;width:48px;height:48px;border-radius:18px;background:url('../img/common/ico_menu1_1.svg') no-repeat 50% 50% #f1f4f3;background-size:24px auto;content:''}
    .loan-prod-ul .loan-prod-li > a.type:before{background-image:url('../img/common/ico_menu1_2.svg')}
    .loan-prod-ul .loan-prod-li > a.type2:before{background-image:url('../img/common/ico_menu1_5.svg')}
    .loan-prod-ul .loan-prod-li > a.type3:before{background-image:url('../img/common/ico_menu1_3.svg')}
    .loan-prod-ul .loan-prod-li > a.type4:before{background-image:url('../img/common/ico_menu1_4.svg')}
    .loan-prod-ul .loan-prod-li > a.type5:before{background-image:url('../img/common/ico_menu1_6.svg')}
    .loan-prod-ul .loan-prod-li > a:after{position:absolute;top:50%;right:0;transform:translateY(-50%);width:16px;height:16px;background:url('../img/main/ico_detail_arrow.png') no-repeat;background-size:16px auto;content:''}
    .loan-prod-ul .loan-prod-li:hover > a,
    .loan-prod-ul .loan-prod-li:focus > a{box-shadow:none;transform:none}
    .loan-prod-ul .loan-prod-li .tx1 span{font-weight:600}
    .loan-prod-ul .loan-prod-li .tx1 span,
    .loan-prod-ul .loan-prod-li .tx3{display:none}
    .loan-prod-ul .loan-prod-li .tx1{font-size:16px;}
    .loan-prod-ul .loan-prod-li .tx2{overflow:hidden;margin-top:0;padding-right:20px;font-size:14px;line-height:22px;font-weight:400;white-space:nowrap;text-overflow:ellipsis;word-break:break-all;}
    .btn-loan-all{font-size:14px;line-height:48px}

    /* main - 이런 서비스는 어떠세요 */
    .main-service-wrap{margin-top:48px}
    .service-ul{flex-wrap:wrap;margin-top:14px}
    .service-ul > li{width:calc(50% - 5px);margin-top:10px}
    .service-ul > li > a{padding:28px 0}
    .service-ul > li > a:before{width:56px;height:56px;background-size:56px auto}
    .service-ul .tx1{margin-top:16px;font-size:13px}
    .service-ul .tx2{margin-top:8px;font-size:16px}
    .ser-banner{margin-top:16px;padding:13px 36px 10px 24px;height:48px;background-color:#ebeef2}
    .ser-banner .name{font-size:14px}
    .ser-banner .name img{margin-top:0}
    .ser-banner .tag{display:none}
    .ser-banner strong{max-width:calc(100% - 110px);margin-left:8px;font-size:14px;font-weight:600}
    .ser-banner button{width:32px;height:32px;background-size:16px auto;right:8px}

    /* 오늘의 새소식 */
    .main-bottom-cont{margin-top:48px}
    .main-bottom-cont .main-inner{display:block}
    .main-bottom-cont .mbc-left,
    .main-bottom-cont .mbc-right{width:100%;height:auto;padding:24px;border-color:#f7f7f7;box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.04);}
    .main-news{margin-top:20px}
    .main-news > li > a{padding-right:0;background:none}
    .main-news > li > a strong{display:block;font-size:15px}
    .main-news > li > a strong em{text-overflow:ellipsis;white-space:nowrap;height:auto}
    .main-news > li > a strong:after{display:none}
    .main-news > li > a span{position:relative;display:inline-block;margin-top:6px;font-size:13px}
    .main-news > li.new > a span:after{position:absolute;top:0;right:-10px;width:6px;height:6px;border-radius:50%;background-color:#ef4040;content:''}
    .main-news > li + li{margin-top:24px}

    /* 고객서비스 */
    .mbc-right{margin-top:24px}
    .cus-service{margin-top:0}
    .cus-service > li{width:100%}
    .cus-service > li > a{position:relative;height:48px;padding-left:64px;background-size:48px auto;font-size:16px;font-weight:600}
    .cus-service > li > a:after{position:absolute;top:50%;right:0;margin-top:-8px;width:16px;height:16px;background:url('../img/ui_common/ico_arrow.png') no-repeat;background-size:16px auto;content:''}
    .cus-service > li > a span{margin-top:2px;font-size:14px}

    /* 하단배너 */
    .main-bottom-bn{margin-top:24px}
    .bottom-bn-in{height:108px}
    .bottom-bn-in:after{width:132px;height:132px;top:4px;right:0;background-image:url('../img/main/bg_ico.png');background-size:132px auto}
    .bottom-bn-in > p{padding-left:24px;font-size:20px;line-height:28px}

    /* 쉬운 홈 */
    .main-content.easy .top-sub-bn > li > a .tx{display:none}
    .main-content.easy .top-sub-bn > li > a:after{top:auto;bottom:20px}
    .main-content.easy .top-sub-bn > li:nth-child(1) .img{bottom:20px}
    .main-content.easy .top-sub-bn > li > a p{font-size:20px}
    .main-content.easy .main-menu-slide,
    .main-content.easy .main-loan-product,
    .main-content.easy .main-service-wrap,
    .main-content.easy .main-bottom-cont,
    .main-content.easy .main-bottom-bn,
    .main-content.easy .sns-wrap02{display:none !important}
    .main-content.easy .easy-link-wrap{display:block}

}
