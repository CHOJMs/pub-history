.organization-chart .bottom.device-pc > ul li.col-4 .sub-col-wrap ul {display: flex; width: 100%; max-width: 188px; min-width: 188px; flex-direction: column; justify-content: flex-start; margin-top: 0; padding: 11px 0; border-radius: 0 0 8px 8px; border: 1px solid #dfe3e8; border-bottom: none;}
.organization-chart .bottom.device-pc > ul li.col-4 .sub-col-wrap ul li {line-height: 1.6; font-size: 15px; color: #707478; padding-left: 24px; letter-spacing: -0.05em; margin-top: 0; border: none;width: inherit; min-width: auto; max-width: 188px; height: inherit; justify-content: inherit;}
.organization-chart .bottom.device-pc > ul li.col-4 .sub-col-wrap ul li::before {display: inline-flex; margin: 0 5px 0 2px; content: "·"; line-height: 1.6; font-size: 17px; font-weight: 400; color: rgb(112, 116, 120);}
.organization-chart .bottom.device-pc ul li p.sil {background: #f5f5f5;}

