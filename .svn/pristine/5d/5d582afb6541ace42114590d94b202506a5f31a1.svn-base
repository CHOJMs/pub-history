@charset "UTF-8";


/* 회사소개 */
.history-box {padding: 40px 40px 0 40px; border-radius: 16px; background: #f1f5fa;}
.history-box > ul > li {padding-left: 24px; display: flex; flex-wrap: wrap; align-items: flex-start; overflow: hidden;}
.history-box > ul > li + li {padding-top: 24px;}
.history-box > ul > li strong {position: relative; display: inline-flex; width: 116px; line-height: 1; font-size: 20px; font-weight: 600; color: #1571eb;}
.history-box > ul > li strong::before {content: ""; display: block; position: absolute; left: -24px; top: 50%; transform: translateY(-50%); width: 10px; height: 10px; background: #1a3755; border-radius: 16px; z-index: 1;}
.history-box > ul > li strong::after {content: ""; display: block; position: absolute; left: -20px; top: 10px; width: 2px; height: 100vh; background: #dfe3e8;}
.history-box > ul > li + li strong::after {top: -40px}
.history-box > ul > li ul {display: flex; flex-direction: column;}
.history-box > ul > li ul li {display: inline-flex; flex-wrap: wrap; align-items: flex-start; line-height: 1.3; font-size: 17px; font-weight: bold;}
.history-box > ul > li ul li + li {margin-top: 11px;}
.history-box > ul > li ul li span {display: inline-flex; margin-right: 20px; line-height: 1.3; font-size: 16px; font-weight: 600;}
.history-box > ul > li ul li p {line-height: 1.3; font-size: 17px; color: #5c6773; font-weight: 400;}
.history-box > ul > li:last-child ul {margin-bottom: 40px;}

.business-box {display: flex; justify-content: space-between; flex-wrap: wrap;}
.business-box li {position: relative; display: inline-flex; width: calc(50% - 12px); height: 216px;}
.business-box li:nth-child(2) ~ li {margin-top: 16px;}
.business-box li span {font-size: 18px; font-weight: 700; color: #fff; position: absolute; left: 50%; bottom: 17px; transform: translateX(-50%);}
.business-box li:nth-child(1) {background: url('../img/ui_common/img_his_pc01.png') no-repeat 0 0; background-size: 388px auto;}
.business-box li:nth-child(2) {background: url('../img/ui_common/img_his_pc02.png') no-repeat 0 0; background-size: 388px auto;}
.business-box li:nth-child(3) {background: url('../img/ui_common/img_his_pc03.png') no-repeat 0 0; background-size: 388px auto;}
.business-box li:nth-child(4) {background: url('../img/ui_common/img_his_pc04.png') no-repeat 0 0; background-size: 388px auto;}

/* CEO인사말 */
.ceo-box {position: relative;padding: 41px 64px 64px;border-radius: 16px;background: #f0f9f4;}
.ceo-box .ceo-head {padding-left: 40px;margin-bottom: 47px;position: relative;}
.ceo-box .ceo-head h4 {font-size: 18px;font-weight: 600;letter-spacing: -0.18px;line-height: 34px;color: #00a88b;}
.ceo-box .ceo-head .ceo-tl {font-size: 24px;font-weight: 600;letter-spacing: -0.24px;line-height: 34px;}
.ceo-box .ceo-head .ceo-tl span {color: #00a88b;}
.ceo-box .ceo-head figure {position: absolute;right: 56px;top: -54px;height: 204px;overflow: hidden;}
.ceo-box .ceo-head figure img {width: 188px; height: auto;}
.ceo-box .ceo-white-box {position: relative; font-size: 16px; color: #5c6773; padding: 40px; border-radius: 16px; background: #fff; box-shadow: 0 4px 8px rgb(13 31 54 / 8%);}
.ceo-box .ceo-white-box p {font-size: 16px; color: #5c6773;}
.ceo-box .ceo-white-box p:nth-child(1) ~ p {margin-top: 24px;}
.ceo-box .ceo-white-box .ceo-sign {position: relative; color: #222426; font-size: 16px; text-align: right; margin-top: 24px; padding-top: 30px; border-top: 1px solid #dfe3e8;}
.ceo-box .ceo-white-box .ceo-sign strong {font-weight: 600;}
.ceo-box .ceo-white-box .ceo-sign img {width: 56px; height: auto; margin-left: 8px;}

/* 영업점안내 */
.store-info-box li {display: flex; border-radius: 16px; line-height: 18px; margin-top: 24px; border: 1px solid #dfe3e8; justify-content: space-between;}
.store-info-box li div.add-info-box {padding: 32px 0 32px 32px;}
.store-info-box li div.add-info-box li {display: block; border: none; font-size: 14px; color: #5c6773; margin-top: 12px;justify-content: flex-start; border-radius: 0;}
.store-info-box li div.add-info-box li:first-of-type, .store-info-box li:first-of-type {margin-top: 0px;}
.store-info-box li div.add-info-box li.addr-tel a {color: #5c6773; padding-left: 32px; background: url('../img/ui_common/ico_about_tel.png') no-repeat 0 0; background-size: 16px auto;}
.store-info-box li div.add-info-box li.addr-fax {padding-left: 32px; background: url('../img/ui_common/ico_about_fax.png') no-repeat 0 0; background-size: 16px auto;}
.store-info-box li div.add-info-box li.addr-map {display: inline-block; padding-left: 32px; background: url('../img/ui_common/ico_about_map.png') no-repeat 0 0; background-size: 16px auto;}
.store-info-box li div.add-info-box li.addr-map p + p {padding-top: 8px;}
.store-info-box li div.add-info-box li.addr-map img {width: 16PX; height: 16px; margin-right: 4px; vertical-align: text-top;}
.store-info-box li div.map-area {z-index:1;width: 400px; border-radius: 0 16px 16px 0; overflow: hidden; background: #5c6773;}
.store-info-box #container {height: 240px;}
.store-tit {font-size: 18px; font-weight: 700; margin-bottom: 8px;}
.store-txt {font-size: 15px; margin-bottom: 24px;}

/* 조직도 */
.organization-chart {margin: 0 auto;}
.organization-chart .device-pc {display: block;}
.organization-chart .device-mobile {display: none;}
.organization-chart .top {position: relative; margin-left: auto; margin-right: auto;}

.organization-chart .top ul {width: calc(100% - 0px); display: flex; align-items: center; flex-direction: column;}
.organization-chart .top:after {content: ""; display: block; position: absolute; top: 25px; left: 50%; width: 1px; height: 143px; border-left: 1px solid #dfe3e8; z-index: -2;}
.organization-chart .top ul li {display: inline-flex; justify-content: center; align-items: center; margin-bottom: 40px; width: 240px; height: 56px; line-height: 1; font-size: 18px; font-weight: 600; color: #fff; text-align: center; border-radius: 28px;}
.organization-chart .top ul li.black {background: #00a88b;}
.organization-chart .top ul li.black:after{content: ""; display: block; position: absolute; top: 25px; left: 50%; width: 1px; height: 75px; border-left: 1px solid #dfe3e8; z-index: -1;}
.organization-chart .top ul li.blue {background: #00a88b;color: #fff;margin-bottom: 38px; z-index:1}

.organization-chart .top div.gamsa {position: absolute; top: 0; left: calc(70% + 60px); width: 188px; color:#222426;;}
.organization-chart .top div.gamsa p {position: relative;display: inline-flex;justify-content: center;align-items: center;width: 188px;height: 56px;line-height: 1;font-size: 16px;font-weight: 700;color: #222426;border-radius: 28px;background: #f0f9f4;z-index: 2;}
.organization-chart .top div.gamsa::after {content: ""; position: absolute; right: calc(100% + -16px); top: 23%; height: 0; width: calc(100% + 47px); border-top: 1px solid #dfe3e8; z-index: -1;}
.organization-chart .top div.gamsa ul {margin-top: 8px;}
.organization-chart .top div.gamsa ul li {display: flex; justify-content: center; height: initial; line-height: 1.6; width: 188px; height: 48px; font-size: 16px; color: #222426; letter-spacing: -0.05em; background: #fff; border: 1px solid #dfe3e8; align-items: center; margin-bottom: 0;font-weight:300}
.organization-chart .top div.gamsa ul:after {content: ""; position: absolute; left: 50%; top: 17%; height: 0;width: 1px; height: 60px; border-left: 1px solid #dfe3e8; z-index: -1;}
.middle-line {width: 611px; height: 23px; margin-left: 95px; border-top: 1px solid #dfe3e8;}
