.organization-chart .bottom.device-pc > ul li.col-3 div.col-wrap div ul::before {content: ""; position: absolute; right: calc(100% + -25px); top: -87px; left: 50%; height: 100%;border-left: 1px solid #dfe3e8; z-index: -1;}
.organization-chart .bottom.device-pc > ul li.col-3 div.col-wrap div ul:after {content: ""; position: absolute; right: calc(100% + -25px); top: -22px; left: 50%; height: 100%;border-left: 1px solid #dfe3e8; z-index: -1;}
.organization-chart .bottom.device-pc > ul li.col-3 div.col-wrap div p + ul::before {content: ""; position: absolute; right: calc(100% + -25px); top: -143px; left: 50%; height: calc(100% + 56px); border-left: 1px solid #dfe3e8; z-index: -1;}
.organization-chart .bottom.device-pc > ul li.col-3 div.col-wrap div p + ul:after {content: ""; position: absolute; right: calc(100% + -25px); top: -79px; left: 50%; height: calc(100% + 56px); border-left: 1px solid #dfe3e8; z-index: -1;}
