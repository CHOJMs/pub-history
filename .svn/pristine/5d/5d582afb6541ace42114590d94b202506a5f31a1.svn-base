.organization-chart .bottom.device-pc > ul li ul {position: relative; margin-top: 8px;}
.organization-chart .bottom.device-pc > ul li ul li {position: relative;display: inline-flex; flex-wrap:wrap; line-height: 1.6;width: 188px;min-width: 188px;font-size: 16px;letter-spacing: -0.05em;background: #fff;margin-top: 8px;margin-bottom: 0;height: 48px;border: solid 1px #dfe3e8;align-items: center;justify-content: center;border-radius: 28px;}
