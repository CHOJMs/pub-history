@charset "UTF-8";

body {width: 100%; margin: 0; padding: 0;}
* {box-sizing: border-box;}
:focus {outline: none;}

p, a, h1, h2, h3, h4, h5, h6 {transform: skew(-0.1deg);}

.wrap {width: 100%; transition: .3s;}

.sub-process {height: auto; background: #f7f7f7;}
.sub-process .contents {position: relative; display: flex; width: 800px; flex-direction: column; justify-content: space-between; margin: auto; padding: 152px 80px 120px 80px; min-height: calc(100vh - 0px); background: #fff; box-shadow: 0 3px 32px rgb(0 0 0 / 16%);}
.sub-process .contents h2.s-t1 {font-size: 40px; font-weight: 600; color: #242f4b; text-align: center; line-height: 1.64; letter-spacing: -0.88px; margin-bottom: 64px;}
.sub-process .contents .tit-info {font-size: 32px; font-weight: 600; letter-spacing: -0.56px; line-height: 1.5; margin-bottom: 40px;}
.sub-process .contents .process-wrap {padding-bottom: 40px;}
.sub-process .process-wrap .pro-box {position: relative;}
.division-box::before {content: ''; display: block; height: 8px; width: calc(100% - -160px); margin-top: 64px; margin-bottom: 64px; margin-left: -80px; background: #f7f8f9;}

/* 폰트사이즈 */
.font14 {font-size: 14px;}

/* 타이틀 */
.s-txt {font-size: 16px; font-weight: 600; margin-bottom: 8px;}
.s-txt01 {font-size: 18px; font-weight: 700; margin-bottom: 16px;}
.s-txt01 + .mb20 {font-size: 14px;}
.s-txt02 {font-size: 18px; font-weight: 400; margin-top: 32px; color: #5e6167; line-height: 1.5;}
.s-txt03 {font-size: 20px; font-weight: 700; margin-bottom: 12px;}
.s-txt.mb32 {font-size: 20px; font-weight: 600; margin-bottom: 32px;}
.s-txt.mb20.mt32 {font-size: 20px; font-weight: 600; margin-top: 40px;}
.s-txt.mb20 {font-size: 20px; font-weight: 600; margin-bottom: 20px;}
.s-txt.mt40 {font-size: 20px; font-weight: 600; margin-bottom: 32px;}
.s-txt.mb20.mt40 {margin-bottom: 20px;}
.s-txt02.type01 {font-size: 16px; margin-top: 20px;}

.info-txt {font-size: 14px; color: #888; margin-top: 8px;}

.pop-cont-txt {text-align: center;}

/* etc */
.mob {display: none;}
.pc {display: block;}
img {max-width: 100%;}
.a-l {text-align: left !important;}
.ellipsis {overflow: hidden; white-space: nowrap; text-overflow: ellipsis;}

/* 패딩 */
.pt40 {padding-top: 40px !important;}

/* 마진 */
.mt8 {margin-top: 8px;}
.mt20 {margin-top: 20px;}
.mt24 {margin-top: 24px;}
.mt32 {margin-top: 32px;}
.mt40 {margin-top: 40px;}
.mb0 {margin-bottom: 0px !important;}
.mb8 {margin-bottom: 8px !important;}
.mb16 {margin-bottom: 16px;}
.mb20 {margin-bottom: 20px;}
.mb32 {margin-bottom: 32px;}
.mb40 {margin-bottom: 40px;}

/* 공통 */
input[type="radio"] {width:1px; height:1px; margin-left: -1px; appearance:none;-webkit-appearance:none;-moz-appearance:none; opacity: 0;}
input[type="checkbox"] {width:1px; height:1px; margin-left: -1px; appearance:none;-webkit-appearance:none;-moz-appearance:none; opacity: 0;}
input {font-family: "pretendard"; opacity: 1;}
button {font-family: "pretendard"; cursor: pointer; border: none;}
input[type="text"],
input[type="number"],
input[type="tel"],
input[type="email"],
input[type="password"] {width:100%; height: 56px; padding: 0 16px; font-size: 16px; font-weight: 500; border-radius: 12px; letter-spacing: -0.05em; color: #5e6167; border: 1px solid #d8d8d8; box-sizing: border-box; vertical-align: top; appearance: none;}
input[type="text"]:hover,
input[type="number"]:hover,
input[type="tel"]:hover,
input[type="email"]:hover,
input[type="password"]:hover {border:1px solid #222426; transition: all .5s; color: #242f4b;}
input[type="text"]:focus,
input[type="number"]:focus,
input[type="tel"]:focus,
input[type="email"]:focus,
input[type="password"]:focus {border:1px solid #d8d8d8; outline:none; color: #242f4b;}
input::-ms-input-placeholder,
input::-webkit-input-placeholder,
input::-moz-placeholder,
input::-moz-placeholder {color: #737d9a; opacity: .6;}
input::placeholder {color: #737d9a; opacity: .6;}
input:focus, input:hover {border-color:rgb(34, 36, 38) !important;}

/* 자동완성배경색 없음 */
input:-webkit-autofill { -webkit-box-shadow: 0 0 0 31px #fff inset ; -webkit-text-fill-color: #000; box-shadow: 0 0 0px 1000px #fff inset;}
input:-webkit-autofill, input:-webkit-autofill:hover, input:-webkit-autofill:focus, input:-webkit-autofill:active {transition: background-color 5000s ease-in-out 0s; }
input:autofill, input:autofill:hover, input:autofill:focus, input:autofill:active {-webkit-text-fill-color: #000; -webkit-box-shadow: 0 0 0px 1000px #fff inset; box-shadow: 0 0 0px 1000px #fff inset; transition: background-color 5000s ease-in-out 0s;}

/* 폼객체 : 라디오버튼 */
.cont-radio-tab {display: block;}
.cont-radio-tab .com-inp {width: 100%; height: 58px;}
.cont-radio-tab .com-inp input {overflow:hidden; position:absolute; border:0; margin:0 0 0 0px; padding: 0; width: 100%; height: 58px;}
.cont-radio-tab .com-inp label {width: 100%; display:inline-flex; justify-content: center; align-items: center; position:relative; height: 58px; font-size: 16px; color:#242f4b; font-weight: 700; cursor:pointer; left: 0; border: 1px solid #d8d8d8; background: #fff; padding: 0; border-radius: 12px;}
.cont-radio-tab .com-inp input:checked + label {color: #00a88b; background: #fff; border: 1px solid #00a88b;}
.cont-radio-tab .com-inp:last-of-type {margin-top: 12px;}
.cont-radio-tab .com-inp label:focus, .cont-radio-tab .com-inp label:hover {color: #00a88b; background: #fff; border: 1px solid #00a88b;}

/* 폼객체 : 라디오버튼 */
.inp-radio {position: relative; display: flex; align-items: center; justify-content: flex-start;}
.inp-radio .radiobox {display: inline-flex; width: 50%; height: 56px; margin-left: 8px; align-items: center; justify-content: center;}
.inp-radio .radiobox:first-of-type {margin-left: 0;}
.inp-radio .radiobox label {width: 100%; display:inline-flex; justify-content: center; align-items: center; position:relative; height: 56px; font-size: 16px; color:#242f4b; font-weight: 700; cursor:pointer; left: 0; background: #fff; border: 1px solid #d8d8d8; padding: 0; border-radius: 12px; margin: 0;}
.inp-radio .radiobox input {overflow:hidden; position:absolute; border:0; margin:0 0 0 0px; padding: 0; height: 58px;}
.inp-radio .radiobox input:checked + label {position: relative; border-color: #00a88b; background: #fff; color: #00a88b; padding: 0;}

/* 폼객체 : 셀렉트박스 */
.accordion-sel-box li {position: relative; margin-bottom: 8px; border-radius: 12px; border: 1px solid #d8d8d8;}
.accordion-sel-box li:focus, .accordion-sel-box li:hover {border-color:rgb(34, 36, 38);}
.accordion-sel-box li .q {position: relative;}
.accordion-sel-box li .q a {display: block; font-size: 16px; font-weight: 500; color: #242f4b; padding: 18px 50px 17px 16px;}
.accordion-sel-box li .q a > p {font-weight: 400; color: #3965ff;}
.accordion-sel-box li .q a .ico-arrow {position: absolute; right: 16px; top: 16px; width: 24px; height: 24px; font-size: 0; background: url('../img/common2/ico_select.png') no-repeat; background-size: 24px; transition: 0.3s;}
.accordion-sel-box li.show .q a .ico-arrow {background-size: 24px; transform: rotate(-180deg);}
.accordion-sel-box li .a {display: none; font-size: 16px; color: rgb(112, 116, 120); line-height: 26px; word-break: break-all; word-wrap: break-word;}
.accordion-sel-box li .a > ul > li {border: none; padding: 0 16px 16px 16px; margin: 0;}
.accordion-sel-box li .a > ul > li > a {display: block; width: 100%;}
.accordion-sel-box li:last-child {margin-bottom: 0;}
.accordion-sel-box li:last-child .a .box-gray {margin-bottom: 0;}
.accordion-sel-box li.error {border-color: #ff3a3a !important;}

/* 아코디언 */
.accordion-info-box {margin: 32px 0 40px 0;}
.accordion-info-box li {position: relative; margin-bottom: 12px; border-radius: 12px; border: 1px solid #d8d8d8;}
.accordion-info-box li:focus, .accordion-sel-box li:hover {border-color:rgb(34, 36, 38); transition: all .5s;}
.accordion-info-box li .q {position: relative;}
.accordion-info-box li .q a {display: block; font-size: 16px; font-weight: 500; color: #242f4b; text-align: left; padding: 18px 50px 17px 16px;}
.accordion-info-box li .q a .ico-arrow {position: absolute; right: 16px; top: 16px; width: 24px; height: 24px; font-size: 0; background: url('../img/common2/ico_select.png') no-repeat; background-size: 24px; transition: 0.3s;}
.accordion-info-box li.show .q a .ico-arrow {background-size: 24px; transform: rotate(-180deg);}
.accordion-info-box li .a {display: none; font-size: 16px; color: rgb(112, 116, 120); line-height: 26px; word-break: break-all; word-wrap: break-word;}
.accordion-info-box li .a > ul > li {border: none; padding: 0 16px 16px 16px; margin: 0;}
.accordion-info-box li .a > ul > li > a {display: block; width: 100%;}
.accordion-info-box li:last-child {margin-bottom: 0;}
.accordion-info-box li .list-2 {margin-top: 16px;}
.accordion-info-box li .list-2 li {border: none; padding: 0 16px; margin-bottom: 12px;}
.accordion-info-box li .list-2 li span.p-tit {font-size: 16px;}

/* 완료 : 아코디언 */
.finish-box .accordion-info-box li .q a {font-size: 20px; font-weight: 700; padding: 18px 50px 17px 20px;}
.finish-box .accordion-info-box li .list-2 {margin-top: 60px}
.finish-box .accordion-info-box li .list-2 li {padding: 0 20px;}
.finish-box .accordion-info-box li .list-2 li span.p-tit,
.finish-box .accordion-info-box li .list-2 li span.p-txt {font-size: 14px; color: #5e6167;}

.com-inp {position: relative; margin-top: 32px;}
.com-inp:first-of-type {margin-top: 0;}
.com-inp label {font-size: 16px; display: flex; text-align: left; margin-bottom: 8px; font-weight: 500;}
.com-inp input[type="file"], input[type="file"] {position: absolute; width: 100%; height: 56px; opacity: 0; cursor: pointer; z-index: 1;}
.com-inp .inp-area {position: relative; display: flex; border-radius: 12px; border: 1px solid #d8d8d8;}
.com-inp .inp-area input {flex: 1; border: none;}
.com-inp .inp-area .dash {width: 30px; flex: none; font-size: 16px; text-align: center; height: 54px; line-height: 54px;}
.com-inp .inp-area input:last-of-type {padding-left: 10px;}
.com-inp  .f-red {font-size: 12px; color: #ff3a3a; padding-top: 8px; padding-left: 8px;}
.com-inp .inp-area.inp-col2 {position: relative; width: 100%; border: none; display: inline-flex;}
.com-inp .inp-area.inp-col2 input {width: 50%; border: 1px solid #d8d8d8;}
.com-inp .inp-area.inp-col2 input:last-of-type {padding-left: 20px;}
.com-inp .inp-area.inp-col2 > div {width: 50%; position: relative;}
.accordion-sel-box.type01 {width: 100%; position: absolute; left: 0; background: #fff; z-index: 2;}
.accordion-sel-box.type01 li .q a {padding: 17px 50px 16px 16px;}
.com-inp .inp-area:focus, .com-inp .inp-area:hover {border-color:rgb(34, 36, 38);}

/* 주소 */
.com-inp .add {padding: 0 50px 0 16px;}

/* 은행확인 */
.com-inp .s-txt + div {position: relative;}
.com-inp .ico-bank, .ico-bank {position: absolute; left: 16px; top: 50%; width: 24px; height: 24px; transform: translateY(-50%); display: inline-block; z-index: 2;}
.com-inp a.alink {display: block; padding: 0; height: 56px;}
.com-inp a.alink input {padding: 0 16px 0 16px; border: 1px solid #d8d8d8;}
.com-inp a.alink.bank input {padding: 0 16px 0 50px;}
.com-inp a.alink input:focus, .com-inp a.alink input:hover {border-color: #3a65ff; transition: all .5s;}
.com-inp a.ico-select::after {content: '선택'; font-size: 0; width: 24px; height: 24px; position: absolute; right: 16px; top: 16px; background: url('../img/common2/ico_select.png') no-repeat; background-size: 24px; pointer-events: none;}

.inp-btn {position: relative;}
.com-inp .btn {position: absolute; right: 16px; top: 16px; display: inline-flex; height: 24px; width: 85px; color: #737d9b; font-size: 12px; font-weight: 600; border-radius: 8px; background: #eef2f9; align-items: center; justify-content: center;}
.inp-btn + .mt20 {font-size:14px; color: #888; margin-top: 8px; margin-bottom: 56px;}

.input-file {display: flex; width: 100%; height: 56px; color: #ccc; padding: 0 16px; align-items: center; border-radius: 12px; border: 1px solid #d8d8d8;}
.input-file::after {content: '파일등록'; display: flex; width: 60px; height: 24px; color: #737d9b; font-size: 12px; font-weight: 600; line-height: 24px; text-align: center; position: absolute; right: 16px; bottom: 15px; border-radius: 8px; background: #eef2f9; align-items: center; justify-content: center;}

/* 검색 */
.btn-search {position: absolute; right: 16px; bottom: 15px; font-size: 0; width: 27px; height: 24px; background:url('../img/common2/ico_search.png') no-repeat center center; border:none; background-size: 24px auto;}

/* 약관동의 */
.agree-box .agree-total-box {background: #f7f7f9; border-radius: 12px; padding: 0 16px; margin-bottom: 21px;}
.agree-box .agree-total-box .checkbox label {font-size: 18px; font-weight: 600; color: #242f4b; padding: 15px 0 15px 28px; height: 56px;}
.agree-box .agree-total-box .checkbox {margin: 0;}
.agree-box .agree-total-box .checkbox a.arrow {top: 20px;}
.agree-box .agree-total-box .checkbox .chk+.label:after {top: 50%; transform: translateY(-50%);}
.agree-box .checkbox {position: relative; margin-bottom: 18px; margin-left: 16px; margin-right: 16px}
.agree-box .checkbox label {display: inline-block; font-size: 16px; color: #5e6167; font-weight: 400; width: calc(100% - 30px); height: 22px; line-height: 24px;}
.agree-box .agree-total-box .checkbox .wa-arrow {top: 50%; transform: translateY(-50%);}
.agree-box .checkbox .wa-arrow {position: absolute; right: 0; width: 16px; height: 16px;}
.agree-box .checkbox .wa-arrow img.agr-pc {width: 16px; height: 16px; display: block;}
.agree-box .checkbox .wa-arrow img.agr-mob {display: none;}
.agree-box .checkbox .chk+.label:after{content: ''; position:absolute; top: 0; left:0; width: 22px; height: 22px; transition: .3s; background:url('../img/common2/ico_check_off.png') no-repeat center center; border:none; background-size: 22px 22px; transform: none;}
.agree-box .checkbox .chk:checked+.label:after{content: ''; margin-top:0; background: url('../img/common2/ico_check_on.png') no-repeat center center; border:none; border-radius: 50%; background-size: 22px 22px;}
.agree-box .checkbox .chk.chk_1+.label {padding-left: 28px;}
.agree-box .checkbox:last-of-type { margin-bottom: 0px;}
.agree-box .checkbox ul {margin-top: 12px; margin-left: 30px;}
.agree-box .checkbox ul li {table-layout: fixed; display: flex; justify-content: space-between; flex-flow: row wrap;}
.agree-box .checkbox ul li .checkbox {width: 50%; margin-left: 0px; margin-bottom: 6px; margin-right: 0;}
.agree-box .checkbox ul li .checkbox:last-child {margin-bottom: 0;}
.agree-box .checkbox ul li .checkbox .chk.chk_1+.label{font-size: 14px;}
.popup-half .agree-box .agree-total-box {border-radius: 16px;}
.popup .agree-box .agree-total-box {padding: 0 16px;}

/* 버튼 */
.btn-close {position: absolute; right: 80px; top: 40px; z-index: 3;}
.btn-close a {display: block; width: 32px; height: 32px;}

/* fixed : 버튼 */
.fixed-btn-area {position: relative; left: 50%; bottom: 0; width: 100%; transform: translatex(-50%); box-sizing: border-box; z-index: 2; background: linear-gradient(180deg, rgb(255 255 255 / 0%) 0%, rgb(255 255 255 / 90%) 19%, rgb(255 255 255 / 1));}
.btn-box {display: flex; width: 100%; table-layout: fixed; text-align: center; justify-content: center;}
.btn-box .btn {padding-left: 0 !important; vertical-align: top; width: calc(50% - 8px); flex: 1; margin-left: 8px;}
.btn-box .btn:first-child {padding-left: 0px; margin-left: 0px;}
.btn-box .btn button {width: 100%; height: 56px; color: #fff; background: #00a88b; border-radius: 12px; font-size: 16px; text-align: center; font-weight: 700;}
.btn-box .gray button, .btn-box .gray a {color: #dddddd; background: #f8f8f8;}
.btn-box .gray01 button, .btn-box .gray01 a {color: rgb(92, 103, 115); background: rgb(235, 238, 242);}
.popup .fixed-btn-area {position: fixed; left: 0; bottom: 0; width: 100%; padding: 40px; box-sizing: border-box;  margin-top: 0 !important; transform: inherit;}
.btn-box .line button {color: #00a88b; background: #fff; border: 1px solid #00a88b;}
.btn-box .wid120 {flex: none; width: 120px;}
.btn-txt {display: block; text-align: center; width: 100%;}
.btn-txt a {display: inline-block; width: 100%; margin-top: 20px; font-size: 14px; text-decoration: underline; text-underline-position: under;}
.btn-box .mob-btn {display: block;}
.btn-box .mob-btn + span {padding-left: 0px; margin-left: 8px;}

/* fixed : 모바일백신 안내팝업 버튼 */
.popup .btn-area {position: fixed; left: 0; bottom: 0; width: 100%; padding: 0 20px 20px 20px; box-sizing: border-box;  margin-top: 0 !important; transform: inherit;}
.btn_box {display: flex; width: 100%; table-layout: fixed; text-align: center; justify-content: center;}
.btn_box .btn {padding-left: 0 !important; vertical-align: top; width: calc(50% - 8px); flex: 1; margin-left: 8px;}
.btn_box .btn:first-child {padding-left: 0px; margin-left: 0px;}
.btn_box .btn button {width: 100%; height: 56px; color: #fff; background: #00a88b; border-radius: 12px; font-size: 16px; text-align: center; font-weight: 700;}
.btn_box .btn_line button {color: #00a88b; background: #fff; border: 1px solid #00a88b;}

/* 이용안내 */
.call-phone {font-size: 18px; color: #888; text-align: center; padding-bottom: 32px; background: #fff;}
.call-phone .c-tel {color: #00a88b;}

/* 본인인증 */
.confi-detail-box {margin-bottom: 56px;}
.confi-detail-box li {visibility: hidden; height: 0; margin-top: 8px; transition:all .8s; transform: translateY(50%); opacity: 0;}
.confi-detail-box li.active {transform: translateY(0); opacity: 1; height: auto; visibility: visible;}
.confi-detail-box li:first-child {margin-top: 0;}
.confi-detail-box li a {display: inline-block; font-size: 16px; font-weight: 500; border-radius: 4px; padding: 4px 12px; background: #f7f7f9;}
.confi-detail-box li a::after {content: ''; display: inline-block; width: 1.6rem; height: 1.6rem; margin-left: .4rem; vertical-align: middle; background: url('../img/common2/ico_arrow01.png') no-repeat; background-size:1.6rem auto; margin-bottom: 2px;}

.form-list {position: relative; display: table; width: 100%; transition: all .4s; visibility: visible;}
.form-list li {position: relative; padding-bottom: 32px;}
.form-list li:last-of-type {padding-bottom: 0px;}
.form-list .h-tit {font-size: 16px; font-weight: 500; letter-spacing: -0.05em; color: #242f4b; margin-bottom: 8px;}
.form-list .inp-data {position: relative; display: flex;}
.form-list .inp-data span {position: relative; flex: 1;}
.form-list .inp-data input {padding: 19px 16px 18px 16px;}
.form-list .inp-data input:focus, .form-list .inp-data input:hover {border-color:rgb(34, 36, 38) !important;}
.form-list .inp-data .f-red {font-size: 12px; color: #ff3a3a; padding-top: 8px;}
.form-list .btn-clear {position: absolute; width: 24px; height: 24px; right: 16px; top: 16px; font-size: 0; background: url('../img/common2/ico_del.png') no-repeat; background-size: 24px auto;}
.form-list .inp-data.col2 {display: block;}
.form-list .inp-data.col2 div {display: flex; border-radius: 12px; border: 1px solid #d8d8d8;}
.form-list .inp-data.col2 div input {border: none;}
.form-list .inp-data.col2 div:focus, .form-list .inp-data.col2 div:hover {border-color:rgb(34, 36, 38) !important;}
.form-list .inp-data .dash {width: 20px; flex: none; font-size: 16px; text-align: center; line-height: 58px; color: #242f4b;}
.error div,
.error input {border-color: #ff3a3a !important;}

/* popup - 레이아웃 */
.popup-btn {margin: 50px; padding: 5px 10px; font-size: 16px; border: 1px solid #000;}
.popup-800 {max-width: 800px !important;}

/* popup popup-01 */
.popup-layer {position: relative; width: 100%; margin: 0 auto;}
.popup-layer.active .popup.popup-01 {opacity: 1; visibility: visible; overflow: hidden;}
.popup-01 {opacity: 0; visibility: hidden; position: fixed; top: 50%; left: 50%; transform: translate(-50%, -50%); max-width: 480px; width: 100%; height: 680px; box-shadow: 0 -0.05rem 10px rgba(0, 0, 0, 0.1); border-radius: 20px; background: #fff; z-index: 2; transition: all 0.3s;}
.popup-layer, .dim { visibility: hidden;}
.popup-layer.active, .dim.active { visibility: visible;}
.dim {opacity: 0; visibility: hidden; position: fixed; left: 0; top: 0; width: 100%; height: 100%; background: rgba(0, 0, 0, 0.3); transition: all 0.3s; z-index: 9990;}
.dim.active {opacity: 1; visibility: visible;}

.popup-01 {z-index: 9991;}

.popup .inner {padding: 40px 0 0 0; height: 100%;}
.popup-close {position: absolute; right: 20px; top: 20px;}
.popup-close a {display: inline-block; width: 32px; height: 32px;}

.popup-contents {width: 100%; height: 100%; overflow-y: auto; padding: 0 40px 16px 40px;}
.popup-contents::-webkit-scrollbar {width: 9px;}
.popup-contents::-webkit-scrollbar-track {border-radius: 0px; background: transparent;}
.popup-contents::-webkit-scrollbar-thumb {border-radius: 20px; background: #bcc0c5; border: 3px solid #ffffff;}
.col-cnt-type01::-webkit-scrollbar {width: 9px;}
