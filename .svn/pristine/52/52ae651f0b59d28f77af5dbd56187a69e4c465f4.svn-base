.popup[data-shape=tooltip] { position: absolute; z-index: 99; top: 5.5rem; left: 2.2rem; opacity: 0.8; padding: 2rem 1.6rem; border-radius: 0.6rem; color: #fff; background-color: #000; box-shadow: 0 0.2rem 0.6rem 0 rgba(0, 0, 0, 0.16); font-size: 1.6rem; font-weight: 400; letter-spacing: -0.32px; line-height: 1.5; }
