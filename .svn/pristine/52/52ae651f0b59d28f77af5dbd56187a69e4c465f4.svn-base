    .ly_popup .wrap_popup { margin: 0 2rem; }
    .ly_popup .wrap_popup header .pop_tit { font-size: 1.8rem; }
    .ly_popup.full .wrap_popup { max-height: 100%; border-radius: 0; padding: 1.4rem 2rem 3.4rem; }
    .ly_popup.full .wrap_popup header .btn_popclose { top: -0.8rem; right: 0; }
    .ly_popup.full .wrap_popup header .btn_download { top: -0.8rem; right: 4.4rem; }
    .pc_none { display: none; }
    .mo_none { display: block !important; }

    /* step progress */
    .step_progress_wrap {position:fixed; top:4.8rem; width:100%; background-color:#fff; z-index:9; padding:1rem 0;}
    .step_progress {max-width:47rem; margin:3rem auto 4.4rem;}
    .step_progress ol {max-width:47rem; width:auto; margin-left:10%;}
    .step_progress ol li:before {width:1rem; height:1rem;}
    .step_progress ol li:after {width:2rem; height:2rem; top:-1rem; left:-0.5rem;}
    .step_progress ol li em {display:none;}
    .step_progress ol li a {font-size:1.4rem; left:10%; transform:translateX(-50%);}

    /* list */
    .data_list li .obj_key,
    .data_list span.li .obj_key {width:55%; font-size:1.6rem;}


    .ico_easy_consult { width: 2rem; height: 2rem; background-image: url(/static/carverse/img/main/icon_easy_consult.png); background-size: 2rem 2rem; margin-right: .4rem; }
    .ico_call_consult { width: 2rem; height: 2rem; background-image: url(/static/carverse/img/main/icon_call_consult.png); background-size: 2rem 2rem; margin-right: .4rem; }
    .ico_top { width: 1.5rem; height: 1.5rem; background-image: url(/static/carverse/img/main/icon_top_24.png); background-size: 1.5rem 1.5rem;}
}