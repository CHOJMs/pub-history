.tab_menu_wrap .tab_menu li a {transition:all 0.3s;}

.tab_contents { margin-top: 5.2rem; }

/* Step Progress */
.step_progress { position: relative; max-width: 77.2rem; width: 100%; margin: 7rem auto 12rem; }
.step_progress:after { display: none; content: ""; position: absolute; top: 0.4rem; left: 50%; width: 66rem; height:0.1rem; transform: translateX(-50%); background-color: #3a65ff; }
.step_progress ol { display: flex; justify-content: space-between; width: 71rem; margin: 0 auto; margin-left: 8rem; }
.step_progress ol li { position: relative; width: 11rem; height:0.1rem; background-color: #d8d8d8;}
.step_progress ol li:before,
.step_progress ol li:after { display: block; content: ""; position: absolute; border-radius: 50%; }
.step_progress ol li:before { z-index: 2; top: -0.5rem; left: 0; width: 1.2rem; height: 1.2rem; background-color: #d8d8d8; }
.step_progress ol li:after { display: none; z-index: 1; top: -1.1rem; left: -0.6rem; width: 2.4rem; height: 2.4rem; opacity: 0.3; background-color: #00a88b; }
.step_progress ol li:last-child { background-color: #fff; }

.step_progress ol li a {position:absolute; top:-0.6rem; left:-2.4rem; width:60%; height:5rem; z-index:5;  padding-top: 2.75rem; color: #ccc; text-align:center;}

.step_progress ol li.done { background-color: #00a88b;}
.step_progress ol li.done:hover::before {transform:scale( 1.3, 1.3 );}
.step_progress ol li.done a { color: #242f4b; }
.step_progress ol li.done:before { background-color: #00a88b; transition:all 0.3s; }
.step_progress ol li.done:after { display: none; }

.step_progress ol li.current a { color: #242f4b; }
.step_progress ol li.current:before { background-color: #00a88b; }
.step_progress ol li.current:after { display: block; }


