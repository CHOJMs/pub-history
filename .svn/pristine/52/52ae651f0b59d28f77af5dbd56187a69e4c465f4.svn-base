.list_type li { position: relative; padding-left: 1.1rem; }
.list_type li:before { position: absolute; content: ""; border-radius: 50%; }
.list_type.type1 li { margin-top: 1.2rem; font-size: 1.4rem; font-weight: 400; letter-spacing: -0.28px; line-height: 1.57; }
