/* Control Box */
.control_box { display: flex; align-items: center; justify-content: space-between; }
.control_box button { font-size:1.4rem; width: auto; min-width: auto; flex: none; }

/* List */
.data_list { display: block; flex: 1; width: 100%; }
.data_list li,
.data_list span.li { display: flex; justify-content: space-between; width: 100%; color: #5e6167; }
