@charset "UTF-8";
/* 기준폰트 */
@font-face { font-family: "Pretendard"; font-weight: 700; font-display: swap; src: local("Pretendard Bold"), url(/static/carverse/font/Pretendard-Bold.woff2) format("woff2"), url(/static/carverse/font/Pretendard-Bold.woff) format("woff"); }
@font-face { font-family: "Pretendard"; font-weight: 600; font-display: swap; src: local("Pretendard SemiBold"), url(/static/carverse/font/Pretendard-SemiBold.woff2) format("woff2"), url(/static/carverse/font/Pretendard-SemiBold.woff) format("woff"); }
@font-face { font-family: "Pretendard"; font-weight: 500; font-display: swap; src: local("Pretendard Medium"), url(/static/carverse/font/Pretendard-Medium.woff2) format("woff2"), url(/static/carverse/font/Pretendard-Medium.woff) format("woff"); }
@font-face { font-family: "Pretendard"; font-weight: 400; font-display: swap; src: local("Pretendard Regular"), url(/static/carverse/font/Pretendard-Regular.woff2) format("woff2"), url(/static/carverse/font/Pretendard-Regular.woff) format("woff"); }

html { font-size: 62.5%; width:100%; }

body { min-width: 32rem; color: #242f4b; font: 1.6rem/130% "Pretendard", "sans-serif"; -webkit-font-smoothing: antialiased; -webkit-text-size-adjust: none; -moz-osx-font-smoothing: grayscale; }

a, a:link, a:visited, a:hover, a:active { color: #242f4b; }
span {font-family: "Pretendard","sans-serif";}

/* 간격 */
.mt00 {margin-top:0 !important;}

/* 공통 */
.txt_hidden { display: none; }

/* 중간라인 */
.line_gr { width: 100%; height: 0.8rem; background-color: #f7f7f9; }

/* 버튼 */
.btn_group { display: flex; justify-content: space-between; flex-wrap: wrap; gap: 0.8rem; }
.btn_group > button,
.btn_group a { display: inline-flex; align-items: center; justify-content: center; min-width: 33.4%; flex: 1; }
.btn_group > button.full,
.btn_group a.full { min-width: 100% !important; }
.btn_group.button_bg + .btn_group.button_bg {margin-top: 10px;}
.btn_fixed { display: flex; position: fixed; left: 0; right: 0; align-items: center; bottom: 3.4rem; width: 100%; text-align: center; z-index: 777; }
.btn_fixed button { display: block; }

.btn { display: inline-flex; align-items: center; width: 100%; border-radius: 1.2rem; font-size: 1.6rem; vertical-align: middle; text-align: center; cursor: pointer; font-family: "Pretendard", "sans-serif";}

/* Size */
.btn.large { height: 5.6rem; padding: 0 2rem; font-size: 1.6rem; font-weight: 700; letter-spacing: -0.32px; line-height: 1.5; }
.btn.middle { height: 4.8rem; padding: 0 2rem; }
.btn.small { height: 3.2rem; padding: 0 1.2rem; }
.btn.xsmall { display: block; margin: 0 auto; height: 2.4rem; border-radius: 0.8rem; font-size: 1.2rem; }

 /* Color */
.btn.gray { color: #737d9b; background-color: #eef2f9; }
.btn.color_green { color: #00a88b; background-color: #f0faf4; }
.btn.blue { color: #fff; background-color: #00a88b; }
.btn.blue_line { border: 0.1rem solid #00a88b; color: #00a88b; background-color: #fff; }
.btn.gray_line { border: 0.1rem solid #ccc; color: #888; background-color: #fff; }
.btn:disabled,
.btn.disabled { color: #ccc; border-color: #d8d8d8; background-color: #f8f8f8; }

.btn .ico { display: inline-block; width: 2.4rem; height: 2.4rem; margin-right: 0.4rem; background-color: green; }

.btn.btn_more { flex: none; width: 63rem; min-width: auto; margin: 0 auto; font-weight: 700; background-color: #fff; }
.btn.btn_more:hover .ico {transform:rotate(90deg);}
.btn.btn_more .ico { background: url(/static/carverse/img/common/ico_more@2x.png) no-repeat 0 0; background-size: cover; transition:all 0.3s; }

.btn_txt { padding-bottom: 0.2rem; border-bottom: 0.1rem solid #5e6167; color: #5e6167; font-weight: 500; cursor: pointer; }
.btn_txt.middle { font-size: 1.4rem; letter-spacing: -0.22px; line-height: 1.14; }
.btn_txt.small { font-size: 1.2rem; letter-spacing: -0.19px; line-height: 1.33; }

.btn_accordion.type_selct {font-family: "Pretendard","sans-serif"; position:relative; background-color: #fff; width: 100%; padding: 1.6rem 4rem 1.6rem 1.6rem; text-align: left; font-weight: 600; font-size: 1.6rem; line-height: 2.4rem; letter-spacing: -0.32px;}
.btn_accordion.type_selct:disabled {background-color:#f8f8f8; border-radius:1.2rem;}
.btn_accordion.type_selct:disabled span {color:#ccc; font-weight:500;}
.btn_accordion.type_selct:disabled::after {content:""; background: url(/static/carverse/img/common/icon_24_arrow_down_grey.svg);}
.btn_accordion.type_selct::after {content:""; position:absolute; right:1.6rem; top:1.6rem; background: url(/static/carverse/img/common/icon_24_arrow_down.svg) no-repeat; background-size: 2.4rem; width:2.4rem; height:2.4rem; transition:all 0.3s;}
.btn_accordion.type_selct.active::after {transform:rotate(180deg);}
.btn_accordion.type_selct .selct_value { font-size:1.4rem; font-weight: 400; color: #00a88b; display: block; margin-top: 0.4rem; }

.btn_tooltip { width: 1.6rem; height: 1.6rem; margin-right: 0.8rem; background: url(/static/carverse/img/common/ico_tooltip.svg) no-repeat 0 0; }
.btn_tooltip span { display: none; position: absolute; width:0.1rem; height:0.1rem; padding: 0; margin:-0.1rem; overflow: hidden; clip: rect(0, 0, 0, 0); }

/* 아이콘 */
.ico_estimate { width: 2rem; height: 2rem; background-image: url(/static/carverse/img/main/icon_estimate.svg); }
.ico_easy_consult { width: 2.4rem; height: 2.4rem; background-image: url(/static/carverse/img/main/icon_easy_consult.png); background-size: 2.4rem 2.4rem; margin-right: .8rem; }
.ico_call_consult { width: 2.4rem; height: 2.4rem; background-image: url(/static/carverse/img/main/icon_call_consult.png); background-size: 2.4rem 2.4rem; margin-right: .8rem; }
.ico_cs { width: 2rem; height: 2rem; background-image: url(/static/carverse/img/main/icon_cs.svg); }
.ico_close { width: 1.6rem; height: 1.6rem; background-image: url(/static/carverse/img/main/icon_close.svg); }
.ico_open { width: 1.6rem; height: 1.6rem; background-image: url(/static/carverse/img/main/icon_open.svg); }
.ico_top { width: 1.8rem; height: 1.8rem; background-image: url(/static/carverse/img/main/icon_top_24.png); background-size: 1.8rem 1.8rem;}
.ico_close_popup { width: 3.2rem; height: 3.2rem; background-image: url(/static/carverse/img/common/icon_32_close.svg); }
.icon_warning { width: 6.4rem; height: 6.4rem; background-image: url(/static/carverse/img/main/icon_64_warning.svg); }
.ico_download { width: 3.2rem; height: 3.2rem; background-image: url(/static/carverse/img/common/icon_32_download.svg); }
.icon_document { width: 2.4rem; height: 2.4rem; background: url(/static/carverse/img/common/icon_24_paper.svg) no-repeat 0 0; }
.icon_car { width: 2.4rem; height: 2.4rem; background: url(/static/carverse/img/common/icon_24_car.svg) no-repeat 0 0; }
.icon_arr_r { width: 2.4rem; height: 2.4rem; background: url(/static/carverse/img/common/icon_24_arrow_r.svg) no-repeat 0 0; }
.icon_arr_b { width: 1.6rem; height: 1.6rem; background: url(/static/carverse/img/common/icon_16_arrow_down.svg) no-repeat 0 0; }
.icon_file { width: 1.6rem; height: 1.6rem; background: url(/static/carverse/img/common/icon_16_file.svg) no-repeat 0 0; }

/* 모바일메뉴 드롭다운*/
.list_accordian details summary { list-style: none; }
.list_accordian details summary h2 { width: 100%; height: 5.6rem; padding: 2rem 1.8rem; text-align: left; font-size: 1.6rem; cursor: pointer; position: relative; }
.list_accordian details summary h2 a {display:block; width:100%;}
.list_accordian details summary h2.icon_arrow::after {content:""; position:absolute; top:1.8rem; right:1.8rem; width:2rem; height:2rem; background: url(/static/carverse/img/common/icon_12_arrow_down.svg) no-repeat center; transition:all 0.3s; }
.list_accordian details[open] summary h2.icon_arrow::after {transform:rotate(-180deg);}

.list_accordian .list_in { width: 100%; }
.list_accordian .list_in li a { display: block; padding: 1rem 2rem; font-size: 1.4rem; background-color: #F7F7F9; }

.accordion_text.type_only { position: relative; margin-top: 1.2rem; border: 0.1rem solid #d8d8d8; border-radius: 1.2rem; }
.accordion_text.type_only.accordion_on { border: 0.1rem solid rgb(34, 36, 38) !important; }
.accordion_text.type_only.accordion_on .accordion_btn::before { content: ""; display: block; position: absolute; top: 1.5rem; right: 1.6rem; width: 2.4rem; height: 2.4rem; background: url(/static/carverse/img/common/icon_24_arrow_up.svg) no-repeat 0 0/2.4rem auto; }
.accordion_text.type_only.error { border: 0.1rem solid #ff3a3a; }
.accordion_text.type_only .accordion_btn::before { content: ""; display: block; position: absolute; top: 1.5rem; right: 1.6rem; width: 2.4rem; height: 2.4rem; background: url(/static/carverse/img/common/icon_24_arrow_down.svg) no-repeat 0 0/2.4rem auto; }
.accordion_text.type_only .accordion_btn button { position: relative; padding: 1.6rem; width: 100%; text-align: left; font-size: 1.6rem; font-weight: 700; z-index: 999; }
.accordion_text.type_only .accordion_btn span { font-weight: 500; font-size: 1.4rem; color: #3a65ff; }
.accordion_text.type_only ul { padding-left: 1.6rem; }
.accordion_text.type_only ul li { padding-bottom: 1.6rem; }
.accordion_text.type_only ul li a { display: block; font-weight: 300; }
.accordion_text.type_only ul li a:hover { display: block; color: #00a88b; }

/* Flex */
.flex_j_c { justify-content: center; }

/* Checkbox, Radio */
.checkbox_group,
.radio_group { display: inline-flex; align-items: center; position: relative; }

.checkbox_group label,
.radio_group label { position: relative; display: flex; align-items: center; cursor: pointer; }

.checkbox_group input[type=checkbox],
.radio_group input[type=radio] { position: absolute; width:0.1rem; height:0.1rem; padding: 0; margin: -0.1rem; overflow: hidden; clip: rect(0, 0, 0, 0); border: 0; }

.checkbox_group.col,
.radio_group.col { flex-direction: column; }

.checkbox_group.col .inner,
.radio_group.col .inner { margin-bottom: 0.4rem; }

.checkbox_group label:before { display: inline-block; content: ""; width: 2.4rem; height: 2.4rem; margin-right: 0.4rem; background: url(/static/carverse/img/common/checkbox_off@2x.png) no-repeat 0 0; background-size: 2.4rem; }
.checkbox_group input:checked + label:before { display: inline-block; content: ""; width: 2.4rem; height: 2.4rem; margin-right: 0.4rem; background-repeat: no-repeat; background-position: 0 0; background-size: 2.4rem; }
.checkbox_group input + label:before { display: inline-block; content: ""; width: 2.4rem; height: 2.4rem; margin-right: 0.4rem; }
.checkbox_group input:checked + label:before { background: url(/static/carverse/img/common/checkbox_on@2x.png) no-repeat 0 0; background-size: 2.4rem; }
.checkbox_group input:disabled + label:before { background: url(/static/carverse/img/common/checkbox_disabled@2x.png) no-repeat 0 0; background-size: 2.4rem; }
.checkbox_group input:checked:disabled + label:before { background: url(/static/carverse/img/common/checkbox_checked_disabled@2x.png) no-repeat 0 0; background-size: 2.4rem; }

.checkbox_group.type_btn { width: 100%; flex-wrap: wrap; }
.checkbox_group.type_btn .inner { margin-right: 0; }
.checkbox_group.type_btn .inner input + label { min-height: 5.6rem; width: 100%; padding: 2rem 2.4rem; border-radius: 1.2rem; border:0.1rem solid #d8d8d8; font-weight: 700; background-color: #fff; }
.checkbox_group.type_btn .inner input + label:before { content: none; }
.checkbox_group.type_btn .inner input + label.large { height: 5.6rem; }
.checkbox_group.type_btn .inner input:checked + label { border-color: #00a88b;}
.checkbox_group.type_btn .inner input:checked + label .data_list .obj_key {font-weight:600; color:#242f4b;}
.checkbox_group.type_btn .inner input:checked + label .data_list .obj_value {color:#242f4b;}

.checkbox_group.type_btn .inner input:disabled + label { color: #ccc; background-color: #f8f8f8; border:none; }
.checkbox_group.full .inner { width: 100%; flex-wrap: nowrap; }

.radio_group label:before { display: inline-block; content: ""; width: 2.4rem; height: 2.4rem; margin-right: 0.4rem; background: url(/static/carverse/img/common/radio_off@2x.png) no-repeat 0 0; background-size: 2.4rem; }
.radio_group input + label:before { display: inline-block; content: ""; width: 2.4rem; height: 2.4rem; margin-right: 0.4rem; }
.radio_group input:checked + label:before { background: url(/static/carverse/img/common/radio_on@2x.png) no-repeat 0 0; background-size: 2.4rem; }
.radio_group input:disabled + label:before { background: url(/static/carverse/img/common/radio_disabled@2x.png) no-repeat 0 0; background-size: 2.4rem; }
.radio_group input:checked:disabled + label:before { background: url(/static/carverse/img/common/radio_checked_disabled@2x.png) no-repeat 0 0; background-size: 2.4rem; }
.radio_group.type_btn { width: 100%; flex-wrap: wrap; }
.radio_group.type_btn .inner { height: 5.6rem; width: calc(50% - 0.4rem); }
.radio_group.type_btn .inner:nth-child(odd) { margin-right: 0.8rem; }
.radio_group.type_btn .inner:nth-child(n+3) { margin-top: 2rem; }
.radio_group.type_btn .inner input + label { background-color: #fff; border-radius: 2.8rem; border: 0.1rem solid #d8d8d8; justify-content: center; height: 5.6rem; font-weight: 700; }
.radio_group.type_btn .inner input + label:before { content: none; }
.radio_group.type_btn .inner input:disabled + label,
.radio_group.type_btn .inner input:disabled:hover + label { color: #ccc; background-color: #f8f8f8; border:none; }
.radio_group.type_btn .inner input:hover + label,
.radio_group.type_btn .inner input:checked + label { background-color: #fff; border-radius: 2.8rem; border: 0.1rem solid #00a88b; box-sizing: border-box; color: #00a88b; }

.radio_group.type_btn.axis_x { align-items: center; }
.radio_group.type_btn.axis_x .inner { flex: 1; width: calc(25% - 0.8rem); margin: 0 0 0 0.8rem; }
.radio_group.type_btn.axis_x .inner:nth-of-type(1) { width: 25%; margin-left: 0; }
.radio_group.full .inner { width: 100%; }
.radio_group.full .inner:nth-child(odd) { margin-right: 0; }
.radio_group.full .inner:nth-child(n+2) { margin-top: 2rem; }

/* Input */
input[type=text]::-ms-clear { display: none; }

input[type=search]::-webkit-search-cancel-button { display: none; }

input[type=text],
input[type=password],
input[type=email],
input[type=tel],
input[type=search],
input[type=number],
input[type=file],
input[type=date],
input:-webkit-autofill,
input:-webkit-autofill:hover,
input:-webkit-autofill:focus,
input:-webkit-autofill:active { -webkit-box-shadow: 0 0 0 0 white inset; font: 1.6rem "Pretendard", sans-serif; }

/* input[type="password"] */
input[type=password]::-ms-reveal,
input[type=password]::-ms-clear { display: none; }

.input_wrap .input_box ~ .input_box { margin-top: 3.2rem; }
.input_wrap .input_box h3 { margin-bottom: 0.8rem; }

.input_field { display: inline-block; position: relative; }
.input_field input { width: 100%; height: 5.6rem; padding: 1.9rem 4rem 1.8rem 1.6rem; border:0.1rem solid #d8d8d8; border-radius: 1.2rem; color: #5e6167; }
.input_field input.active { border-color: rgb(34, 36, 38); outline: 0; }
.input_field input[readonly] { border-color: #d8d8d8; background-color: #f7f7f9; }
.input_field.error { border-color: #ff3a3a; border-radius: 1.2rem; }
.input_field .btn_clear { display: none; overflow: hidden; position: absolute; top: 50%; right: 1.6rem; width: 2.4rem; height: 2.4rem; transform: translateY(-50%); background: url(/static/carverse/img/common/icon_24_textclear.svg) no-repeat center; background-size: 2.4rem; }
.input_field .btn_clear span { position: absolute; width:0.1rem; height:0.1rem; padding: 0; margin:-0.1rem; overflow: hidden; clip: rect(0, 0, 0, 0); border: 0; }
.input_field.birth { border:0.1rem solid #d8d8d8; border-radius: 1.2rem; }
.input_field.birth:has(.active) { border-color: rgb(34, 36, 38); outline: 0; }
.input_field.birth input { width: 40%; border: 0; }
.input_field.birth input[type=password] { font-size: 1.6rem; }
.input_field.birth .btn_clear { position: static; transform: none; }
.input_field .input_btn { overflow: hidden; background-color: #eef2f9; border-radius: 0.8rem; color: #737d9a; font-size: 1.2rem; font-weight: 600; line-height: 2; position: absolute; top: 50%; right: 1.6rem; transform: translateY(-50%); padding: 0 0.8rem; height: 2.4rem; }

/* 검색인풋 */
.search_wrap { border-radius: 2.8rem; width: 43.2rem; height: 5.6rem; position: relative; }
.search_wrap .input_field { width: 100%; }
.search_wrap .input_field input { background-color: #f7f7f9; border-radius: 2.8rem; border: none; padding-right: 9.2rem; }
.search_wrap .input_field input.active { background-color: #fff; border: 0.1rem solid rgb(34, 36, 38); }
.search_wrap .input_field input::placeholder { color: #737d9b; opacity: 1; /* Firefox */}
.search_wrap .input_field input::-ms-input-placeholder { /* Edge 12 -18 */ color: #737d9b; }
.search_wrap .input_field .btn_clear { right: 6.8rem; }
.search_wrap .btn_search { background: url(/static/carverse/img/common/icon_input_search.svg) no-repeat 0 0; background-size: 3.2rem; width: 3.2rem; height: 3.2rem; position: absolute; top: 50%; right: 2.4rem; transform: translateY(-50%); }

.search_wrap.type_btn .input_field { height: 100%; }
.search_wrap.type_btn .input_field p { display: flex; height: 100%; padding: 0 1.6rem; align-items: center; background-color: #f7f7f9; border-radius: 2.8rem; border: none; padding-right: 9.2rem; color: #737d9b; }

/* Input - placeholder */
input::placeholder,
textarea::placeholder { color: #ccc; font-size: 1.6rem; font-weight: 400; letter-spacing: -0.32px; line-height: 1.5; }

/* Select */
select { height: 5.6rem; padding: 1.7rem 4rem 1.5rem 1.6rem; border:0.1rem solid #d8d8d8; border-radius: 1.2rem; color: #242f4b; font: 1.6rem "Pretendard", sans-serif; background: url(/static/carverse/img/common/icon_24_arrow_down.svg) no-repeat right 1.2rem top 50%; background-size: 2.4rem; -webkit-appearance: none; appearance: none; }
select:focus { outline:0.1rem solid rgb(34, 36, 38); }
select.error { outline:0.1rem solid #ff3a3a; }
select[disabled] { border-color: #d8d8d8; color: #ccc; background-color: #f8f8f8; }
select.full { width: 100%; }

.error_message { color: #ff3a3a; font-size: 1.2rem; letter-spacing: -0.48px; line-height: 2; }

/* Color */
.color_sec_03 { color: #00a88b; font-weight:700; }

.bg_black { background-color: #242f4b; }

.bg_gray50 { background-color: #f7f7f9; }

/* 가로 사이즈 */
.w_default { width: 100%; }

.hidden { display: none; position: absolute; width:0.1rem; height:0.1rem; padding: 0; margin:-0.1rem; overflow: hidden; clip: rect(0, 0, 0, 0); }

/* Heading, Text */
.sub_t1 { margin-top: 15.2rem; font-size: 4.4rem; font-weight: 600; text-align: center; letter-spacing: -0.88px; line-height: 1.64; }
.sub_t1_m { font-size: 3.2rem; line-height: 4.8rem; letter-spacing: -0.64px; margin-bottom: 4rem; word-break: keep-all; }
.sub_t2 { font-size: 3.2rem; font-weight: 600; letter-spacing: -0.64px; line-height: 1.5; word-break: keep-all; }

.heading1_b { font-size: 5rem; font-weight: 600; letter-spacing: -0.1rem; line-height: 1.44; }
.heading1_r { font-size: 5rem; font-weight: 400; letter-spacing: -0.1rem; line-height: 1.44; }
.heading2 { font-size: 4.4rem; font-weight: 600; letter-spacing: -0.88px; line-height: 1.64; }
.heading3 { font-size: 3.8rem; font-weight: 600; letter-spacing: -0.76px; line-height: 1.37; }

.btn_txt_1 { font-size: 3.2rem; font-weight: 600; letter-spacing: -0.64px; line-height: 1.06; }

.body1_b { font-size: 2.8rem; font-weight: 700; letter-spacing: -0.56px; line-height: 1.29; }
.body1_sb { font-size: 2.8rem; font-weight: 600; letter-spacing: -0.56px; line-height: 1.86; }
.body2_b { font-size: 2.6rem; font-weight: 700; letter-spacing: -0.52px; line-height: 1.23; }
.body3_b { font-size: 2.4rem; font-weight: 700; letter-spacing: -0.48px; line-height: 1.33; }
.body3_m { font-size: 2.4rem; font-weight: 500; letter-spacing: -0.48px; line-height: 1.33; }
.body4_sb { font-size: 2rem; font-weight: 600; letter-spacing: -0.4px; line-height: 1.2; }
.body4_b { font-size: 2rem; font-weight: 700; letter-spacing: -0.4px; line-height: 1.2; }
.body4_m { font-size: 2rem; font-weight: 500; letter-spacing: -0.4px; line-height: 1.2; }
.body4_r { font-size: 2rem; font-weight: 400; letter-spacing: -0.4px; line-height: 1.2; }
.body5 { font-size: 1.8rem; font-weight: 400; letter-spacing: -0.36px; line-height: 1.33; }
.body6_b { font-size: 1.6rem; font-weight: 700; letter-spacing: -0.32px; line-height: 1.5; }
.body6_r { font-size: 1.6rem; font-weight: 400; letter-spacing: -0.32px; line-height: 1.5; }
.body7_b { font-size: 1.4rem; font-weight: 700; letter-spacing: -0.28px; line-height: 1.71; }
.body7_m { font-size: 1.4rem; font-weight: 500; letter-spacing: -0.28px; line-height: 1.71; }

/* Tab Menu */
.tab_menu_wrap { position: relative; }
.tab_menu_wrap .tab_menu { display: flex; padding: 1.2rem; border-radius: 2.4rem; background-color: #fff; }
.tab_menu_wrap .tab_menu li { flex: 1; }
