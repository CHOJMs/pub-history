.data_none p:before { content: ""; background: url(/static/carverse/img/common/icon_64_warning@2x.png) no-repeat 0 0/6.4rem; position: absolute; top: 0; left: 50%; transform: translateX(-50%); width: 6.4rem; height: 6.4rem; }
