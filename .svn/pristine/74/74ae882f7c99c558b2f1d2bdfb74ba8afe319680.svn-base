<script type="text/javascript" src="/static/admin/js/common.js"></script>

<script type="text/javascript">

	$(document).ready(function(){

	});

	// 조회
	function goSearch(pageNum){

		var f = document.frm;
		f.METHOD.value = "AdminPCodeList";
		f.PGE_NO.value = pageNum;
		f.action = "/admin/AdminCodeAction.do";
		EncForm(f);
		f.submit();
	}

	
	/*function goDetail(seqNum,userId){

		var f = document.actionForm;
		f.METHOD.value = "UserInfoDtl";
		f.SEQ_NUM.value = seqNum;
		f.ID.value = userId;
		f.action = "/admin/AdminUserInfoAction.do";
		EncForm(f);
		f.submit();


	}*/

	// 입력화면 이동
	function goInsert(){

		var f = document.actionForm;
		f.METHOD.value = "AdminPCodeInsFrm";
		f.action = "/admin/AdminCodeAction.do";
		EncForm(f);
		f.submit();


	}

	// 변경
	function goUpdate(stockcd,cstocknm,rmscd,comcode,stocknum,prdcd,cstockcnm,stocknm,cnm){

		var f = document.actionForm;
		f.C_STOCK_CD.value = stockcd;
		f.C_STOCK_NM.value = cstocknm;
		f.C_RMS_CD.value = rmscd;
		f.COMCODE.value = comcode;
		f.STOCKNUM.value = stocknum;
		f.C_PRDCT_CD.value = prdcd;
		f.C_STOCK_CNM.value = cstockcnm;
		f.STOCK_NM.value = stocknm;
		f.C_NM.value = cnm;

		f.METHOD.value = "AdminPCodeUpdFrm";
		f.action = "/admin/AdminCodeAction.do";
		EncForm(f);
		f.submit();


	}


	// 삭제 ajax
	function goDelete(parent){
		if(confirm("삭제하시겠습니까?")){
			var frm  = document.actionForm;
			var sFrm = document.sendForm;
			frm.METHOD.value      = "AdminPCodeDel";	
			frm.STOCKNUM.value    = parent;
			sendAjaxAction(frm , sFrm, "callbackDelete", "/admin/AdminCodeAction.do");			
		}
	}


	//삭제 - 콜백
	function callbackDelete(result){
		var frm = document.frm;

		var INIpluginData = $(result).find("INIpluginData").attr("value");

		var ResDoc = Idecrypt(INIpluginData);
		var count  = $(ResDoc).find("RESULT").attr("value");

		if(count == "OK"){
			alert("정상적으로 삭제가 완료 되었습니다.");

			goSearch('1');
		}else{
			alert("삭제중 오류가 발생했습니다.");
		}
	}

</script>
</head>




