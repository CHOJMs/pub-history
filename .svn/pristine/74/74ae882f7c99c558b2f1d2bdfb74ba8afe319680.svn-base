			<input type="hidden" name="COMCODE" />
			<input type="hidden" name="STOCKNUM" />
			<input type="hidden" name="C_PRDCT_CD" />
			<input type="hidden" name="C_STOCK_CNM" />
			<input type="hidden" name="STOCK_NM" />
			<input type="hidden" name="C_NM" />
				

		</form>

		<form name="frm" method="post">

		  <input type="hidden" name="METHOD" />
		  <input type="hidden" name="INIpluginData" />
