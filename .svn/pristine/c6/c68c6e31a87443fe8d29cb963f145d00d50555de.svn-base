<script type="text/javascript">

	$(document).ready(function(){

	});


	function goInsert(){
		if(validate()){

			var frm  = document.frm;

			frm.METHOD.value = "AdminAutoCarCodeIns";
			frm.action = "/admin/AdminCodeAction.do";
			EncForm(frm);
			frm.submit();
		}
	}


	function validate(){
		var CAR_CD1 = $("input[name='CAR_CD1']").val();
	
		if( CAR_CD1 == '' ){
			alert("차종코드를 입력하세요.");
			$("input[name='CAR_CD1']").focus();
			return false;
		}

		return true;	
	}

	// 목록
	function goSearch(){

		var f = document.frm;
		f.METHOD.value = "AdminAutoCarCodeList";
		f.action = "/admin/AdminCodeAction.do";
		EncForm(f);
		f.submit();
	}

</script>
