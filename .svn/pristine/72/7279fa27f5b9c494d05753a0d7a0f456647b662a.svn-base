/* 공통 노이미지 */
img[src$="noimg_photo.png"] {opacity: 0 !important; width: 100% !important;}
.car-thumb-box .nodata {display: flex; align-items: center; justify-content: center; height: 80px; border-radius: 16px; background-color: #f7f7f9;}

/* 차량옵션정보 */
.schfilter-list {display: flex; flex-direction: column; gap: 10px}
.schfilter-box .schfilter-item {display: flex;}
.schfilter-box .schfilter-item .tit {color: #000; font-weight: 700; flex-shrink: 0; width: 50px; height: 45px; display: inline-flex; align-items: center;}
.schfilter-box .schfilter-item .caption {display: flex; gap: 8px; flex-wrap: wrap; width: 100%;}
.schfilter-box .schfilter-item .caption.scroll {flex-wrap: nowrap; overflow-x: auto; white-space: nowrap;}
.schfilter-box .schfilter-item .caption::-webkit-scrollbar {height: 12px;}
.schfilter-box .schfilter-item .caption::-webkit-scrollbar-track {border-radius: 0px; background: #f5f5f5; border-top: 1px solid #d3d5d8;}
.schfilter-box .schfilter-item .caption::-webkit-scrollbar-thumb {border-radius: 6px; background: #bcc0c5;}
.chk-schfilter .chktxt {display: flex; justify-content: center; align-items: center; border-radius: 8px; min-height: 45px; background-color: #fff; padding: 10px; min-width: 80px; box-shadow: inset 0 0 0 1px #d3d5d8; background: #fff; color: #5C6773; font-weight: 500; text-overflow: ellipsis; overflow: hidden; word-break: break-all;}
.chk-schfilter {position: relative;}
.chk-schfilter input[type="radio"],
.chk-schfilter input[type="checkbox"] {position: absolute; width: 1px; height: 1px; padding: 0; margin: -1px; overflow: hidden; clip: rect(0, 0, 0, 0); border: 0;}
.chk-schfilter input:checked + .chktxt {box-shadow: inset 0 0 0 2px rgb(18, 151, 239); background: #f5faff; color: #1571eb;}
.schfilter-box .nodata {display: flex; align-items: center; justify-content: center; height: 80px; border-radius: 16px; background-color: #f7f7f9;}

/* 테이블안 인풋 타입 */
.tbl-cominp {position: relative;}
.tbl-cominp .inp-txt-right span {color: #919191; font-weight: 500;}
.tbl-cominp .inp-txt-right span:last-child {position: absolute; right: 10px; top: 50%; transform: translateY(-50%);}
.tbl-cominp input[type=text],
.tbl-cominp input[type=tel],
.tbl-cominp input[type=email],
.tbl-cominp input[type=password] {width: 100%; border: 1px solid #d3d5d8; border: 1px solid #d3d5d8; height: 46px !important;border-radius: 4px !important; padding: 0 10px; width: 100%;}
.bbs-table .inp-inline {display: flex; gap: 5px;}
.bbs-table .inp-inline .tbl-cominp.widPer {width: 70px; flex-shrink: 0;}

/* 다중선택용 셀렉트 모양의 체크박스 */
.selchk-box {width: 100%;}
.selchk-box .selchk-selected {height: 56px; line-height: 56px; color: #919191; border: 1px solid #d3d5d8; font-weight: 500; border-radius: 8px; font-family: "pretendard"; font-size: 16px; padding: 0 52px 0 24px; -webkit-appearance:none; -moz-appearance:none; appearance:none; background: url('/static/admin/img/new_common/ico_sel_arrow.png') no-repeat right 20px center; background-size: 24px; overflow: hidden;}
.selchk-box .selchk-selected.color {color: #222426;}
.selchk-box .selchk-option {display: none; position: absolute; top: 100%; left: 0; right: 0; border: 1px solid #d3d5d8; border-radius: 8px; z-index: 2;
background: #fff; box-shadow: 0 8px 8px rgb(0 0 0 / 8%); margin-top: -1px; overflow-y: auto; max-height: 390px;}
.selchk-box.active .selchk-option {display: block;}
.selchk-box .selchk-list .selchk-item {padding: 4px 0;}
.selchk-box .btn-group {margin: 10px 8px 20px;}
.inp-area .selchk-box .selchk-list,
.inp-area .selchk-box .selchk-selected {padding-left: 120px;}

/* 데이터 없음 */
.noDataTxt {display: flex; align-items: center; justify-content: center; width: 100%;}

/* 첨부파일 리스트형 */
.cntattach-box {position: relative; border: 1px solid #d3d5d8; border-radius: 8px; min-height: 56px; display: flex;}
.cntattach-tit {width: 144px; flex-shrink: 0; padding: 12px 24px; display: flex; align-items: center;}
.attachfile-box {display: flex; align-items: center;}
.attachfile-list {padding: 12px 24px; display: flex; align-items: center; min-height: 56px; min-width: 0; border-left: 1px solid #d3d5d8; border-right: 1px solid #d3d5d8; width: 100%;}
.attachfile-none { color: #919191;}
.attachfile-btnwrap {width: 127px; flex-shrink: 0; display: flex; align-items: center; justify-content: center;}
.attachfile-btnwrap label {display: inline-block; background: #1a3755; padding: 8px 14px; cursor: pointer; color: #fff; font-size: 14px; border-radius: 4px;
vertical-align: middle; line-height: inherit; width: 80px;}
.attachfile-fileWrap {display: flex; flex-direction: column; gap: 5px 0; width: 100%;}
.attachfile-file {padding-left: 20px; position: relative; display: flex; align-items: center;}
.attachfile-file::before {content: ''; position: absolute; left: 0; top: 0; width: 20px; height: 20px; margin-right: 8px; background: url('/static/admin/img/new_common/ico_file.png') no-repeat 0 0; background-size: 20px;}
.attachfile-file .attachfile-del {display: inline-block; width: 16px; height: 16px; margin-left: 8px; flex-shrink: 0;}
.attachfile-file .attachfile-del img {vertical-align: top;}
.attachfile-file .attachfile-name {white-space: nowrap; display: block; text-overflow: ellipsis; overflow: hidden;}

/* 대시보드 상단 정보 */
.dashboard-info-list {display: flex; align-items: center; gap: 12px}
.dashboard-info-item {width: 100%; text-align: center; border-radius: 8px; border: 1px solid #d3d5d8; display: flex; flex-direction: column; justify-content: center; padding: 20px 10px;}
.dashboard-info-item .ico {display: block; height: 48px;}
.dashboard-info-item:hover .ico {transform: rotateY(360deg); transition-duration: 0.4s;}
.dashboard-info-item .ico img {width: auto; height: 100%;}
.dashboard-info-item .dashboard-info-tit {color: #5c6773; font-weight: 500;}
.dashboard-info-item .won {font-weight: 700; font-size: 20px;}

/* 차트 */
.chartArea {display: flex; gap: 20px; width: 100%;}
.chartArea-item {width: calc(100% - 20px);}
.chartBox {border:1px solid #d3d5d8; border-radius: 8px; padding: 20px; width: 100%;;}

/* 볼륨인센티브 */
.car-incentive-list {display: flex; gap: 20px; width: 100%;}
.car-incentive-item {width: calc(100% - 20px); border: 1px solid #d3d5d8; border-radius: 8px;}
.car-incentive-item.car-graph-box {width: calc(100% - 60px);}

/* 차모양 그래프 */
.car-graph-box {padding: 25px 30px;}
.car-graph-list {display: flex; align-items: center; gap: 10px;}
.car-graph-item {text-align: center;}
.car-graph-item .won {display: flex; align-items: center; justify-content: center; background: url('/static/admin/img/new_common/ico_car_incentive_bg.png') no-repeat 0 0; background-size: cover; width: 48px; height: 48px;}
.car-graph-list .car-graph-item.active .won {color: #fff;}
.car-graph-list.rent .car-graph-item.active .won {background-image: url('/static/admin/img/new_common/ico_car_incentive_01.png');}/* 렌트 */
.car-graph-list.lease .car-graph-item.active .won {background-image: url('/static/admin/img/new_common/ico_car_incentive_02.png');}/* 국산리스 */
.car-graph-list.lease-importe .car-graph-item.active .won {background-image: url('/static/admin/img/new_common/ico_car_incentive_03.png');}/* 수입리스 */

.car-graph-wrap:nth-of-type(1) {margin-top: 0;}
.car-graph-wrap {margin-top: 20px; display: flex; align-items: center;}
.car-graph-wrap .car-graph-tit {display: inline-flex; flex-shrink: 0; width: 100px;}
.car-graph-wrap .car-graph-tit-line {font-size: 18px; position: relative; font-weight: 500;}
.car-graph-wrap .car-graph-tit-line::after {content: ""; position: absolute; left: -4px; bottom: -2px; width: calc(100% + 8px); height: 12px;
background-color: #b7f4ff;}
.car-graph-wrap .car-graph-tit-line span {position: relative; z-index: 1;}

/* 볼륨인센티브 결과 */
.result-car-box {padding: 30px 0;}
.result-car-list {display: flex; height: 100%;}
.result-car-item {width: 100%; padding: 0 15px; display: flex; flex-direction: column; text-align: center; position: relative;}
.result-car-list .result-car-item::before {content: ""; width: 1px; height: 100%; position: absolute; left: 0; top: 0; background-color: #d3d5d8;}
.result-car-list .result-car-item:nth-of-type(1)::before {display: none;}
.result-car-item .result-car-tit {display: block; font-weight: 500; font-size: 16px; margin-bottom: 5px;}
.result-car-item .result-car-top .ico {height: 48px;}
.result-car-item .result-car-top .ico img {width: auto;}
.result-car-item .result-car-btm .won {font-size: 20px; font-weight: 700;}
.result-car-item .result-car-btm .result-message-wrap {border-top: 1px dashed #d3d5d8; padding-top: 20px; margin-top: 20px;}
.result-car-item .result-car-btm .result-message-money { display: block; font-size: 20px; font-weight: 700; color: #00a88b; animation: crescendo 1s alternate infinite ease-in;}
@keyframes crescendo {
  0%   {transform: scale(.8);}
  100% {transform: scale(1);}
}
.result-car-item .result-car-btm .result-message-wrap .result-message-tit {margin: 10px 0 0; font-weight: 500; font-size: 14px; color: #5c6773; display: block;}
.result-car-item .result-car-btm .result-message-wrap .result-message-won {font-weight: 700; font-size: 16px;}
.result-car-item .result-car-btm .result-message-done {background: #efeef3; border-radius: 4px; display: inline-flex; align-items: center; justify-content: center; width: 100%; height: 80px; font-size: 18px; font-weight: 700;}
.result-car-item .result-car-btm .result-message-ready {display: inline-flex; align-items: center; justify-content: center; width: 100%; height: 80px;}

/* 월별수수료 */
.priceList {display: flex; gap: 20px; width: 100%;}
.price-item {border: 1px solid #d3d5d8; border-radius: 20px; width: 100%; display: flex; align-items: center; overflow: hidden; }
.price-item.wid {width: calc(40% - 20px);}
.price-item .price-body {padding: 20px; width: 100%;}
.price-item .price-tit {font-weight: 500; color: #5C6773; margin-bottom: 10px;}
.price-item .price-caption {display: flex; align-items: center; align-items: center;}
.price-item .price-caption .won {font-size: 24px; color: #2a2a2a;}

.price-item .price-caption .per,
.priceDetail-caption .per {display: inline-flex; align-items: center; justify-content: center; background-color: #e4f9ee !important;
color: #31cf80 !important; border-radius: 9999em; padding: 0 10px; font-size: 15px; height: 26px; margin-right: 4px;}
.priceDetail-caption .won {font-weight: normal; line-height: 1.5; font-weight: 500; font-size: 20px;}
.priceDetailList {position: relative; width: 100%; padding: 20px 25px;}
.priceDetailList::before {content: ""; position: absolute; left: 0; top: 50%; transform: translateY(-50%); height: calc(100% - 40px); width: 1px; background-color: #d3d5d8;}
.priceDetail-body {display: flex; align-items: center; width: 100%; justify-content: space-between;}
.priceDetail-caption{display: flex; align-items: center;}

.agentValueBox .nodata {display: flex; align-items: center; justify-content: center; height: 80px;}
.agentValueList {width: 100%; display: flex; gap: 20px; flex-wrap: wrap; position: relative;}
.agentValueList.link .agentValue-item {cursor: pointer;}
.agentValueList.link .agentValue-item:hover,
.agentValueList.link .agentValue-item:active {box-shadow: inset 0 0 0 2px rgb(18, 151, 239);}
.agentValue-item {width: calc(25% - 15px); background: #fff; border-radius: 10px; box-shadow: inset 0 0 0 1px #d3d5d8;}
.agentValue-item.active {background-color: #f5faff !important; box-shadow: inset 0 0 0 2px rgb(18, 151, 239);}
.agentValue-item.active * {color: #1571eb !important; border-color: rgb(18, 151, 239) !important; }
.agentValue-item .agentValue-tit {font-size: 20px; font-weight: 600; white-space: nowrap; overflow: hidden; display: block; text-overflow: ellipsis;}
.agentValue-item .agentValue-topWrap {border-bottom: 1px solid #d3d5d8; padding-bottom: 10px; margin-bottom: 10px;}
.agentValue-item .agentValue-topWrap .count {text-align: right; color: #000; margin-top: 4px}
.agentValue-item .agentValue-topWrap .count strong {font-weight: 600;}

/* 스크롤자동 테이블 */
.money-table-scroll {width: 100%; overflow-x: auto; -webkit-overflow-scrolling: touch;}
.money-table-scroll::-webkit-scrollbar {height: 12px;}
.money-table-scroll::-webkit-scrollbar-track {border-radius: 0px; background: #f5f5f5; border-top: 1px solid #d3d5d8;}
.money-table-scroll::-webkit-scrollbar-thumb {border-radius: 6px; background: #bcc0c5;}
.money-table th {font-weight: normal;}
.money-table th,
.money-table td {padding: 5px;}
.money-table th.a_r,
.money-table td.a_r {text-align: right !important;}
.money-table {border-collapse: collapse; white-space: nowrap; width: 100%;}
.agentValue-inner {padding: 20px;}

/* 사진팝업 (사진크기 자동) */
.popup.view-autoPhoto {height:800px;}
.popup.view-autoPhoto .inner {padding-top:24px;}
.popup.view-autoPhoto h2 {text-align:center;}
.popup.view-autoPhoto .flex-item-top .item-cnt {padding:40px 80px; position:relative; overflow:hidden; background-color:#f7f7f9;}
.popup.view-autoPhoto .swiper {overflow:inherit; position:static;}
.popup.view-autoPhoto .swiper-wrapper {z-index:0;}
.popup.view-autoPhoto .swiper-slide {position:relative;}
.popup.view-autoPhoto .swiper-button-next::after,
.popup.view-autoPhoto .swiper-button-prev::after {display: none;}
.popup.view-autoPhoto .swiper-button-next {background: url('../img/common/ico_arrow_right.png') no-repeat center / 40px; right:24px;}
.popup.view-autoPhoto .swiper-button-prev  {background: url('../img/common/ico_arrow_left.png') no-repeat center / 40px; left:24px;}
.popup.view-autoPhoto .swiper-slide .autoPhoto-cover {background: #f7f7f9 url('../img/new_common/noimg_photo.png') no-repeat 50% 50%; display: flex; align-items: center; justify-content: center; height: 500px;}
.popup.view-autoPhoto .swiper-slide .autoPhoto-cover img {background-color: #f7f7f9; width: auto;}

/* 보험관리 - 파일첨부팝업 */
.flex-item-top .list-type{margin-bottom:40px}
.list-type > li{position:relative;padding-left:10px;font-size:15px;color:#5C6773;line-height:1.5;text-align:left}
.list-type > li:before{position:absolute;top:9px;left:0;width:3px;height:3px;border-radius:50%;background:#5c6773;content:'';}
.list-type + .filebox{margin-top:16px}
.sms-tx + .sms-tx02{margin-top:10px}
.sms-tx02{color:#707478;line-height:24px}