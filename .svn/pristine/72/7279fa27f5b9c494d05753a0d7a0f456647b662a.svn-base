/* 대출접수현황 말줄임 */
/* 대출접수현황 툴팁, 말줄임 */
.scrollWrap {position: relative;}
.scrollWrap span.tooltip .tooltip-ellipsis{position: relative;cursor:pointer;text-decoration: underline;z-index: 1; text-underline-position: under;}
.scrollWrap span.tooltip .tooltip-contents {visibility: hidden;position: absolute;padding: 10px 16px 10px 16px;background: rgb(240, 249, 244);text-align: left;border-radius: 4px;border: 1px solid rgb(0, 168, 139);top: 54px;left: 0;width: 200px;z-index: 2;font-size: 14px;color: rgb(34,36,38);line-height: 24px;}
.scrollWrap span.tooltip .tooltip-contents:after {content: ""; position: absolute; left: 50%; bottom: 100%; transform: translate(-50%, 2px); border-bottom: 13px solid rgb(240, 249, 244); border-top: 13px solid transparent; border-left: 8px solid transparent; border-right: 8px solid transparent; z-index: 2;}
.scrollWrap span.tooltip .tooltip-contents:before {content: ""; position: absolute; left: 50%; bottom: 100%; transform: translate(-50%, 0px); border-bottom: 13px solid rgb(0, 168, 139); border-top: 13px solid transparent; border-left: 8px solid transparent; border-right: 8px solid transparent; z-index: 2;}
.scrollWrap span.tooltip:hover .tooltip-contents {visibility: visible;}
.scrollWrap span.tooltip:hover .tooltip-ellipsis{z-index: 3;}

