
@charset "utf-8";

@import url("calendar.css");

/* 관리자 CSS */

/* font */
@font-face {
    font-family: 'pretendard';
    font-style: normal;
    font-weight: 400;
    src: url('../../common/css/font/Pretendard-Regular.eot');
    src: url('../../common/css/font/Pretendard-Regular.eot?iefix') format('eot'), url('../../common/css/font/Pretendard-Regular.woff') format('woff'), url('../../common/css/font/Pretendard-Regular.ttf') format('truetype');
}
@font-face {
    font-family: 'pretendard';
    font-style: normal;
    font-weight: 500;
    src: url('/static/common/css/font/Pretendard-Medium.eot');
    src: url('/static/common/css/font/Pretendard-Medium.eot?iefix') format('eot'),
         url('/static/common/css/font/Pretendard-Medium.woff') format('woff'),
         url('/static/common/css/font/Pretendard-Medium.ttf') format('truetype');
}
@font-face {
    font-family: 'pretendard';
    font-style: normal;
    font-weight: 600;
    src: url('/static/common/css/font/Pretendard-SemiBold.eot');
    src: url('/static/common/css/font/Pretendard-SemiBold.eot?iefix') format('eot'),
         url('/static/common/css/font/Pretendard-SemiBold.woff') format('woff'),
         url('/static/common/css/font/Pretendard-SemiBold.ttf') format('truetype');
}
@font-face {
    font-family: 'pretendard';
    font-style: normal;
    font-weight: 700;
    src: url('/static/common/css/font/Pretendard-Bold.eot');
    src: url('/static/common/css/font/Pretendard-Bold.eot?iefix') format('eot'),
         url('/static/common/css/font/Pretendard-Bold.woff') format('woff'),
         url('/static/common/css/font/Pretendard-Bold.ttf') format('truetype');
}

html, body {width:100%; height:100%; font-family:'pretendard'; font-size:16px; line-height:1.2; letter-spacing: -0.01em; color:#222426;}
* {margin:0; padding:0; border:0; vertical-align:middle; box-sizing:border-box;}
ul, li {list-style: none; margin: 0; padding: 0;}
img {border: none; vertical-align: middle; width: 100%;}

a {color:#222426; text-decoration:none;}
a:hover {color: #1571eb; text-decoration: none;}

:focus {outline: none;}

/* 테이블 */
table, th, td {border: none; border-spacing: 0; border-collapse: collapse; word-break: keep-all; word-break: break-all;}
td img {vertical-align: middle;}
legend, caption{display:none;}

input[type=text],
input[type=password],
input[type=submit],
input[type=tel] {height: 56px !important; border-radius: 8px; padding-left: 24px; font-size: 16px; font-weight: 500; letter-spacing: -0.16px; box-sizing: border-box; appearance: none; font-family: "pretendard"; color: #222426; padding-right: 53px; background: transparent;}
input[type=submit]{padding:0 20px; color:#fff; background:#0e4194; border-color:#0e4194;}
input[type=checkbox] {appearance: auto; }
input[type="radio"] {background: transparent; appearance: auto;}
input[type="radio"] {display: inline-block;  width:26px; height:26px; vertical-align: middle; appearance: none; outline:none;  background: #fff;}

textarea {box-sizing:border-box; width:100%; height:130px; background: transparent; vertical-align:top; border:none; font-size: 16px; color:#222426; font-family: pretendard; word-break:break-all; font-weight: 500; letter-spacing: -0.16px; font-family: "pretendard"; resize: none;}
textarea::placeholder {color: #919191;}
textarea::-webkit-scrollbar {width: 12px;}
textarea::-webkit-scrollbar-track {border-radius: 0px; background: transparent;}
textarea::-webkit-scrollbar-thumb {border-radius: 30px; background: #d3d5d8; border: 3px solid #ffffff;}

input[type=button]{vertical-align:top;}
input::placeholder {color: #919191; line-height: 56px;}

/* 파일 필드 숨기기 */
input[type="file"] {position: absolute; width: 1px; height: 1px; padding: 0 !important; margin: -1px; overflow: hidden; clip:rect(0,0,0,0); border: 0;}

/* disabled */
.disabled {border: 1px solid #f5f5f5 !important; background: #f5f5f5 !important; border-radius: 8px;}
.btn a.disabled {color: #fff; background: #bcc0c5 !important; border: 1px solid #bcc0c5 !important; cursor: default;}

/* select - disabled */
.disabled .selchk-selected,
.disabled .select-selected,
.disabled .calendar,
.disabled input, .disabled textarea,
.com-inp.disabled .inp-area {border: none !important;}

/* 체크박스 : 로그인 */
.checkbox-1 {overflow:hidden; position: relative;}
.checkbox-1 input {position:absolute; left:0; top:0; width: 100%; height: 100%; margin:0; background-color: transparent; opacity: 0;}
.checkbox-1 label {display: block; padding: 3px 0 3px 32px; background: url('../img/new_common/ico_check_off.png') no-repeat 0 0; background-size: 24px;}
.checkbox-1 input:checked:after {display: none;}
.checkbox-1 input:checked + label {color: #5C6773; background: url('../img/new_common/ico_check_on1.png') no-repeat 0 0; background-size: 24px;}

/* 체크박스 */
.checkbox {overflow:hidden; position: relative; margin: 0;}
.checkbox input {position:absolute; left:0; top:0; width: 100%; height: 100%; margin:0; background-color: transparent; opacity: 0 !important;}
.checkbox label {display: block; padding: 3px 0 3px 32px; font-size: 16px; background: url('../img/new_common/ico_chk_off.png') no-repeat 0 0; background-size: 24px; line-height: inherit;}
.checkbox input:checked:after {display: none;}
.checkbox input:checked + label {color: #222426; background: url('../img/new_common/ico_chk_on.png') no-repeat 0 0; background-size: 24px;}
.checkbox input.disabled + label {color: #919191; background: url('../img/new_common/ico_chk_disa.png') no-repeat 0 0; background-size: 24px;}
.checkbox input.disabled-chk + label {color: #919191; background: url('../img/new_common/ico_chk_disa01.png') no-repeat 0 0; background-size: 24px;}
.checkbox.bold label{font-size:18px;font-weight:700;line-height:1;}

/* 체크박스 -  텍스트미노출 */
.checkbox.single {display: inline-block;}
.checkbox.single input {width: 24px; height: 24px;}
.checkbox.single label {font-size: 0; width: 24px; height: 24px; padding: 0; background: url('../img/new_common/ico_chk_off.png') no-repeat 0 0; background-size: 24px;}

/* 체크박스 -  텍스트미노출 */
.radiobox.single {display: inline-block;}
.radiobox.single input {width: 24px; height: 24px;}
.radiobox.single label {font-size: 0; width: 24px; height: 24px; padding: 0; background: url('../img/new_common/ico_radio_off.png') no-repeat 0 0; background-size: 24px;}

/* 라디오박스 */
.inp-radio {position: relative; display: flex; align-items: center; justify-content: flex-start; padding: 15px 0 15px 24px; border-left: 1px solid #d3d5d8;}
.radiobox {overflow:hidden; position: relative; margin-left: 40px;}
.radiobox:first-of-type {margin-left: 0px;}
.radiobox input {position:absolute; left:0; top:0; width: 100%; height: 100%; margin:0; background-color: transparent; opacity: 0 !important;}
.radiobox label {display: block; padding: 3px 0 3px 32px; background: url('../img/new_common/ico_radio_off.png') no-repeat 0 0; background-size: 24px; line-height: inherit;}
.radiobox input:checked:after {display: none;}
.radiobox input:checked + label {color: #222426; background: url('../img/new_common/ico_radio_on.png') no-repeat 0 0; background-size: 24px;}
.radiobox input.disabled + label {background: url('../img/new_common/ico_radio_disa.png') no-repeat 0 0; background-size: 24px; color: #919191;}
.radiobox input.disabled-chk + label {background: url('../img/new_common/ico_radio_disa01.png') no-repeat 0 0; background-size: 24px; color: #919191;}

/* 라디오박스 : 탭 유형 */
.radio-tab {display: flex; justify-content: space-between; flex-wrap: wrap; align-items: center; width: 100%;}
.radio-tab input {position:absolute; left:0; top:0; width: 100%; height: 100%; margin:0; background-color: transparent; opacity: 0 !important; padding: 0 !important;}
.radio-tab .com-inp {flex: 1; display: inline-flex; margin: 0 !important;}
.radio-tab .com-inp label {border: 1px solid #d3d5d8; background: #fff; border-left: none; color: #5C6773; font-weight: 500; width: 100%; display: inline-flex; align-items: center; justify-content: center;}
.radio-tab .com-inp input:checked + label {position: relative; border-color: #1571eb; background: #f5faff; color: #1571eb;}
.radio-tab .com-inp input:checked + label::before {content: ''; position: absolute; left: -1px; top: -1px; width: 1px; height: 100%; background: #1571eb; border-top: 2px solid #1571eb;}
.radio-tab .com-inp:first-child input:checked + label::before {top: 5px; height: 79%;}
.radio-tab .com-inp:first-child label {border-radius: 8px 0 0 8px; border-left: 1px solid #d3d5d8;}
.radio-tab .com-inp:last-child label {border-radius: 0 8px 8px 0;}

/* 라디오박스 : 탭 유형2(주소조회) */
.post-box {padding: 0 40px 40px; background: #fff;}
.radio-tab1 {display: flex; justify-content: space-between; flex-wrap: wrap; align-items: center; width: 100%; margin-bottom: 24px;}
.radio-tab1 input {position:absolute; left:0; top:0; width: 100%; height: 100%; margin:0; background-color: transparent; opacity: 0 !important; padding: 0 !important;}
.radio-tab1 .com-inp {flex: 1; display: inline-flex; margin: 0 !important; min-height: 40px;}
.radio-tab1 .com-inp label {border-bottom: 1px solid #d3d5d8; background: #fff; border-left: none; color: #919191; font-size: 20px; font-weight: 500; width: 100%; display: inline-flex; align-items: center; justify-content: center; padding-bottom: 17px; line-height: 21px;}
.radio-tab1 .com-inp input:checked + label {position: relative; border-bottom: 3px solid #1571eb; color: #222426; font-weight: 700; padding-bottom: 16px;}

/* 라디오박스 : 탭 list 유형 */
.radio-list {border-top:1px solid #d3d5d8;}
.radio-list li {display: flex; justify-content: flex-start;}
.radio-list li > div.txt {display: inline-flex; justify-content: flex-start; align-items: center; border:1px solid #d3d5d8; border-right: none; width: calc(100% - 160px); height: 56px; padding-left: 24px; padding-right: 20px; border-top: none;}
.radio-list li > div.txt span {font-size: 14px; color: #fff; display: flex; width: 24px; height: 24px; margin-right: 8px; background: #1a3755; border-radius: 50%; align-items: center; justify-content: center;}
.radio-list li > div.txt p {width: calc(100% - 32px);}
.radio-list li .radio-tab {padding-left: 0; width: 160px;}
.radio-list li .radio-tab .com-inp label {border-radius: 0 !important; border-top: none;}
.radio-list li .radio-tab .com-inp input:checked + label::before {content: ''; position: absolute; left: -1px; top: -1px; width: 1px; height: 100%; background: #1571eb; border-top: 1px solid #1571eb;}
.radio-list li .radio-tab .com-inp input:checked + label::after {content: ''; position: absolute; left: 0px; top: -1px; width: 100%; height: 1px; background: #1571eb; }
.radio-list .inp-box {padding-left: 0; width: 160px;}
.radio-list .inp-box .com-inp {margin-bottom: 0;}
.radio-list .inp-box .com-inp input {border-radius: 0; border-top: none; padding-left: 24px;}

/* ect */
.ico-info {position: relative; font-size: 14px; line-height: 22px; color: #555555; padding-left: 24px; text-align: left; word-break: break-all; word-wrap: break-word;}
.ico-info::before {content: ''; position: absolute; left: 0; top: 2px; margin-right: 8px; display: inline-block; width: 16px; height: 16px; background: url('/static/admin/img/new_common/ico_info.png') no-repeat; background-size: 16px;}
.dash {position: relative; flex: none; width: 15px; text-align: center; color: #fff; margin-right: -23px;}
.dash::before {content: ''; display: block; position: absolute; left: 50%; top: 50%; width: 8px; height: 1px; margin: 1px 0 0 -5px; background: #707478;}

/* margin */
.mt0 {margin-top: 0px !important;}
.mt4 {margin-top: 4px;}
.mt8 {margin-top: 8px !important;}
.mt10 {margin-top: 10px;}
.mt16 {margin-top: 16px;}
.mt20 {margin-top: 20px;}
.mt24 {margin-top: 24px !important;}
.mt40 {margin-top: 40px !important;}
.mt48 {margin-top: 48px !important;}
.mt64 {margin-top: 64px !important;}
.mt70 {margin-top: 70px !important;}
.mt80 {margin-top: 80px !important;}
.mt-60 {margin-top: -60px !important;}
.mb0 {margin-bottom: 0px !important;}
.mb36 {margin-bottom: 36px !important;}
.mb40 {margin-bottom: 40px !important;}
.mb60 {margin-bottom: 60px !important;}
.mb64 {margin-bottom: 64px;}
.mb72 {margin-bottom: 72px;}
.mr0 {margin-right: 0px !important;}
.mr24 {margin-right: 24px !important;}
.mr80 {margin-right: 80px !important;}
.mr-20 {margin-right: -20px;}


/* padding */
.pt20 {padding-top: 20px !important;}
.pr0 {padding-right: 0px !important;}
.pr100 {padding-right: 100px !important;}
.pr110 {padding-right: 110px !important;}
.pb0 {padding-bottom: 0px !important;}

/* 고정픽셀 */
.w-70 {width: 70px !important;}
.w-80 {width: 80px !important;}
.w-100 {width: 100px !important;}
.w-120 {width: 120px !important;}
.w-150 {width: 150px !important;}
.w-160 {width: 160px !important;}
.w-180 {width: 180px !important;}
.w-200 {width: 200px !important;}
.w-240 {width: 240px !important;}
.w-320 {width: 320px;}
.w-560 {width: 560px !important;}

/* 퍼센트기준 */
.w-per50 {width:50% !important;}

/* 정렬 */
.a-r {text-align: right;}
.a-l {text-align: left;}
.a-c {text-align: center;}
.v-t{vertical-align:top;}
.float-r {float: right;}
.flex-r {display: flex; justify-content: flex-end !important;}

/* box-color */
.box-red {background: #fef1f1; border-radius: 8px; text-align: center; font-size: 14px; color: #ef4040; padding: 12px 0;}

/* header */
.headerwrap {display: flex; align-items: center; position: fixed; top: 0; width:100%; height:96px; min-width:10px; background:#1a3755; z-index: 3;}
.headerwrap h1{margin-left: 64px; font-size: 0;}
.headerwrap h1 img {width: 160px;}
.headerwrap .gnb-menu {margin-left: 308px; width:100%; display: flex; align-items: center; justify-content: space-between;}
.headerwrap .gnb-menu li {display: inline-flex;}
.headerwrap .gnb-menu li a {display: inline-flex; align-items: center; padding: 0 16px; height: 96px; position: relative; font-size: 18px; font-weight: 500; color: #fff}
.headerwrap .gnb-menu li a.on, .header .gnb-menu li a:hover {color: #fff}
.headerwrap .gnb-menu li a.on::after,
.headerwrap .gnb-menu li a:hover::after {content: ""; display: block; position: absolute; bottom: 0; left: 16px; width: calc(100% - 32px); height: 4px; background: #fff; transition: all 0.3s; z-index: 2;}
.headerwrap .gnb-menu .logout a {font-size: 16px; color: #fff; font-weight: 500; padding-right: 64px;}
.headerwrap .gnb-menu .logout img {width: 16px; height: 16px; margin-left: 8px;}

/* 서브레이아웃 */
.sub-contents {width:100%; min-width:1700px; position: relative; display: inline-flex; justify-content: flex-start; min-height: calc(100vh - 154px); margin: 96px auto 0;}
.sub-contents .contents {width: calc(100% - 280px); margin-left: 280px; padding: 48px 64px 80px 64px;}
.sub-contents .contents .head-tit {position: fixed; top: 96px; width: calc(100% - 280px); background: #fff; padding: 48px 64px 48px 64px; margin-left: -64px; z-index: 3;}
.sub-contents .contents h2 {font-size: 40px; font-weight: 700;}
.sub-contents .contents h3 {font-size: 24px; font-weight: 700; margin-bottom: 24px; margin-top: 80px;}
.sub-contents .contents h3:first-child {margin-top: 0;}
.sub-contents .contents h3 span {font-size: 18px;}
.sub-contents .contents .s-contWrap { margin-top: 112px;}
.sub-contents .contents h3 .count {display: inline-flex; color: #5c6773;}

/* left */
.left-wrap {width: 280px; position: fixed; top: 96px; background: #f1f5fa; min-height: 100%; z-index: 2;}
.left-menu {overflow-y: auto; max-height: calc(100vh - 174px); padding: 38px 0 44px 0;}
.left-menu li a {position: relative; width:100%; font-size: 18px; color: #5c6773; font-weight: 500; display: inline-flex; align-items: center; padding: 16px 0 16px 64px;}
.left-menu li a.on, .left-menu li a:hover {color: #1571eb; padding: 16px 0 16px 64px;}
.left-menu::-webkit-scrollbar {width: .4rem;}
.left-menut::-webkit-scrollbar-track {border-radius: 3rem; background: #fff; margin: .5rem 0;}
.left-menu::-webkit-scrollbar-thumb {border-radius: 3rem; background: rgb(211, 215, 221);}

/* footer */
.footerwrap {width:100%; min-width:1700px; border-top:1px solid #dfe3e8; position: relative; background: #fff; z-index: 2;}
.footerwrap .company_info {text-align: center; font-size: 13px; color: #707478; width:100%; margin:0 auto; padding:20px 0 20px 0px; display: flex; justify-content: center;}
.footerwrap .company_info span::after {content: ''; display: inline-block; width: 1px; height: 12px; margin: 0 8px; background: #dfe3e8; vertical-align: middle;}
.footerwrap .company_info address {font-style: normal;}
.footerwrap .company_info address span {vertical-align: top;}
.footerwrap .company_info .copyright::before {content: ''; display: inline-block; width: 1px; height: 12px; margin: 0 8px; background: #dfe3e8; vertical-align: middle;}
.footerwrap .company_info .copyright strong {font-weight: 600;}

/* scroll-box */
.x-scrollBox {overflow-x: scroll;}
.x-scrollBox::-webkit-scrollbar {height: 12px;}
.x-scrollBox::-webkit-scrollbar-track {border-radius: 0px; background: #f5f5f5; border-top: 1px solid #d3d5d8;}
.x-scrollBox::-webkit-scrollbar-thumb {border-radius: 6px; background: #bcc0c5;}
.y-scrollBox {overflow-y: scroll; margin-right: -20px; padding-right: 20px;}
.y-scrollBox::-webkit-scrollbar {width: 6px;}
.y-scrollBox::-webkit-scrollbar-track {border-radius: 0px; background: transparent;}
.y-scrollBox::-webkit-scrollbar-thumb {border-radius: 3px; background: #919191;}

.scrollBox {overflow: auto;}
.scrollBox::-webkit-scrollbar {width: 6px; height: 12px;}
.scrollBox::-webkit-scrollbar-track {border-radius: 0px; background: #f5f5f5;}
.scrollBox::-webkit-scrollbar-thumb {border-radius: 6px; background: #bcc0c5;}

.no-scroll {overflow-y: inherit !important;}

/* flex */
.flex-area {display: flex; justify-content: space-between; align-items: center;}
.flex-area .txt-right {position: relative; top: inherit !important; transform: inherit !important;}
.flex-area .inp-chk {min-width: 460px;}

/* 폰트정렬 */
.txt-right {position: absolute; right: 24px; top: 20px; font-size: 14px; color: #00a88b !important;}

/* 폰트색상 */
.font-blue {color: #1571eb; font-size: 13px; text-decoration: underline; text-underline-position: under;}
.font-blue:hover {text-decoration: underline;}
.font-red {color: #ef4040;}
.font-green {color: #00a88b;}
.info-green {color: #00a88b; margin-top: 16px; font-size: 14px;}

.s-txt {font-size: 12px; color: #919191; margin-top: 7px; display: block;}
.s-txt1 {font-size: 12px; color: #919191; margin: 5px 0; font-weight: 500;}
.s-txt1 span {color: #222426;}
.s-txt1.single {margin-bottom: 16px; text-align: right;}
.flex-wrap .s-txt1 {position: absolute; right: 0; bottom: 16px; margin: 0;}

/* 블릿 */
.bull-none {color: #707478; font-size: 15px; margin-bottom: 7px; flex: inherit !important;}
.bull-none + .bull-none {margin-bottom: 0px;}

/* 달력아이콘 */
.ico-cal {font-size: 18px; color: #5C6773; display: inline-flex; align-items: center;}
.ico-cal::before {content: ''; display: inline-flex; width: 24px; height: 24px; margin-right: 8px; background: url('../img/new_common/ico_cal.png') no-repeat 0 center; background-size:24px auto;}

/* 더보기  */
.btn_more a { display:block; position:relative; color:#5C6773; font-size: 15px; font-weight: 500; text-align:center; margin: 8px 0 24px 0; padding:8px 0; background-image:none;}
.btn_more a i.more {width: 16px; height: 16px; display:block; position:absolute;  left: calc(50% + -37px); top:9px; background:url('../img/new_common/ico_more.png') no-repeat 0 0 / 16px 16px;}

/* 버튼 */
.btn {display: block; margin: 0; line-height: inherit; padding: 0; background: transparent;}
.btn a {display: inline-block; padding: 10px 16px 9px 16px; border-radius: 4px; font-weight: 500; margin-left: 8px; border: 1px solid #dfe3e8;}
.btn a.blue {color: #fff; background: #1571eb; border: 1px solid #1571eb;}
.btn a.black {color: #fff; background: #222426; border: 1px solid #222426;}
.btn a.gray {color: #fff; background: #bcc0c5; border: 1px solid #bcc0c5;}
.btn a.line {color: #1571eb; background: #fff; border: 1px solid #1571eb; /*margin: 0;*/}
.btn a.black-line {color: #222426; background: #fff; border: 1px solid #222426;}
.btn a.ico-re {padding: 10px 16px 9px 35px !important; color: #1571eb; border: 1px solid #1571eb; background: url('../img/new_common/ioc_refresh.png') no-repeat left 14px center; background-size: 16px;}
.btn a.ico-plus {padding: 10px 16px 9px 34px !important; color: #1571eb; border: 1px solid #1571eb; background: url('../img/new_common/ico_plus.png') no-repeat left 14px center; background-size: 16px;}
.btn a.ico-plus1 {padding: 10px 16px 9px 34px !important; color: #fff; background: url('../img/new_common/ico_plus01.png') no-repeat left 15px center #222426; background-size: 16px;}
.btn a.ico-minus {padding: 10px 16px 9px 34px !important; color: #fff; background: url('../img/new_common/ico_minus.png') no-repeat left 15px center #222426; background-size: 16px;}
.btn a.ico-excel {padding: 11px 24px 11px 44px !important; background: url('../img/new_common/ico_excel.png') no-repeat left 16px center; background-size: 24px;}
.btn a.ico-inquiry {padding-left: 33px !important; color: #1571eb;border:1px solid #1571eb;background: url('../img/new_common/ico_inquiry.png') no-repeat left 16px center; background-size: 16px auto;}
.popup-contents .btn a.ico-excel {color: #222426; font-size: 14px; border: 1px solid #dfe3e8; background: url('../img/new_common/ico_excel.png') no-repeat left 16px center; background-size: 24px;}

.c-btn {position: absolute; top: 50%; right: 24px; transform: translateY(-50%);}
.c-btn a {display: inline-flex; background: #1a3755; height: 32px; align-items: center; justify-content: center; font-size: 14px; color: #fff; font-weight: 500; padding: 0 13px; border-radius: 4px;}

