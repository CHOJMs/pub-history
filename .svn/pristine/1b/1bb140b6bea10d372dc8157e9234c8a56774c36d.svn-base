/* 툴팁 */
.tooltip {display: inline-block;font-size: 0;width: 1.8rem;height: 1.8rem;vertical-align: middle;background: url('../img/ui_common/ico_tooltop.png') no-repeat 0 0;background-size: 1.8rem;margin-left: .4rem;}
.tooltip.navy {background: url('../img/ui_common/ico_tooltop02.png') no-repeat 0 0;background-size: 1.8rem;}
.tooltip.type01::after  { text-align: center; left: -110px !important; width: calc(100% - -275px) !important;}
.tooltip.type02::after  { text-align: center; left: -78px !important; width: 226px !important;}
.tooltip.type03::after  { text-align: center; left: -240px !important; text-align: left;}
.tooltip.type04::after  {white-space: pre;left: -102px !important;width: auto !important;font-size: 12px;line-height: 15px;}
.tooltip.type05::after{text-align: center; left: -110px !important; width: calc(100% - -275px) !important;}
.tooltip.type06::after  {white-space: pre;left:-50px  !important;width: auto !important;}

.tooltip[title] {position: relative; display: inline-block; cursor: pointer; font-size: 13px;}
.tooltip[title]::after {content: attr(title); display: none; color: #fff; position: absolute; top: 27px; left: -170px; padding: 8px 16px; background: rgb(0, 168, 139); cursor: text; width: calc(100% - -32rem); box-shadow: 0 4px 8px rgb(0 0 0 / 16%); z-index: 2; border-radius: 6px;}
.tooltip[title]::before {content: ''; display: none; width: 1.4rem; height: 1.2rem; position: absolute; top: 19px; left: 2px; background: url('../img/ui_common/ico_arrow_tooltip.png') no-repeat 0 0; background-size: 1.4rem;}
.tooltip.navy[title]::after {content: attr(title); display: none; color: #fff; position: absolute; top: 27px; left: -170px; padding: 8px 16px; background: rgb(26, 55, 84); cursor: text; width: calc(100% - -32rem); box-shadow: 0 4px 8px rgb(0 0 0 / 16%); z-index: 2; border-radius: 6px;}
.tooltip.navy[title]::before {content: ''; display: none; width: 1.4rem; height: 1.2rem; position: absolute; top: 19px; left: 2px; background: url('../img/ui_common/ico_arrow_tooltip2.png') no-repeat 0 0; background-size: 1.4rem;}

.tooltip[title]:focus:after,
.tooltip[title]:active:after,
.tooltip[title]:focus:before,
.tooltip[title]:active:before {display: block;}
.tooltip.navy[title]:hover:before,
.tooltip.navy[title]:hover:after {display: block;}

