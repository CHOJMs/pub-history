/* tab - 라인박스형  */
/* 신분증촬영 */
.line-tab.type01{margin-top: 48px;}
.line-tab.type01::before {display: none;}
.line-tab.type01 > ul > li > a {position: relative; font-size: 18px; color: #5c6773; font-weight: 500; height: 56px; line-height: 56px; border: 1px solid #dfe3e8; border-left: none;}
.line-tab.type01 > ul > li.active > a {color: #00a88b; border: 1px solid #00a88b; z-index: 2;}
.line-tab.type01 > ul > li:first-of-type a {border-radius: 8px 0 0 8px; border-left: 1px solid #d3d5d8;}
.line-tab.type01 > ul > li:last-of-type a {border-radius: 0 8px 8px 0; border-left: none;}
.line-tab.type01 > ul > li.active a::before {content: ''; position: absolute; left: -1px; top: -1px; width: 1px; height: 48px; background: #00a88b; border-top: 2px solid #00a88b;}
.line-tab.type01 > ul > li.active a::after {content: ''; position: absolute; left: -1px; bottom: -1px; width: 1px; height: 48px; background: #00a88b; border-top: 2px solid #00a88b;}
.line-tab.type01 > ul > li.active:first-of-type a {border-color:#00a88b;}
.line-tab.type01 > ul > li:first-of-type a::before {height: 73%; top: 6px;}
.line-tab.type01 > ul > li:first-of-type a::after {height: 73%; bottom: 6px;}
.line-tab.type01 > ul > li:last-of-type a::before {height: calc(100% + 1px);}
.line-tab.type01 > ul > li:last-of-type a::after {height: calc(100% + 1px);}
.line-tab.type01 .line-tab-list {margin-top: 24px;}
.line-tab.type01 .line-tab-list .list-dot li {font-size: 14px; padding-bottom: 12px;}
.line-tab.type01 .line-tab-list .list-dot li.type {color:#00a88b}

