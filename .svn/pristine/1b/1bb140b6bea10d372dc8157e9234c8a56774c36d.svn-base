/* 완료 : 아이콘 */
.f-ico-comp::before {display: block; content: ''; width: 12rem; height: 12rem; background: url('../img/chatbot/ico_check_finish.png') no-repeat center center; background-size:12rem auto; margin: auto;}
.f-ico-errow::before {display: block; content: ''; width: 12rem; height: 12rem; background: url('../img/chatbot/ico_errow_finish.png') no-repeat center center; background-size:12rem auto; margin: auto;}
.sub-process .f-ico-comp::before {display: block; content: ''; width: 12rem; height: 12rem; background: url('../img/ui_common/ico_check_finish.png') no-repeat center center; background-size:12rem auto; margin: auto;}
.sub-process .f-ico-comp::after {display: block; content: ''; width: 260px; height: 84px; background: url('../img/ui_common/ico_check_finish01.png') no-repeat center center; background-size:260px auto; margin: auto; position: absolute; top: -30px; left: calc(50% - 130px);}
.sub-process .f-ico-comp.none::after {display:none;}
.sub-process .f-ico-errow::before {display: block; content: ''; width: 12rem; height: 12rem; background: url('../img/ui_common/ico_error_finish.png') no-repeat center center; background-size:12rem auto; margin: auto;}


.sub-process .f-ico-comp::after,
.sub-contents .f-ico-comp::after {width: 260px; height: 84px; animation: 0.3s ease finish-icon; animation-delay: 0.1s;}

@keyframes finish-icon {
    0% {
        transform: scale(.6); opacity: 0; top: -27px;
    }
    70% {
        transform: scale(.6); opacity: 0; top: -27px;
    }
    100% {
        transform: scale(1); opacity: 1;
    }
}

