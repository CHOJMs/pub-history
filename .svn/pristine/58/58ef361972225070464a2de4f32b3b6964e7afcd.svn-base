/* 약관동의서 : 스와이퍼 */
.agreement-wrap {position: relative; width: 100%; height: 100%; background: #fff; box-shadow: 0 2px 4px rgb(0 0 0 / 16%); overflow-y: auto;}
.agreement-wrap .swiper-box {width: 100%;}
.agreement-wrap .swiper-box .agreefix {margin-top: 50px;}

.agreement-wrap .swiper-box .slide {position: relative; float: left; width: 100%; overflow: hidden; opacity: 0;}
.agreement-wrap .swiper-box .slide li img {max-width: 100%;}
.agreement-wrap .swiper-box .slide .slick-dots{display: none!important;}
.agreement-wrap .swiper-box .slide.slick-initialized{opacity: 1;}
/*.agreement-wrap .swiper-box .slide .slick-slide {outline: 0; border: 1px dotted transparent;}
.agreement-wrap .swiper-box .slide .slick-slide:focus {border: 1px dotted rgb(34, 36, 38);}*/
.agreement-wrap .swiper-box .slide .slick-slide {height: auto;}
.agreement-wrap .swiper-box .slide-arrow {position: absolute; top: 50%; transform: translateY(-50%); padding: 0 4px; font-size: 0px; z-index: 1; cursor: pointer; opacity: .9;}
.agreement-wrap .swiper-box .slick-prev {width: 40px;height: 40px;left: 8px;background: url('../../img/ui_common/ico_swiper_prev.png') no-repeat 0 0;background-size: 40px;}
.agreement-wrap .swiper-box .slick-next {width: 40px;height: 40px;right: 8px;left: auto;background: url('../../img/ui_common/ico_swiper_next.png') no-repeat 0 0;background-size: 40px;}
.agreement-wrap .swiper-box .prev-arrow{left: 0;}
.agreement-wrap .swiper-box .next-arrow{right: 0;}
.agreement-wrap .swiper-box .slide-paging{position: absolute; right: 12px; top: 12px; width: 56px; height: 32px; line-height: 32px; text-align: center; font-size:14px; color: rgb(92, 103, 115); cursor:default; border-radius: 8px; background: rgb(235, 238, 242);}
.agreement-wrap .swiper-box .slide-paging .page{color: rgb(34, 36, 38);}

.slide-iframe {height: 425px; padding: 15px 0 0 15px;}


.slick-prev,
.slick-next {font-size: 0; line-height: 0; position: absolute; top: 50%; display: block; width: 40px; height: 40px; padding: 0; -webkit-transform: translate(0, -50%); -ms-transform: translate(0, -50%); transform: translate(0, -50%); cursor: pointer; color: transparent; border: none; background: transparent;}
.slick-prev:hover,
.slick-prev:focus,
.slick-next:hover,
.slick-next:focus {color: transparent; background: transparent;}
.slick-prev:hover,
.slick-prev:focus,
.slick-next:hover,
.slick-next:focus {opacity: 1;}

.slick-prev.slick-disabled,
.slick-next.slick-disabled {opacity: .6 !important;}

/* popup popup-01 */
.popup-layer {position: relative; width: 100%; margin: 0 auto;}
.popup-layer.active .popup.popup-01 {opacity: 1; visibility: visible; overflow: hidden;}
.popup-layer.active .popup.popup-01.pop1 {left: calc(50% - 307px);max-height: 100%;}
.popup-layer.active .popup.popup-01.pop2 {left: calc(50% + 290px);max-width: 680px;max-height: 620px;}
.popup-layer.active .popup.popup-01.pop-center {left: 50%}
.dim {opacity: 0; visibility: hidden; position: fixed; left: 0; top: 0; width: 100%; height: 100%; background: rgba(0, 0, 0, 0.3); transition: all 0.3s;}
.dim.active {opacity: 1; visibility: visible;}
.dim.loding_dim, .small_dim { background: rgba(0, 0, 0, 0.3);}
.popup {opacity: 0; visibility: hidden; position: fixed; top: 50%; left: 50%; transform: translate(-50%, -50%); max-width: 900px; width: 100%; height: 680px; box-shadow: 0 -0.05rem 1.0rem rgba(0, 0, 0, 0.1); border-radius: 20px; background: #fff; z-index: 2; transition: all 0.3s;}
.popup-layer, .dim { visibility: hidden;}
.popup-layer.active { visibility: visible; z-index: 9999;}
.popup-contents {width: calc(100% + .8px); height: calc(100% - 39px); overflow-y: auto; padding: 0 40px 16px 40px;}
.flex-col {display: flex; flex-direction: column; justify-content: space-between;}

/* 공통 : 팝업 */
.popup-calc {bottom: auto; max-width: 480px;}
.popup-calc .inner {padding:22px 0 0 0; height: 100%;}
