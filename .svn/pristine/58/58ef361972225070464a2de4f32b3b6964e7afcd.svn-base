/* fixed : 버튼 */
.popup .fixed-btn-area {position: fixed; left: 0; bottom: 0; width: 100%; padding: 0 40px 40px 40px; box-sizing: border-box; background: linear-gradient(180deg, #ffffff2e 18%, #ffffff 100%); margin-top: 0 !important;}
.popup .fixed-btn-area .btn-box {display: table; width: 100%; table-layout: fixed; text-align: center;}
.popup .fixed-btn-area .btn-box .btn {display: table-cell; vertical-align: top; background: transparent; padding-left: 15px;}
.popup .fixed-btn-area .btn-box .btn:first-child {margin-left: 0; padding-left: 0;}
.popup .fixed-btn-area .btn-box .btn button {height: 56px; font-size: 18px; font-weight: 500; width: 100%; color: #fff; font-family: "pretendard"; border-radius: 8px; background: rgb(21, 113, 235);}
.popup .fixed-btn-area .btn-box .btn.gray button {background: rgb(188, 192, 197); color: rgb(255, 255, 255);}
.popup .fixed-btn-area .btn-box .btn.gray01 button {background: rgb(235, 238, 242); color: rgb(92, 103, 115);}
.popup .fixed-btn-area .btn-box .btn.line button {background: #fff; color: rgb(21, 113, 235); border: 1px solid rgb(21, 113, 235);}
.popup .fixed-btn-area .btn-box .btn.btn-line button {background: #fff; color: rgb(92, 103, 115); border: transparent;}
.popup .pad-small .fixed-btn-area {padding: 0 2rem 2rem 2rem;}

