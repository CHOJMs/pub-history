.popup-calc .popup-clau {padding: 8px !important; background: rgb(247, 247, 247);}
.popup-calc .popup-clau .pop-cnt-wrap {background: #fff; padding: 12px; overflow-y: auto; min-height: 100%; word-break: break-all; word-wrap: break-word;}

