@charset'utf-8';

br.device-mobile { display: none;}

.mt40 { margin-top: 40px;}
.mb8 { margin-bottom: 8px;}

/* 공통 */
button {border: none; background-color: transparent; font-family: "KOHO", "Noto Sans KR";}
input.chk {color: #1e1e1e; font-size: 1.4rem; }
input[type="radio"] {height: 2.4rem; width: 2.4rem; opacity: 0;}
input[type="checkbox"] {height: 2.4rem; width: 2.4rem; opacity: 0;}
input[type="text"],
input[type="number"],
input[type="tel"],
input[type="email"],
input[type="password"] {width:100%; height:4.8rem; padding:0 16px; font-size:16px; font-family: "KOHO", "Noto Sans KR"; color:#222426; border-radius: 4px; border:1px solid #d3d5d8; box-sizing: border-box;vertical-align: top; appearance: none;}
input[type="text"]:hover,
input[type="number"]:hover,
input[type="tel"]:hover,
input[type="email"]:hover,
input[type="password"]:hover {border:1px solid #222426; transition: all .5s; color: #222426;}
input[type="text"]:focus,
input[type="number"]:focus,
input[type="tel"]:focus,
input[type="email"]:focus,
input[type="password"]:focus {border:1px solid #222426; outline:none; color: #222426;}
input::-ms-input-placeholder,
input::-webkit-input-placeholder,
input::-moz-placeholder,
input::-moz-placeholder {color: #707478;}
input::placeholder, textarea::placeholder {color: #999999;}

select {-webkit-appearance:none; -moz-appearance:none; appearance:none; }
select::-ms-expand {display:none; }
select { font-family: "KOHO", "Noto Sans KR"; letter-spacing: -0.05em; border-left: none; border-top: none; border-right: none;}
select em {color: rgb(255, 36, 36); font-size: 1.4rem;}
select:focus, select:hover, textarea:focus, textarea:hover {border:1px solid #222426; transition: all .5s; color: #222426;}

textarea {box-sizing:border-box; width:100%; height:130px; vertical-align:top; font-size:15px; font-family: "KOHO", "Noto Sans KR"; color:#999999; padding: 12px 16px; border-radius: 4px; border:1px solid #d3d5d8; word-break:break-all;}

.disabled { background:#f5f5f5; color:#222426 !important; border:1px solid #d3d5d8 !important;}
.readonly {background-color: transparent; color: #222426; border:1px solid #eeeff0 !important;}

.box_form .text_data {display:-webkit-flex;display:-moz-flex;display:-ms-flex;display:-o-flex;display:flex; clear:both; display:table\9; width:100%\9; margin-top: 6px;}
.box_form .text_data .form_text {margin-right:0px;}
.box_form .text_data .form_sel {margin-right:6px;}
.box_form .text_data .form_post {margin-bottom:6px;}

.box_form .text_data .form {-webkit-flex:2;-moz-flex:2;flex:2;}
.box_form .text_data .select_box {-webkit-flex:1;-moz-flex:1;flex:1;/*flex:1 1 0;*/ display:inline-block; /*background:url("../img/common/bg_select.png") no-repeat right center / 22px 7px #fff;*/ display:table-cell\9;}
.box_form .text_data .select_box.disabled {background:url("../../img/common/bg_select_off.png") no-repeat right center / 22px 7px #f2f2f2; border:1px solid #c4c4c4;}

.text_data {display:flex; clear:both; position: relative; align-items: center;}
.text_data .form {-webkit-flex:1; -moz-flex:1; flex:1; margin-right: 10px; position: relative;}
.text_data .form:last-of-type {margin-right: 0px;}
.text_data .dash {padding-top:0; margin-right:10px; margin-left: 0px; padding-left:0; font-size:1.4rem; color: #222426; display:inline-block;}
.text_data .btn_cont { position: absolute; right: 0; top: 0;}
.text_data .btn_cont button {padding:14px 20px; cursor:pointer; border-radius: 0 4px 4px 0; background:#707478; border:none; color:#fff; font-size:16px; line-height: 2rem;}
.text_data .form + .checkbox, .form + .selectbox { padding-left: 16px;}
.text_data.wid_220 {width: 220px;}

/* inpui타입 - 동의서보기 아이콘*/
.form_box, .checkbox, .agr_sub_check {position: relative;}
.checkbox span.img { position: absolute; right: 0;}
.checkbox span.img a {display: inline-block; width: 24px; height: 24px; position:absolute; right: 0; top: 0; font-size: 16px; color: #999999; text-decoration: underline;}

/* 체크박스 */
.checkbox {margin-bottom: 1.6rem;}
.checkbox:last-child {margin-bottom: 0rem;}
.chk{overflow:hidden; position:absolute; border:0; background:#fff; margin:0 0 0 0rem; width:24px; height:24px;}
.chk+.label{display:inline-block; position:relative; padding-left:3.2rem; font-size:16px; height:24px; line-height: 24px; font-weight: 500; border: none; color: #222426; cursor:pointer; box-sizing:border-box;}
.chk+.label:after{position:absolute; top:0; left:0; width:24px; height:24px; margin-top:0rem; transition: .3s; background:url('../../img/ico_check_off.png') no-repeat center center #fff; border:.1rem solid #d3d3d3; border-radius: 50%; background-size:1.2rem auto; content: ''}
.chk:checked+.label{color:#222426;}
.chk:checked+.label:after{margin-top:0; background: url('../../img/ico_check_on.png') no-repeat center center #1297ef; border:.1rem solid #1297ef; border-radius: 50%; background-size:1.2rem auto;}
.chk+.label.font22 {font-size: 1.8rem; font-weight: 700 !important;}

/*라디오버튼*/
.rchk{overflow:hidden; position:absolute; border:0; background:#fff; margin:0 0 0 0px; width:2.4rem; height:2.4rem;}
.rchk+.rchk-label{display:inline-block; position:relative; height:2.4rem; padding-left:3.2rem; font-size:1.7rem; font-weight: 500; line-height:2.4rem; color: rgb(34, 36, 38); cursor:pointer}
.rchk+.rchk-label:after{position:absolute;top:0; left:0; width:2.4rem; height:2.4rem; margin-top:0px; background:#fff; border:.1rem solid #d3d3d3; border-radius: 50%; background-size:1.6rem;content: ''}
.rchk+.rchk-label:before {background:#bcc0c5; width: .8rem; height: .8rem; border-radius: 50%; display: block; content: ''; position: absolute; top: .8rem; left: .8rem; z-index: 1; /*transform: scale(2.5, 2.5); opacity: 0;*/}
.rchk:checked+.rchk-label:after{background:rgb(18, 151, 239); background-size:1.6rem; border:.1rem solid rgb(18, 151, 239);}
.rchk:checked+.rchk-label:before {background:#fff; width: .8rem; height: .8rem; display: block; content: ''; position: absolute; top: .8rem; left: .8rem; transition: .3s; transform: scale(1.1, 1.1); opacity: 1;}
.inline_rchk li {display: inline-block !important; width: 45%;}
.inline_rchk li.mt10 {margin-top: 1.7rem !important;}
.inline_rchk_1 li {display: inline-block !important; width: 32.6%;}
.inline_rchk_2 li {display: inline-block !important; width: 19%;}
.rchk.disabled+.rchk-label {color: rgb(188, 192, 197);}
.rchk.disabled+.rchk-label:after {background:#ebecee; border:.1rem solid #d3d3d3;}
.rchk.disabled+.rchk-label:before {background: #bcc0c5;}
.rchk+.rchk-label.pl28, .chk.chk_1 + .label.pl28, .chk + .label.pl28 {padding-left: 0; width: 2.4rem;}

/* 셀렉박스 */
.selectbox {position: relative; flex: 1; margin-right: 10px;}
.select{width:100%; height: 48px; padding:0 35px 0 16px; font-size:16px; color: #707478; line-height:38px; border: 1px solid #d3d5d8; border-radius: 4px; background: url('../../img/ico_select.png') no-repeat center right 0; background-position: top 50% right 10px; background-size:auto 2rem; box-sizing: border-box;}
.select:disabled, .select.disabled {background: url('../../img/ico_select_dis.png') no-repeat center right 0; background-color: #f5f5f5; background-size:auto 2rem; background-position: top 50% right 10px;}
.select:hover{}
.select:focus{outline:none;}
.selectbox:first-child, .select:first-child, .select.disabled:first-child {margin-right: 0px !important;}

.btn_fixed { width: 960px; margin: auto; padding-bottom: 100px;}
.btn_box {width:100%; display: table; padding:0; text-align:center;}
.btn_box button, .btn_box a {background: rgb(34, 36, 38); border:1px solid #222426; padding: 16px 0; text-align:center; font-size:20px; color:#fff; width:100%; cursor:pointer; border-radius: 8px; font-weight: 500;}
.btn_box .btn {flex:1; width: 180px; display:inline-block; text-align:center; margin:0; vertical-align: top;}
.btn_box .btn + .btn { padding-left: 16px;}
.btn_box .btn.w_320 { width: 320px;}
.btn_box .gray button, .btn_box .gray a {background: #707478; color:#fff; border: 1px solid #707478}
.btn_box .line button, .btn_box .line a {background: #fff; color: #222426; border: 1px solid #222426;}
.btn_box .gray1 button, .btn_box .gray1 a {background: #ebecee; color: #222426; border: 1px solid #ebecee;}
.btn_box a { display: inline-block; text-align: center;}

.popup-contents .btn_box .btn_gray a, .popup-contents .btn_box .gray a {width: 100%; background: rgb(235, 236, 238); color:rgb(34, 36, 38); border: none; font-size: 1.6rem; padding:1.6rem 1rem; font-weight:500;}


/* 첨부파일-버튼 */
.file_box { border: 1px solid #d3d5d8; width: 164px; height: 220px; border-radius: 4px; display: flex; flex-wrap: wrap; flex-direction: column; justify-content: space-between;}
.file_box p { height: 170px; overflow: hidden;}
.btn_file_box {position:relative; height:48px; overflow:hidden;}
.btn_file_box  .btn,
.btn_file_box  a.btn_file {margin:0; background:#707478; border-radius: 0 0 4px 4px; color:#fff; text-align:center; height:48px; line-height:48px; font-size: 16px; display:block;}
.btn_file_box  .btn.gray {background: rgb(235, 236, 238); color: rgb(34, 36, 38);}
.file_input_hidden {font-size:31px; /*width:100%;*/ position:absolute; right:0px; top:0px; opacity:0; filter: alpha(opacity=0); -ms-filter: alpha(opacity=0); cursor:pointer;}


/* table -view*/
.viewType {width:100%; table-layout:fixed; border-top: .2rem solid #222426;}
.viewType * {vertical-align:middle;}
.viewType th {padding:24px 0; text-align:left; color:rgb(34, 36, 38); font-size:1.6rem; font-weight: 500; border-top:.1px solid #ebecee; border-bottom:1px solid #ebecee;}
.viewType th:nth-of-type(even) { text-align: center;}
.viewType td {padding:24px 0 24px 16px; text-align:left; color:#707478; font-size:1.5rem; border-top:.1rem solid #ebecee; border-bottom:.1rem solid #ebecee;}
.viewType td:nth-of-type(1) { padding-left: 0;}
.viewType td .text_data + .text_data { margin-top: 12px;}
.viewType th .selectbox { flex: none; width: 143px;}
.viewType th .select { border:none; font-size: 16px; color: #222426; padding: 0 28px 0 0; background: url('../../img/career/ico_select01.png') no-repeat center right 0; background-size: 24px 24px;}
.viewType th span { vertical-align: top; margin-left: 4px;}
.viewType th span a.plus { display: inline-flex; width: 24px; height: 24px; vertical-align: top;}
.viewType th.ver_top { vertical-align: top; padding-top: 35px;}
.viewType th.ver_top.pt24 {padding-top: 24px;}
.viewType th.ver_top > .text_data { margin-top: -11px; margin-right: 15px;}
.viewType th.ver_top > .text_data .form { margin-right: 5px;}

/* table -view*/
.dataType {width:100%; table-layout:fixed;}
.dataType * {vertical-align:middle;}
.dataType th { padding:18px 0; text-align:left; color:#222426; font-size:16px; font-weight: 500; border-bottom: 2px solid #222426; }
.dataType td {padding:18px 0 18px 0; text-align:left; color:#707478; font-size:16px; border-top:1px solid #ebecee; border-bottom:1px solid #ebecee; word-break: break-all;}
.dataType td .list-type-dot li {font-size: 16px;}
.noData p { text-align: center; padding: 100px 0;}

.flex-btn {display: flex; justify-content: space-between; position: relative;}
.flex-btn .btn.btn-blk { position: absolute; right: 0; bottom: 16px;}
.flex-btn .btn.btn-blk a {display: inline-flex; align-items: center; justify-content: center; padding: 0 10px; width: 64px; height: 32px; line-height: 1; font-size: 13px; color: #fff; background: rgba(34, 36, 38, 1); border-radius: 4px; letter-spacing: -0.06em;}

/* 박스컬러 */
.blue_box { background: #E9F7FE; border-radius: 8px; padding: 25px 40px;}
.blue_box .chk+.label { font-size: 17px;}
.gray_box { background: #f5f5f5; border-radius: 4px; padding: 32px 40px; margin-top: 40px;}
.gray_box .list li p.tl { font-size: 16px; color: #222426; font-weight: 500; margin-bottom: 4px;}
.gray_box .list li p + p { font-size: 16px; color: #707478;}
.gray_box .list li + li { margin-top: 16px;}
.gray_box .list li div p.tl { font-size: 15px; color: #222426; font-weight: 500; margin-bottom: 4px; margin-top: 8px;}
.gray_box .list li div p + p { font-size: 15px; color: #707478;}
.gray_box + .txt { font-size: 16px; color: #222426; margin-top: 24px;}
.popup-contents .gray_box + .txt { font-size: 16px; color: #222426; margin-top: 18px;}

/* 채용결과확인 */
.pass_box { background: #f5f5f5; padding: 25px 40px;}
.pass_box > div.txt { text-align: center; padding: 39px 0 15px;}
.pass_box > div.txt > p.tl { font-size: 32px; color: #222426; font-weight: 700; margin-bottom: 8px;}
.pass_box > div.txt > p.tl span { color: #1297ef;}
.pass_box > div.txt > p + p { font-size: 18px; color: #222426; font-weight: 500; margin-bottom: 40px;}
.pass_box > div.txt .white_box { background: #fff; padding: 25px 0; text-align: center;}
.pass_box > div.txt .white_box > ul { display: flex; flex-wrap: wrap; justify-content: space-between; width: 480px; margin: auto;}
.pass_box > div.txt .white_box > ul li { font-size: 15px; color: #222426;}
.pass_box > div.txt .white_box > ul li span + span { color: #707478; margin-left: 10px;}

/* 채용공고 */
.pass_box1 { position: relative; background: #f5f5f5; border-radius: 4px; padding: 64px 60px;}
.pass_box1::after { content: ""; display: block; position: absolute; right: 60px; bottom: 50px; width: 180px; height: 120px; background:url('../../img/career/img_finish.png') center center / cover no-repeat;}
.pass_box1 > div.txt > p.tl { font-size: 32px; color: #222426; font-weight: 700; margin-bottom: 24px; width: 505px;}
.pass_box1 > div.txt > p + p { font-size: 16px; color: #222426;}

/* 채용공고-전형절차 */
.step_box { display: flex;}
.step_box li { flex: 1; display: block; position: relative;}
.step_box li + li { padding-left: 24px;}
.step_box li::after { display: block; content: ""; position: absolute; left: 0; top: 50%; width: 100%; height: 1px; background: url('../../img/career/ico_dotted01.png') repeat-x 0 0 / auto .1rem;}
.step_box li span { position: relative; width: 172px; height: 56px; font-size: 16px; font-weight: 500; border-radius: 4px; display: inline-flex; justify-content: center; align-items: center; background: rgb(233, 247, 254); box-sizing:border-box; z-index:2;}
.step_box li:first-child :after {display: none;}
.step_box li:last-child span { color: #fff; background: #1297ef; box-shadow: 0 0 20px rgb(0 0 0 / 16%);}

.list-type-dot li {line-height: 1.3; font-size: 14px; font-weight: 400; color:rgb(112, 116, 120); letter-spacing: -0.05em; padding-left: 4px; text-indent: -4px;}
.list-type-dot li::before {display: inline-block; content: "·"; margin-right: 4px; font-size: 18px;}
.list-type-dot li + li {margin-top: 6px;}

/* 유의사항 */
.gray1_box {background: rgb(245, 245, 245); border-radius: .4rem; padding: 3rem 4rem; margin-top: 40px; border-top: 1px solid rgb(235, 236, 238);}
.gray1_box .h_tit {font-size: 1.6rem; font-weight: 700; margin-bottom: 1rem; margin-top: 0;}
.gray1_box .list-type-dot li, .gray2_box .list-type-dot li, .gray1_box .list-type-dash li {line-height: 1.3; font-size: 14px; font-weight: 400; color:rgb(112, 116, 120); letter-spacing: -0.05em; padding-left: 4px; text-indent: -4px;}
.gray1_box .list-type-dot li::before, .gray2_box .list-type-dot li::before {display: inline-block; content: "·"; margin-right: 4px; font-size: 18px;}
.gray1_box .list-type-dot li + li, .gray2_box .list-type-dot li + li {margin-top: 6px;}
.gray1_box .list-type-dot li:last-of-type, .gray2_box .list-type-dot li:last-of-type {margin-bottom: 0;}
.gray1_box .list-type-dot li strong, .gray2_box .list-type-dot li strong { color: rgb(34, 36, 38);}
.gray1_box .list-type-dash li::before { display: inline-block; content: "-"; margin-right: 4px; font-size: 15px; vertical-align: top;}

/*  폰트컬러 */
.font_blue { color: #1297ef;}
.line {border-bottom: 1px solid #ebecee; padding-bottom: 8px;}

/* 로그인 */
.login_box { border-top:1px solid #ebecee; padding-top: 48px; padding-bottom: 110px;}
.login_box .login {width: 607px; margin: auto; position: relative;}
.login_box .login div { display: flex; margin-bottom: 12px;}
.login_box .login div label { font-size: 16px; font-weight: 500; width: 83px; display: inline-flex; flex-wrap: wrap; align-items: center;}
.login_box .login div input { width: 400px;}
.login_box .login div input[type="button"] { border: none; background: #222426; width: 108px; height: 108px; font-size: 16px; color: #fff; font-weight: 500; border-radius: 8px;}
.login_box .login div:nth-of-type(3) { position: absolute; right: 0; top: 0;}
.login_box .login div > div { margin-bottom: 0;}

.btn_blue_txt a {position: relative; display: block; color: rgb(18, 151, 239); font-size: 16px; font-weight: 500; padding-right: 16px; margin-left: 83px; margin-top: 26px;}
.btn_blue_txt a:after { content: ''; width: 20px; height: 20px; display: inline-block; position: absolute; top: 0.1rem; background:url('../../img/ico_arrow01.png') no-repeat right 0 / 20px 20px}

/* 탭 수정 */
.tab_box { display: flex; margin-bottom: 0rem; border-bottom: 1px solid #ebecee;}
.tab_box ul { display: flex; width: 100%; flex-wrap: wrap; justify-content: space-between;}
.tab_box li { display: inline-flex; width: 50%;}
.tab_box li a {display: inline-flex; align-items: center; justify-content: center; width: 100%; color: #707478; font-weight: 500; font-size: 18px;  border-bottom: 3px solid #fff; padding-bottom:10px; }
.tab_box li a.on { border-bottom: 3px solid #1297ef; color: #222426;}
.tab {margin-top: 5.6rem;}
.popup .tab .form_box label { font-size: 16px; font-weight: 500; width: 120px; display: inline-flex; flex-wrap: wrap; align-items: center;}
.popup .tab .form_box div { display: flex; margin-bottom: 12px;}
.popup .tab .form_box div > div { width: calc(100% - 120px); margin-bottom: 0;}
.popup .tab .form_box div > div.selectbox { margin-right: 10px !important;}

/* 팝업-width */
.popup-career .popup-contents { padding-bottom: 40px;}
.popup.popup-career { max-width: 560px !important; padding-bottom:0;}
.popup.popup-career .btn-area { padding-top: 16px;}
.popup.popup-career .btn_box .btn { width: 100%;}
.popup.popup-career .btn_box button, .popup.popup-career .btn_box a {padding: 12px 0; font-size: 18px; font-weight: 500;}
.popup.popup-career .pop_cont .font_gray { font-size: 14px; color: #707478;}

/* button 버튼 */
.popup .btn_box .btn {flex:1; width:1%; display: table-cell; text-align:center; margin:0 .5rem; padding-left:0; vertical-align: top;}

/* 팝업 버튼*/
.btn-area .btn_box .btn {display:inline-block;flex:1; width:240px; margin: 0 5px;}

/* box */
.white_box {background: #fff; border-radius: .8rem; padding: 3.3rem 4rem; letter-spacing: -0.05em; box-shadow: 0 0 1.2rem rgba(0, 0, 0, 0.05);}

/* 슬라이드 - 자주하는질문 */
.select-box.sel_view.t_line > ul {padding-bottom: 0px;}
.select-box:first-of-type { border-top: 1px solid rgb(235, 236, 238);}
.select-box > a {border-bottom: 1px solid rgb(235, 236, 238); position: relative; padding: 2.4rem 0; display: flex; justify-content: flex-start; align-items: center; font-size: 18px; font-weight: 500; color: rgb(34, 36, 38);}
.select-box > a::after {position: absolute; right: 0; top: 50%; transform-origin: 50% 0; transform: rotate(0) translateY(-50%); display: block; content: ""; width: 16px; height: 16px; background: url('../../img/ico_arrow07.png') no-repeat left 0 / 1.6rem auto; transition: all 0.3s;}
.select-box > a span { color: rgb(18, 151, 239); margin-right: 1.5rem; font-weight: 700;}
.select-box > a.active {border-bottom: none;}
.select-box > a.active::after {transform: rotate(180deg) translateY(-50%);}
.select-box > div { background: #f5f5f5; padding: 2.4rem; font-size: 1.5rem; letter-spacing: -0.05em;}
.select-box > div {display: none;}
.select-box > div ul {padding-bottom: 18px;}
.select-box > div ul li + li {margin-top: 8px;}
.select-box > div ul li {display: flex; flex-wrap: wrap; align-items: flex-start; font-size: 14px; letter-spacing: -0.05em;}
.select-box > div ul li span {display: inline-flex; width: 100px; margin-right: 6px; color: rgb(112, 116, 120);}
.select-box > div ul li p {width: calc(100% - 106px); color: rgb(34, 36, 38);}
.select-box > p.noData {position: relative; padding: 17px 17px 0 17px; text-align: center; justify-content: flex-start; align-items: center; height: 56px; font-size: 14px; font-weight: 400; color: rgb(112, 116, 120);}

/* 직무선택 */
.job_list { display: flex; flex-wrap: wrap; justify-content: space-between; align-items: center;}
.job_list li { position: relative; display: inline-flex; width: 304px; height: 240px; margin-bottom: 24px;}
.job_list li a { display: inline-flex;  width: 304px; height: 240px; border-radius: 8px;}
.job_list li a:hover, .job_list li a.on { box-shadow: 7px 10px 18px rgba(0, 0, 0, 0.6); border-radius: 8px; transition: all .4s;}
.job_list li a span { font-size: 22px; font-weight: 700; color: #fff; padding: 30px 0 0 30px;}
.job_list li a span::after { display: inline-flex; content: ""; opacity: 0; margin-left: -27px; width: 32px; height: 16px; transition: all .5s; background:url('../../img/career/ico_arrow.png') no-repeat right 0 / 32px 16px;}
