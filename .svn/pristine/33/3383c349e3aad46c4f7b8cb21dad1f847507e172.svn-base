	.wrap {width:100%; position:relative; min-height:inherit;}
	.main_contaier.web_main {position:relative; width:100%;}
	.main_contaier.web_main#common_body {margin-top:0px;}
	
	.bg_box {height:465px;}
	
	
		
	/*메인 top popup*/
	.openPop {clear:both; width:100%; min-width:1280px; height:100px; background:#676767; padding:15px 0px; margin:0 auto; position: relative; z-index:10;}
	.ex_show{float:left;margin-right:40px;cursor:pointer}
	.open_txt {color:#fff !important; width:1280px; margin:0 auto; font-size:20px; height:75px; padding:0 0px 0 0px;}
	.open_txt .bullet {overflow:hidden; text-overflow:ellipsis; white-space:nowrap; width:95%; padding-bottom:10px;}
	.inner {width:100%;}
	.closeBtn {width:20px; height:20px; position:absolute; top:10px; right:calc(50% - 636px); mright:calc(50% - 636px);argin-right:0; opacity:0.8;}
	.closeTxt {position:absolute; top:75px; color:#fff !important; font-size:14px; right:calc(50% - 636px); opacity:0.8; margin-right:0;}
	
	/* footer */
	.footerwrap {display:block;}
	
	.web_open_list1 li {padding:15px 0px 15px 0px;}
	.web_open_list2 li, .web_open_list3 li {display:inline-block; vertical-align:top; font-size:16px; height:60px; padding:6px 15px 10px 15px; background:url('../img/bg_m_pop_line.png') 0 top repeat-y;}
	.web_open_list2 li:first-child, .web_open_list3 li:first-child {display:inline-block; background-image:none !important; padding-left:0;}
	.web_open_list2 li.mb_notice, .web_open_list3 li.mb_notice {display:inline-block; padding-left:15px; background:url('../img/bg_m_pop_line.png') 0 top repeat-y;}	
	.web_open_list2 li {width:49%;}
	.web_open_list3 li {width:32%;}
	
