	.pc_visual_box {width:100%; height:744px; display:block; background:#017dc7;}
	.pc_visual_box ul {width:1280px; margin:0 auto; padding:70px 0px 0 0px; overflow:hidden;}
	.pc_visual_box li {float:left; vertical-align:top; width:417px; height:564px; padding-top:32px; border:6px solid #017dc7; box-sizing:border-box; margin:0 4px 0 5px;}
	.pc_visual_box li:hover {border-color:#fff;}
		
	.pc_visual_box li.box01 {background-size:218px 223px !important;}
	.pc_visual_box li.selected, 
