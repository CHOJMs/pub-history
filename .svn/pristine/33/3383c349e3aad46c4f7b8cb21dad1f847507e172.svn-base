	.pc_visual_box li.box02:hover{background:url("../img/2019/bg_m_visual_i02.png") no-repeat right bottom / 168px 236px;}
	.pc_visual_box li.box03:hover{background:url("../img/2019/bg_m_visual_i03.png") no-repeat right bottom / 180px 245px;}
	
	.pc_visual_box li.on {border:6px solid #fff; background:url("../img/2019/bg_m_visual_i01.png") no-repeat right bottom / 52% auto;}
