======================================================================================================================== */

#m_visual_box {background-color:#007dc6;}
.m_visual {position:relative;margin:0 auto;width:100%;max-width:1200px;}
.m_visual .txt{z-index:2;position:absolute;top:140px;right:80px;}
.m_visual .visual_list{width:100%;height:590px; overflow:hidden;}

.m_visual .visual_control{position:absolute; bottom:20px; left:calc(50% - 55px);text-align:right;}
.m_visual .visual_control button{margin:0 5px; padding:0; width:15px; height:15px; font-size:0; border:none;}
.m_visual .visual_control .dotbox{display:inline;}
.m_visual .visual_control ul.slick-dots{display:inline;}
.m_visual .visual_control ul.slick-dots li{display:inline;line-height:17px;}
.m_visual .visual_control ul.slick-dots li button{display:inline-block;overflow:hidden;border-radius:50%; border:2px solid #fff; background-color:#007dc6; transition:width 500ms ease-in-out 0s;}
.m_visual .visual_control button.auto{background-repeat:no-repeat; background-position:center center;}
.m_visual .visual_control button.play{width:20px;height:19px;background:url('../img/2019/icon_visu_stop.png') no-repeat 0 0;}
.m_visual .visual_control button.pause{width:20px;height:19px;background:url('../img/2019/icon_visu_play.png') no-repeat 0 0;}
.m_visual .visual_control ul.slick-dots li.slick-active button{background-color:#fff;}
.m_visual .visual_list .slick-slide a{display:inline-block; outline:none;}
/*.m_visual .visual_list .slick-slide a:hover {color:#fff !important;}
.m_visual .visual_list .slick-slide a:active, .m_visual .visual_list .slick-slide a:visited {color:#fff !important;}*/

.m_box1, .m_box2, .m_box3 {border:4px solid #fff; height:500px; margin:30px 20px; text-align:left; padding:40px 30px; overflow:hidden;}
.m_box1 {background:url("../img/2019/bg_m_visual_i01.png") no-repeat right bottom / 175px  auto;}
.m_box2 {background:url("../img/2019/bg_m_visual_i02.png") no-repeat right bottom / 135px  auto;}
.m_box3 {background:url("../img/2019/bg_m_visual_i03.png") no-repeat right bottom / 140px auto;}
.visual_list dt {font-size:40px; color:#fff;}
.visual_list dd {font-size:17px; color:#fff; padding-top:10px; position:relative; transition: all 0.4s; word-break:keep-all;}
/*.visual_list dd span {border:1px solid #7cbde1; position:absolute; left:0px; top:70px;}*/
.visual_list dd span a {width:130px; height:40px; color:#fff; text-align:center; font-size:16px; display:block; line-height:40px; margin-top:25px; border:1px solid #7cbde1; transition:all 0.4s linear; display: block !important;}
.visual_list dd span a:hover {background:#025b91;}

.bottom_box1 {padding:35px 0 0 0;}
.bottom_box1 li ,
.bottom_box2 li {width:100%; height:inherit; display:block; margin:0; box-shadow:none; border-radius:inherit;}
.bottom_box1 li:nth-child(1) {background:url("../img/2019/bg_visual_box1_icon1.png") no-repeat 0 50% / 25px auto;}
.bottom_box1 li:nth-child(2) {background:url("../img/2019/bg_visual_box1_icon2.png") no-repeat 0 50% / 25px auto;}
.bottom_box1 li:nth-child(3) {background:url("../img/2019/bg_visual_box1_icon3.png") no-repeat 0 50% / 25px auto;}
.bottom_box1 li:nth-child(4) {background:url("../img/2019/bg_visual_box1_icon4.png") no-repeat 0 50% / 25px auto;}
.bottom_box1 li:nth-child(5) {background:url("../img/2019/system/icon_system_i22_off.png") no-repeat 0 50% / 25px auto;}
.bottom_box1 li:nth-child(6) {background:url("../img/2019/bg_visual_box2_icon4.png") no-repeat 0 50% / 25px auto;}
.bottom_box1 li a {display:inline-block; color:#fff; font-size:17px; font-weight:bold; padding:10px 0 8px 50px;}

.bottom_box2 {padding:25px 0 0 0;}
.bottom_box2 li:nth-child(1) {background:url("../img/2019/bg_visual_box2_icon5.png") no-repeat 0 50% / 25px auto;}
.bottom_box2 li:nth-child(2) {background:url("../img/2019/bg_visual_box2_icon2.png") no-repeat 0 50% / 25px auto;}
.bottom_box2 li:nth-child(3) {background:url("../img/2019/bg_visual_box2_icon4.png") no-repeat 0 50% / 25px auto;}
.bottom_box2 li:nth-child(4) {background:url("../img/2019/system/icon_system_i22_off.png") no-repeat 0 50% / 25px auto;}
.bottom_box2 li:nth-child(5) {background:url("../img/2019/system/icon_system_i21_off.png") no-repeat 0 50% / 25px auto;}
.bottom_box2 li:nth-child(6) {background:url("../img/2019/bg_visual_box2_icon1.png") no-repeat 0 50% / 27px auto;}
.bottom_box2 li a {display:inline-block; color:#fff; font-size:17px; font-weight:bold; padding:10px 0 8px 50px;}

.bottom_box3 {padding:25px 0px 0 0;}
.bottom_box3 li {height:inherit; display:block; margin:0; background-image:none;}
.bottom_box3 li a {display:inline-block; color:#fff; font-size:17px; font-weight:bold; padding:5px 0 2px 0px;}
.bottom_box3 li a span {display:inline-block; width:120px; margin-right: 21px; vertical-align:middle;}
.bottom_box1 li a:hover,
.bottom_box2 li a:hover,
.bottom_box3 li a:hover {color:#a1fff9;}

/* Slider */
.slick-slider{position:relative;display: block;box-sizing:border-box;}
.slick-list{position:relative;display:block;overflow:hidden;}
.slick-list.dragging{cursor:pointer;}
.slick-track{position:relative;top:0;left:0;display:block;}
.slick-track:before,.slick-track:after{display:table;content:'';}
.slick-track:after{clear:both;}
.slick-loading .slick-track{visibility:hidden;}
.slick-slide{display:none;float:left;height:100%;min-height:1px;text-align:center;}
[dir='rtl'] .slick-slide{float:right;}
.slick-slide img{display:block;width:100%;}
.slick-slide.slick-loading img{display:none;}
.slick-initialized .slick-slide{display:block;}
.slick-loading .slick-slide{visibility:hidden;}
.slick-vertical .slick-slide{display:block;height:auto;border:1px solid transparent;}
.slick-arrow.slick-hidden{display:none;}



