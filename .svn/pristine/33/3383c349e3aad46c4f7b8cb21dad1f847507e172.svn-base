	.pc_visual_box .bg_box li a:hover {color:#a1fff9 !important;}
	.pc_visual_box .bg_box dt {font-size:46px; color:#fff;}
	.pc_visual_box .bg_box dd {font-size:18px; color:#fff; padding-top:16px; position:relative; transition: all 0.4s;}
	.pc_visual_box .bg_box dd span a {border:1px solid #7cbde1; width:130px; height:50px; color:#fff; text-align:center; font-size:16px; display:block; line-height:50px; margin-top:27px; transition:all 0.3s linear;}
	.pc_visual_box .bg_box dd span a:hover {background:#025b91; border:1px solid #017dc7;}
		
	.pc_visual_box li .bottom_box1 {padding:72px  0 0 0; width:inherit;}
	.pc_visual_box li .bottom_box1 li ,
	.pc_visual_box li .bottom_box2 li {width:100%; height:inherit; display:block; margin:0; box-shadow:none; border-radius:inherit;}
	.pc_visual_box li .bottom_box1 li:nth-child(1) {background:url("../img/2019/bg_visual_box1_icon1.png") no-repeat 0 50% / 31px auto;}
	.pc_visual_box li .bottom_box1 li:nth-child(2) {background:url("../img/2019/bg_visual_box1_icon2.png") no-repeat 0 50% / 31px auto;}
	.pc_visual_box li .bottom_box1 li:nth-child(3) {background:url("../img/2019/bg_visual_box1_icon3.png") no-repeat 0 50% / 31px auto;}
	.pc_visual_box li .bottom_box1 li:nth-child(4) {background:url("../img/2019/bg_visual_box1_icon4.png") no-repeat 0 50% / 29px auto;}
	.pc_visual_box li .bottom_box1 li:nth-child(5) {background:url("../img/2019/system/icon_system_i22_off.png") no-repeat 0 50% / 29px auto;}
	.pc_visual_box li .bottom_box1 li:nth-child(6) {background:url("../img/2019/bg_visual_box2_icon4.png") no-repeat 0 50% / 30px auto;}
	.pc_visual_box li .bottom_box1 li a {display:block; color:#fff; font-size:20px; font-weight:bold; padding:11px 0 10px 50px;}
	
	.pc_visual_box li .bottom_box2 {padding:22px 0 0 0; width:inherit;}
	.pc_visual_box li .bottom_box2 li:nth-child(1) {background:url("../img/2019/bg_visual_box2_icon5.png") no-repeat 0 50% / 31px auto;}
	.pc_visual_box li .bottom_box2 li:nth-child(2) {background:url("../img/2019/bg_visual_box2_icon2.png") no-repeat 0 50% / 31px auto;}
	.pc_visual_box li .bottom_box2 li:nth-child(3) {background:url("../img/2019/bg_visual_box2_icon4.png") no-repeat 0 50% / 30px auto;}
	.pc_visual_box li .bottom_box2 li:nth-child(4) {background:url("../img/2019/system/icon_system_i22_off.png") no-repeat 0 50% / 29px auto;}
	.pc_visual_box li .bottom_box2 li:nth-child(5) {background:url("../img/2019/system/icon_system_i21_off.png") no-repeat 0 50% / 29px auto;}
	.pc_visual_box li .bottom_box2 li:nth-child(6) {background:url("../img/2019/bg_visual_box2_icon1.png") no-repeat 0 50% / 31px auto;}
	.pc_visual_box li .bottom_box2 li a {display:block; color:#fff; font-size:20px; font-weight:bold; padding:8px 0 13px 50px;}
	
	.pc_visual_box li .bottom_box3 {padding:22px 0px 0 0; width:inherit;}
	.pc_visual_box li .bottom_box3 li {height:inherit; display:block; margin:0; background-image:none;}
	.pc_visual_box li .bottom_box3 li a {display:block; color:#fff; font-size:20px; font-weight:bold; padding:8px 0 8px 0px;}
	.pc_visual_box li .bottom_box3 li a span {display:inline-block; margin-right: 21px;}
	.pc_visual_box li .bottom_box1 li a:hover,
	.pc_visual_box li .bottom_box2 li a:hover,
	.pc_visual_box li .bottom_box3 li a:hover {color:#008dd6;}
	
	
	/* 공지사항 */
	.bottom_box {height:277px; width:1280px; margin:auto; display:block; clear:both; position:relative; background:#fff;}
	
	.notice_box {float:left; width:33.33%; height:73px; display:block; vertical-align:top; position:relative; padding-top:0px; padding-left:30px; border-right:1px solid #d9d9d9; border-bottom:none; }
	.notice_box h2 {font-size:20px; font-weight:bold; margin:0; padding:0px 0 13px 0; border:none;}
	.notice_box ul {padding-left:0px; border-bottom:none;}
	.notice_box li {display:block; position:relative; margin-right:24px;}
	.notice_box li a {display:block; font-size:18px; line-height:22px; padding:9px 100px 0px 0px; margin-bottom:10px; overflow:hidden; text-overflow:ellipsis; word-wrap:break-word; white-space:nowrap;}
	.notice_box li a:hover {color:#008dd6;}
	.notice_box li span {position:absolute; right:5px; top:5px; border-radius:8px; color:#007dc6; font-size:18px; width:100px; height:30px; line-height:28px; text-align:right;}
	.notice_box a.more {position:absolute; top:0px; right:30px; display:block;}
	.notice_box a.more img {width:18px; height:18px;}
	.notice_box li.noData {text-align:center; font-size:18px; padding:9px 0 !important;}
	
	/* 자주하는질문 */	
	.faq_box {float:left; width:33.33%; height:73px; vertical-align:top; position:relative; margin-top:0; padding-top:0px; padding-left:30px; border-right:1px solid #d9d9d9;}
	.faq_box h2 {font-size:20px; font-weight:bold; margin:0; padding:0px 0 17px 0; border:none;}
	.faq_box ul {padding-left:0px; padding-top:0px; border-bottom:none;}
	.faq_box li {display:inline-block;}
	.faq_box li a {display:inline-block; text-align:center; border-radius:80px; color:#fff; font-size:15px; height:30px; width:98px; line-height:30px; margin-right:13px; background:#606060;}
	.faq_box li a:hover {background:#017dc7;}
	.faq_box a.more {position:absolute; top:0px; right:30px; display:block;}
	.faq_box a.more img {width:18px; height:18px;}
	
	/* 배너 */	
	.banner_box {padding:52px 0px 60px; width:100%; border-top:none; background:#f4f4f4;}
	.banner_box .banner_wrap {display:table; width:1280px; margin:0 auto; overflow:hidden; padding:0; background:none;}
	.banner_box .banner_wrap h2 {color:#000; font-size:22px; border:none; background:none; padding-top:0px; padding-bottom:37px; padding-left:0;}
	.banner_box .banner_wrap .banner_col {width:324px; height:240px; position:relative; animation-delay:.7s; float:left; display:block; padding:0 20px 0 0; margin-bottom: 0px; transition:all 0.4s linear; overflow:hidden;}
	.banner_box .banner_wrap .banner_col a {display:block; height:240px; transition:all 0.4s; color:#fff; font-size:22px; font-weight: bold; text-align:center; padding-top:105px;}
	.banner_box .banner_wrap .banner_col a:hover,
	.banner_box .banner_wrap .banner_col a:focus {padding-top:75px; transition:all 0.4s; background-color:#000; background-color: rgba( 0, 0, 0, 0.5 );}
	.banner_box .banner_wrap .banner_col a .btn_go { display:block; opacity:0; width:128px; height:38px; line-height:34px; font-size:16px; color:#009fc5; padding-top:0px; transition:all 0.4s; border-radius:0px; border:1px solid #019fc6; margin:15px auto 0;}
	.banner_box .banner_wrap .banner_col a:hover .btn_go,
	.banner_box .banner_wrap .banner_col a:focus .btn_go { display:block; opacity:1; padding-top:0px; transition:all 0.4s;}
	.banner_box .banner_wrap .banner_col a .blind {background:#000; transition:all 0.4s; opacity:0.4; width:304px; height:100%; position:absolute; top:240px; z-index:1;}
	.banner_box .banner_wrap .banner_col a:hover .blind,
	.banner_box .banner_wrap .banner_col a:focus .blind {background:#000; transition:all 0.4s; opacity:0; top:0; width:304px; height:100%; position:absolute; top:0;z-index:1;}
	.banner_box .banner_wrap .banner_col .ban01 {width:304px; height:100%; background:url("../img/2019/img_m_banner01.jpg") no-repeat 0 0 / 100% 240px;}
	.banner_box .banner_wrap .banner_col .ban02 {width:304px; height:100%; background:url("../img/2019/img_m_banner02.jpg") no-repeat 0 0 / 100% 240px;}
	.banner_box .banner_wrap .banner_col .ban03 {width:304px; height:100%; background:url("../img/2019/img_m_banner03.jpg") no-repeat 0 0 / 100% 240px;}
	.banner_box .banner_wrap .banner_col .ban04 {width:304px; height:100%; background:url("../img/2019/img_m_banner04.jpg") no-repeat 0 0 / 100% 240px;}
	.banner_box .banner_wrap .banner_col:nth-child(5) {padding-right:0; width:304px;}

	
