@charset'utf-8';
.wrap {margin-bottom:0;}
.bg_box {height:485px;}
.is-selected  .bg_box {transform:scaleY(1) !important; transition:all 0.3s ease;}
.carousel-cell .bg_box {transform:scaleY(0.9); transition:all 0.3s ease;}

/* ---- page dots ---- */
.flickity-page-dots {position:absolute; width:100%; bottom:30px; padding:0; margin:0; list-style:none; text-align:center; line-height:1;}
.flickity-rtl .flickity-page-dots { direction: rtl; }
.flickity-page-dots .dot {display:inline-block; width:20px; height:20px; margin: 0 8px; border:3px solid #bdc1c3; background:#edeff3; border-radius:50%; cursor:pointer;}
.flickity-page-dots .dot:before {background:none;}
.flickity-page-dots .dot.is-selected { background:#0771b9; border:3px solid #0771b9; opacity:1;}
.flickity-button {display:none;}

.pc_visual_box {display:none;}

/* footer */
.footerwrap {display:block;  clear:both;}

/*메인 top popup*/
.openPop {clear:both; width:100%; height:80px; background:#676767; padding:13px 0px; position: relative; z-index:10;}
.ex_show{float:left;margin-right:20px;cursor:pointer}
.open_txt {color:#fff !important; font-size:15px; height:55px; padding:0 15px;}
.open_txt p {overflow:hidden; text-overflow:ellipsis; white-space:nowrap; width:92%; padding-bottom:5px; }
.open_txt a {color:#fff;}
.inner {width:100%; height:1px;}
.closeBtn {width:15px; height:15px; position:absolute; right:15px; top:12px; opacity:0.8;}
.closeTxt {color:#fff !important; font-size:11px; position:absolute; right:15px; top:60px; opacity:0.8;}

.web_open_list1 li {padding:0 30px 0 0px; position:relative;}
.web_open_list2 li, .web_open_list3 li {display:none; height:55px; padding:0 15px; line-height:21px; font-size:14px; background:url('../img/bg_m_pop_line.png') 0 top repeat-y;}
.web_open_list2 li.mb_notice, .web_open_list3 li.mb_notice {display:block; background-image:none; padding-left:0;}
.web_open_list1 li a,
.web_open_list2 li a,
.web_open_list3 li a {display:-webkit-box !important;display:block; width:100%; overflow:hidden; text-overflow:ellipsis; -webkit-box-orient:vertical; word-break:keep-all; -webkit-line-clamp:2;/*글자수*/ line-height:1.5em; max-height:3em;}


#back-home {display:none !important;}


