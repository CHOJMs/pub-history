	.service_banner_box ul {padding-top:47px;}
	.service_banner_box li {float:left; width:142px; position:relative; padding-bottom:0px; border-left:1px solid #d1d1d1; height:inherit;}
	.service_banner_box li a {color:#000; font-size:18px; display:block; padding-top:70px; padding-bottom:0px; text-align:center; font-weight:bold; transition: transform .3s ease-out;}
	.service_banner_box li a:hover i {width:9px; height:17px; display:inline-block; margin-left:8px; background:url("../img/2019/icon_arrow.png") 0 0 no-repeat;}
	.service_banner_box li:nth-child(1) {background:url("../img/2019/icon_m_i08.png") no-repeat center 0 / 50px auto;}
	.service_banner_box li:nth-child(2) {background:url("../img/2019/icon_m_i07.png") no-repeat center 0 / 50px auto;}
	.service_banner_box li:nth-child(3) {background:url("../img/2019/icon_m_i06.png") no-repeat center 0 / 50px auto;}
	.service_banner_box li:nth-child(4) {background:url("../img/2019/icon_m_i03.png") no-repeat center 0 / 50px auto;}
	.service_banner_box li:nth-child(5) {background:url("../img/2019/icon_m_i05.png") no-repeat center 0 / 50px auto;}
	.service_banner_box li:nth-child(6) {background:url("../img/2019/icon_m_i06.png") no-repeat center 0 / 50px auto;}
	.service_banner_box li:nth-child(7) {background:url("../img/2019/icon_m_i04.png") no-repeat center 0 / 50px auto;}
	.service_banner_box li:nth-child(8) {background:url("../img/2019/icon_m_i02.png") no-repeat center 0 / 50px auto;}
	.service_banner_box li:nth-child(9) {background:url("../img/2019/icon_m_i01.png") no-repeat center 0 / 50px auto;}
	.service_banner_box li:first-child {border-left:none;}
	
	
	/* =====================================================================================================================
		2020년 css 추가
	======================================================================================================================== */
	
	/* mydgb */
	.mydgb_box {position:absolute; top:-70px; left:calc(50% - 508px); width:1016px; background:#fff; border-radius:90px; margin-bottom:0px; padding:40px 60px; z-index:2;}
	.mydgb_box h2 {color:#000; font-size:22px; border:none; background:none; display:inline-block; padding:0;}
	.mydgb_box ul {margin-left:55px; display:inline-block; width:757px;}
	.mydgb_box ul li {display:inline-block; width:236px; margin-left:21px; padding-left:0; background:url("../img/2020/icon_m_arrow.png") no-repeat 88% 50% / 11px auto;}
	.mydgb_box ul li:first-child {margin-left:0; padding-left:0;}
	.mydgb_box ul li a {display:inline-block; font-size:22px; width:236px; height:58px; line-height:58px; padding:0 30px;}
	

	
	
