/* mydgb */
.mydgb_box {position:relative; width:100%; background:#fff; padding:25px 20px 30px 20px; z-index:2; margin-bottom:4px;}
.mydgb_box h2 {color:#000; font-size:18px; border:none; background:none; display:block; padding:0 0 20px 0; margin-top:0;}
.mydgb_box ul {margin-left:0px; display:table; width:100%;}
.mydgb_box ul li {display:block; width:100%; margin-top:7px; background:url("../img/2020/icon_m_arrow.png") no-repeat 90% 50% / 11px auto;}
.mydgb_box ul li:first-child {padding-left:0; margin-top:0px;}
.mydgb_box ul li a {display:inline-block; width:100%; font-size:16px; height:55px; line-height:55px; padding:0 30px; border-radius:80px; border:1px solid #909090;}

/* 공지사항 */
.bottom_box {width:100%; margin:auto; display:block; clear:both; position:relative; background:#f4f4f4;}
.bottom_box:after {clear:both; content:""; display:block;}
.notice_box {float:left; width:100%; display:block; vertical-align:top; position:relative; padding:0;}
.notice_box h2 {font-size:18px; font-weight:bold; margin:0; padding:0px 0 0px 0; border:none;}
.notice_box ul {padding-left:0px; border-bottom:1px solid #d1d1d1; padding-bottom:16px;}
.notice_box li {display:block; position:relative; margin-right:0px; }
.notice_box li a {display:block; font-size:16px; line-height:22px; padding:8px 100px 0px 0px; margin-bottom:10px; overflow:hidden; text-overflow:ellipsis; word-wrap:break-word; white-space:nowrap;}
.notice_box li a:hover {color:#008dd6;}
.notice_box li span {position:absolute; right:0px; top:5px; border-radius:8px; color:#007dc6; font-size:16px; width:90px; height:30px; line-height:28px; text-align:right;}
.notice_box a.more {position:absolute; top:0px; right:0px; display:block;}
.notice_box a.more img {width:18px; height:18px;}
.notice_box li.noData {text-align:center; font-size:18px; padding:9px 0 !important;}

/* 자주하는질문 */	
.faq_box {float:left; width:100%; vertical-align:top; position:relative; padding:0; margin-top:26px;}
.faq_box h2 {font-size:18px; font-weight:bold; margin:0; padding:0px 0 12px 0; border:none;}
.faq_box ul {padding-left:0px; padding-top:0px; border-bottom:1px solid #d1d1d1; padding-bottom:30px;}
.faq_box li {display:inline-block;}
.faq_box li a {display:inline-block; text-align:center; border-radius:80px; color:#fff; font-size:16px; height:32px; width:97px; line-height:32px; margin-right:10px; background:#606060;}
.faq_box li a:hover {background:#017dc7;}
.faq_box li:nth-child(3) a {margin-right:0;}
.faq_box a.more {position:absolute; top:0px; right:0px; display:block;}
.faq_box a.more img {width:18px; height:18px;}

/* 고객상담센터 */
.service_box {display:block; width:100%; padding:0 20px; margin-top:26px;}
.service_box  .customer_center {float:left; width:100%; vertical-align:top; position:relative; padding-bottom:22px; padding-top:26px; border-bottom:none;}
.service_box  .customer_center h2 {font-size:18px; font-weight:bold; margin:0; padding:0px 0 6px 0; border:none;}
.service_box  .customer_center .txt {font-size:16px; color:#000; font-weight:bold; padding-bottom:0px; padding-top:3px; margin-left:0; display:block; padding-left:0; vertical-align:middle;}
.service_box  .customer_center .tel {font-size:40px; color:#007dc6; font-weight:bold; margin-left:0; display:block; line-height:40px;}

/* service */
.service_banner_box {width:100%; padding:27px 20px 0 20px; background:#fff; margin-bottom:4px;}
.service_banner_box:after {clear:both; content:""; display:block;}
.service_banner_box h2 {color:#000; font-size:18px; border:none; background:none; padding:0px 0px 27px 0px; margin-top:0;}
.service_banner_box li {float:left; width:25%; position:relative; padding-bottom:0px; height:102px;}
.service_banner_box li a {color:#000; font-size:14px; display:block; padding-top:44px; padding-bottom:24px; text-align:center; font-weight:bold; line-height: 16px; transition: transform .3s ease-out;}
.service_banner_box li a:hover i {width:9px; height:17px; display:inline-block; margin-left:8px; background:url("../img/2019/icon_arrow.png") 0 0 no-repeat;}
.service_banner_box li:nth-child(1) {background:url("../img/2019/icon_m_i08.png") no-repeat center 0 / 37px auto;}
.service_banner_box li:nth-child(2) {background:url("../img/2019/icon_m_i07.png") no-repeat center 0 / 37px auto;}
.service_banner_box li:nth-child(3) {background:url("../img/2019/icon_m_i06.png") no-repeat center 0 / 37px auto;}
.service_banner_box li:nth-child(4) {background:url("../img/2019/icon_m_i03.png") no-repeat center 0 / 37px auto;}
.service_banner_box li:nth-child(5) {background:url("../img/2019/icon_m_i05.png") no-repeat center 0 / 37px auto;}
.service_banner_box li:nth-child(6) {background:url("../img/2019/icon_m_i06.png") no-repeat center 0 / 37px auto;}
