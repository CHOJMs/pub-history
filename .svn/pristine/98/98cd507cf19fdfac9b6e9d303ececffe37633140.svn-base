@charset "UTF-8";

/* MF-000-000 */
.flex-btn {display: flex; justify-content: space-between;}
.flex-btn .btn.btn-blk {margin-top: -4px;}
.flex-btn .btn.btn-blk a {display: inline-flex; align-items: center; justify-content: center; padding: 0 10px; height: 32px; line-height: 1; font-size: 13px; color: #fff; background: rgba(34, 36, 38, 1); border-radius: 4px; letter-spacing: -0.05em;}
.popup-contents .flex-btn .btn.btn-blk {margin-top: -1px;}
.select-box > div ul {padding-bottom: 18px;}
.select-box > div ul li + li {margin-top: 8px;}
.select-box > div ul li {display: flex; flex-wrap: wrap; align-items: flex-start; font-size: 14px; letter-spacing: -0.05em;}
.select-box > div ul li span {display: inline-flex; width: 100px; margin-right: 6px; color: rgb(112, 116, 120);}
.select-box > div ul li p {width: calc(100% - 106px); color: rgb(34, 36, 38);}


/*유의사항*/
.box_cauinfo {padding-top: 4.8rem;}
.box_cauinfo h2, .box_cauinfo .tl {font-size: 1.6rem; padding-bottom: 1rem; font-weight: 700;}
.box_cauinfo li {font-size: 1.4rem; color: rgb(112, 116, 120); padding-left: 1.2rem; position: relative; margin-bottom: 1rem;}
.box_cauinfo li:before {content: ''; position: absolute; display: inline-block; left: 0; top: .7rem; width: .8rem; height: .8rem; margin-right: 1.2rem; border: .1rem solid rgb(112,116,120); border-radius: 50%;}
.box_cauinfo li:last-child {margin-bottom: 0rem;}

.list_bull li {font-size: 1.4rem; color: rgb(112, 116, 120); margin-bottom: 1rem; padding-left: 1.2rem; position: relative;}
.list_bull li:before {content: ''; position: absolute; display: inline-block; left: 0; top: .7rem; width: .6rem; height: .6rem; margin-right: .6rem; border: .1rem solid rgb(112,116,120); border-radius: 50%;}


.list_bull-normal { margin-top: 1.6rem;}
.list_bull-normal li {font-size: 1.4rem; color: rgb(112, 116, 120); padding: 0 !important; position: relative; margin-bottom: 1rem;}
.list_bull-normal li:last-child {margin-bottom: 0rem;}
.list_bull-normal li a{display: inline-block;padding-left: 27px;height: 21px;}
.list_bull-normal li a:before {background: #bcc0c5;width: .7rem;height: .7rem;border-radius: 50%;display: block;content: '';position: absolute;top: .7rem;left: .7rem;z-index: 1;}
.list_bull-normal li a:after {position:absolute;top:0; left:0;width: 2.1rem;height: 2.1rem; margin-top:0px; background:#fff; border:.1rem solid #d3d3d3;border-radius: 50%;background-size: 1.6rem;content: ''}padding-left{position:absolute;top:0; left:0; width:2.4rem; height:2.4rem; margin-top:0px; background:#fff; border:.1rem solid #d3d3d3; border-radius: 50%; background-size:1.6rem;content: ''}
.list_bull-normal li a.active:before {background:#fff;display: block;content: '';position: absolute;transition: .3s;transform: scale(1.3, 1.3);opacity: 1;}
.list_bull-normal li a.active:after {background:rgb(18, 151, 239); background-size:1.6rem; border:.1rem solid rgb(18, 151, 239);}

.list-normal, .list-type-dash { padding-left: 0px !important;}
.list-normal li {font-size: 1.7rem; color: rgb(112, 116, 120); margin-bottom: 6px; padding-left: 0px !important; text-indent: 0px !important;}
.list-normal li:before {content: '' !important;}
.list-normal li > .list-type-dash li:before {content: '-' !important;}
.list-normal.mb16 li {margin-bottom: 16px;}

.list-type-dot ul li > .list-type-dash li::before {display: inline-block; content: "-"; margin-right: 4px; font-size: 18px;}
.list-type-dot ul li strong {font-weight: 700; color: rgb(34, 36, 38); font-size: 1.9rem; }
.list-type-dot.t16 ul li + li {margin-top: 16px;}
.list-type-dot.t32 ul li + li {margin-top: 40px;}
.line.mt24 {margin-top: 4rem !important; padding-bottom: 4rem !important;}

/* 파일관리 */
.box_file input.ui_file {position: absolute; width:1px; height:1px; padding:0; margin:-1px; overflow:hidden; clip:rect(0,0,0,0); border:0;} /* 파일 필드 숨기기 */
.box_file .num {font-size: 14px; border: 1px solid rgb(211, 213, 216); border-radius: 4px; padding: 24px 16px; margin-top: 24px;}
.box_file .num span {color: #1297ef; font-family: Koho; font-weight: 700;}
.box_file .num ul{border-top:1px solid rgb(112, 116, 120, 0.15); margin-top: 24px; padding-top: 2px;}
.box_file .num li {position:relative; margin:15px 0; padding-right: 35px; font-size: 14px; line-height: 22px; overflow:hidden; white-space:nowrap; text-overflow:ellipsis;}
.box_file .num li:last-child {margin-bottom: 0;}
.box_file .num a.file_delete {width:20px; height:20px; cursor:pointer; background:#707478; color:#777; border-radius:50%; border:1px solid #e3e3e3; position:absolute; top:50%; right:0px; top:calc(50% - 10px); text-align:center;}
.box_file .num a.file_delete img {width:7px; height:7px; margin-top:-7px;}

/* 첨부파일-버튼 */
.btn_file_box {position:relative; height:56px;}
.btn_file_box  .btn,
.btn_file_box  a.btn_file {margin:0; background:#1297ef; border-radius: .8rem; color:#fff; text-align:center; height:56px; line-height:56px; font-weight: 500; font-size: 1.6rem; display:block;}
.btn_file_box  .btn.gray {background: rgb(235, 236, 238); color: rgb(34, 36, 38);}
.btn_file_box  button:focus.btn.gray {outline: 3px dotted #333;}
.file_input_hidden {font-size:38px; width:100%; position:absolute; right:0px; top:0px; opacity:0; filter: alpha(opacity=0); -ms-filter: alpha(opacity=0); cursor:pointer;}

.r_list li {padding-bottom: 2.2rem;}
.r_list li .txt {padding-bottom: 1.2rem;}

.call_phone {font-size: 1.8rem; text-align: center;}
.call_phone .tit {font-weight: 700;}
.call_phone .phone {font-weight: 700; color: rgb(18, 151, 239);}
.unit_txt {font-size: 1.2rem; padding: 8px 0 8px 0;}

/* 스탁론 전화번호 */
.call_phone1 {font-size: 1.6rem; color: rgb(34, 36, 38); margin-top: 3.2rem;}
.call_phone1 .tit {font-weight: 700;}
.call_phone1 .phone {font-weight: 700; color: rgb(18, 151, 239);}

/* box_스타일 */
.blue_box .txt {font-size: 1.6rem; color: rgb(34, 36, 38); padding-bottom: 0;}
.blue_box .txt2 {font-size: 1.8rem; color: rgb(34, 36, 38); font-weight: 500;}
.blue_box .info_txt {margin-top: 1.6rem;}
.blue_box .info_txt li {font-size: 1.5rem; line-height: 2.3rem; letter-spacing: -0.05em; color: rgb(112, 116, 120); margin-bottom: 1.2rem;}
.blue_box .info_txt li:last-child {margin-bottom: 0;}
.blue_box .info_txt.list-type-dash li {padding-left: 4px; text-indent: -4px;}
.blue_box .info_txt.list-type-dash li::before {display: inline-block; content: "-"; margin-right: 4px; font-size: 15px; vertical-align: top;}
.blue_box.mt40 {margin-top: 5.6rem !important; margin-bottom: 2.5rem; padding: 2.4rem;}
.blue_box.m1 {margin-top: 4rem !important; margin-bottom: 1.7rem; padding: 2.4rem;}
.yellow_box {font-size: 1.6rem; color: rgb(34, 36, 38); padding: 2.4rem; letter-spacing: -0.05em; border-radius: .8rem; background: rgb(255, 243, 165); margin-bottom: 4.8rem;}
.yellow_box .txt { color: rgb(34, 36, 38); font-size: 1.8rem; font-weight: 700; padding-bottom: 2.4rem;}

/* 잠시대기중 */
.box_waiting { position: relative; background:url('../img/ico_waiting.png') no-repeat center 4px / 120px auto; text-align: center;}
.box_waiting p.txt1 {font-size: 1.6rem; color: rgb(34, 36, 38); padding-top: 12rem; text-align: center; font-weight: 500; padding-bottom: 1.6rem;}
.box_waiting p.txt1 span { color: rgb(18, 151, 239);}
.box_waiting p.txt2 {font-size: 1.4rem; color: rgb(112, 116, 120);}
.box_waiting p.icon { position: absolute; width: 62px; height: 80px; top: 25px; left: 40%; transform: translate(-50%, 0); animation: 1.4s linear 0s infinite alternate move_eye; background: url('../img/ico_waiting1.png') center center / 100% no-repeat;}

@keyframes move_eye { from { margin-left: 190px; } to { margin-left: 80px; } }

/* 인증서가 없습니다 */
.box_confi {background:url('../img/ico_nodata.png') no-repeat center 10rem / 12rem auto;}
.box_confi p.txt1 {font-size: 1.6rem; color: rgb(34, 36, 38); padding-top: 26rem; text-align: center; font-weight: 500; padding-bottom: 1.6rem;}
.box_confi p.txt2 {font-size: 1.4rem; color: rgb(112, 116, 120); text-align: center;}



/* 인증번호받기 */
.auth_t { margin-top: 3.2rem;}

/* 버튼 위아래 간격 */
.btn_m1 {margin-top: 3.6rem;}

/* 스탁론 */
.ico_tooltip1 {font-size: 2.2rem; line-height: 3.2rem; border-top: none; color: #fff; padding-left: 4rem; margin-bottom: 3.2rem; margin-top: 1.2rem;}
.ico_tooltip1::before { display: block; content: ""; width: 3.2rem; height: 3.2rem; position: absolute; left: 2.4rem; background:url('../img/ico_tooltip1.png') no-repeat 0 0 / 3.2rem auto;}
.ico_tooltip1 + p.txt {font-size: 1.6rem; color: rgb(211, 213, 216); font-weight: 400; border-top: none; padding: 0 0 1.2rem 3.2rem; margin-top: -1.8rem; text-align: left;}
.ico_tooltip2 {font-size: 2.2rem; line-height: 3.2rem; border-top: none; color: #fff; padding-left: 4rem; margin-bottom: 3.2rem; margin-top: 1.2rem;}
.ico_tooltip2::before { display: block; content: ""; width: 3.2rem; height: 3.2rem; position: absolute; left: 2.4rem; background:url('../img/ico_tooltip2.png') no-repeat 0 0 / 3.2rem auto;}

/*완료 텍스츠 추가*/
.finish_box .fini_t_box .txt1 {font-size: 1.6rem; text-align: center; color: rgb(112, 116, 120); margin-top: 0rem; padding-bottom: 4rem;}

.group_box {margin-bottom: 4rem;}
.group_box .no_data {margin-top: 0px; padding: 0; border: none;}
.group_box .no_data p { padding-top: 19rem; border-top: 1px solid #000; font-size: 1.5rem; color: rgb(34, 36, 38); background:url('../img/ico_nodata.png') no-repeat center 8rem / 8rem auto;}

