/* 2024-07-08 고객만족도 조사 추가 */
.survey_cont{padding:40px 20px 120px;}
.survey_tit{position:relative;padding-left:22px;margin:40px 0 12px;font-size:16px;color:#222426;line-height:24px;font-weight:600}
.survey_tit:first-of-type{margin-top:0}
.survey_tit span{position:absolute;top:4px;left:0;width:16px;height:16px;text-align:center;font-size:12px;font-weight:bold;background:#222426;color:#fff;line-height:16px}
.sur_inp_area{display:flex;}
.sur_inp_area.col{display:flex;flex-direction:column;gap:8px}
.sur_inp_area.col .sur_inp{margin-left:0}
.sur_inp_area+.sur_inp_area{margin-top:7px}
.sur_inp_area .sur_inp{flex:1;margin-left:7px}
.sur_inp_area .sur_inp:first-child{margin-left:0}
.sur_inp_area .sur_inp input{position:absolute;overflow:hidden}
.sur_inp_area .sur_inp label{display:block;border:1px solid #dfe3e8;background:#fff;border-radius:8px;line-height: 40px;text-align:center;color:#5c6773;font-weight:500}
.sur_inp_area .sur_inp input:checked+label{border-color:#21a088;color:#21a088;font-weight:600;background-color:#f2faf8}
.sur_inp_area .sur_inp input:disabled+label{border-color:#dfe3e8;color:#bcc0c5;background-color:#f5f5f5}
.sur_inp_area .sur_inp02 input{position:absolute;overflow:hidden}
.sur_inp_area .sur_inp02 label{position:relative;display:block;padding:11px 14px 11px 44px;border:1px solid #dfe3e8;background:#fff;border-radius:8px;font-size:16px;line-height: 24px;text-align:left;color:#5c6773;font-weight:500}
.sur_inp_area .sur_inp02 label:after{position:absolute;top:13px;left:16px;width:20px;height:20px;background-size:20px auto;background-repeat:no-repeat;content:''}
.sur_inp_area .sur_inp02 input[type="checkbox"]+label:after{background-image:url('../img/ui_common/ico_check02_off.png');}
.sur_inp_area .sur_inp02 input[type="checkbox"]:checked+label:after{background-image:url('../img/ui_common/ico_check02_on.png');}
.sur_inp_area .sur_inp02 input[type="radio"]+label:after{background-image:url('../img/ui_common/ico_radio_off.png');}
.sur_inp_area .sur_inp02 input[type="radio"]:checked+label:after{background-image:url('../img/ui_common/ico_radio_on.png');}
.sur_inp_area .sur_inp02 input:checked+label{border-color:#21a088;color:#00a88b;background-color:#f2faf8}
.sur_inp_area .sur_inp02 input:disabled+label{border-color:#dfe3e8;color:#bcc0c5;background-color:#f5f5f5}
.sur_textarea{padding:16px;border:1px solid #dfe3e8;border-radius: 8px;}
.sur_textarea textarea{width:100%;border:0;color:#5c6773;font-size:16px;font-weight:500;line-height:24px}
.sur_textarea textarea::placeholder{color:#919191}
.sur_textarea p{margin-top:8px;color:#bcc0c5;font-size:12px;}
.btn_survey_wrap{position:sticky;bottom:20px;display:flex;gap:8px;padding:0 20px 20px;margin-top:40px;background:linear-gradient(180deg, #ffffff2e 8%, #ffffff 100%)}
.btn_survey{width:100%;background:#0da286;color:#fff;border-radius: 8px;line-height: 56px;text-align: center;font-size:16px;font-weight:500}
.btn_survey.gray{background:#eaeef1;color:#5c6773}
.sur_tx01{margin-top:40px;color:#222426;font-size:22px;font-weight:600;text-align:center}
.sur_tx02{margin-top:12px;font-size:16px;color:#707478;line-height:24px;text-align:center}
.survey_wrap .error-wrap{padding-top:144px}
.survey_wrap .fixed-btn-area{padding-top:44px}

