/* sub_layout */
.sub_contaier h1 {display: block; font-size: 4rem; color: rgb(34, 36, 38); font-weight: 700; text-align: center; padding-bottom: 8rem;}
.sub_contaier h2 {font-size: 1.8rem; color: rgb(112, 116, 120); font-weight: 700; padding-bottom: 2.4rem; margin-top: 5.6rem;}
.sub_contaier .contents { width: 96rem; margin: auto; padding: 17.2rem 0rem 0 0;}
.contPd_wrap h2 {margin-top: 5.6rem; color: rgb(34, 36, 38); font-size: 2.2rem;}
