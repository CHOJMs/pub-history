.contPd_wrap .tit_info .txt { font-size: 1.4rem; padding-bottom:0; font-weight: 400; line-height: 3rem;}

/* mydgb */
.user_info { position: relative; font-size:2.2rem; padding: 3.2rem 4rem; color: rgb(34, 36, 38); letter-spacing: -0.05em; border-radius: .4rem; background: #fff; box-shadow: 0 0.2rem 1.8rem rgba(102, 102, 102, 0.15);}
.user_info span {font-weight: 700;}
.user_info div {position: absolute; right: 4rem; top: 3.4rem; display: block;}
.user_info div a { font-size: 1.6rem; font-weight: 500; color: rgb(34, 36, 38); text-decoration: underline;}
.user_info div a.logout {background: rgb(235, 236, 238); font-size: 1.4rem; text-align: center; padding: .6rem 1.2rem; margin-left: 1.6rem; text-decoration: none; border-radius: .4rem; letter-spacing: -0.05em;}

/* hashtag */
.hashtag li {display: inline-flex; flex-wrap: wrap; margin-right: 1.6rem; padding-bottom: .8rem;}
.hashtag li a {font-size: 1.4rem; color: rgb(18, 151, 239); font-weight: 500;}

.list_ban li {display: inline-flex; margin-left: 1.6rem; background: rgb(18, 151, 239); }
.list_ban li:first-child {margin-left: 0;}
.list_ban li a {display: block; padding: 1.8rem 4rem; color: #fff; font-size: 1.8rem; font-weight: 500;}
.list_ban li a i::after { display: inline-block; content: ""; width: 2rem; height: 2rem; background:url('../img/ico_arrow04.png') no-repeat right 4px  / 2rem auto;}

.groud_box {margin-bottom: 56px;}
.my_infotxt {font-size: 1.8rem; line-height: 2.8rem; padding: 0 0 3.2rem; letter-spacing: -0.05em;}
.my_infotxt1 {font-size: 2rem; line-height: 2.8rem; padding: 0 0 3.2rem; letter-spacing: -0.05em; font-weight: 700;}

/* 간격 */
.bg_pattern {display: block; height: 10px; background: url(../img/main/line_01.png) repeat;}
.bg_pattern + h2, .bg_pattern + h2:first-of-type, .pc_btn_box + .bg_pattern,  .pc_btn_box + .dgb_block, .bg_pattern + .dataType,
.list-type-dot.t16 + .bg_pattern, .bg_graybox + .contPd_wrap { margin-top: 5.6rem;}
.gray_line_box + .bg_pattern  { margin-top: 1.6rem;}
.dgb_block + .pc_btn_box, .white_box + .pc_btn_box { margin-top: 8rem;}
.bg_pattern.pc_none {display: none;}
.h_tit + .dataType {margin-top: 1.6rem;}
.viewType + .h_tit, .dataType + .h_tit, .list_bull + .h_tit {margin-top: 4rem;}
.blue_box + .checkbox {margin-top: 16px;}
.gray_line_box + .btn_more, .select-box + .btn_more {margin-top: 4rem;}


/* 배너 */
.mydgb_banner {border-top:1px solid rgb(235, 236, 238); margin: 5.6rem 0; clear: both;}
.mydgb_banner::after {display: block; content: ""; clear: both;}
.mydgb_banner li {float: left; position: relative; width: 50%; height: 8rem; line-height: 8rem; border-bottom:1px solid rgb(235, 236, 238); padding-left: 8.4rem;}
.mydgb_banner li::before {display: block; content: ""; position: absolute; right: 0; top: 2.4rem; width: 3.2rem; height: 3.2rem; background:url('../img/ico_arrow03.png') no-repeat right 50%  / 3.2rem auto;}
.mydgb_banner li a {width: 100%; display: inline-block; font-size: 1.8rem; font-weight: 500; color:rgb(34, 36, 38)}
.mydgb_banner li.li01 {background:url('../img/ico_banner1.png') no-repeat 4rem 50%  / 3.2rem auto;}
.mydgb_banner li.li02 {background:url('../img/ico_banner2.png') no-repeat 4rem 50%  / 3.2rem auto;}
.mydgb_banner li.li03 {background:url('../img/ico_banner3.png') no-repeat 4rem 50%  / 3.2rem auto;}
.mydgb_banner li.li04 {background:url('../img/ico_banner4.png') no-repeat 4rem 50%  / 3.2rem auto;}

/*스와이퍼 배너*/
.mySwiper {max-width: 960px; margin-bottom: 5.6rem; padding-bottom: 3.2rem !important;}
.mySwiper .swiper-slide {text-align: left; height: 105px; padding: 2.4rem 4rem 0; border-radius: .4rem; font-size: 18px; margin-left: 0; justify-content: center; align-items: center;}
.mySwiper .swiper-slide p.tit {width:100%; font-size: 1.8rem; color: rgb(34, 36, 38); font-weight: 700; padding-bottom: .6rem;}
.mySwiper .swiper-slide.slide1 {background: rgb(224, 229, 216);}
.mySwiper .swiper-slide.slide2 {background: rgb(216, 221, 229);}
.mySwiper .swiper-slide.slide3 {background: #d8ecec;}

.mySwiper .swiper-pagination {bottom: 0 !important; text-align: center;}
.mySwiper .swiper-pagination span {display: inline; line-height: 17px; margin: 0 3px !important; opacity: 1; background: none;}
.mySwiper .swiper-pagination span {display: inline-block; cursor: pointer; width: .8rem; height: .8rem; border:1px solid rgb(188, 192, 197); overflow: hidden; font-size: 0; border-radius: 8px;}
.mySwiper .swiper-pagination span.swiper-pagination-bullet-active {width: 2.4rem; height: .8rem; background: rgb(46, 46, 46); border:1px solid rgb(46, 46, 46); border-radius: 5px;}

/* mydgb-call*/
.mydgb_call {max-width: 960px; width: 100%; margin: 0 auto; background: rgb(245, 245, 245); border-radius: .4rem; text-align: center; padding: 2.7rem 0;}
.mydgb_call div {display: inline-block; font-size: 1.8rem; color: rgb(34, 36, 38); font-weight: 500;}
.mydgb_call div p {font-size: 1.6rem; color: rgb(112, 116, 120); font-weight: 400; position: absolute; left: -164px; top: 27px;}
.mydgb_call div.em {font-size: 3rem; font-weight: 700; display: inline-block; vertical-align: top; padding: .5rem 0 0 2.4rem; position: relative;}
.mySwiper-wrap + .mydgb_call { margin-bottom: 100px;}
.sub_contaier + .mySwiper-wrap { margin-top: -100px;}

/* 신청중인 상품 */
.grid_box { display: flex; flex-wrap: wrap; justify-content: space-between; margin-bottom: 3.2rem;}
.grid_box .item_box {width: calc(50% - 20px); padding: 3.2rem; margin-bottom: 2.4rem; border:1px solid rgb(211, 213, 216); border-radius: .4rem;}
.grid_box .item_box .tl {position: relative; width: 100%; padding-bottom: 2.5rem;}
.grid_box .item_box .tl span { position: relative; font-size: 2rem; font-weight: 700; padding-right: 2rem;}
.grid_box .item_box .tl span a { font-size: 2rem; color: rgb(34, 36, 38); display: inline-block; text-decoration: underline; text-underline-position: under;}
.grid_box .item_box .tl span::before {display: block; content: ""; position: absolute; right: 0; top: .4rem; width: 2rem; height: 2rem; background:url('../img/ico_arrow03.png') no-repeat right 50%  / 2rem auto;}
.grid_box .item_box .tl .btn {position: absolute; right: 0; top: 0; margin: 0; font-size: 1.4rem; font-weight: 500; padding: .6rem 1.4rem; border-radius: 2rem; background: rgb(255, 243, 165);}
.grid_box .item_box .tl .btn.black {background: rgb(112, 116, 120); color: #fff;}
.grid_box .item_box .list_1 li > span > a {color: rgb(34, 36, 38); font-style: normal;}
.grid_box .item_box .list_1 li > span > a > span > em {font-size: 2.2rem; padding-right: .2rem; font-style: normal;}
.grid_box .item_box .list_1 li > span > a > span{position: relative;}
.grid_box .item_box .list_1 li > span > a > span::after{content: ""; position: absolute; width: 100%; height: 2px; bottom: -1px; left: 0; background-color:rgb(34, 36, 38);}
.grid_box .item_box .list_1 li > span > a > span > em + em {font-size: 1.6rem; padding-right: 0;}
.grid_box .item_box .list_1 li > span > em {font-size: 2.2rem; padding-right: .2rem;}
.grid_box .item_box .list_1 li > span > em + em {font-size: 1.6rem; padding-right: 0;}
.grid_box .item_box .list_1 li > span.tit > strong {color: rgb(34, 36, 38); padding-right: .3rem; font-weight: 700;}
.grid_box .item_box .btn {display: flex; margin-top: 2.3rem;}
.grid_box .item_box .btn li { width: calc(33.333% - 0.1rem);}
.grid_box .item_box .btn li:last-of-type a {margin-right: 0;}
.grid_box .item_box .btn li a {display: block; font-weight: 500; padding: 1.2rem 0; text-align: center; font-size: 1.6rem; color: rgb(34, 36, 38); border-radius: .4rem; margin-right: 1.2rem; background: rgb(235, 236, 238);}
.grid_box .item_box .btn li a.blue { background: rgb(18, 151, 239); color: #fff;}

