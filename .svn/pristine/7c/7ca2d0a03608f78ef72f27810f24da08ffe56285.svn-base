<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="ko" lang="ko">
<head>
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<%@ page contentType="text/html; charset=euc-kr" language="java" %>

<title>DGB 캐피탈 관리자</title>
<link type="text/css" rel="stylesheet" href="/static/admin/css/admin_new.css">
<link rel="shortcut icon" type="image/x-icon" href="/static/admin/img/common/dgb.ico" />

<!-- SecureFrame Start-->

<!-- SecureFrame End-->

<script type="text/javascript" src="/static/admin/js/dgbjquery-1.11.1.js"></script>
<script type="text/javascript" src="/static/admin/js/jquery.easing.js"></script>
<script type="text/javascript" src="/static/admin/js/css_browser_selector.min.js"></script>
<script type="text/javascript" src="/static/admin/js/commonEncode.js"></script>
<script type="text/javascript" src="/static/admin/js/common.js"></script>

<script type="text/javascript">

	$(document).ready(function(){

	});

	function validate(){

		if( $("input[name='C_STOCK_CD']").val() == "" ){
			alert("증권사코드를 입력하세요.");
			$(this).focus();
			return false;
		}
		
		if( $("input[name='C_STOCK_NM']").val() == "" ){
			alert("상품명(일부)을 입력하세요.");
			$(this).focus();
			return false;
		}

		if( $("input[name='C_RMS_CD']").val() == "" ){
			alert("RMS코드를 입력하세요.");
			$(this).focus();
			return false;
		}
		
		if( $("input[name='COMCODE']").val() == "" ){
			alert("연계업무코드를 입력하세요.");
			$(this).focus();
			return false;
		}

		if( $("input[name='C_PRDCT_CD']").val() == "" ){
			alert("상품코드를 입력하세요.");
			$(this).focus();
			return false;
		}

		if( $("input[name='C_STOCK_CNM']").val() == "" ){
			alert("상품명(전체)을 입력하세요.");
			$(this).focus();
			return false;
		}

		if( $("input[name='STOCK_NM']").val() == "" ){
			alert("증권사명을 입력하세요.");
			$(this).focus();
			return false;
		}

		if( $("input[name='C_NM']").val() == "" ){
			alert("기관명을 입력하세요.");
			$(this).focus();
			return false;
		}

		return true;	
	}

	function goUpdate(){
		if(validate()){

			var frm  = document.frm;
			frm.METHOD.value = "AdminPCodeUpd";
			frm.action = "/admin/AdminCodeAction.do";
			EncForm(frm);
			frm.submit();
		}
	}

	// 목록
	function goSearch(){

		var f = document.frm;
		f.METHOD.value = "AdminPCodeList";
		f.action = "/admin/AdminCodeAction.do";
		EncForm(f);
		f.submit();
	}

</script>
</head>




<body>
	<div class="wrapper">
		<div class="header">
			<jsp:include page="/admin/inc/header_new.jsp" flush="false"> 
				<jsp:param name="menuNo" value="2"/> 
			</jsp:include>
		</div> <!--// header -->

		<div class="container">
			<div class="local-menu">
				<jsp:include page="/admin/inc/leftMenu.jsp" flush="false"> 
					<jsp:param name="menuNo" value="2"/>
					<jsp:param name="subMenuNo" value="3"/>
				</jsp:include>
			</div> <!--// leftarea -->


		<form name="frm" method="post">
			<input type="hidden" name="METHOD" />
			<input type="hidden" name="INIpluginData" />
			<input type="hidden" name="LOGIN_SVC" value="N"/>
			<input type="hidden" name="menuNo" value="2" />
			<input type="hidden" name="subMenuNo" value="3" />
			<input type="hidden" name="STOCKNUM" value="" />

			<!-- 조회 조건 -->
			<input type="hidden" name="CODEMENU_SELECT"	value="" />
			<input type="hidden" name="CODE_SELECT"	value=""/>
			<input type="hidden" name="PGE_NO"	value="" />

			<div class="content-wrap">
				<h3>코드 관리(스탁론)</h3>
				<div class="contents">
					<h4>코드 정보 변경</h4>
					<p class="align-right"><em>* 필수항목입니다.</em></p>
					<br>
					<fieldset class="form-type bbs-form">
						<div class="dl-type tb">
							<dl>
								<dt>
									<label for="C_STOCK_CD">증권사코드 <em>*</em></label>
								</dt>
								<dd><div><input type="text" id="C_STOCK_CD" name="C_STOCK_CD" value="289" class="wd2" maxlength="3"></div></dd>
							</dl>
							<dl>
								<dt>
									<label for="C_STOCK_NM">상품명(일부) <em>*</em></label>
								</dt>
								<dd><div><input type="text" id="C_STOCK_NM" name="C_STOCK_NM" value="NH점프론(S&C-고정금리)" class="wd2"></div></dd>
							</dl>
							<dl>
								<dt>
									<label for="C_RMS_CD">RMS코드 <em>*</em></label>
								</dt>
								<dd><div><input type="text" id="C_RMS_CD" name="C_RMS_CD" value="C" class="wd2" maxlength="1"></div></dd>
							</dl>
							<dl>
								<dt>
									<label for="COMCODE">연계업무코드 <em>*</em></label>
								</dt>
								<dd><div><input type="text" id="COMCODE" name="COMCODE" value="289C" class="wd2" maxlength="4"></div></dd>
							</dl>
							<dl>
								<dt>
									<label for="C_PRDCT_CD">상품코드 <em>*</em></label>
								</dt>
								<dd><div><input type="text" id="C_PRDCT_CD" name="C_PRDCT_CD" value="04" class="wd2" maxlength="2"></div></dd>
							</dl>
							<dl>
								<dt>
									<label for="C_STOCK_CNM">상품명(전체) <em>*</em></label>
								</dt>
								<dd><div><input type="text" id="C_STOCK_CNM" name="C_STOCK_CNM" value="NH점프론(S&C-고정금리)-대환대출" class="wd2"></div></dd>
							</dl>
							<dl>
								<dt>
									<label for="STOCK_NM">증권사명 <em>*</em></label>
								</dt>
								<dd><div><input type="text" id="STOCK_NM" name="STOCK_NM" value="NH투자증권" class="wd2" maxlength="7"></div></dd>
							</dl>
							<dl>
								<dt>
									<label for="C_NM">기관명 <em>*</em></label>
								</dt>
								<dd><div><input type="text" id="C_NM" name="C_NM" value="S&C" class="wd2" maxlength="4"></div></dd>
							</dl>
							<dl>
								<dt>
									<label>구분코드</label>
								</dt>
								<dd><div>046</div></dd>
							</dl>

						</div>
						<br>
						<br>
						<p class="align-center">
							<a href="#none" class="btn h42" onClick="javascript:goUpdate();">수정</a>
							<a href="#none" class="btn h42 neg" onClick="javascript:goSearch();">목록</a>
						</p>
					</fieldset>
				</div><!--// contents -->
			</div><!--// content-wrap -->
			</form>

		</div><!--// container -->

		<div class="footer">
			<jsp:include page="/admin/inc/footer.jsp" flush="false"></jsp:include>
		</div><!--// footer -->
	</div><!--// wrapper -->

<script language="javascript" src="/static/admin/js/nProtect.js"></script>

</body>
</html>