/* ui-datepicker custom */
.ui-widget.ui-widget-content {z-index:20001 !important; border:1px solid #c5c5c5; border-radius: 4px; box-shadow: 0 -5px 10px rgb(0 0 0 / 42%);}
.ui-datepicker-week-end, .ui-datepicker-week-end a{color:#dc3900;}
.ui-datepicker .ui-datepicker-header {padding: 4px 0;background: #222426;border-radius: 4px 4px 0 0;margin: -4px -4px 0 -2px;border:none;}
.ui-datepicker .ui-datepicker-prev, .ui-datepicker .ui-datepicker-next {top: 5px;}
.ui-widget-header .ui-icon {background-image: url("../img/2019/icon_calender_arrow01.png"); background-size: 15px 27px;}
.ui-datepicker-trigger {position:absolute;right:12px;top: calc(50% - 13px);cursor:pointer;text-indent:-5000px;border:none;width: 25px;height: 27px;background: url("../../../img/dealer/2022/ic-calendar.svg") no-repeat 0 0 / 100% auto;}
.ui-datepicker .ui-datepicker-prev, .ui-datepicker .ui-datepicker-next {position: absolute;top:6px;width: 1.8em;height:1.8em;}
.ui-datepicker .ui-datepicker-prev {left: 7px;}
.ui-datepicker .ui-datepicker-prev span {background-position: 0 0;background-size:13px 36px;width: 20px;height: 22px;background: url(/static/web/img/ico_arrow04_1.png) no-repeat 0 0 / 100% auto;margin-left: -10px;margin-top: -10px;}
.ui-datepicker .ui-datepicker-next {right:4px;}
.ui-datepicker .ui-datepicker-next span {background-position: 0 0;background-size:13px 36px;width: 20px;height: 22px;background: url(/static/web/img/ico_arrow04.png) no-repeat 0 0 / 100% auto;margin-left: -12px;margin-top:  -10px;}
.ui-datepicker .ui-datepicker-next-hover span {}
.ui-datepicker .ui-datepicker-prev-hover {left:7px; top:6px; background-color:#2485f5; border:1px solid #2485f5}
.ui-datepicker .ui-datepicker-next-hover {right:4px; top:6px; background-color:#2485f5; border:1px solid #2485f5}
.ui-datepicker .ui-datepicker-title select {margin: 3px 3px 3px 5px;}
.ui-datepicker select.ui-datepicker-year {width:30% !important; height:23px; border:none; border-radius:4px; margin:3px 5px 3px 0px; background: transparent; color: #fff;}
.ui-datepicker select.ui-datepicker-month {width:15% !important; height:23px; border:none; border-radius:4px;margin:3px 0px 3px 8px; background: transparent; color: #fff;}

.ui-datepicker {width: 17em;}
.ui-datepicker .ui-datepicker-title * {color: #222426;}
.ui-datepicker .ui-datepicker-title select {font-size: 13px;}
.ui-datepicker table {font-size: 0.9em;}
.ui-datepicker td span, .ui-datepicker td a {text-align: center !important;width: 30px;height: 30px;/* line-height: 19px; */background: #f3f3f3 !important;border-radius: 50%;border: none !important;}
.ui-state-highlight, .ui-widget-content .ui-state-highlight, .ui-widget-header .ui-state-highlight {border: 1px solid #f3f3f3!important;background: #f3f3f3 !important;color: #454545 !important;}
.ui-datepicker select.ui-datepicker-month, .ui-datepicker select.ui-datepicker-year {width: 35% !important;}
.ui-datepicker td span, .ui-datepicker td a {text-align: center;}

.ui-icon-circle-triangle-w { background-position: 0px 0px; }
.ui-icon-circle-triangle-e { background-position: 0px 28px; }
html .ui-button.ui-state-disabled:active {border:1px solid #f6f6f6; background:#f6f6f6;}
.ui-datepicker td span, .ui-datepicker td a {
    border: 1px solid #f6f6f6;
    background: #f6f6f6;
    font-weight: normal;
    color: #454545;
    padding: 4px;
    text-align:right;
    line-height: 2em;
    font-size: 1em;
}
.ui-datepicker .ui-datepicker-title {
    margin: 0 23px;
    line-height: 18px;
    text-align: center;
    color: #fff;
}
.ui-state-highlight,
.ui-widget-content .ui-state-highlight,
.ui-widget-header .ui-state-highlight {
    border: 1px solid #828282;
    background: #8d8d8d;
    color: #fff;
    border-radius: 50%;
}
.ui-state-active,
.ui-widget-content .ui-state-active,
.ui-widget-header .ui-state-active,
a.ui-button:active,
.ui-button:active,
.ui-button.ui-state-active:hover {
    border: 1px solid #1297ef !important;
    background: #1297ef !important;
    color: #ffffff;
}
.ui-corner-all,
.ui-corner-top,
.ui-corner-left,
.ui-corner-tl {
    padding: 0 3px 0 3px;
}
.ui-datepicker .ui-state-disabled {opacity:0.35;}

