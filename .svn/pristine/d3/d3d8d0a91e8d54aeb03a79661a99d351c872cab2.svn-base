/* 본문바로가기*/
.skip {
    position: absolute;
    left: 0;
    top: -100px;
    width: 100%;
    height: 50px;
}

.skip:focus {
    top: 0;
}

/* ------------------------------------------------------------------------------ */
/* reset */
* {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
:after,
:before {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
html,
body {
    margin: 0;
    padding: 0;
    width: 100%;
    height: 100%;
    color: rgb(34, 36, 38);
    font-size: 10px;
}

body {
    margin: 0;
    padding: 0;
    background-color: #fff;
    font-family: 'koho', 'NotoKr', sans-serif, "돋움", Dotum;
    font-size: 16px;
    color: #555;
    letter-spacing: -.5px;
    -webkit-text-size-adjust: none;
    -ms-text-size-adjust: none;
    word-break: keep-all;
}

html,
body,
div,
p,
span,
a,
em,
i,
u,
h1,
h2,
h3,
h4,
ol,
ul,
li,
dl,
dt,
dd {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

ol,
ul,
li {
    list-style: none;
}

em,
i {
    font-style: normal;
}

a {
    text-decoration: none;
    cursor: pointer;
}
a:active,
a:hover {
  outline: 0;
}
a:focus {outline: 1px dotted #333;}

button {
    border: none;
    cursor: pointer;
    background-color: transparent;
}

input[type="text"],
input[type="password"],
select {
    font-family: inherit;
    box-sizing: border-box;
}

input::placeholder {
    font-size: 16px;
    color: #999;
}

input:hover,
input:focus,
input:active {
    outline: none;
    background-color: transparent !important;
}
input:disabled {
     background-color: transparent !important;
}

select {
    -moz-appearance: none;
    -webkit-appearance: none;
    appearance: none;
    border: none;
}

select::-ms-expand {
    display: none;
    display: hidden;
}

select:hover,
select:focus,
select:active {
    outline: none !important;
}
/* checkbox, radio */
input[type="checkbox"] {width:1px; height:1px; margin-left: -1px; appearance:none;-webkit-appearance:none;-moz-appearance:none}
input[type="radio"]{width:1px; height:1px; margin-left: -1px; appearance:none;-webkit-appearance:none;-moz-appearance:none}

.label-ui {
    position: relative;
    display: inline-block;
    cursor: pointer;
}

input[type=checkbox] + .label-ui .ico {
    position: absolute;
    top: 3px;
    left: 0;
    width: 24px;
    height: 24px;
    display: inline-block;
    background: url('../../../img/dealer/2022/check.svg') center center no-repeat;
}

input[type=checkbox]:checked + .label-ui .ico {
    background: url('../../../img/dealer/2022/check-on.svg') center center no-repeat;
}

.label-ui .txt {
    display: inline-block;
    font-weight: 500;
    color: #222;
    font-size: 16px;
    line-height: 1.8;
}

.label-ui .ico + .txt {
    padding-left: 32px;
}

.label-ui input[type=radio] ~ .txt {
    padding-left: 25px;
}


table {
    width: 100%;
    border-spacing: 0 !important;
    border-collapse: separate;
}

table tr,
table th,
table td {
    font-weight: 400;
    box-sizing: border-box;
}

caption{
    position:relative;
    width:0;
    height:0;
    overflow:hidden;
    line-height:0;
    text-indent:-5000px;
}

/* Hide HTML5 number type Up and Down arrows. */
input[type="number"]::-webkit-outer-spin-button,
input[type="number"]::-webkit-inner-spin-button {
  -webkit-appearance: none;
  margin: 0;
}

/*2016-09-30 접근성 관련 수정*/
input[type="radio"]:focus + label {
  outline: 1px dotted #333;
}
input[type="checkbox"]:focus + label {
  outline: 1px dotted #333;
}

h1:focus,
span:focus,
div:focus{
    outline:0;
}
.rt{
    text-align: right;
}

/* ------------------------------------------------------------------------------ */
/* 공통 */
.blind {
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 0;
    height: 0;
    clip: rect(0, 0, 0, 0);
}

