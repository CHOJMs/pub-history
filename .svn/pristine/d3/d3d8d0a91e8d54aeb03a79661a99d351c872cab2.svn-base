/* scroll  */
.scroll-css {
    scrollbar-width: 8px;
    scrollbar-color: #bbb transparent;
    scrollbar-face-color: #bbb;
    scrollbar-track-color: #f9f9f9;
    scrollbar-arrow-color: #bbb;
    scrollbar-highlight-color: #bbb;
    scrollbar-3dlight-color: none;
    scrollbar-shadow-color: #bbb;
    scrollbar-darkshadow-color: none;
    overflow-x: hidden;
    overflow-y: auto;
}

.scroll-css::-webkit-scrollbar {
    width: 8px;
    height: 8px;
}

.scroll-css::-webkit-scrollbar-track {
    background-color: transparent;
}

.scroll-css::-webkit-scrollbar-thumb {
    border-radius: 4px;
    background-color: #bbb;
}

.scroll-css::-webkit-scrollbar-button {
    width: 0;
    height: 0;
}

.scroll-css::-webkit-scrollbar-corner {
    background-color: transparent;
}

/* 버튼 */
.btn-cont {
    position: relative;
    height: 44px;
    padding-left: 44px;
    padding-right: 46px;
    margin-left: 5px;
    border: 1px solid #ddd;
    border-radius: 4px;
    background-color: #fff;
    font-weight: 500;
    font-size: 14px;
    color: #222;
    letter-spacing: -.8px;
}

.btn-excel:before {
    content: '';
    position: absolute;
    left: 12px;
    top: 50%;
    width: 28px;
    height: 28px;
    background: url(../../../img/dealer/2022/ic-excel.svg) no-repeat center 4px;
    transform: translateY(-50%)
}

.btn-history:before {
    content: '';
    position: absolute;
    left: 12px;
    top: 50%;
    width: 28px;
    height: 28px;
    background: url(../../../img/dealer/2022/ic-history.svg) no-repeat center 4px;
    transform: translateY(-50%)
}

.btn-cont i {
    position: absolute;
    right: 15px;
    top: 50%;
    width: 16px;
    height: 16px;
    background: gold;
    transform: translateY(-50%)
}

.btn-cont .download {
    background: url(../../../img/dealer/2022/ic-download.svg) no-repeat center center;
}

.btn-cont .arrow {
    background: url(../../../img/dealer/2022/ic-arrow.svg) no-repeat center center;
}

/* ------------------------------------------------------------------------------ */
/* layout */
.wrap {
    position: relative;
    width: 100%;
    min-width: 1600px;
    height: auto;
    min-height: 100%;
    display: flex;
    overflow: hidden;
    justify-content: space-between;
    background-image: linear-gradient(to bottom, #ecf2fa, #f6fafd 48%, #fff);
}

.snb {
    position: fixed;
    left: 0;
    top: 0;
    width: 160px;
    height: 100vh;
    border-radius: 0 24px 24px 0;
    box-shadow: 0 8px 16px 0 rgba(4, 34, 69, 0.16);
    background-color: #fff;
    flex-shrink: 0;
    z-index: 1;
}

.contents {
    padding: 0 80px 120px;
    margin-left: 160px;
    flex: 1 1 auto;
    transition: all .2s ease-out;
    max-width: 1747px;
}

@media screen and (min-width: 1921px) {
    .contents {
        max-width: 1747px;
        padding: 0 0 120px 160px;
        margin: 0 auto;
    }
}


/* ------------------------------------------------------------------------------ */
/* lnb */
.snb {
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-direction: column;
}

.snb h1 {
    width: 103px;
    height: 88px;
    margin-top: 40px;
}

.snb h1 a {
    display: block;
    width: 100%;
    height: 100%;
    background: url(../../../img/dealer/2022/h1-logo.png) no-repeat center center;
    text-indent: 1000%;
    white-space: nowrap;
    overflow: hidden;
    pointer-events: none;
    background-size:120px auto;
}

.footer {
    margin-bottom: 40px;
    font-size: 14px;
}

.nav li {
    margin: 40px 0;
    text-align: center;
}

.nav li a {
    display: inline-block;
    width: 100%;
    font-weight: 700;
    font-size: 16px;
    color: #222;
    opacity: 0.3;
}

.nav li a i {
    display: block;
    width: 48px;
    height: 48px;
    margin: 0 auto 4px;
    opacity: 0.3;
}

.nav li a.on,
.nav li a.on i {
    opacity: 1;
    pointer-events: none;
}

.ic-nav01 {
    background: url(../../../img/dealer/2022/nav01.svg) no-repeat center center;
}

.ic-nav02 {
    background: url(../../../img/dealer/2022/nav02.svg) no-repeat center center;
}

.ic-nav03 {
    background: url(../../../img/dealer/2022/nav03.svg) no-repeat center center;
}

/* ------------------------------------------------------------------------------ */
/* contents */
.cont-head {
    position: relative;
    height: 248px;
    padding-top: 80px;
    background: url(../../../img/dealer/2022/img-cont-head.svg) no-repeat right bottom;
}

.head-tp {
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.cont-head h2 {
    font-weight: 700;
    font-size: 40px;
    color: #222;
}

.cont-summ {
    margin-bottom: 48px;
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
}

.summ-box {
    position: relative;
    height: 224px;
    margin-left: 24px;
    border-radius: 8px;
    box-shadow: 0 2px 16px 0 rgba(4, 34, 69, 0.08);
    background-color: #fff;
}

.summ-box.type1 {
    margin-left: 0;
    background-color: #2485f5;
}

.summ-box.type1,
.summ-box.type3 {
    width: 26%;
    padding: 36px 32px;
}

.summ-box.type2 {
    width: 48%;
    padding: 36px 32px;
}

.summ-box.type1 dl dt {
    display: block;
    margin-bottom: 6px;
    color: #fff;
    opacity: 0.7;
}

.summ-box.type1 dl dd {
    display: block;
    font-size: 20px;
    color: #fff;
    line-height: 1.5;
}

.summ-box.type1 dl dd span {
    font-weight: 700;
}
.summ-box.type2 {
    display: flex;
    flex-flow: row wrap;
}

.summ-box.type2 dl {
    position: relative;
    width: 33.3333%;
}

.summ-box.type2 dl:after {
    content: '';
    position: absolute;
    right: 23px;
    top: 0;
    width: 1px;
    height: 52px;
    background-color: #eee;
}

.summ-box.type2 dl:nth-of-type(3n):after,
.summ-box.type2 dl:last-of-type:after {
    content: none;
}

.summ-box.type2 dl:nth-of-type(4),
.summ-box.type2 dl:nth-of-type(5) {
    margin-top: auto;
}

.summ-box.type2 dt {
    margin-bottom: 7px;
    color: #666;
}

.summ-box.type2 dd {
    font-weight: 500;
    font-size: 20px;
    color: #222;
}

.summ-box.type2 dd span {
    font-weight: 700;
    font-size: 22px;
}

.summ-box.type2 dd.total {
    color: #2485f5;
}

.d-day {
    display: inline-block;
    height: 32px;
    padding: 0 10px;
    margin: 4px 0 -4px 6px;
    border: 1px solid #2485f5;
    border-radius: 16px;
    background-color: #fff;
    font-weight: 700;
    font-size: 14px;
    color: #2485f5;
    line-height: 30px;
    vertical-align: bottom;
}

.summ-box.type3 {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.summ-title {
    line-height: 1.6;
    font-size: 20px;
    color: #222;
}

.summ-title span {
    font-weight: 700;
    color: #2485f5;
}

.summ-title span span{
    font-weight: 500;
    color: #2485f5;
}

.chart-wrap {
    position: relative;
}

.bar-bg {
    position: relative;
    height: 12px;
    border-radius: 6px;
    background-color: #eee;
    overflow: hidden;
}

.bar-bg .bar {
    position: absolute;
    height: 12px;
    border-radius: 6px;
    background-image: linear-gradient(to left, #2485f5, #deeeff);
}

.chart-axis-x {
    position: relative;
    margin-top: 6px;
    display: flex;
    justify-content: space-between;
}

.chart-axis-x .from,
.chart-axis-x .to {
    font-size: 14px;
    color: #999;
}

.chart-axis-x .current {
    position: absolute;
    margin-left: -48px;
    font-weight: 500;
    font-size: 14px;
    color: #222;
}

.chart-tooltip {
    position: absolute;
    top: -40px;
    padding: 5px 8px;
    border: 1px solid #2485f5;
    border-radius: 4px;
    box-shadow: 0 16px 16px 0 rgba(10, 34, 62, 0.16);
    background-color: #fff;
    font-weight: 700;
    font-size: 14px;
    color: #2485f5;
    transform: translateX(-50%);
    z-index: 1;
}

.chart-tooltip i {
    position: absolute;
    left: 50%;
    bottom: -6px;
    width: 10px;
    height: 6px;
    transform: translateX(-50%);
    overflow: hidden;
}

.chart-tooltip i:before {
    content: '';
    position: absolute;
    left: 0;
    top: -6px;
    width: 8px;
    height: 8px;
    border: 1px solid #2485f5;
    background-color: #fff;
    transform: rotate(45deg);
}

.row {
    margin-top: 24px;
}

.btn-logout {
    position: absolute;
    right: 32px;
    bottom: 32px;
    width: 32px;
    height: 32px;
    background: url(../../../img/dealer/2022/ic-logout.svg) no-repeat center center;
}

