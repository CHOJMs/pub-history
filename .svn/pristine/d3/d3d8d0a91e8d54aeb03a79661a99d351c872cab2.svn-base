    height: 56px;
    border-radius: 4px;
    background-color: #222;
    font-weight: 700;
    font-size: 18px;
    color: #fff;
    text-align: center;
}

label {
    position: relative;
    font-weight: 500;
    font-size: 16px;
    color: #222;
}

label.astr:before {
    content: '*';
    position: absolute;
    right: -8px;
    top: 0;
    font-weight: 500;
    font-size: 16px;
    color: #f42c2c;

}

.cont-util {
    margin-bottom: 16px;
    display: flex;
    justify-content: space-between;
    align-items: flex-end;
}

.cont-table {
    border-radius: 8px;
    box-shadow: 0 2px 2px 0 rgba(0, 0, 0, 0.08);
    background-color: #fff;
    overflow-x: auto;
    overflow-y: hidden;
}

.tbl-list th {
    height: 64px;
    padding: 23px 20px;
    background-color: #f5f5f5;
    font-weight: 500;
    font-size: 16px;
}

.tbl-list td {
    height: 52px;
    padding: 9px 20px;
    border: 1px solid transparent;
    word-break: break-all;
    text-align: center;
}

.tbl-list tr:last-of-type td:first-of-type {
    border-radius: 0 0 0 8px;
}

.tbl-list tr:last-of-type td:last-of-type {
    border-radius: 0 0 8px 0;
}

.tbl-list tr {
    transition: all .16s ease-out;
}

.tbl-list tr:hover {
    background-color: #f5f9ff;
    box-shadow: inset 0px 0px 0 1px #2485f5;
}

.tbl-list tr.no-data {
    transition: all .0s ease-out;
}

.tbl-list tr.no-data td {
    padding: 76px;
    color: #999;
}

.tbl-list tr.no-data td:hover {
    background-color: #fff;
    box-shadow: none;
}

.paging {
    margin: 24px 0 0;
}

.paging ul {
    display: flex;
    justify-content: center;
    align-items: center;
}

.paging ul li {
    margin: 0 2px;
}

.paging ul li button {
    width: 32px;
    height: 32px;
    font-weight: 700;
    color: #999;
}

.paging ul li button.on {
    border-radius: 50%;
    background-color: #2485f5;
