/* 세로스크롤_table */
.fixed-table {width:100%; height:240px; position:relative; /*padding-top:40px;*/ border-bottom:1px solid #e8e8e8;}
.fixed-table .header-bg {background:#ebf0f6; height:40px; position:absolute; top:-40px; right:0; left:0; border-top:1px solid #d1d9e0; border-bottom:1px solid #d1d9e0;}
