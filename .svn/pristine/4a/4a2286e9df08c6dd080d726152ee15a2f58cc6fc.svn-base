@charset "UTF-8";

h2.txt {font-size:22px; line-height:26px; font-weight:bold; position:relative; color:#000; padding:0 0px 15px;}
h2 .open_img, .open_img {width:18px; height:18px; margin-left:5px;}
h2 span {font-size:10px; color:#f75248; position:absolute; right:16px; top:37px;}
h2 span.red_txt {font-size:17px; color:#f75248; position:initial; right:inherit; top:72px;}
h2 a:focus {outline:1px dotted #333;}

button {border: none; background-color: transparent; font-family: "pretendard", "KOHO", "Noto Sans KR";}
select {-webkit-appearance:none; -moz-appearance:none; appearance:none; }
select::-ms-expand {display:none; }

/* 정렬 */
.a_l { text-align: left;}
.a_r { text-align: right;}

em {position: relative; color:#f75248; font-size:14px; padding-left:5px; padding-right:5px;}
em span{opacity: 0; width: 10px; height: 10px; position: absolute; top: 5px; left: 5px;}


/* a 버튼 */
.btn button.disabled,
.btn a.disabled {color: #fff !important; background: #bcc0c5 !important; border: 1px solid #bcc0c5 !important; cursor: default;}/* 버튼 비활성화 */
.btn_box {width:100%; display:table; padding:0; text-align:center;}
.btn_box .btn {flex: 1; display: table-cell; width: 1%; text-align: center;}
.btn_box .btn a {background: #00a88b; display: inline-block; border: 1px solid #00a88b; padding:1.5rem 0; text-align:center; font-size:1.6rem; color:#fff; font-weight: 500; width:100%; cursor:pointer; border-radius:.8rem; letter-spacing: -0.9px;}
.btn_txt a {position: relative; display: block; color: rgb(34, 36, 38); font-size: 1.8rem; line-height: 3rem; font-weight: 700; padding-right: 1.6rem;}
.btn_txt a:after { content: ''; width: 1.6rem; height: 1.6rem; display: inline-block; position: absolute; top: 0.8rem; background:url('../img/ico_arrow02.png') no-repeat right 0 / 1.6rem 1.6rem;}
.btn_txt.a_r {display: none;}
.btn_box .btn_gray,
.btn_box .btn_black,
.btn_box .gray,
.btn_box .black {display: table-cell; flex: 1; width: 1%;}
.btn_box .btn_gray a, .btn_box .gray a {width: 100%; background: rgb(235, 236, 238); color:rgb(34, 36, 38); border: none; font-size: 1.6rem; padding:1.6rem 1rem; font-weight:500;}
.btn_box .btn_black a, .btn_box .black a {width: 100%; background: rgb(34, 36, 38); color:#fff; font-size: 2rem; padding:1.8rem 1rem; font-weight:500; border: none;}
.btn_box .gray_1 a,.btn_box .gray_1 button {background:#707070; border:1px solid #707070;}

.btn_cont button {padding:1.4rem 0; width:12rem; cursor:pointer; border-radius:.4rem; letter-spacing: -0.08rem; background:#ebecee; border:none; color:rgb(34, 36, 38); font-size:1.6rem; line-height: 2rem; font-weight: 500;}

.text_data {display:flex; clear:both; position: relative; margin-top: .9rem;}
.text_data .form {-webkit-flex:1; -moz-flex:1; flex:1; margin-left: 2.4rem; position: relative;}
.text_data .form:first-child {margin-left: 0px;}
.text_data .form+.form {margin-left: 2.4rem;}
.text_data .dash {padding-top:.8rem; margin-left:1.2rem; margin-right: -1.2rem; padding-left:0; font-size:1.4rem; color: rgb(34, 36, 38); display:inline-block;}
.text_data .btn_cont { margin-left: 1.2rem; min-width: 100px;}
.text_data .form.wid30, .box_form .text_data .form.wid30 {flex-basis: 30%;}
.text_data .time {color: rgb(18, 151, 239); position: absolute; right: 1rem; top: calc(50% - 13px); font-family: "pretendard", "KOHO"; font-weight: 700;}
.text_data .txt {align-items: center; padding-left: 1.2rem; display: flex; font-size: 1.6rem; padding-bottom: 0;}
.text_data .a_r {text-align: right; letter-spacing: 0; padding-right:.5rem;}
.text_data .form .txt {align-items: center; padding:0; display: flex; font-size: 1.8rem; position: absolute; right: 10px; top:4px; height: 3.2rem;}
.text_data .form.app_text input {padding-right: 35px; text-align: right;}
.text_data .form.app_text input::placeholder {padding-right: 1px;}

/* 셀렉박스 */
.selectbox {position: relative; flex: 1; margin-left: 2.4rem;}
.select{width:100%; height: 40px; padding:0 10px; font-size:14px; color: #222426; line-height:38px; border: 1px solid #bcc3ce; border-radius: 4px; background: url('../../img/ico_select.png') no-repeat center right 0; background-position: top 50% right 10px; background-size:auto 2rem; box-sizing: border-box;}
.select:disabled, .select.disabled {background: url('../../img/ico_select_dis.png') no-repeat center right 0; background-color: #f2f2f266; background-size:auto 2rem; background-position: top 50% right 10px;}
.select:hover{}
.select:focus{outline:none;}
.selectbox:first-child, .select:first-child, .select.disabled:first-child {margin-left: 0px !important;}
.selectbox + .selectbox,
.selectbox + .text_data {margin-top: 6px !important; margin-left: 0;}

/* 체크박스 */
.checkbox {margin-bottom: 1.6rem;}
.chk:checked+.label{color:#222426;}
.chk:checked+.label:after{margin-top:0; background: url('../../img/ico_check_on.png') no-repeat center center #1297ef; border:.1rem solid #1297ef; border-radius: 50%; background-size:1.2rem auto;}
.chk+.label.font22 {font-size: 1.8rem; font-weight: 700 !important;}


/* 실명확인 */
.box_form.box_none {border-bottom:none; padding-bottom:0;}
.box_form:after {clear:both; content:""; display:block;}
.box_form dt {font-size:14px; color:#222426; margin-bottom:7px; font-weight:normal;}
.box_form dd {margin-bottom:17px;}
.box_form dd:last-child {margin-bottom:0;}
.box_form dt .red_text {color:#f75248; padding-left:5px;}
.box_form dt em {position: relative; color:#f75248; font-size:14px; padding-left:5px;}
.box_form dt em span{opacity: 0; width: 10px; height: 10px; position: absolute; top: 5px; left: 5px;}
.box_form dt.txt_left {display:inline-block; width:25%; margin-bottom:25px;}
.box_form dd.txt_right {float:right;  text-align:right; width:70%; font-size:14px; color:#333;}
.red_text1 {color:#f75248 !important;}
.box_form p.box_txt_content {font-size:12px; margin-top:5px;}
p.cn_txt {font-size:14px;}

.form {background-color:#fff; /*border:1px solid #666;*/ box-sizing:border-box; display:block; overflow:hidden; position:relative;}
input[type="text"], input[type="password"], input[type="number"], input[type="tel"], input[type="email"], input[type="date"] {
    box-shadow: none;
    border: 0;
    box-sizing:border-box;
    color:#222426;
    font-size:14px;
    height:40px;
    line-height:20px;
    overflow:hidden;
    padding:0 10px;
    vertical-align:top;
    width:100%;
    word-break:break-all;
    border:1px solid #bcc3ce;
    border-radius:4px;
    appearance: none;
    -webkit-appearance: none;
    font-family: "pretendard", "KOHO", "Noto Sans KR", "SemiBold", "droid sans fallback", "AppleSDGothicNeo", sans-serif, "돋움", Dotum;
}
.input_txt_right {font-size:14px; color:#000; text-align:right; padding-right:10px !important;}
.input_bg {background:#f2f2f2;}
input:-webkit-autofill {-webkit-box-shadow: 0 0 0 1000px #fff inset;}

/* 체크박스 */
.checkbox {margin-bottom: 1.6rem;}
.checkbox:last-child {margin-bottom: 0rem;}
