.chk+.label:after{position:absolute; top:0; left:0; width:2.4rem; height:2.4rem; margin-top:0rem; transition: .3s; background:url('../../img/ico_check_off.png') no-repeat center center #fff; border:.1rem solid #d3d3d3; border-radius: 50%; background-size:1.2rem auto; content: ''}
.chk:checked+.label{color:#222426;}
.chk:checked+.label:after{margin-top:0; background: url('../../img/ico_check_on.png') no-repeat center center #1297ef; border:.1rem solid #1297ef; border-radius: 50%; background-size:1.2rem auto;}
.popup-contents input[type="text"], .popup-contents input[type="password"] { padding-left: 10px !important; color: #222426 !important; border:1px solid #bcc3ce !important; border-radius: 4px !important; height: 40px !important;}

