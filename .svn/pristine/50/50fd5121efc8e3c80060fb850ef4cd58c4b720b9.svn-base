		if($('#PGE_NO').val() == "")
			$('#PGE_NO').val("iPageNo");
		if($('#menuNo').val() == "")
			$('#menuNo').val("3");
		if($('#subMenuNo').val() == "")
			$('#subMenuNo').val("SUB_MENU");
		if($('#INSERT_KEY').val() == "")
			$('#INSERT_KEY').val("0");
	});

//window.alert($.fn.jquery);
	// 조회
	function goSearch(pageNum){

		var f = document.frm;
		f.METHOD.value = "AdminBoardListSelect";
		f.PGE_NO.value = pageNum;
		f.action = "/admin/AdminBoardAction.do";
		EncForm(f);
		f.submit();
	}


	//파일다운로드
	function download(fileName){
		var path = "upload";

		window.location.href = "/view/common/inc/FILE_DOWNLOAD.jsp?file_name="+fileName+"&file_path="+path;
	}


	//상세보기
