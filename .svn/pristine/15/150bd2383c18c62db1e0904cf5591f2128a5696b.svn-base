<script type="text/javascript">

	$(document).ready(function(){

	});


	function goInsert(){
		if(validate()){

			var frm  = document.frm;

			frm.METHOD.value = "AdminPCodeIns";
			frm.action = "/admin/AdminCodeAction.do";
			EncForm(frm);
			frm.submit();
		}
	}


	function validate(){

		if( $("input[name='C_STOCK_CD1']").val() == "" ){
			alert("증권사코드를 입력하세요.");
			$(this).focus();
			return false;
		}

		if( $("input[name='C_STOCK_NM1']").val() == ""){
			alert("상품명(일부)을 입력하세요.");
			$(this).focus();
			return false;
		}

		if( $("input[name='C_RMS_CD1']").val() == "" ){
			alert("RMS코드를 입력하세요.");
			$(this).focus();
			return false;
		}

		if( $("input[name='COMMCODE1']").val() == "" ){
			alert("업무구분코드를 입력하세요.");
			$(this).focus();
			return false;
		}

		if( $("input[name='STOCKNUM1']").val() == ""){
			alert("구분코드를 입력하세요.");
			$(this).focus();
			return false;
		}

		if( $("input[name='C_PRDCT_CD1']").val() == ""){
			alert("상품코드를 입력하세요.");
			$(this).focus();
			return false;
		}

		if( $("input[name='C_STOCK_CNM1']").val() == ""){
			alert("상품명(전체)을 입력하세요.");
			$(this).focus();
			return false;
		}

		if( $("input[name='STOCK_NM1']").val() == ""){
			alert("증권사명를 입력하세요.");
			$(this).focus();
			return false;
		}

		if( $("input[name='C_NM1']").val() == ""){
			alert("기관명를 입력하세요.");
			$(this).focus();
			return false;
		}

		return true;	
	}

	// 목록
	function goSearch(){

		var f = document.frm;
		f.METHOD.value = "AdminPCodeList";
		f.action = "/admin/AdminCodeAction.do";
		EncForm(f);
		f.submit();
	}

</script>
