<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="ko" lang="ko">
<head>
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<%@ page contentType="text/html; charset=euc-kr" language="java" %>

<title>DGB 캐피탈 관리자</title>
<link type="text/css" rel="stylesheet" href="/static/admin/css/admin_new.css">
<link rel="shortcut icon" type="image/x-icon" href="/static/admin/img/common/dgb.ico" />

<!-- SecureFrame Start-->

<!-- SecureFrame End-->

<script type="text/javascript" src="/static/admin/js/dgbjquery-1.11.1.js"></script>
<script type="text/javascript" src="/static/admin/js/jquery.easing.js"></script>
<script type="text/javascript" src="/static/admin/js/css_browser_selector.min.js"></script>
<script type="text/javascript" src="/static/admin/js/commmd5.js"></script>       <!-- md5 -->
<script type="text/javascript" src="/static/admin/js/commonEncode.js"></script>
<script type="text/javascript" src="/static/admin/js/common.js"></script>

<script type="text/javascript">

	$(document).ready(function(){

	});


	function goInsert(){
		if(validate()){

			var frm  = document.frm;

			frm.METHOD.value = "AdminCCodeIns";
			frm.action = "/admin/AdminCodeAction.do";
			EncForm(frm);
			frm.submit();
		}
	}


	function validate(){

		if( $("input[name='COMMON_CD1']").val() == "" ){
			alert("코드를 입력하세요.");
			return false;
		}

		if( $("input[name='COMMON_NM1']").val() == ""){
			alert("코드명을 입력하세요.");
			return false;
		}

		if( $("input[name='PARENT_CD1']").val() == "" ){
			alert("구분코드를 입력하세요.");
			return false;
		}

		return true;	
	}

	// 목록
	function goSearch(){

		var f = document.frm;
		f.METHOD.value = "AdminCCodeList";
		f.action = "/admin/AdminCodeAction.do";
		EncForm(f);
		f.submit();
	}

</script>
</head>




<body>
	<div class="wrapper">
		<div class="header">
			<jsp:include page="/admin/inc/header_new.jsp" flush="false"> 
				<jsp:param name="menuNo" value="2"/> 
			</jsp:include>
		</div> <!--// header -->

		<div class="container">
			<div class="local-menu">
				<jsp:include page="/admin/inc/leftMenu.jsp" flush="false"> 
					<jsp:param name="menuNo" value="2"/>
					<jsp:param name="subMenuNo" value="2"/>
				</jsp:include>
			</div> <!--// leftarea -->


		<form name="frm" method="post">
			<input type="hidden" name="METHOD" />
			<input type="hidden" name="INIpluginData" />
			<input type="hidden" name="LOGIN_SVC"  value="N"/>
			<input type="hidden" name="menuNo"	value="2" />
			<input type="hidden" name="subMenuNo"	value="2" />

			<!-- 조회 조건 -->
			<input type="hidden" name="CODEMENU_SELECT"	value="" />
			<input type="hidden" name="CODE_SELECT"	value="" />
			<input type="hidden" name="PGE_NO"	value="" />


			<div class="content-wrap">
				<h3>코드 관리(공통)</h3>
				<div class="contents">
					<h4>코드 정보 등록</h4>
					<p class="align-right"><em>* 필수항목입니다.</em></p>
					<br>
					<fieldset class="form-type bbs-form">
						<div class="dl-type tb">
							<dl>
								<dd style="text-align:center;width:600px;font-size:11pt;line-height:10px;">코드<em>*</em></dt>
								<dd style="text-align:center;width:600px;font-size:11pt;line-height:10px;">코드명 <em>*</em></dt>
								<dd style="text-align:center;width:600px;font-size:11pt;line-height:10px;">구분코드 <em>*</em></dt>
							</dl>
							<dl>	
								<dd><div style="margin-left:70px;"><input type="text" id="COMMON_CD1" name="COMMON_CD1" class="wd3" maxlength="8"></div></dd>
								<dd><div style="margin-left:70px;"><input type="text" id="COMMON_NM1" name="COMMON_NM1" class="wd3"></div></dd>
								<dd><div style="margin-left:70px;"><input type="text" id="PARENT_CD1" name="PARENT_CD1" class="wd3" maxlength="8"></div></dd>
							</dl>
							<dl>
								<dd><div style="margin-left:70px;"><input type="text" id="COMMON_CD2" name="COMMON_CD2" class="wd3" maxlength="8"></div></dd>
								<dd><div style="margin-left:70px;"><input type="text" id="COMMON_NM2" name="COMMON_NM2" class="wd3"></div></dd>
								<dd><div style="margin-left:70px;"><input type="text" id="PARENT_CD2" name="PARENT_CD2" class="wd3" maxlength="8"></div></dd>						
							</dl>
							<dl>
								<dd><div style="margin-left:70px;"><input type="text" id="COMMON_CD3" name="COMMON_CD3" class="wd3" maxlength="8"></div></dd>
								<dd><div style="margin-left:70px;"><input type="text" id="COMMON_NM3" name="COMMON_NM3" class="wd3"></div></dd>
								<dd><div style="margin-left:70px;"><input type="text" id="PARENT_CD3" name="PARENT_CD3" class="wd3" maxlength="8"></div></dd>				
							</dl>
							<dl>
								<dd><div style="margin-left:70px;"><input type="text" id="COMMON_CD4" name="COMMON_CD4" class="wd3" maxlength="8"></div></dd>
								<dd><div style="margin-left:70px;"><input type="text" id="COMMON_NM4" name="COMMON_NM4" class="wd3"></div></dd>
								<dd><div style="margin-left:70px;"><input type="text" id="PARENT_CD4" name="PARENT_CD4" class="wd3" maxlength="8"></div></dd>				
							</dl>
							<dl>
								<dd><div style="margin-left:70px;"><input type="text" id="COMMON_CD5" name="COMMON_CD5" class="wd3" maxlength="8"></div></dd>
								<dd><div style="margin-left:70px;"><input type="text" id="COMMON_NM5" name="COMMON_NM5" class="wd3"></div></dd>
								<dd><div style="margin-left:70px;"><input type="text" id="PARENT_CD5" name="PARENT_CD5" class="wd3" maxlength="8"></div></dd>				
							</dl>
						</div>
						<br>
						<br>
						<p class="align-center">
							<a href="#none" class="btn h42" onClick="javascript:goInsert();">등록</a>
							<a href="#none" class="btn h42 neg" onClick="javascript:goSearch();">목록</a>
						</p>
					</fieldset>
				</div><!--// contents -->
			</div><!--// content-wrap -->
			</form>

		</div><!--// container -->

		<div class="footer">
			<jsp:include page="/admin/inc/footer.jsp" flush="false"></jsp:include>
		</div><!--// footer -->
	</div><!--// wrapper -->

<script language="javascript" src="/static/admin/js/nProtect.js"></script>


</body>
</html>