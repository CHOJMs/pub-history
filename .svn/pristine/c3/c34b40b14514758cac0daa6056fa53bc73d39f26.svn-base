	// 조회
	function goSearch(pageNum){

		var f = document.frm;
		f.METHOD.value = "AdminBoardListSelect";
		f.PGE_NO.value = pageNum;
		f.action = "/admin/AdminBoardAction.do";
		EncForm(f);
		f.submit();
	}


