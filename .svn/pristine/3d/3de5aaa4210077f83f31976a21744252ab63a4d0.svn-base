.step-id-box li div {margin-top: -2px; font-size: 16px; font-weight: 500; color: #5c6773; width: calc(100% - 3.2rem); word-break: break-all; word-wrap: break-word;}
.step-id-box li div .qr-box {width: 340px; /* height: 120px; */ text-align: center; padding: 12px 0; margin-top: 12px; border-radius: 16px; border: 1px solid #dfe3e8;}
.step-id-box li div .qr-box img {width: 160px; height: 160px;}
.step-id-box li div .id-box {width: 340px; height: 120px; text-align: center; text-align: center; margin-top: 12px; border-radius: 16px; background:#f4f4f4;}
.step-id-box li div .id-box img {width: 160px; height: 120px;}
.step-id-box li:last-child {margin-bottom: 0;}
.step-id-box .id-pc {display: block;}
.step-id-box .id-mob {display: none;}
.info-step-box{margin-top:30px;background:rgb(242, 244, 246);padding:2rem 2.4rem;border-radius:.8rem;}
.info-step-box ul li{position:relative;margin-top:6px;padding-left:16px;font-size:12px;color:rgb(34, 36, 38);line-height: 1.8rem;}
.info-step-box ul li:first-child{margin-top:0}
.info-step-box ul li span{position:absolute;top:1px;left:0;font-size:11px;padding:0 5px;height:15px;line-height:15px;}

/* 공동인증서 선택 */
.certi-list-wrap{padding:24px 0}
.certi-list-wrap .no-databox{margin-top:200px}
.certi-list > li{padding:20px;background-color:#fff;border:2px solid #fff;border-radius:12px;}
.certi-list > li.on{border-color:#00a88b}
.certi-list > li + li{margin-top:16px}
.certi-btn{margin-top:20px;display:flex;justify-content:space-between;}
.certi-btn a{display:block;width:calc(50% - 4px);font-size:14px;font-weight:500;line-height:40px;border-radius:4px;text-align:center;color:#5c6773}
.certi-btn .cer-gray{background-color:#ebeef2;}
.certi-btn .cer-line{border:1px solid #dfe3e8;background-color:#fff}

/* 공동인증서 가져오기 */
.certi-import{margin-top:16px;}
.certi-import .certi-import-img{height:144px;padding:20px 24px;background:url('../img/ui_common/img_certi_mob.png') no-repeat 50% 50% #f7f7f7;background-size:287px auto;border-radius:12px;}
.certi-import .tx{margin-top:16px;font-size:14px;color:#5c6773;line-height:22px;text-align:center}

/* 세션만료 */
.runout-box{display:flex;justify-content:space-between;align-items:center;margin-top:16px;padding:16px 24px;background:#f7f7f7;border-radius:8px;}
.runout-box p{display:flex;align-items:center;font-size:14px;color:#5c6773;font-weight:500}
.runout-box p:before{display:inline-block;margin-right:4px;width:24px;height:24px;background:url('../img/ui_common/ico_runout.png') no-repeat;background-size:24px auto;content:''}
.runout-box span{font-size:14px;color:#00a88b;font-weight:600}

/* 선택적 동의 안내 팝업 */
.popup.popup-half .popup-contents.flex-col.pad-small.img_char{padding:0 20px 76px 20px}
.img_char .fixed-btn-area{background:none}
.img_pop_char{max-width:320px;margin:24px auto 0;}

/* 상담 진행중 팝업 */
.date-area02{display:flex;align-items:center;justify-content:space-between;margin-top:16px;padding:16px 24px;background-color:#f7f7f7;border-radius:8px;}
.date-area02 .tit{display:flex;align-items:center;font-size:14px;font-weight:500;color:#5c6773}
.date-area02 .tit:before{display:inline-block;margin-right:8px;width:24px;height:24px;background:url('../img/ui_common/ico_calendar02.png') no-repeat;background-size:24px auto;vertical-align:middle;content:''}
.date-area02 .date{font-size:14px;font-weight:600;color:#00a88b}

/* 보험가입 조건 상세 */
.terms-pop{margin-top:24px}

