/* 지도-경로 */
.box_column {padding-top: 7px;}
.box_column .drag_box {position: relative; border: 1px solid #ebecee; border-radius: 4px; margin-top: 24px; padding: 16px; background: #fff;}
.box_column .drag_box .box {padding: 0 8px; text-align: center; display: inline-block; border-radius: 4px; color: #fff; font-size: 14px; height: 30px; line-height: 30px;}
.box_column .drag_box .blue {background: rgb(18, 114, 239);}
.box_column .drag_box .red {background: rgb(245, 81, 90);}
.box_column .drag_box .dr_icon {position: absolute; right: 16px; top: 18px; width: 24px; cursor: pointer;}
.box_column .drag_box > ul {padding-bottom: 24px; padding-top: 20px;}
.box_column .drag_box > ul li + li {margin-top: 8px;}
.box_column .drag_box > ul li {display: flex; flex-wrap: wrap; align-items: flex-start; font-size: 14px; letter-spacing: -0.06em;}
.box_column .drag_box > ul li span {display: inline-flex; width: 100px; margin-right: 6px; color: rgb(112, 116, 120);}
.box_column .drag_box > ul li p {width: calc(100% - 106px); color: rgb(34, 36, 38);}
.box_column .drag_box .btn_box .btn a {padding: 1rem 0; font-size: 14px;}
.drag_noData {border: 1px solid #ebecee; border-radius: 4px; margin-top: 24px; margin-bottom: 30px; padding: 40px;}
.drag_noData p {text-align: center; font-size: 14px; color: #707478;}

/* MF-000-000 */
.flex-btn {display: flex; justify-content: space-between;}
.flex-btn .btn.btn-blk {margin-top: -4px;}
.flex-btn .btn.btn-blk a {display: inline-flex; align-items: center; justify-content: center; padding: 0 10px; height: 32px; line-height: 1; font-size: 13px; color: #fff; background: rgba(34, 36, 38, 1); border-radius: 4px; letter-spacing: -0.06em;}
.popup-contents .flex-btn .btn.btn-blk {margin-top: -1px;}

/*대출상환 완료*/
.finish_box {width: 100% !important; margin: auto; margin-top: 24px; position: relative; padding: 13.2rem 2rem 2rem 2rem;}
.finish_box .fini_t_box {text-align: center;}
.finish_box .fini_t_box:after {content: ''; width: 9.8rem; height: 9.8rem; position: absolute; top: 0; left: 50%; transform: translate(-50%, 0%); border-radius: 50%; background-color: rgb(18, 151, 239); transition: .3s;}
.finish_box .fini_t_box:before {content: ''; width: 4.5rem; height: 3.1rem; position: absolute; top: 0; left: 50%; transform: translate(-50%, 100%); background:url('../../img/ico_check_finish.png') no-repeat 0 0 / 4.5rem auto; z-index: 1; transition: .3s;}
.finish_box .fini_t_box .tit_info {display: block; font-size: 2.2rem; line-height: 3.5rem; font-weight: 700; padding-bottom: 2.4rem;}
.finish_box .fini_t_box .txt {font-size: 1.4rem; line-height: 2.4rem; font-weight: 400; letter-spacing: -0.05rem; color: rgb(34, 36, 38);}
.finish_box .fini_t_box .txt strong {font-weight: 700;}
.finish_box .fini_t_box .txt .number {font-family: Koho;}

/* 오토금융-한도조회결과 */
.flex_tl {display: flex; justify-content: space-between;}
.flex_tl span {font-size: 12px; color: #707478;}
.flex_tl span a {display: inline-block; margin-bottom: 12px;}
.flex_tl span img {width: 28px;}
.flex_tl .h_tit {font-size: 18px !important;}
.view_box { display: block;}
.view_box .box { border: 1px solid rgb(235, 236, 238); border-radius: 4px; overflow: hidden; padding: 0 16px; margin-bottom: 16px;}
.view_box .box .t_bg {background: #ecf7fe; text-align: center; height: 52px; line-height: 52px; font-size: 16px; font-weight: 700; margin: 0 -16px;}
.view_box .box ul {padding: 11px 0 16px 0;}
.view_box .box ul li p strong {font-weight: 500;}
.view_box .box ul li p strong.font_blue {padding: 0; margin: 0;}
.view_box .box .font_blue {font-size: 14px; text-align: right; padding-bottom: 16px; margin-top: -8px;}
.view_box .box .btn_box .btn { width: 1%; display: table-cell; padding-left: 10px; padding-bottom: 16px;}
.view_box .box .btn_box .btn:first-of-type {padding-left: 0;}
.view_box .box .btn_box button {padding: 1.1rem 0; font-size: 14px;}
.view_box .noData {text-align: center;}

/* 슬라이드 */
.view_box .select-box {border-top: 1px solid rgb(34, 36, 38); margin-bottom: 32px;}
.view_box .select-box > div {border-top: 1px solid rgb(211, 213, 216); padding: 0;}
.view_box .select-box > div ul {padding-bottom: 0;}
.view_box .select-box > div ul li:first-of-type span {font-weight: 500; color: #222426;}
.view_box .select-box > div ul li {padding: 12px 17px; margin-top: 0; border-bottom: 1px solid rgb(211, 213, 216);}
.view_box .select-box > div ul li span {width: 25% ; display: inline-block; text-align: center; margin-right: 0;}
.view_box .select-box > div ul li span:first-of-type {text-align: left;}
.view_box .select-box > div ul li span:last-of-type {text-align: right;}
.view_box .select-box > div ul li:last-of-type {border-bottom: none;}

.list_1 li {display: flex; word-wrap: break-word; margin-bottom: 8px; font-size: 14px; justify-content: space-between; flex-wrap: wrap;}
.list_1 li:last-of-type {margin-bottom: 0;}
.list_1 li span {width: 130px; margin-right: 6px; display: table-cell; color: rgb(112, 116, 120);}
.list_1 li span + span { color: #222426; text-align: right;}
.list_1 li p {text-align: right; width: calc(100% - 136px); color: #222426;}
.list_1 li p.font_red {color: rgb(255, 36, 36);}

/* 인증센터-팝업 */
.popup-contents .white_box { padding: 1.5rem 2rem;}
.white_box .list_1 li { display: flex; padding-bottom: .9rem; word-wrap: break-word; justify-content: space-between; flex-wrap: wrap;}
.white_box .list_1 li > span { font-size: 15px; color: #222426; text-align: right; font-weight: 700;}
.white_box .list_1 li > span.tit { font-size: 14px; text-align: left; font-weight: 500;}
.confi_box .list_1 li > span { width: 60%; font-weight: 700; line-height: 2.7rem; margin-right: 0;}
.confi_box .list_1 li > span.tit { font-weight: 500; width: 40%;}

/* 신청중인 상품 */
.item_box {width: 100%; padding: 15px; margin-bottom: 2.4rem; border:1px solid rgb(211, 213, 216); border-radius: .4rem;}
.item_box .tl {position: relative; width: 100%; padding-bottom: 15px;}
.item_box .tl p {width: 60px; text-align: center; margin: 0; font-size: 15px; font-weight: 700; padding: .6rem 1.4rem; border-radius: 2rem; background: rgb(255, 243, 165);}
.item_box > .list li {display: flex; word-wrap: break-word; margin-bottom: 8px; font-size: 14px;}
.item_box > .list li span.tit {width: 130px; text-align: left; margin-right: 6px; display: table-cell; font-size: 15px; color: rgb(112, 116, 120);}
.item_box > .list li span {text-align: right; width: calc(100% - 136px); color: #222426; font-size: 15px; font-weight: 500;}
.item_box .nodata {text-align: center; font-size: 15px; font-weight: 500; height: 80px; line-height: 80px;}

.pdf_view { background: #f6f6f6; height: 150px;}

/* nodata */
.s_noData_box { text-align: center; font-size: 15px; font-weight: 500; padding: 24px 0 ;  border-top:1px solid #d3d5d8; border-bottom:1px solid #d3d5d8;}

.bond_no_data {font-size:15px !important; text-align:center; padding-top:50px;}
.bond_cont .a li a { padding:0; font-size:14px; border:none; display:initial;}

