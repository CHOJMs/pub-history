    .schfilter-box .nodata {display: flex; align-items: center; justify-content: center; background-color: #f7f7f9; border-radius: .8rem; height: 5rem;}

    /* 줄모양 리스트 */
    .line-box .line-list {border-top: .1rem solid rgb(34, 36, 38);}
    .line-box .line-list li {padding: 1rem 0; border-bottom: .1rem solid #d3d7dd;}
    .line-box .nodata {display: flex; align-items: center; justify-content: center; height: 8rem; border-bottom: .1rem solid #d3d7dd;}

    /* 팝업 사진보기 */
    .popup.view-autoPhoto .inner {padding-top: 2.4rem;}
    .popup.view-autoPhoto h2 {text-align:center;}
    .popup.view-autoPhoto .flex-item-top .item-cnt {padding: 1.5rem 2rem; position:relative; overflow:hidden;}
    .popup.view-autoPhoto .swiper {overflow:inherit; position:static;}
    .popup.view-autoPhoto .swiper-wrapper {z-index:0;}
    .popup.view-autoPhoto .swiper-slide {position:relative;}
    .popup.view-autoPhoto .flex-item-top .item-cnt img {max-width: 100%; max-height: 100%; display:block; width: auto; height: auto;}
    .popup.view-autoPhoto .swiper-button-next::after,
    .popup.view-autoPhoto .swiper-button-prev::after {display: none;}
    .popup.view-autoPhoto .swiper-button-next {background: url('../../img/mobile_office/ico_arrow_right.png') no-repeat center / 40px; right: -0.5rem;}
    .popup.view-autoPhoto .swiper-button-prev  {background: url('../../img/mobile_office/ico_arrow_left.png') no-repeat center / 40px; left: -0.5rem;}
    .popup.view-autoPhoto .swiper-slide .autoPhoto-cover { background: #f7f7f9 url('../../img/mobile_office/noimg_photo.png') no-repeat 50% 50%;
    display: flex; align-items: center; justify-content: center; height: 50rem;}
    .popup.view-autoPhoto .swiper-slide .autoPhoto-cover img {background-color: #f7f7f9; width: auto;}

    /* 서류제출 URL발송 */
    .inp-total-list {display: flex; flex-direction: column; gap: 1.2rem;}
    .inp-total-item label {font-size: 1.6rem !important; font-weight: 500; color: rgb(34, 36, 38) !important; padding: 15px 16px 15px 45px !important;  width: 100%; height: inherit !important;
    border: .1rem solid rgb(223, 227, 232) !important; border-radius: .8rem; background: #fff !important;}
    .inp-total-item label::after{top: 15px !important; left: 15px !important;}

    /* 타이틀 리스트 */
    .titcaption-list {display:flex; flex-direction: column; gap: 1rem; word-break: break-all;}
    .titcaption-item {display: flex;}
    .titcaption-item .titcaption-tit {width: 9rem; font-size: 1.4rem; color: rgb(112, 116, 120); flex-shrink: 0;}
    .titcaption-item .titcaption-tit .essential {color: #f75248; padding-left: 5px; width: auto;}
    .titcaption-item .titcaption-caption {color: #222426;}
    .titcaption-list.widtit40 .titcaption-item .titcaption-tit {width: 4rem;}
    .titcaption-list.widtit120 .titcaption-item .titcaption-tit {width: 12rem;}

    .select-box .titcaption-list li + li {margin: 0;}
    .select-box .titcaption-item {flex-wrap: nowrap;}
    .select-box .titcaption-item .titcaption-tit .essential {width: auto;}

    .vertical-secWrap {display: flex; flex-direction: column; height: 100%; max-height: 100%;}
    .vertical-secWrap .vertical-sec {flex: 1; margin: 0 -2rem -2rem;}
    .vertical-secWrap .vertical-sec .vertical-secinner {padding: 0 2rem 2rem;}
    .vertical-secWrap .vertical-sec.bg-gray {background-color: #f7f7f7 !important;}

    /* 첨부파일 유동적 */
    .attachfile-list {display: flex; flex-direction: column; gap: 0.8rem 0;}
    .attachfile-item {display: flex; align-items: center; font-size: 1.8rem; min-height: 64px; cursor: pointer; border: 0.1rem solid rgb(223, 227, 232); border-radius: 0.8rem;
    background: #fff; color: rgb(34, 36, 38); font-weight: 500;}
    .attachfile-item .attachfile-tit {color: rgb(92, 103, 115); width: 11rem; text-align: left; padding-right: 0.6rem; word-break: keep-all; word-wrap: break-word; font-weight: 400; font-size: 1.3rem; padding: 1.2rem 1.4rem; flex-shrink: 0; line-height: 1.4;}
    .attachfile-item .attachfile-link {width: 100%;}
    .attachfile-item input[type="text"] {padding: 0 1rem !important; white-space: nowrap; text-overflow: ellipsis; overflow: hidden; border: none !important; font-size: 1.6rem; border: none; font-weight: 500; color: #222426 !important}
    .attachfile-item input[type="text"]::placeholder {color: #919191;}
    .popup-contents .attachfile-item input[type="text"] {border: none !important}


/* //END: 2024년 신규추가 AG영업관리 포털 구축 */

}

