.fing_wrap > .sub_contaier > .contents .btn_fixed { position: fixed; bottom: 20px; padding-right: 20px;}
.fing_main_box { padding-top: 2rem;}

/* 기본로그인설정 */
.block_rchk li { margin-bottom: 3.2rem; display: block;}
.block_rchk li .rchk+.rchk-label { font-weight: 500; height: 2.4rem; line-height: 2.4rem; padding-left: 32px;}
.block_rchk li .rchk+.rchk-label:after { height: 2.4rem; width: 24px;}
.block_rchk li .rchk:checked+.rchk-label:before, .block_rchk li .rchk+.rchk-label:before { top: 8px; left: 8px;}

.cloud_step {margin-top:.7rem; display:block;}
.cloud_step li {padding-bottom:3rem; position:relative; display: table;}
.cloud_step li span {position:relative; border-radius:50%; background:rgb(255, 236, 112); font-family: Koho; font-weight: 700; font-size: 1.6rem; text-indent:0px; text-align:center; width:3.6rem; height:3.6rem; line-height: 3.6rem; margin-right:1.6rem; display:inline-block; box-sizing:border-box; z-index:2;}
.cloud_step li div {display: table-cell; margin-top: -5px; font-size: 1.7rem;}
.cloud_step li p {color:rgb(112, 116, 120); font-size: 1.6rem; padding-top: .4rem; font-weight: 400;}
.cloud_step li .tl {display: inline-block; padding-left:4.4rem; padding-top: 0; font-size: 1.7rem; color:rgb(34, 36, 38); font-weight: 500;}
.cloud_step li::after {position:absolute; display: block; content: ""; width:.1rem; height:calc(100% + -5.5rem); top:4.6rem; left:1.8rem; background:url('../../img/ico_dotted.png') repeat-y 0 0 / .1rem auto;}
.cloud_step li:last-child:after {display: none;}
.cloud_step li:last-child {padding-bottom:0rem;}
.cloud_step li p img {margin-top: 17px; width: 302px;}

.h_tit.mt32 { margin-bottom: 32px;}
.popup-contents .cloud_step + h2 { margin-top: 4.8rem; padding-bottom: 1rem;}

/* 카카오로딩 */
