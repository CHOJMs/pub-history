/* bg-color*/
.gray_box {background:#f6f6f6; border-radius: 4px; text-align: center; font-size: 15px; padding:30px 15px; line-height:22px; word-wrap:break-word; word-break:keep-all; position:relative;}

/* 블릿 */
.icon_bullet {display: block; position: relative; text-align: left; color:rgb(112, 116, 120); line-height:1.8rem; font-size:1.2rem; letter-spacing: -0.06rem; padding-left: 1.2rem; margin-bottom: 1rem; margin-top: 1.6rem; word-wrap:break-word; word-break:keep-all;}
.icon_bullet:before {content: ''; position: absolute; display: inline-block; left: 0; top: .7rem; width: .6rem; height: .6rem; margin-right: .6rem; border: .1rem solid rgb(112,116,120); border-radius: 50%;}
.icon_bullet a {color: rgb(112,116,120); font-size: 1.2rem;}

.list_bull li {font-size: 1.2rem; margin-bottom: .8rem; position: relative; color: rgb(112, 116, 120); padding-left: 1.2rem; margin-bottom: .8rem;}
.list_bull li:before {content: ''; position: absolute; display: inline-block; left: 0; top: .7rem; width: .6rem; height: .6rem; margin-right: .6rem; border: .1rem solid rgb(112,116,120); border-radius: 50%;}

/* 스탁론 전화번호 */
.call_phone1 {font-size: 1.4rem; color: rgb(34, 36, 38); margin-top: 3.2rem;}
.call_phone1 .tit {font-weight: 700;}
.call_phone1 .phone {font-weight: 700; color: rgb(18, 151, 239);}
.unit_txt {font-size: 1.2rem; padding: 8px 0 8px 0;}

input[type="checkbox"] {height: 28px; width: 28px; opacity: 0;}

/* 간격 */
h3 + .form_box {margin-top: 12px;}
h2 + .form_box {margin-top: 24px;}
h2.tl + .form_box {margin-top: 0px;}
.btn_box + h3, .btn_box + div > h3 {margin-top: 40px;}
.icon_bullet + h3.txt { margin-top: 40px;}


/*라디오버튼*/
.rchk{overflow:hidden; position:absolute; border:0; background:#fff; margin:0 0 0 0px; width:2.8rem; height:2.8rem; opacity: 0;}
.rchk+.rchk-label{display:inline-block; position:relative; height:2.8rem; padding-left:4rem; padding-right: .6rem; font-size:1.5rem; line-height:2.8rem; color: rgb(34, 36, 38); cursor:pointer}
.rchk+.rchk-label:after{position:absolute;top:0; left:0; width:2.8rem; height:2.8rem; margin-top:0px; background:#fff; border:.1rem solid #d3d3d3; border-radius: 50%; background-size:1.6rem;content: ''}
.rchk+.rchk-label:before {background:#bcc0c5; width: .8rem; height: .81rem; border-radius: 50%; display: block; content: ''; position: absolute; top: 1rem; left: 1rem; z-index: 1; /*transform: scale(2.5, 2.5); opacity: 0;*/}
.rchk:checked+.rchk-label:after{background:rgb(18, 151, 239); background-size:1.6rem; border:.1rem solid rgb(18, 151, 239);}
.rchk:checked+.rchk-label:before {background:#fff; width:.8rem; height:.8rem; display: block; content: ''; position: absolute; top: 1rem; left: 1rem; transition: .3s; transform: scale(1.1, 1.1); opacity: 1;}
.inline_rchk li {display: inline-block !important; width: 45%;}
.rchk.disabled+.rchk-label {color: rgb(188, 192, 197);}
.rchk.disabled+.rchk-label:after {background:#ebecee; border:.1rem solid #d3d3d3;}
.rchk.disabled+.rchk-label:before {background: #bcc0c5;}
.rchk+.rchk-label.pl28, .chk.chk_1+.label.pl28 {padding-left: 2.8rem !important; padding-right: 0 !important;}

/* 파일찾기 */
.bg_file_box {background:#f6f6f6; padding:15px 15px 15px 15px; margin-top:0px; line-height:22px; word-wrap:break-word; word-break:keep-all; position:relative;}
.bg_file_box .item-icon {padding-left:45px; padding-right:55px; line-height:26px; background:url("../img/2019/icon_file.png") no-repeat 0 50% / 31px auto;}
.bg_file_box .item-title {overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}
.bg_file_box .item-size {font-size:12px; color:#b2b2b2; line-height:12px;}
.bg_file_box a.file_delete {width:35px; height:35px; cursor:pointer; background:#fafafa; color:#777; border-radius:4px; border:1px solid #e3e3e3; position:absolute; top:50%; right:15px; top:calc(50% - 17px); text-align:center;}
.bg_file_box a.file_delete img {width:16px; height:20px; margin-top:6px;}

/* 파일관리 */
.box_file input.ui_file {position: absolute; width:1px; height:1px; padding:0; margin:-1px; overflow:hidden; clip:rect(0,0,0,0); border:0;} /* 파일 필드 숨기기 */
.box_file .num {font-size: 14px; border: 1px solid rgb(211, 213, 216); border-radius: 4px; padding: 24px 16px; margin-top: 24px;}
.box_file .num span {color: #1297ef; font-family: Koho; font-weight: 700;}
.box_file .num ul{border-top:1px solid rgb(112, 116, 120, 0.15); margin-top: 24px; padding-top: 2px;}
.box_file .num li {position:relative; margin:15px 0; padding-right: 35px; font-size: 14px; line-height: 22px; overflow:hidden; white-space:nowrap; text-overflow:ellipsis;}
.box_file .num li:last-child {margin-bottom: 0;}
.box_file .num a.file_delete {width:20px; height:20px; cursor:pointer; background:#707478; color:#777; border-radius:50%; border:1px solid #e3e3e3; position:absolute; top:50%; right:0px; top:calc(50% - 10px); text-align:center;}
.box_file .num a.file_delete img {width:7px; height:7px; margin-top:-7px;}

/* 첨부파일-버튼 */
.btn_file_box {position:relative; height:56px; overflow:hidden;}
.btn_file_box  .btn,
.btn_file_box  a.btn_file {margin:0;background:#1297ef;border-radius: .8rem;color:#fff;text-align:center;height:56px;/* line-height:56px; */font-weight: 500;font-size: 1.6rem;display:block;}
.btn_file_box  .btn.gray {background: rgb(235, 236, 238); color: rgb(34, 36, 38);}
.btn_file_box  .btn {width: 100%; font-size: 1.6rem;}
.file_input_hidden {font-size:38px; width:100%; position:absolute; right:0px; top:0px; opacity:0; filter: alpha(opacity=0); -ms-filter: alpha(opacity=0); cursor:pointer;}
.btn.green {color:#00a88b; background: #fff; border: 1px solid #00a88b;}
.btn_file_box button.btn.green::before {content: ''; display: inline-block; width: 1.6rem; height: 1.6rem; background: url('../../img/ui_common/ico_file_plus.png') no-repeat center 0 #fff; background-size: 1.6rem auto; vertical-align: middle; margin-right: 4px;}

/* 첨부파일 */
h3.txt .rent_filebox, .h_tit .rent_filebox {float: none; display: inline-block; margin-left: 8px; position: relative;}
.rent_filebox .btn_file {display:inline-block; width:100%; padding:0; text-align:center; color:#fff; font-size:inherit; line-height:normal; vertical-align:middle; background-color:#575c66; cursor:pointer; border:none; border-radius:5px;}
.rent_filebox .btn {border:none; background-color: transparent; text-decoration: underline; padding:0; text-align:center; font-size:14px; color:#1297ef; cursor:pointer; border-radius:5px;}
.rent_filebox .btn + .btn { padding-left: 8px;}
.rent_filebox input.ui_photo {font-size:38px; width:60px; position:absolute; left:0px; top:0px; opacity:0; filter: alpha(opacity=0); -ms-filter: alpha(opacity=0); cursor:pointer;} /* 파일 필드 숨기기 */
.box_writeType .rent_filebox .btn_file {padding:8px 0;}
.box_writeType .rent_filebox .btn {padding:8px 0 8px 8px;}

/* 전세 - 실사사진첨부 */
.bg_photo_box {background: #f2f2f2; padding:5px 15px; border-radius: 4px;}
.bg_photo_box li {position:relative; margin:15px 0; padding-right: 35px; font-size: 14px; line-height: 22px; word-break: break-all;}
.bg_photo_box a.file_delete {width:20px; height:20px; cursor:pointer; background:#707478; color:#777; border-radius:50%; border:1px solid #e3e3e3; position:absolute; top:50%; right:0px; top:calc(50% - 10px); text-align:center;}
.bg_photo_box a.file_delete img {width:7px; height:7px; margin-top:-7px;}
.bg_photo_box li .noData { align-items: center; display: flex; justify-content: center; background: #f2f2f2;}

