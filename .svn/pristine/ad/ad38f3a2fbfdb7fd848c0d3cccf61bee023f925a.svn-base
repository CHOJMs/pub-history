    .header-setting-list .alram {font-size: 0; line-height: 0; position: relative;}
    .header-setting-list .alram::before {content: ""; display: inline-block; width: 3.2rem; height: 3.2rem; background-image: url('../../img/mobile_office/ico_total_alram.svg'); background-size: cover;}/* 알람 */
    .header-setting-list .alram i {color: #fff; font-size: 1.1rem; position: absolute; right: 0; top: 0; width: 1.6rem; height: 1.6rem; border-radius: 50%; background-color: rgb(239 64 64); box-shadow: 0 .3rem .6rem rgba(239, 64, 64, 0.24); display: inline-flex; align-items: center; justify-content: center; font-style: normal;}
    .header-all-btn {font-size: 0; line-height: 0;}
    .header-all-btn::before {content: ""; display: inline-block; width: 3.2rem; height: 3.2rem; background-image: url('../../img/mobile_office/ico_total_all.svg'); background-size: cover;}/* 전체메뉴버튼 */
    
    /* 전체메뉴 */
    .logo-partner {background: url('../../img/mobile_office/logo_partner@2x.png') no-repeat 0 0; background-size: cover; width: 11.2rem; height: 2rem; font-size: 0; line-height: 0;}
    .wrap.bg_3 {background: #f4f5f7; min-height: 100%;}
    .menu-list {display: flex; flex-direction: column; gap: 1.2rem;}
    .menu-item {background-color: #fff; border-radius: 1.2rem;}
    .menu-item .user-wrap {padding: 2rem 2.4rem;}

    .user-wrap {display: flex; align-items: center;}
    .user-wrap .avater {flex-shrink: 0; width: 6.4rem; height: 6.4rem; background: url('../../img/mobile_office/avatar_img@2x.png') no-repeat 0 0; background-size: cover; margin-right: 1.6rem;}
    .user-wrap .user {flex-grow: 1; min-width: 0; margin-right: 0.8rem;}
    .user-wrap .user-name {font-size: 1.8rem; font-weight: 700; white-space: nowrap; overflow: hidden; text-overflow: ellipsis; color: rgb(34, 36, 38); letter-spacing: -0.018rem;}
    .user-wrap .user-info {margin-top: 0.8rem; display: flex; align-items: center;}
    .user-wrap .user-info .badge {display: inline-flex; align-items: center; flex-shrink:0;justify-content: center; height: 2rem; padding: 0 0.4rem; border-radius: 0.4rem; font-size: 1.2rem; font-weight: 500; background-color: rgb(228 244 235); color: rgb(0, 168, 139); margin-right: 0.8rem; letter-spacing: -0.012rem;}
    .user-wrap .user-info .badge.partner {background-color: rgb(239 243 252); color: rgb(21 113 235);}/* 파트너 */
    .user-wrap .user-info .partner-txt {letter-spacing: -0.014rem; color: rgb(92, 103, 115); font-size: 1.4rem; font-weight: 500; white-space: nowrap; overflow: hidden; display: block; text-overflow: ellipsis;}
    .user-wrap .logout {display: inline-flex; align-items: center; justify-content: center; height: 3.2rem; padding: 0 1.4rem; border: 0.1rem solid rgb(223 227 232); border-radius: 9999rem; flex-shrink: 0; font-size: 1.2rem; letter-spacing: -0.012rem; color: rgb(34, 36, 38);}
    
    .menu-item.active .menu-item-inner {display: block;}
    .menu-item.active .menu-toggle {transform: rotate(180deg);}
    .menu-item .menu-item-inner {display: none; padding: 0 2.4rem 2.2rem}
    .menu-tit-wrap {display: flex; margin: 2.2rem 2.4rem 2.2rem;}
    .menu-tit-wrap .menu-tit {font-size: 1.8rem; font-weight: 700; letter-spacing: -0.018rem; flex-grow: 1; color: rgb(34 36 38);}
    .menu-tit-wrap .menu-tit .count {margin-left: 0.8rem; color: rgb(22, 178, 142);}
    .menu-tit-wrap .menu-arr {display: block; flex-shrink: 0; width: 2.4rem; height: 2.4rem; background: url('../../img/mobile_office/ico_menu_arr.svg') no-repeat 0 0;}
    .menu-tit-wrap .menu-toggle {transform: rotate(0deg); transition: all 0.3s; display: block; flex-shrink: 0; width: 2.4rem; height: 2.4rem; background: url('../../img/mobile_office/ico_menu_toggle.svg') no-repeat 0 0;}

    .menu-link-list {display: flex; flex-wrap: wrap; gap: 2.4rem 0.8rem;}
    .menu-link-list .menu-link-item {width: 100%;}
    .menu-link-list.half .menu-link-item {width: calc(50% - 0.4rem);}
    .menu-link-list .menu-link-cont {font-size: 1.6rem; color: rgb(34 36 38); font-weight: 600; letter-spacing: -0.016rem;}
    .menu-link-list .menu-link-cont .new {display: inline-flex; align-items: center; flex-shrink:0;justify-content: center; background-color: rgb(255 229 229); color: rgb(255 0 0); font-size: 0.8rem; letter-spacing: -0.008rem; height: 1.6rem; padding: 0 0.4rem; border-radius: 9999rem; margin-left: 0.4rem;}
    
    .menu-link-list .menu-link-cover {display: flex; align-items: center;}
    .menu-link-list .menu-link-cover .menu-link-cont {display: flex; align-items: center; flex-grow: 1; min-width: 0;}
    .menu-link-list .menu-link-cover .menu-link-cont .intxt {display: block; white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}
    .menu-link-list .menu-link-cover .menu-link-arr {display: block; flex-shrink: 0; width: 1.6rem; height: 1.6rem; background: url('../../img/mobile_office/ico_menu_link_arr.svg') no-repeat 0 0;}  

    .menu-item-wide {margin-left: -2.4rem; margin-right: -2.4rem;}
    .menu-item-wide.mr {margin: 0 -2.4rem 0 0;}
    .menu-quick-box {overflow-x: auto;}
    .menu-quick-box::-webkit-scrollbar {height: 0;}
    .menu-quick-list {display: flex; gap: 0 0.6rem; white-space: nowrap;}
    .menu-quick-item {display: inline-flex; align-items: center; justify-content: center; height: 3.6rem; padding: 0 1.2rem; border-radius: 9999rem; border: 0.1rem solid rgb(223 227 232); font-size: 1.4rem; color: rgb(92 103 115) !important;}
    .menu-quick-item .ico {margin-right: 0.4rem; width: 1.6rem; display: inline-block;}
    .menu-quick-item .ico img {display: block;} 
    .menu-quick-item.active {background-color: rgb(52 58 65); border-color: rgb(52 58 65); color: #fff !important;}
    .menu-item-tab-cont {display: none;}
    .menu-item-tab-cont.active {display: block;}

    .notice-alert {display: flex; align-items: center; background-color: rgb(200 239 236); padding: 1.6rem 2.7rem 1.6rem 2.4rem; border-radius: 1.2rem;}
    .notice-alert .ico {display: block; flex-shrink: 0; width: 2.4rem; height: 2.4rem; background: url('../../img/mobile_office/ico_notice@2x.png') no-repeat 0 0; background-size: cover; margin-right: 1.1rem;}
    .notice-alert .notice-alert-wrap {display: flex; width: 100%; min-width: 0; flex-grow: 1;}
    .notice-alert .notice-alert-txt {color: rgb(34 36 38); display: block; white-space: nowrap; text-overflow: ellipsis; overflow: hidden; min-width: 0;}
    .notice-alert .notice-alert-wrap .new {display: inline-block; background-color: rgb(234 69 69); width: 0.5rem; height: 0.5rem; border-radius: 9999rem; margin-left: 0.4rem; flex-shrink: 0;}
    .notice-alert .arr {display: block; flex-shrink: 0; width: 1.6rem; height: 1.6rem; background: url('../../img/mobile_office/ico_menu_link_arr.svg') no-repeat 0 0; background-size: cover;}

    /* 위로 */
    .scrollTop {position: fixed; right: 2.4rem; bottom: 8rem; background: url('../../img/mobile_office/ico_scrolltop.svg') no-repeat 0 0; background-size: cover; width: 4.6rem; height: 4.6rem; font-size: 0; line-height: 0; text-indent: 9999em; z-index: 100;}

    /* 하단 네비게이션 */
  
    .navi-box {position: relative; height: 8.8rem; width: 100%;}
    .navi-list {position: fixed; left: 0; bottom: 0; display: flex; justify-content: space-between; box-shadow: 0 -0.4rem 1.6rem rgb(0 0 0 / 4%); background-color: #fff; width: 100%; padding: 1.2rem 3.6rem 1.2rem;}
    .navi-item {font-size: 1.1rem; letter-spacing: -0.011rem; color: rgb(188 192 197); display: flex; flex-direction: column; justify-content: center; text-align: center;}
    .navi-item.active {color: rgb(34 36 38);}
    .navi-item .ico {display: block; width: 2rem; height: 2rem; background-size: cover; background-position: 0 0; margin-bottom: 0.4rem;}
    .navi-item .ico.home {background-image: url('../../img/mobile_office/ico_navi_home_off.svg');}/* 홈 */
    .navi-item .ico.alarm {background-image: url('../../img/mobile_office/ico_navi_alarm_off.svg');}/* 알림 */
    .navi-item .ico.pay {background-image: url('../../img/mobile_office/ico_navi_pay_off.svg');}/* 결재 */
    .navi-item .ico.all {background-image: url('../../img/mobile_office/ico_navi_all_off.svg');}/* 전체 */
    .navi-item.active .ico.home {background-image: url('../../img/mobile_office/ico_navi_home_on.svg');}/* 홈 */
    .navi-item.active .ico.alarm {background-image: url('../../img/mobile_office/ico_navi_alarm_on.svg');}/* 알림 */
    .navi-item.active .ico.pay {background-image: url('../../img/mobile_office/ico_navi_pay_on.svg');}/* 결재 */
    .navi-item.active .ico.all {background-image: url('../../img/mobile_office/ico_navi_all_on.svg');}/* 전체 */
    
    /* 전체메뉴 */
    .total-menu-open {position: fixed; right: -100%; top: 0; width: 100%; height: 100vh; background: #fff; overflow-y: auto; z-index: 99; transition: all 0.5s ease-out; visibility: hidden;}
    .total-menu-open.open {right: 0; visibility: visible;}
    .total-menu-titleWrap {display: flex; align-items: center; justify-content: space-between; width: 100%; position: -webkit-sticky; position: sticky; top: 0; z-index: 100; background-color: #fff; padding: 0 2rem; height: 4.8rem;}
    .total-menu-title {color: rgb(34 36 38); font-weight: 700; font-size: 2.4rem;}
    
    .total-menu-innerWrap {padding: 1rem 2rem 0;}
    .total-noti-list {display: flex; align-items: center; text-align: center; justify-content: space-between; height: 5.6rem; background-color: rgb(247 247 247); border-radius: 0.8rem;}
    .total-noti-item {position: relative; font-size: 1.6rem; letter-spacing: -0.016rem; color: rgb(92 103 115) !important; width: 100%; font-weight: 500; padding: 0 2rem;}
    .total-noti-item::before {content: ""; width: 0.2rem; height: 1.2rem; background-color: rgb(224 224 224); position: absolute; left: 0; top: 50%; transform: translateY(-50%);}
    .total-noti-item:nth-of-type(1):before {display: none;}
    .total-menu-setting {display: flex; align-items: center; gap: 0.8rem;}
    .total-menu-setting .alram {font-size: 0; line-height: 0; position: relative;}
    .total-menu-setting .alram::before {content: ""; display: inline-block; width: 3.2rem; height: 3.2rem; background-image: url('../../img/mobile_office/ico_total_alram.svg'); background-size: cover;}/* 알람 */
    
    .total-menu-setting .alram i {color: #fff; font-size: 1.1rem; position: absolute; right: 0; top: 0; width: 1.6rem; height: 1.6rem; border-radius: 50%; background-color: rgb(239 64 64); box-shadow: 0 .3rem .6rem rgba(239, 64, 64, 0.24); display: inline-flex; align-items: center; justify-content: center; font-style: normal;}
    .total-menu-setting .setting {font-size: 0; line-height: 0;}
    .total-menu-setting .setting::before {content: ""; display: inline-block; width: 3.2rem; height: 3.2rem; background-image: url('../../img/mobile_office/ico_total_setting.svg'); background-size: cover;}/* 세팅 */
    .total-menu-close {font-size: 0; line-height: 0;}
    .total-menu-close::before {content: ""; display: inline-block; width: 3.2rem; height: 3.2rem; background-image: url('../../img/mobile_office/ico_total_close.svg'); background-size: cover;}/* 닫기 */
    .total-menu-setting .home {font-size: 0; line-height: 0;}
    .total-menu-setting .home::before {content: ""; display: inline-block; width: 3.2rem; height: 3.2rem; background-image: url('../../img/mobile_office/ico_total_home.svg'); background-size: cover;}/* 홈 */

    
    .total-menu-box {border-top: 0.1rem solid rgb(235 238 242); padding: 3.2rem 0;}
    .total-menu-box:nth-of-type(1) {border-top: none;}
    .total-menu-list {display: flex; flex-direction: column; gap: 2rem 0;}
    .total-menu-tit {font-size: 1.4rem; letter-spacing: -0.014rem; line-height: 2rem; color: rgb(92, 103, 115); margin-bottom: 2rem; display: flex; align-items: center;}
    .total-menu-tit .ico {display: inline-block; width: 2rem; height: 2rem; margin-right: 0.8rem;}
    .total-menu-tit .ico img {display: block;}
    .total-menu-cont {font-size: 1.6rem; line-height: 1.4; color: rgb(34, 36, 38); font-weight: 600; word-break: break-all;}

}



/* Mobile View */
@media only screen and (max-width:1024px) {

    /* 아이폰 - 노치 */
    body.ios {}
    body.ios #header {padding-top: var(--ios); height: calc(var(--ios) + 50px);}
    body.ios #header::after {height: 0;}
    body.ios #header.mobile-active {height: calc(var(--ios) + 52px);}
    body.ios #header .mobile-btn {top: calc(var(--ios) + 9px);}
    body.ios #header .gnb-menu {top: calc(var(--ios) + 9px);}
    body.ios #header .header-inner {padding-top: var(--ios);}
    body.ios #header .header-inner::before {top: var(--ios);}
    body.ios #header .header-inner > div > ul > li > ul {top: calc(var(--ios) + 150px); height: calc(100% - calc(var(--ios) + 150px));}
    body.ios #header .gnb-close {top: calc(var(--ios) + 28px);}
    body.ios #common_body {padding-top: var(--ios);}
    body.ios .sub_contaier h1 {top: calc(var(--ios) + 12px);}
    body.ios .sub_contaier p.device-mobile, body.ios .sub_contaier h2.tl {top: calc(var(--ios) + 12px);}
    body.ios .mobile-back {top: calc(var(--ios) + 9px);}
    body.ios .sub_contaier .box_form {padding-top: var(--ios);}

    h2.tl {font-size:18px;width: 75%; line-height:24px; font-weight:bold; color:#000; position: fixed; top: 13px; left: 50%; transform: translateX(-50%); z-index: 111;}
    h3 {font-size: 18px !important; line-height: 2.4rem; letter-spacing: -0.09rem; margin-top: 24px;}
    .h_tit {font-size: 16px !important; line-height: 2.4rem; letter-spacing: -0.09rem; margin-top: 24px;}

    /*header*/
    #header {padding: 0 20px; position: fixed; display: flex; justify-content: space-between; align-items: center; height: 50px;}
    #header .logo a img {width: 120px; font-size: 22px; font-weight: 700;}

    .wrap.bg_3 .header {background-color:#f4f5f7}
    #header.sub h1.logo {display: none;}
    #header.sub.rent_gnb { height: 50px; border-bottom: none;}

    #header {height: 50px;}
    .gnb-menu {left: auto; top: 9px; right: 20px; width: 32px; height: 32px;}
    .mobile-btn {display: block; position: absolute; left: 20px; top: 9px;}
    .mobile-btn ul {display: flex;}
    .mobile-btn ul li a {display: inline-flex; width: 32px; height: 32px;}
    .mobile-btn ul li a i::before {display: inline-block; content: ""; width: 8px; height: 8px; position: absolute; left: 19px; top: 6px; background:url('../../img/mobile_office/icon_alram_new.png') no-repeat 0 0 / 8px auto;}
    .mobile-btn ul li + li {margin-left: 16px;}
    .mobile-btn ul li.alram a { }
    .mobile-btn ul li.back { margin-left: 0px}
    .mobile-btn ul li.back a { width: 32px; height: 32px;}
    .gnb-close {top: 18px; left: auto; right: 10px; width: 32px; height: 32px;}
    .mobile-menu {position: relative; height: 115px; padding: 22px 20px 16px;}
    .mobile-menu p {font-size: 18px; font-weight: bold; color:rgb(34, 36, 38);}
    .mobile-menu p a {font-size: 18px; letter-spacing: -0.01em;}
    .mobile-logout {display: inline-flex; justify-content: center; align-items: center; font-size: 12px; position: absolute; bottom: 16px; right: 20px; width: 64px; height: 26px; background: rgb(235, 236, 238); border-radius: 4px;}
    .mobile-notice {display: inline-flex; justify-content: center; align-items: center; font-size: 12px; position: absolute; bottom: 16px; right: 90px; width: 64px; height: 26px; background: rgb(235, 236, 238); border-radius: 4px;}
    .mobile-logout a { color: rgb(34, 36, 38);}
    .mobile-notice a { color: rgb(34, 36, 38);}
    .mobile-back { position: absolute; left: 20px; top: 9px; width: 16px; height: 28px; display: block;}

    .header-inner > .mobile-menu > ul {position: unset; flex-wrap: unset; margin-top: 20px;}
    .header-inner > .mobile-menu > ul > li + li::before {height: 14px;}
    .header-inner > .mobile-menu > ul > li > a {font-size: 17px;}
    .header-inner > .mobile-menu > ul > li.setting {position: absolute; right: 58px; top: 18px;}
    .header-inner > .mobile-menu > ul > li.setting a {width: 32px; height: 32px; font-size: 0; background: url('../../img/ico_setting.png') center center / cover no-repeat;}
    .header-inner > .mobile-menu > ul > li.setting::before {display: none;}
    .header-inner > .mobile-menu > ul > li.alram {position: absolute; right: 105px; top: 18px;}
    .header-inner > .mobile-menu > ul > li.alram a {width: 32px; height: 32px; font-size: 0; background: url('../../img/ico_alram.png') center center / cover no-repeat;}
    .header-inner > .mobile-menu > ul > li.alram::before {display: none;}
    .header-inner > .mobile-menu > ul > li.alram a i::before {display: inline-block; content: ""; width: 8px; height: 8px; position: absolute; left: 19px; top: 6px; background:url('../../img/mobile_office/icon_alram_new.png') no-repeat 0 0 / 8px auto;}
    .header-inner > .mobile-menu > ul > li.home {position: absolute; right: 58px; top: 18px;}
    .header-inner > .mobile-menu > ul > li.home a {width: 32px; height: 32px; font-size: 0; background: url('../../img/ico_home.png') center center / cover no-repeat;}
    .header-inner > .mobile-menu > ul > li.home::before {display: none;}

    .main-line, .main-line-dsr {display: none;}

    .header-inner {flex-wrap: unset; padding-bottom: 0;}
    .header-inner::before {left: 46.66%;}
    .header-inner > div.left {margin-top: 24px; padding-left: 20px; padding-top: 0;}
    .header-inner > div.right {padding-left: 20px; padding-top: 24px;}
    .header-inner > div > ul {width: calc(46.66% - 10px); z-index: 2;}
    .header-inner > div.right > ul > li {margin-right: 0 !important;}
    .header-inner > div.left > ul > li {margin-right: 0 !important;}
    .header-inner > div > ul > li.active::before {left: calc(100% - 2px); width: 2px;}
    .header-inner > div > ul > li > ul {display: none; top: 150px; left: 100%; transform: translateX(46.66%); width: 100%; height: calc(100% - 150px); padding-bottom: 100px; overflow-y: auto; transition: all 0.5s ease-out; z-index: 1;}
    .header-inner > div > ul > li > div {position: absolute; top: 150px; left: 46.66%;}
    .header-inner.open > div > ul > li > div {position: fixed; z-index: -1;}
    .header-inner.open > div > ul > li.active > div {z-index: 1;}
    .header-inner.open > div > ul > li.active ul {left: 0;}
    .header-inner > div > ul > li.active > ul {top: 140px}


    .header-inner .flex-list {width: calc(100% - 46.66%); height: calc(100% - 172px); padding-bottom: 100px; overflow-y: auto;}

    .btn-half {flex-direction: column;}
    .btn-half a + a {margin-left: 0; margin-top: 8px;}
    .btn-full {position: fixed; left: -100%; bottom: 20px; width: 100%; padding: 0 20px; transition: all 0.5s ease-out; z-index: 1;}
    .btn-full a {padding: 0 20px; width: 100%; height: 54px; justify-content: space-between; font-size: 15px; border-radius: 8px 8px 0 0;}
    .btn-full a::after {content: "다운로드"; width: auto; height: auto; background: none; line-height: 1.5; font-size: 13px; border-bottom: 1px solid #fff;}
    .btn-full.btn-color a::after {content: "등록하기";}

    .rent_gnb .header-inner > .mobile-menu { font-size: 15px; padding: 22px 20px 16px;}
    .rent_gnb .header-inner > div > ul > li.active::before {left: calc(100% - 3px);}

    .footerwrap .pc_footer {display: none;}

    /* PC_GNB */
    #nav {display: none;}

    /*sut_layout*/
    .sub_contaier {display: block; width: 100%;}
    .sub_contaier::after {content: ''; display: block; clear: both;}
    .sub_contaier .contents {padding: 50px 0 0 0; height: initial;}
    .sub_contaier .contents::after {content: ''; display: block;}
    .sub_process { height: auto;}
    .contents {padding-left: 0;}

    /* 프로세스단계*/
    .sub_process .process_wrap .pro_box {width: inherit;}
    .sub_process .process_wrap + .btn_fixed {width:100%; height: 11.2rem; margin-left: 0; border-top:none; background: transparent;}
    .sub_process .process_wrap {padding: 2rem; width: 100%; position: absolute; overflow: hidden; top: 5rem; overflow-y: auto; bottom: 14rem; background-image: none;}
    .pro_txt_box {padding-bottom: 3rem;}
    .pro_txt_box .tit_info {font-size: 2rem; line-height: 3rem; width: 100%; align-items: flex-end;}
    .pro_txt_box .tit_info .txt_num {display: block;}
    .pro_txt_box .txt {padding-top: .8rem; font-size: 1.4rem;}



    .scoll { overflow-y: auto; position: absolute; bottom: 116px; top: 50px; width: 100%;}
    .scoll_1 { overflow-y: auto; position: absolute; bottom: 180px; top: 50px; width: 100%;}
    .mob_nobtn {bottom: 0 !important;}

}