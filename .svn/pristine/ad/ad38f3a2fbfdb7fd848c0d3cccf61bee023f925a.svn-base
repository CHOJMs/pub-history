@charset'utf-8';

body {font-size: 14px;}

.mo_office_wap, .mo_office_wap + footer { display: none;}

h2.tl {font-size:20px; line-height: 26px; padding:0 0px 15px; position: relative; left:0; top: 0;}
h2 .open_img {width:18px; height:18px;}
h3.txt {font-size: 18px; color:#222426; font-weight: 700; padding-bottom: 12px; position: relative; margin-top: 40px;}
h3.txt:first-child, .h_tit:first-child {margin-top: 0;}
h3.txt span {color: rgb(112, 116, 120); font-size: 14px; padding-left: 8px; font-weight: 500;}
h3:first-of-type {margin-top: 0;}
.form_box + h3:first-of-type {margin-top: 40px;}
.h_tit{font-size: 18px !important; line-height: 2.7rem; margin-bottom: .8rem; margin-top: 40px; color: rgb(34, 36, 38); letter-spacing: -0.08rem; font-weight: 700;}
.h_tit em, h3 em {position: relative; color:#f75248; font-size:1.8rem; padding-left:.2rem;}
.h_tit em span, h3 em span {opacity:0; width:1.5rem; height:1.5rem; position:absolute; top:.5rem; left:.5rem;}

.h2_line {border-bottom: 1px solid rgb(211, 213, 216); padding-bottom: 12px; margin-bottom: 12px; margin-top: 40px;}
.h2_line:first-of-type {margin-top: 0;}

.wrap {width: 100%; transition: .3s; min-height: calc(100% - 288px);}
.wrap:after {content: ''; display: block; clear: both;}

#tab_skip {position:relative; width:100%; z-index:9999;}
#tab_skip a {background:rgb(34, 36, 38); left:0; opacity:0; outline:medium none; padding:0 1rem; position:absolute; text-align:center; top:-3rem; transition:all 0.3s ease 0s; width:100%;}
#tab_skip a:active, #tab_skip a:focus {cursor:pointer; opacity:1; text-decoration:none; top:0;}
#tab_skip span {color:#fff; display:inline-block; font-size:1rem; line-height:1.6rem; padding:7px 6px 8px 0;}

/*header*/
.gnb-menu {display: none;}
.mobile-menu {display: none;}
.gnb-close {display: none;}
.mobile-btn-wrap {display: none;}
.mobile-btn {display: none;}
.mobile-logout {display: none;}
.mobile-notice {display: none;}
.mobile-back { display: none;}

#header.sub h1.logo {display: none;}

#header {position: fixed; left: 0; top: 0; width: 100%; display: flex; height: 92px; z-index: 999; transition: all 0.5s; overflow: hidden;}
#header.fixed {background: #fff;}
#header.sub {border-bottom: 1px solid rgb(211, 213, 216);}
#header.active {height: 540px; background: #fff;}
#header.isActive {height: auto;}
#header.mobile-active {height: 100vh; height: calc(var(--vh, 1vh) * 100); z-index: 3000;}

.header-inner {position: relative; padding: 0 60px 40px; width: calc(100% - 80px); display: flex; justify-content: space-between; z-index: 1;}
#header.main .header-inner::after {opacity: 0;}
#header.main.active .header-inner::after {opacity: 1;}
#header::after {content: ""; display: inline-block; position: absolute; top: 92px; left: 0; width: 100%; height: 1px; background: rgb(211, 213, 216);}
.header-inner > div > ul {display: flex;}

.header-inner > div.left > ul > li:last-child {margin-right: 0;}
.header-inner > div.left > ul > li {margin-right: 48px;}
.header-inner > div.left > ul > li:nth-of-type(1) {margin-right: 25px;}
.header-inner > div.left > ul > li:nth-of-type(2) {margin-right: 42px;}
.header-inner > div.right > ul > li:nth-of-type(1) {margin-right: 44px;}
.header-inner > div.left {margin-right: 0px;}

#header .header-inner > div > ul > li > a {color:rgb(34, 36, 38);}
#header.active .header-inner > div > ul > li > a {color: rgba(34, 36, 38, 0.4);}
#header.active .header-inner > div > ul > li:hover > a {color: rgba(34, 36, 38, 1);}

.header-inner > div > ul > li > a {margin-bottom: 30px; position: relative; display: inline-flex; align-items: center; padding-top: 14px; height: 92px; font-size: 20px; font-weight: bold; color: rgba(34, 36, 38, 0.4); letter-spacing: -0.1em; transition: all 0.3s; white-space: nowrap;}
.header-inner > div > ul > li > a::after {content: ""; display: block; position: absolute; bottom: 0; left: -10px; width: calc(100% + 20px); height: 3px; background: rgba(71, 179, 251, 0); transition: all 0.3s; border-radius: 2px;}
.header-inner > div > ul > li:hover > a {color: rgb(34, 36, 38);}
.header-inner > div > ul > li:hover > a::after {background: rgba(71, 179, 251, 1);}

.header-inner > div > ul > li ul li + li {margin-top: 16px;}
.header-inner > div > ul > li ul li a {line-height: 1.5; font-size: 16px; font-weight: 400; color: rgb(34, 36, 38); transition: all 0.3s;}
.header-inner > div > ul > li ul li a:hover {color:rgb(18, 151, 239);}
.header-inner > div > ul > li ul li.dep2 > a {display: inline-flex; align-items: center;}
.header-inner > div > ul > li ul li.dep2.active > a {color: rgb(18, 151, 239);}
.header-inner > div > ul > li ul li.dep2 > a::after {content: ""; display: inline-flex; margin: 0 0 0 4px; width: 16px; height: 16px; background: url(../../img/ico_menu_close.png) center center / 100% no-repeat; transform-origin: 50% 50%; transform: rotate(0); transition: all 0.3s;}
.header-inner > div > ul > li ul li.dep2.active > a::after {transform: rotate(0); background: url(../../img/ico_menu_open.png) center center / 100% no-repeat;}

.header-inner > div > ul > li ul li.dep2 ul {display: none; margin-top: 16px;}
.header-inner > div > ul > li ul li.dep2 ul li + li {margin-top: 10px;}
.header-inner > div > ul > li ul li.dep2 ul li a {display: inline-flex; position: relative; padding-left: 6px; line-height: 1.42; font-size: 14px; font-weight: 400; color: rgb(34, 36, 38); letter-spacing: -0.07em; transition: all 0.3s;}
.header-inner > div > ul > li ul li.dep2 ul li a:hover {color:rgb(18, 151, 239);}
.header-inner > div > ul > li ul li.dep2 ul li a::before {display: block; content: ""; width: 3px; height: 3px; border-radius: 100%; position: absolute; left: 0; top: 8.5px; background: rgb(188, 192, 197);}

.header-inner .flex-list {display: flex;}
.header-inner .flex-list > div {width: 160px;}
.header-inner .flex-list > div p a, .header-inner .flex-list > div p {font-size: 16px; font-weight: bold; color:rgb(34, 36, 38); letter-spacing: -0.08em;}
.header-inner .flex-list > div ul {margin-top: 16px;}
.header-inner .flex-list > div ul li + li {margin-top: 16px;}
.header-inner .flex-list > div ul li a {line-height: 1.5; font-size: 16px; font-weight: 400; letter-spacing: -0.08em;}

/* 전세자금 - gnb */
#header.sub.rent_gnb { height: 92px; border-bottom: 1px solid rgb(211, 213, 216);}
#header.rent_gnb.active {height: 340px;}
#header.active {height: 540px; background: #fff;}

.rent_gnb .mobile-logo { position: absolute; left: 0; top: 2.6rem; width: 12rem;}
.rent_gnb .header-inner {width: 1170px; margin: 0 auto; padding: 0;}
.rent_gnb .header-inner > div.right {margin-left: 215px;}
.rent_gnb .header-inner > div.right > ul > li { margin-right: 44px;}
.rent_gnb .header-inner > div > ul > li > a { padding-top: 5px;}

/* gnb-전세자금 */
.rent_gnb .pc_user_wrap {display: flex; position: absolute; right: 0; top: 36px;}
.rent_gnb .pc_user_wrap .selectbox {display: table-cell;}
.rent_gnb .pc_user_wrap .user_box {display: table-cell; font-size:16px; font-weight:500; padding-left: 40px;}
.rent_gnb .pc_user_wrap .user_box a {display:inline-block; position: relative; color:#222426; line-height:24px; padding-left: 12px; margin-left:8px;}
.rent_gnb .pc_user_wrap .user_box a::before {content: ''; display: inline-block; width: 4px; height: 4px; position: absolute; left: 0; top: 50%; background: rgb(211, 213, 216); border-radius: 50%;}
.rent_gnb .pc_user_wrap .user_box .font_blue {color:#1297ef;}
.rent_gnb .pc_user_wrap .logo_area img { width:131px;}
.rent_gnb .pc_user_wrap .select {width:100%; height: 24px; padding-right:20px; font-size:16px; font-weight:700; letter-spacing: -0.8px; color:#222426; line-height:24px; border: none; background: url('../../img/mobile_office/ico_rent_select_arrow.png') no-repeat center right 0; background-position: top 50% right 0px; background-size:auto 16px; box-sizing: border-box;}


/* PC-footer */
footer {background:rgba(34, 36, 38, 1); width:100%;}
.footerwrap {display:block; clear:both; width:100%; border:none; margin-top: 60px;}
.footerwrap .pc_footer {text-align:left; width:1170px; height:228px; margin:0 auto; padding:50px 0 20px 0; line-height:34px; display:block; position:relative;}
.footerwrap .pc_footer .pc_f_logo {float:left; width:298px; text-align:left; padding-top:0; margin-left:0px;}
.footerwrap .pc_footer .pc_f_logo img {width:151px; height:29px;}
.footerwrap .pc_footer .pc_f_address li {display:inline-block; padding:0 8px 0 13px; line-height:35px; background:url("../../img/mobile_office/pc_footer_line.png") no-repeat left 13px / 1px 13px;}
.footerwrap .pc_footer .pc_f_address li a {color:rgb(188, 192, 197); font-size:18px; letter-spacing: -0.9px;}
.footerwrap .pc_footer .pc_f_address li:first-child {background-image: none;}
.footerwrap .pc_footer .pc_f_address ul.address {margin-top:44px; margin-left: 310px; text-align:left; margin-bottom:15px;}
.footerwrap .pc_footer .pc_f_address ul.address li {display: block; font-size:16px; line-height:29px; padding: 0; color:rgb(188, 192, 197); background-image:none;}

.footerwrap .pc_footer .mark {position:absolute; right:0; top:54px; width:72px;}
.footerwrap #footer {display: none;}

/*sut_layout*/
.sub_contaier {width: 117rem; margin: 0rem auto; min-height: calc(100% - 384px);}
.sub_contaier .contents {padding-left: 8rem; min-height: 100%; padding: 154px 0 0 0;}
.contentwrap {padding-bottom: 10rem;}

/* 프로세스단계 */
.sub_process {display: block; height: 100%;}
.sub_process .contents {}
.sub_process .process_wrap + .btn_fixed {width: 100%; height: 8rem; position:fixed; bottom: 0; border-top:1px solid #6d6e70; background: #fff;}
.sub_process .process_wrap {padding: 6.4rem 0 12rem 16rem; position: absolute; overflow: hidden; top: 9.6rem; overflow-y: auto; bottom: 8rem; width: 100%; background:url('../img/img_progress.png') no-repeat 0 0 / 25.5% auto; background-position: bottom 12rem right 16rem;}
.pro_box {width: 52rem;}
.pro_txt_box {padding-bottom: 6.5rem;}
.pro_txt_box .tit_info {display: flex; justify-content: space-between; align-items: center; font-size: 4rem; line-height: 5.4rem; font-weight: 700; width: 80rem; letter-spacing: -0.09rem;}
.pro_txt_box .tit_info .txt_num {display: none;}
.pro_txt_box .txt {padding-top: 1.5rem; font-size: 1.6rem; color: rgb(112, 116, 120);}


