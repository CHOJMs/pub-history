    /* 헤더 */

    body.scroll-off {overflow: hidden;}
    .header {width: 100%; height: 4.8rem; padding: 0 2rem; position: -webkit-sticky; position: sticky; display: flex; justify-content: space-between; align-items: center; left: 0; top: 0; width: 100%; z-index: 999; transition: all 0.5s; overflow: hidden;}
    .header.fixed {background: #fff;}
    .header .back,.sub .back {font-size: 0; line-height: 0;}
    .header .back::before {content: ""; display: inline-block; width: 3.2rem; height: 3.2rem; background-image: url('../../img/mobile_office/ico_total_back.svg'); background-size: cover;}/* 뒤로가기 */
