<script>
$(window).on('scroll', function() {
    var scrollTop = $(window).scrollTop();
    $('.h1_tit').each(function() {
        var $this = $(this);
        if (scrollTop+100 >= $this.offset().top) {
            var id = $this.attr('id');
            $('aside a').removeClass('on');
            $('aside a[href="#' + id + '"]').addClass('on');
        }
    });
});
</script>
