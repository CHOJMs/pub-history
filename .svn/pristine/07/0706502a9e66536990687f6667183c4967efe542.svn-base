.list li{position:relative;padding-left:18px;}
.list li:before{position:absolute;top:8px;left:7px;width:3px;height:3px;background:#9e9e9e;border-radius:50%;content:''}
.list li + li{margin-top:5px}
.highlight{font-weight:bold;text-decoration: underline;}
.list a.link,.list a.link:visited,.list a.link:hover{color:#81a9db;font-weight:500;}
.col-group{display:flex}
