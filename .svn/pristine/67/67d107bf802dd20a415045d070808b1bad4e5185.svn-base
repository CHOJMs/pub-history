                            <div class="calendar two-input">
                                <input type="tel" value="" id="date-range3-1"><img src="/static/admin/img/new_common/ico_cal.png">
                                <span class="dash">-</span>
                                <input type="tel" value="" id="date-range3-2"><img src="/static/admin/img/new_common/ico_cal.png">
