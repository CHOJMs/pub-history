<script>
$(document).ready(function(){

	////////////////////////////////////////////////////////////////////////

	// init
	var today = new Date();
	var sDay = new Date(today.setDate(today.getDate()-30)).format("yyyy-MM-dd");
	var eDay = new Date().format("yyyy-MM-dd");

	$('#date-range1').val(sDay + ' ~ ' + eDay);
	$('#date-range2').val(eDay);

	$('#date-range3-1').val(sDay);
	$('#date-range3-2').val(eDay);

	////////////////////////////////////////////////////////////////////////

	$('.range-input').dateRangePicker(
		{
			monthSelect: true,
			yearSelect: true,
	    	separator : ' ~ ',
	    	autoClose: true,
	    	//endDate: moment().endOf('day').format('YYYY-MM-DD'),
	    	getValue: function() {
	    		if($('#date-range1').val()) {
	    			return $('#date-range1').val();
	    		} else {
	    			return '';
	    		}
	    	},
	    	setValue: function(s,s1,s2) {
	    		$('#date-range1').val(s1+' ~ '+s2);
	    	},
		});


/*
	$('#date-range1').dateRangePicker(
	{
		monthSelect: true,
		yearSelect: true,
    	separator : ' ~ ',
    	autoClose: true,
    	//endDate: moment().endOf('day').format('YYYY-MM-DD'),
	});
 */
	$('.one-input').dateRangePicker(
	{
		singleDate: true,
		singleMonth: true,
		monthSelect: true,
		yearSelect: true,
    	autoClose: true,
    	//endDate: moment().endOf('day').format('YYYY-MM-DD'),
    	getValue: function() {
    		if($('#date-range2').val()) {
    			return $('#date-range2').val();
    		} else {
    			return '';
    		}
    	},
    	setValue: function(s,s1,s2) {
    		$('#date-range2').val(s1);
    	},
	});

	$('.two-input').dateRangePicker(
	{
		monthSelect: true,
		//yearSelect: [moment().get('year') - 100, moment().get('year')],
        yearSelect: function(current) {
            return [current - 10, current];
        },
    	separator : ' ~ ',
    	autoClose: true,
    	getValue: function() {
    		if($('#date-range3-1').val() && $('#date-range3-2').val()) {
    			return $('#date-range3-1').val() + " ~ " + $('#date-range3-2').val();
    		} else {
    			return '';
    		}
    	},
    	setValue: function(s,s1,s2) {
    		$('#date-range3-1').val(s1);
    		$('#date-range3-2').val(s2);
    	},
    	//endDate: moment().endOf('day').format('YYYY-MM-DD'),
	});


});
</script>
