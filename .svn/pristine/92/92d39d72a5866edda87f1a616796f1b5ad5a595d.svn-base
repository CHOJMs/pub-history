@charset "UTF-8";
/* Header */
header.header { position:fixed; top:0; left:0; width:100%; box-shadow: 0 2px 6px 0 rgba(0, 0, 0, 0.12); background-color: #fff; z-index:999;}
header.header .inner_pc,
header.header .inner_mo { display: flex; align-items: center; justify-content: space-between; max-width: 128rem; height: 8.8rem; margin: 0 auto; }
header.header .logo { overflow: hidden; width: 18rem; height: 1.5rem; background: url(/static/carverse/img/common/logo.svg) no-repeat center; background-size: cover; }
header.header .logo span { position: absolute; width: 1px; height: 1px; padding: 0; margin: -1px; overflow: hidden; clip: rect(0, 0, 0, 0); border: 0; }
header.header .gnb { display: flex; align-items: center; gap: 5rem; }
header.header .gnb > li { position: relative; }
header.header .gnb > li > a { font-size: 1.8rem; font-weight: 500; letter-spacing: -0.01px; line-height: 1.89; padding:0.8rem 0;}
header.header .gnb > li .depth2 { display: none; position: absolute; top:3.4rem; left: 0; min-width: 16rem; padding: 2rem; border-radius: 1.2rem; box-shadow: 0 0.3rem 0.6rem 0 rgba(0, 0, 0, 0.16); background: #fff;}
header.header .gnb > li .depth2 a { font-size: 1.6rem; font-weight: 500; letter-spacing: -0.32px; line-height: 2.13; transition:all 0.3s; }
header.header .gnb > li .depth2 a.active,
header.header .gnb > li .depth2 a:hover { color: #3965ff; }
header.header .inner_mo { display: none; width: 100%; height: 4.8rem; padding: 0 2rem; }
header.header .inner_mo.main_header {display: none;}
header.header .inner_mo .logo { width: 13.2rem; height: 1.1rem; }
header.header .btn_back span,
header.header .mo_header_direct_go span,
header.header .btn_menu span { display: none; position: absolute; width: 1px; height: 1px; padding: 0; margin: -1px; overflow: hidden; clip: rect(0, 0, 0, 0); }
header.header .btn_menu.show { background: url(/static/carverse/img/common/icon_24_close_bk.svg) no-repeat 0 0; background-size: 3.2rem; }
header.header .btn_back { display: block; min-width: 4.2rem; height: 4.2rem; background: url(/static/carverse/img/common/icon_24_back.svg) no-repeat left; background-size: 2.4rem;}
header.header .mo_tit { display: block; width: 100%; font-size: 1.8rem; font-weight: 600; letter-spacing: -0.36px; line-height: 1.33; text-align: center; padding-right:4.2rem; }
header.header .mo_header_direct_go { display: inline-block; width: 3.2rem; height: 3.2rem; background: url(/static/carverse/img/common/icon_32_home.svg) no-repeat center; background-size: 3.2rem; }
header.header .mo_header_direct_go.mypage {background-image:url(/static/carverse/img/common/icon_24_mypage.svg);}
header.header .util { display: block; height:3.2rem; position: absolute; top: 50%; right: 2rem; transform: translateY(-50%); }
header.header .btn_menu { width: 3.2rem; height: 3.2rem; margin-left: 1rem; background: url(/static/carverse/img/common/icon_24_menu.svg) no-repeat 0 0; background-size: 3.2rem; }
header.header .gnb > li:hover .depth2 {display:block;}

/* Footer */
footer.footer { color: #fff; background-color: #1f222b; }
footer.footer .accordion_listup { width: 28rem; height: 5.6rem; font-size: 1.6rem; text-align: left; line-height: 1.5; }
footer.footer .inner_wrap { display: flex; justify-content: space-between; max-width: 128rem; width: 100%; margin: 0 auto; padding: 3.2rem 0; }
footer.footer .footer_left { display: flex; justify-content: space-between; gap: 4rem; }
footer.footer .footer_logo { width: 15rem; height: 3rem; background: url(/static/web/img/imlogo/ci_darkcolor_black.png) no-repeat 0 0; background-size: 15rem 3rem; }
footer.footer .footer_logo span { display: none; position: absolute; width: 1px; height: 1px; padding: 0; margin: -1px; overflow: hidden; clip: rect(0, 0, 0, 0); }
footer.footer .footer_link { display: flex; align-items: center; }
footer.footer .footer_link a { display: inline-block; position: relative; margin-right: 2rem; padding-right: 2.1rem; color: #ccc; font-size: 1.4rem; letter-spacing: -0.01px; line-height: 2rem; }
footer.footer .footer_link a:after { display: block; content: ""; position: absolute; top: 50%; right: 0; width: 1px; height: 0.4rem; background-color: #b3b6be; transform: translateY(-50%); }
footer.footer .footer_link a:last-child:after { display: none; }
footer.footer .contact_us li { display: flex; margin-top: 1.6rem; color: #ccc; font-size: 1.4rem; letter-spacing: -0.01rem; line-height: 1.43; }
footer.footer .contact_us li address { position: relative; margin-right: 2rem; padding-right: 2.1rem; }
footer.footer .contact_us li address:after { display: block; content: ""; position: absolute; top: 50%; right: 0; width: 1px; height: 0.4rem; background-color: #b3b6be; transform: translateY(-50%); }
footer.footer .contact_us li.customer_center { color: #ccc; font-size: 1.6rem; letter-spacing: -0.01px; line-height: 1.25; font-weight:500;}
footer.footer .contact_us li.customer_center span { margin-left: 1rem; font-size: 1.2rem; line-height: 1.67; }
footer.footer .copyright { margin-top: 1.6rem; color: #ccc; font-size: 1.4rem; letter-spacing: -0.01px; line-height: 1.43; }
footer.footer .btn_similar { width: 33.5rem; height: 5.6rem; padding: 0 1.6rem; border: 1px solid #fff; border-radius: 1.2rem; color: #fff; font-size: 1.6rem; font-weight: 400; letter-spacing: -0.01px; text-align: left; line-height: 1.5; }
footer.footer .footer_sns { display: flex; align-items: center; gap: 2.4rem; margin-top: 1.6rem; }
footer.footer .footer_sns span { display: none; position: absolute; width: 1px; height: 1px; padding: 0; margin: -1px; overflow: hidden; clip: rect(0, 0, 0, 0); }
footer.footer .footer_sns a { display: inline-block; width: 2.4rem; height: 2.4rem; }
footer.footer .footer_sns a.instagram { background: url(/static/carverse/img/common/icon_24_sns_ig.svg) no-repeat 0 0; }
footer.footer .footer_sns a.facebook { background: url(/static/carverse/img/common/icon_24_sns_fb.svg) no-repeat 0 0; }
footer.footer .footer_sns a.youtube { background: url(/static/carverse/img/common/icon_24_sns_youtube.svg) no-repeat 0 0; }
footer.footer .footer_sns a.naver_blog { background: url(/static/carverse/img/common/icon_24_sns_blog.svg) no-repeat 0 0; }

/* 패밀리 사이트 */
.accordion_listup .accor_text { border-color: #fff; }
.accordion_listup .accor_text .accor_btn { position: relative; width: 28rem; height: 5.6rem; border: 0.1rem solid #fff; background-color: #1f222b; border-radius: 1.2rem; z-index: 90; }
.accordion_listup .accor_text .accor_btn button { display: block; width: 100%; height: 100%; color: #fff; line-height: 1.5; text-align: left; padding: 0 1.6rem; font-size: 1.6rem; font-weight: 400; font-family: "Pretendard","sans-serif"; }
.accordion_listup .accor_text .accor_btn::before { content: ""; display: block; position: absolute; top: 1.5rem; right: 1.6rem; width: 2.4rem; height: 2.4rem; background: url(/static/carverse/img/common/icon_24_arrow_down_white.svg); background-size: 100%; z-index: 10; transform: rotate(180deg);  }
.accordion_listup .accor_text .accor_btn.show { border-top-right-radius: 0; border-top-left-radius: 0; border-top: none; }
.accordion_listup .accor_text .accor_btn.show::before { transform: rotate(0deg); }
.accordion_listup .accor_text .accor_cont { position: relative; overflow: hidden; }
.accordion_listup .accor_text .accor_cont.show { overflow: visible; }
.accordion_listup .accor_text .accor_cont.show .link_wrap { top: inherit; bottom: 5.6rem; padding:2rem 1.6rem 0 0; border: 0.1rem solid #fff; border-radius: 1.2rem; border-bottom-left-radius: 0; border-bottom-right-radius: 0; border-bottom: none; display: block; z-index:33;}
.accordion_listup .accor_text .accor_cont.show .link_wrap ul {height:20rem; overflow-y:scroll;}
.accordion_listup .accor_text .accor_cont .link_wrap { position: absolute; bottom: inherit; top: 0; left: 0; right: 0; background-color: #1f222b; display:none; padding-right:1rem;}
.accordion_listup .accor_text .accor_cont.show .link_wrap ul li a { position: relative; height: 100%; z-index: inherit; }
.accordion_listup .accor_text .accor_cont .link_wrap ul li a { padding: 0.8rem 0 0.8rem 1.6rem; display: block; color: #fff;}
.accordion_listup .accor_text .accor_cont .link_wrap ul li:first-child a {padding-top:0;}
.accordion_listup .accor_text .accor_cont .link_wrap ul li a:hover { color: #3a65ff; }
.accordion_listup .accor_text .accor_cont.show ul::-webkit-scrollbar {width:0.6rem;}
.accordion_listup .accor_text .accor_cont.show ul::-webkit-scrollbar-thumb {background: #2c2f39; border-radius: 1rem; opacity:0.5;}
.accordion_listup .accor_text .accor_cont.show ul::-webkit-scrollbar-track {background: #373d4d; border-radius: 1rem;}


/* Contents */
.contents { min-width: 32rem; padding-bottom: 12rem; margin-top:8.8rem; min-height:calc(100vh - 34.4rem);}
.contents.no_gap { padding-bottom: 0; }
.contents .inner_wrap { margin: 0 auto; }
.contents .inner_wrap.full { width: 100%; }
.contents .inner_wrap.full .contents_body { max-width: 128rem; width: 100%; margin: 0 auto; }
.contents .inner_wrap.medium { width: 100%; }
.contents .inner_wrap.medium .contents_body { max-width: 80rem; width: 100%; margin: 0 auto; }
.contents .inner_wrap.medium .contents_body.bg_shadow { background-color: #fff; box-shadow: 0 3px 32px 0 rgba(0, 0, 0, 0.16); padding:6.4rem 8rem 12rem; min-height: 100vh; }
.contents .inner_wrap.medium .contents_body .sub_t1 { margin-top: 0; margin-bottom: 6.4rem; }
.contents .inner_wrap.medium .contents_body:has(.btn_fixed) { padding-bottom: 12rem; }
.contents .inner_wrap.divider:after { display: block; content: ""; position: static; width: 100%; height: 1.2rem; background-color: #f8f8f8; }
.contents .inner_wrap.complet .contents_body { padding-top: 8rem; }

/* ======================= 모바일 ====================== */
@media all and (max-width: 1280px) {
    header.header .inner_pc { padding: 0 2rem; }
    footer.footer .inner_wrap { padding:3.2rem 2rem;}
    footer.footer .footer_link a { margin-right: 1.65vw; padding-right: 1.75vw; }
    footer.footer .contact_us li { display: block; }
    footer.footer .contact_us li address { margin-right: 0; padding-right: 0; }
    footer.footer .contact_us li address:after { display: none; }
    .contents > .inner_wrap { width: 100%; }
    .contents .contents_body {padding:0 2rem;}
}

@media all and (max-width: 799px) {
    header.header { box-shadow: none; }
    header.header .inner_pc { display: none; }
    header.header .inner_mo { display: flex; }
    header.header .btn_menu.show { background: url(/static/carverse/img/common/icon_24_close_bk.svg) no-repeat 0 0; background-size: 3.2rem; }
    header.header .inner_mo { display: flex; }
    #main header .inner_mo.main_header {display:flex;}
    #main header .inner_mo {display: none;}
    header.header .inner_wrap { width: 100%; height: 4.8rem; }
    header.header.all_menu .inner_mo .btn_back,
    header.header.all_menu .inner_mo .mo_tit,
    header.header.all_menu .inner_mo .util { display: none; }
    footer.footer { display: none; }
    .contents { margin-top:7.2rem; padding-bottom:3.4rem; min-height:initial;}
    .contents:has(.step_progress_wrap) {margin-top:14.8rem;}
    .contents .contents_body { padding:0 2rem; }
    .contents .inner_wrap.medium .contents_body.bg_shadow {padding:0 2rem 4rem 2rem; box-shadow:none;}
}