@charset "utf-8";

/* Pretendard-regular - latin_korean */
@font-face {
    font-family: 'pretendard';
    font-style: normal;
    font-weight: 400;
    font-display: SWAP;
    src: url('../../common/css/font/Pretendard-Regular.eot'); /* IE9 Compat Modes */
    src: local(''),
         url('../../common/css/font/Pretendard-Regular.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
         url('../../common/css/font/Pretendard-Regular.woff2') format('font-woff2'), /* Super Modern Browsers */
         url('../../common/css/font/Pretendard-Regular.woff') format('woff'), /* Modern Browsers */
         url('../../common/css/font/Pretendard-Regular.ttf') format('truetype'), /* Safari, Android, iOS */
         url('../../common/css/font/Pretendard-Regular.svg#Pretendard') format('svg'); /* Legacy iOS */
  }
  
