@charset "utf-8";

.wrap {width: 100%; /*min-height: 100%;*/ padding-bottom: 0; background:url('../img/bg_back.png') repeat-y; transition: .3s;}
.wrap.bg-wrap {padding-bottom: 0;}
h1.h1_tl { font-size: 0;}

#tab_skip {position:relative; width:100%; z-index:9999;}
#tab_skip a {background:rgb(34, 36, 38); left:0; opacity:0; outline:medium none; padding:0 1rem; position:absolute; text-align:center; top:-3rem; transition:all 0.3s ease 0s; width:100%;}
#tab_skip a:active, #tab_skip a:focus {cursor:pointer; opacity:1; text-decoration:none; top:0;}
#tab_skip span {color:#fff; display:inline-block; font-size:1rem; line-height:1.6rem; padding:7px 6px 8px 0;}
#tab_skip a:focus span {outline: 3px solid rgb(167, 167, 167);}

/*header*/
.gnb-menu, .gnb-home {display: none;}
.mobile-menu {display: none;}
.gnb-close {display: none;}
.mobile-btn-wrap {display: none;}
.mobile-btn {display: none;}
.mobile-logout {display: none;}
.mobile-back { display: none;}
#header.sub h1.logo {display: none;}

/* 탑 배너 */
.top-banner {position: fixed; left: 0; top: 0; padding: 20px 140px 20px 60px; width: calc(100% - 80px); height: 100px; background: rgb(34, 36, 38); z-index: 998; transition: all 0.3s;}
.top-banner > div { display: flex; flex-wrap: wrap; justify-content: space-between;}
.top-banner > div > div { display: inline-flex; width: calc(33.33% - 10px);}
.top-banner > div > div a { color: #fff; font-size: 16px; font-weight: 500; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; word-wrap: normal;}
.top-banner > div > div a::before {display: inline-block; content: "·"; margin-right: 4px; font-size: 18px;}
.top-banner > div > div + div { margin-top: 6px;}
.top-banner.banner-hidden {height: 0; opacity: 0; visibility: hidden; overflow: hidden;}
.top-banner .checkbox { display: block; text-align: right; margin: 0; position: absolute; right: 120px; top: 75%; transform: translateY(-50%);}
.top-banner .checkbox .chk+.label { color: #fff; font-weight: 400;}
.top-banner .checkbox .chk:checked+.label { color: #fff; font-weight: 400;}
.top-banner .close-btn {position: absolute; right: 60px; top: 75%; width: 40px; height: 40px; transform: translateY(-50%); color: #fff;}
.top-banner .checkbox .chk:focus+.label { outline: 2px dotted #fff !important;}

.main-line, .sub-line {content: ""; display: block; position: fixed; left: 60px; top: 92px; width: calc(100% - 120px); height: 4px; background: #000; z-index: 98; transition: all 0.3s;}
.sub-line {width: calc(100% - 120px);}
.main-line.banner-visible, .sub-line.banner-visible {top: 192px;}
.sub-line.fixed {left: 0; width: 100%;}
.main-line.fixed {left: 0; width: calc(100% - 80px);}

#header {overflow:hidden;position: fixed; left: 0; top: 0; width: 100%; display: flex; height: 88px; z-index: 99; transition: all 0.3s;}
#header.fixed .header-inner {background-color: rgba(255, 255, 255, 0.99);}
#header.sub {border-bottom: 1px solid #ebeef2;}
#header.active {height: 640px;background: #fff; box-shadow: 0 16px 24px rgb(22 26 31 / 12%);} /* 메뉴 높이 스크립트 처리 */
#header.isActive {height: auto;}
#header.mobile-active {height: 100vh; height: calc(var(--vh, 1vh) * 100); z-index: 3000;}
#header.banner-visible {top: 100px;}
.bg-wrap > #header.sub {border-bottom: none;}

.header-inner {position: relative; padding: 0 60px 40px; width: calc(100% - 80px); display: flex; justify-content: space-between; z-index: 1;}
#header.sub .header-inner {width: 100%;}
#header.main .header-inner::after {opacity: 0;}
#header.main.active .header-inner::after {opacity: 1;}
#header.active .header-inner {box-shadow: 0 0 12px rgba(0, 0, 0, 0.12);}

.header-inner::after {content: ""; display: inline-block; position: absolute; top: 92px; left: 0; width: 100%; height: 1px; background-color: rgb(211, 213, 216);}
.header-inner > div > ul {display: flex; height: 100%;}
.header-inner > div.right > ul > li:last-child {padding-right: 0;}
.header-inner > div.left-pc > ul > li:nth-child(4).on:hover a {cursor: pointer;}
.header-inner > div.right {padding-right: 35px;}
#header.sub .header-inner > div.right {margin-right: 0;}

#header .header-inner > div > ul > li > a {color:rgb(34, 36, 38);}
#header.active .header-inner > div > ul > li > a {color: rgba(34, 36, 38, 0.4);}
#header.active .header-inner > div > ul > li:hover > a {color: rgb(18, 151, 239); cursor: default;}
#header.active .header-inner > div > ul > li:nth-child(4):hover a {cursor: pointer;}
#header.active .header-inner > div > ul > li a:hover + ul li a {color: rgb(34, 36, 38, 1);}

.header-inner > div > ul > li > a {margin-bottom: 32px; position: relative; display: inline-flex; align-items: center; padding-top: 0px; height: 92px; font-size: 20px; font-weight: bold; color: rgba(34, 36, 38, 0.4); transition: all 0.3s; white-space: nowrap;}
.header-inner > div > ul > li > a::after {content: ""; display: block; position: absolute; bottom: 0; left: 0; width: 100%; transform-origin: 50% 0; transform: scaleX(0); height: 3px; background: rgba(71, 179, 251, 0); transition: all 0.3s; border-radius: 2px; z-index: 2;}
.header-inner > div > ul > li:hover > a {color: rgb(18, 151, 239);}
.header-inner > div > ul > li:hover > a:after,
.header-inner > div > ul > li.active > a:after,
.header-inner > div > ul > li > a:focus:after {background: rgba(71, 179, 251, 1); transform: scaleX(1);}
.header-inner > div > ul > li > a:focus,
.header-inner > div > ul > li ul li a:focus {color: rgb(18, 151, 239) !important;}

.header-inner > div > ul > li ul li.tit { display: none;}
.header-inner > div > ul > li ul li.tit + li { margin-top: 0;}
.header-inner > div > ul > li ul li + li {/*margin-top: 16px;*/}
.header-inner > div > ul > li ul li a {line-height: 1.5; font-size: 16px; font-weight: 500; color: #919191; transition: all 0.3s; display: block; padding: 8px 0 7px;}
.header-inner > div > ul > li ul li a:hover {color:rgb(18, 151, 239);}
.header-inner > div > ul > li ul li a strong { font-weight: 700;}
.header-inner > div > ul > li ul li a.ico_openwindow {font-weight: 700; position: relative;}
.header-inner > div > ul > li ul li a.ico_openwindow::after {content: '새창이동'; font-size: 0; position: absolute; left: 54px; top: 12px;vertical-align: top; margin-left: 0.4rem; display: inline-block; width: 1.4rem; height: 1.4rem; background: url('../img/ui_common/ico_openwindow.png') no-repeat; background-size: 1.4rem;}
.header-inner > div > ul > li ul li.dep2 > a {display: block; align-items: center; color: #919191;}
.header-inner > div > ul > li ul li.dep2.active > a {color: rgb(18, 151, 239, .5);}
.header-inner > div > ul > li ul li.dep2 > a::after {content: ""; display: inline-block; margin: 0 0 0 4px; width: 16px; height: 16px; background: url(../img/ico_menu_close.png) center 3px / 100% no-repeat; transform-origin: 50% 50%; transform: rotate(0); transition: all 0.3s; opacity: .5;}
.header-inner > div > ul > li ul li.dep2.active > a::after {transform: rotate(0); background: url(../img/ico_menu_open.png) center 3px / 100% no-repeat;}
.header-inner > div > ul > li.on ul li a,
.header-inner > div > ul > li.on ul li.dep2.active ul li a {color: rgb(34, 36, 38, 1);}
.header-inner > div > ul > li.on ul li.dep2.active a,
.header-inner > div > ul > li.on ul li.dep2.active a:hover {color: rgb(18, 151, 239, 1);}
.header-inner > div > ul > li.on ul li a:hover {color:rgb(18, 151, 239, 1);}
.header-inner > div > ul > li.on ul li.dep2.active > a::after {transform: rotate(0); background: url(../img/ico_menu_open.png) center 3px / 100% no-repeat; opacity: 1;}
.header-inner > div > ul > li.on ul li.dep2 ul li a::before {background: rgb(34, 36, 37, 1);}

.header-inner > div > ul > li ul li.dep2 ul {display: none; margin-top: 0px; margin-left: 8px;}
.header-inner > div > ul > li ul li.dep2 ul li + li {/*margin-top: 10px;*/}
.header-inner > div > ul > li ul li.dep2 ul li a {display: block; position: relative; width: calc(100% - 10px); line-height: 1.42; font-size: 14px; font-weight: 400; color: rgb(34, 36, 38, .5); transition: all 0.3s; padding: 5px 0 5px 10px;}
.header-inner > div > ul > li ul li.dep2 ul li:last-child a {padding: 5px 0 10px 10px;}
.header-inner > div > ul > li ul li.dep2 ul li a:hover {color:rgb(18, 151, 239, 1);}
.header-inner > div > ul > li ul li.dep2 ul li a:hover:before {background:rgb(18, 151, 239, 1);}
.header-inner > div > ul > li ul li.dep2 ul li a::before {display: block; content: ""; width: 5px; height: 1px; border-radius: 100%; position: absolute; left: 0; top: 13px; background: rgb(34, 36, 37, .5);}

/*.header-inner .flex-list {display: flex;}
.header-inner .flex-list > div {width: 160px;}*/
.header-inner .flex-list {width: 167px; margin-top: 8px;}
.header-inner .flex-list > div p {font-size: 14px; font-weight: 500; color:rgb(112, 116, 120);}
