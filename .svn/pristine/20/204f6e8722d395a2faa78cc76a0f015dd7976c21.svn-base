.design-sample .tit{margin:30px 0 20px;padding:5px 30px;font-size:22px;font-weight:600;background:#e5d8c7;}
.design-sample img{width:300px;margin:0 0 30px 30px;border:1px solid black}

.pc-view{display:block !important}
.pc-view.inline{display:inline !important}
.mob-view{display:none !important}
/* header */
header{position:fixed;left:0;top:0;width:100%;z-index:99;}
header .header-pc{display:flex;height:88px;background:#fff;border-bottom:1px solid #ebeef2;transition: all 0.3s;}
header .h-inner{display:flex;justify-content:space-between;align-items:center;max-width:1200px;margin:0 auto;height:88px}
header .h-left,
header .h-right{display:flex;align-items: center;}
header .new{display:inline-block;margin-left:4px;padding:0 6px;font-size:10px;font-weight: 600;background:#ffe6e6;color:#ef4040;line-height: 18px;border-radius:4px;top: 50%;transform: translateY(-50%);position: absolute;}
header .menu-list{display:flex;align-items: center;}
header .menu-list > li{position:relative;padding:0 16px}
header .menu-list > li > a{position:relative;display:flex;align-items:center;height:88px;font-size:18px;font-weight:500;color:#222426;line-height:22px}
header .menu-list > li:hover > a,
header .menu-list > li > a:focus{color:#222426;font-weight:600}
header .menu-list > li > a:after{position:absolute;bottom:1px;width:100%;height:3px;background:#00a88b;transform-origin: 50% 0;transform: scaleX(0);transition:all 0.3s;content:''}
header .menu-list > li:hover > a:after,
header .menu-list > li > a:focus:after{transform: scaleX(1);}
header .menu-list .on .s-menu-wrap{visibility:visible;}
header .menu-list .on .s-menu-wrap:after{box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.08);}
header .menu-list .s-menu-wrap{visibility:hidden;position:absolute;z-index:-1;top:88px;left:16px;width:max-content;background:#fff;padding:40px 0;}
header .menu-list .s-menu-wrap:after{position:absolute;z-index:-1;top:0;left:-100vw;height:100%;width:200vw;background:#fff;content:''}
header .menu-list .s-menu-wrap > li{float:left}
header .menu-list .s-menu-wrap > li + li{margin-left:80px}
header .menu-list .s-menu-wrap .new{display:inline-block;margin: 4px 0 0 4px;font-size: 12px;line-height: 20px;width: 39px;position: relative;transform: initial;vertical-align: top;}
header .menu-list .s-menu-wrap .menu-tit{font-size:18px;font-weight:600}
header .menu-list .s-menu-wrap .menu-tit a{color:#222426;line-height:22px;position: relative;}
header .menu-list .s-menu-wrap .menu-tit a:after{display:inline-block;margin-top:5px;width:16px;height:16px;background:url('../img/ui_common/ico_m_arrow.png');background-size:16px auto;vertical-align:top;content:''}
header .menu-list .s-menu-wrap .s-menu-list{margin-top:14px}
header .menu-list .s-menu-wrap .s-menu-list li + li{margin-top:10px}
header .menu-list .s-menu-wrap .s-menu-list li a{position: relative;display:flex;align-items:center;font-size:16px;color:#5c6773}
header .menu-list .s-menu-wrap .s-menu-list li:hover a,
header .menu-list .s-menu-wrap .s-menu-list li a:focus{color:#00a88b;font-weight:600}
header .menu-list .s-menu-wrap .s-menu-list .tag{display:inline-block;margin:2px 0 0 4px;height:18px;line-height:17px;color:#fff;padding:0 4px;background:#ef4040;border-radius:12px;font-size:10px;font-weight:600;vertical-align:top;}
header .menu-list .s-menu-wrap .s-menu-list .tag2{display:inline-block;margin: 1px 0 0 4px;padding:0 8px;background: #ddeafa;font-size:13px;font-weight: 500;color: #1571eb;line-height: 20px;border-radius:4px;vertical-align: top;}
header .menu-list .h-link{display:none;position:fixed;top:128px;left:50%;margin-left:368px;}
header .menu-list .on .h-link{display:block}
header .btn-h-link{position:relative;width:232px;height:120px;padding-left:24px;background:#f0f5f3;border-radius:12px;text-align:left;font-size:18px;color:#222426;font-weight:600}
header .btn-h-link span{display:block;margin-bottom:6px;font-size:14px;color:#00a88b;font-weight:500}
header .btn-h-link:after{position:absolute;top:50%;right:24px;width:48px;height:48px;transform:translateY(-50%);background:url('../img/main/ico_slide_menu04.png') no-repeat;background-size:48px auto;content:''}
header .h-right .menu-list > li{padding:0 8px;font-size:14px}
header .h-right .menu-list > li.on > a{color:#222426}
header .h-right .menu-list > li > a{font-size:14px;color:#5c6773;font-weight:400}
header .h-right .menu-list > li > a:hover{font-weight:600}
header .h1-logo{position:relative;display:flex;align-items:center;margin-right:24px;}
header .h1-logo > a{overflow:hidden;display:block;width:140px;height:88px;background:url(../img/imlogo/ci_basic.png) center no-repeat;background-size:140px auto;text-indent:-9999px;}
/* 홈페이지,디지털브랜치 전환 버튼 */
.h1-link-box{margin-left:4px}
.h1-link-box:hover:after{position:absolute;bottom:0;left:0;right:0;height:35px;content:''}
.h1-link-box:hover .digi-sub-menu{display:block}
.h1-link-box .link{width:20px;height:20px;background:url('../img/ui_common/ico_arrow19_mob.png') no-repeat;background-size:20px auto;cursor:pointer;text-indent:-9999px}
header .digi-menu-list{display:flex;}
header .digi-menu-list > li{position:relative}
header .digi-menu-list > li > a{display:block;padding:0 16px;height:88px;font-size:18px;font-weight:600;color:#222426;line-height:88px}
header .digi-menu-list > li:hover .digi-sub-menu{display:block}
header .digi-sub-menu{display:none;position:absolute;left:50%;bottom:16px;transform:translate(-50%, 100%);padding:8px;border:1px solid #ebeef2;box-shadow: 0 8px 16px 0 rgba(0, 0, 0, 0.08);border-radius:16px;background:#fff}
header .digi-sub-menu li a{display:block;padding:12px 10px;min-width:144px;font-size:16px;font-weight:600;color:#5c6773;text-align:center}
header .digi-sub-menu li a:hover{color:#00a88b;background-color:#f7f9f8;border-radius:8px}

header .h-search-ai > button,
header .h-all-menu > button{overflow:hidden;text-indent:-9999px}
header .h-search-ai{position:relative;margin-left:8px;}
header .h-search-ai > button{width:48px;height:48px;border-radius:50%;background:url('../img/common/ico_ai_search.png') no-repeat 50% 50%;background-size:32px auto;transition:all 0.3s;}
header .h-search-ai.on > button{background-color:#f7f7f7;}

header .h-servicedesk{position:relative;margin-left:8px;width:32px;height:32px;background:url('../img/common/ico_ai_search.png') no-repeat 50% 50%;background-size:32px auto;text-indent:-9999px}

/* 2025-03-06 tip*/
header .h-search-ai > button .tip{display:none; position:absolute;z-index: 1;top: 49px;right: -40px;padding: 0 10px;width:max-content;font-weight: 600;font-size: 13px;color: #222426;background: #fff;line-height:28px;text-indent: 0;border: 1px solid #00a88b;border-radius: 15px;}
.body-ready-tip header .h-search-ai > button .tip{display:block; animation-name: alertShow, alertHide; animation-delay: 0s, 6s; animation-duration: 0.8s, 0.8s; animation-fill-mode:forwards;}
header .h-search-ai > button .tip em{ color: #00a88b;}
header .h-search-ai > button .tip:after{position:absolute;top: -10px;right: 59px;width: 0;height: 0;border-bottom: 6px solid #00a88b;border-top: 4px solid transparent;border-left: 4px solid transparent;border-right: 4px solid transparent;content:'';z-index: -1;}
header .h-search-ai > button .tip:before{position:absolute;top: -9px;right: 59px;width: 0;height: 0;border-bottom: 7px solid #ffffff;border-top: 4px solid transparent;border-left: 4px solid transparent;border-right: 4px solid transparent;content:'';z-index: 12;}
header .h-search-ai.on > button .tip{display:none;}

