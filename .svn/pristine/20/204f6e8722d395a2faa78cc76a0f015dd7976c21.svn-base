    .m-bottom-pad{padding-bottom:62px}

    /* 우측 플로팅 메뉴(상담,영업점찾기 등) */
    .m-bottom-pad .float-area{bottom:80px}
    .float-area{top:inherit;right:20px;bottom:20px}
    .float-area ul li:nth-child(1),
    .float-area ul li:nth-child(2),
    .float-area ul li:nth-child(3),
    .float-area .float-tx3{display:none}
    .float-area ul li a:before{margin-bottom:0}
    .float-area ul li + li{margin-top:0}
    .float-area ul li a{position:relative}
    .float-area .btn-float-close{overflow:hidden;position:absolute;top:-5px;right:-5px;z-index:1;width:30px;height:30px;background:url('../img/common/ico_float_close.png') no-repeat 50% 50%;background-size:20px auto;text-indent:-9999px}
    .float-area ul li:nth-child(4) a:after{transform:translateX(-300px);animation:chatSlideIn 0.3s 1s, chatChar 1.2s 1.3s linear infinite}
    .float-area .float-tx4{display:block;animation:chatSlideIn 0.3s, chatSlideOut 0.5s 1s forwards}
    .main-content.easy .float-area ul li:nth-child(4) a:after{background:none;}
    .main-content.easy .float-area .float-tx4{animation:none}
    @keyframes chatSlideIn{
        0% {transform:translateX(300px);}
        100% {transform:translateX(0px);}
    }
    @keyframes chatSlideOut{
        0% {transform:translateX(0);}
        100% {transform:translateX(-300px);}
    }
    @keyframes chatChar{
        0%{transform:rotate(0)}
        25%{transform:rotate(-5deg)}
        50%{transform:rotate(0)}
        75%{transform:rotate(5deg)}
        100%{transform:rotate(0)}
    }

