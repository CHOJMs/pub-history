.pro_txt_box + h2 {padding-top: 0;}
.pro_txt_box + .h_tit {margin-top: 0;}

/* 프로세스단계-완료(가운데정렬) */
.fin_cen_box {display: flex; flex-direction: column; justify-content: space-between; min-height: calc(100vh - 100px); padding: 14rem 0 0 0 !important; bottom: 0 !important; background:url('../img/img_finish.png') bottom right / 336px no-repeat;}
.fin_cen_box1 {display: flex; flex-direction: column; justify-content: space-between; min-height: calc(100vh - 100px); padding: 14rem 0 0 0 !important; bottom: 0 !important; background-image: none !important;}
.fin_cen_box1 .btn_line_txt, .btn_line_txt {margin-top: 8rem;}
.finish_box .btn_line_txt, .btn_line_txt {text-align: center;}
.finish_box {width: 960px !important; margin: 0 auto; position: relative; padding-top: 17.6rem;}
.finish_box .fini_t_box {text-align: center;}
.finish_box .fini_t_box:after {content: ''; width: 12rem; height: 12rem; position: absolute; top: 0; left: 50%; transform: translate(-50%, 0%); border-radius: 50%; background-color: rgb(18, 151, 239); transition: .3s;}
.finish_box .fini_t_box:before {content: ''; width: 5.6rem; height: 3.8rem; position: absolute; top: 0; left: 50%; transform: translate(-50%, 100%); background:url('../img/ico_check_finish.png') no-repeat 0 0 / 5.6rem auto; z-index: 1; transition: .3s;}
.finish_box .fini_t_box .tit_info {display: block; font-size: 4rem; line-height: 5rem; font-weight: 700; padding-bottom: 4rem;}
.finish_box .fini_t_box .txt {font-size: 2rem; line-height: 3.4rem; padding-bottom: 4.8rem; font-weight: 400; letter-spacing: -0.05rem; color: rgb(34, 36, 38);}
.finish_box .fini_t_box .txt strong {font-weight: 700;}
.finish_box .fini_t_box .txt .number {font-family: Koho;}
.finish_box .fini_t_box .call_phone {font-size: 1.8rem; padding-bottom: 9.7rem; color:rgb(34, 36, 38);}

.finish_box.fail-box {text-align: center;}
.finish_box.fail-box .fini_t_box:after {display: none;}
.finish_box.fail-box .fini_t_box:before {display: none;}



/* 모바일 프로세스 화살표 */
