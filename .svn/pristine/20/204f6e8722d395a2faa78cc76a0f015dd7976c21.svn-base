header .ai-wrap .ai-right .ai-btn-close{background-image:url('../img/common/ico_menu_close.png')}
header .ai-wrap .ai-right .ai-btn-more{background-image:url('../img/common/ico_more.png')}
header .ai-wrap .ai-chat{position:relative;overflow-y:auto;padding:20px 24px;height:calc(100% - 172px)}
header .ai-wrap .ai-chat:after{display:block;clear:both;content:''}
header .ai-wrap .ai-chat .message-user{display:flex;justify-content:flex-end;margin-top:24px;}
header .ai-wrap .ai-chat .message-user + .message-user{margin-top:10px}
