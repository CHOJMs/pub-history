header .ai-wrap .ai-right{position:absolute;top:8px;right:20px}
header .h-search-ai .ai-wrap .ai-right{top:16px}
header .ai-wrap .ai-right button{overflow:hidden;width:32px;height:32px;background-size:32px auto;text-indent:-9999px}
