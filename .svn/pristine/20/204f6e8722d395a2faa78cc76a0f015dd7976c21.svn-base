header .ai-wrap .ai-chat .message-user p{font-size:14px;font-weight:500;color:#fff;line-height:22px}
header .ai-wrap .ai-chat .message-chatGpt{margin-top:24px;min-height:40px;padding:9px 20px 0 52px;background:url('../img/common/bg_chat_char.png') no-repeat left top;background-size:40px auto;font-size:15px}
header .ai-wrap .ai-chat .message-chatGpt:first-child{margin-top:0}
header .ai-wrap .ai-chat .message-chatGpt .tx{max-width:340px;font-size:15px;color:#222426;line-height:23px}
header .ai-wrap .ai-chat .message-chatGpt .tx strong{color:#00a88b;font-weight:600}
header .ai-wrap .ai-chat .message-chatGpt .tx + .tx{margin-top:12px}
header .ai-wrap .ai-chat .message-chatGpt .tx.ex{position:relative;padding-left:37px;margin-top:8px}
header .ai-wrap .ai-chat .message-chatGpt .tx.ex:before{position:absolute;top:3px;left:0;height:18px;padding:0 6px;border-radius:4px;background-color:#dfe3e8;color:#5c6773;font-size:12px;font-weight:700;line-height:16px;content:'ex'}
header .ai-wrap .ai-chat .btn-wrap{display:flex;margin-top:12px}
header .ai-wrap .ai-chat .btn-wrap a{display:block;padding:0 10px;background-color:#e7f7ee;color:#00a88b;font-size:13px;font-weight:500;line-height:32px;border-radius:6px;}
header .ai-wrap .ai-chat .btn-wrap a span{display:inline-block;margin-top:7px;width:16px;height:16px;background:url('../img/common/ico_link.png') no-repeat;background-size:16px auto;vertical-align:top}
header .ai-wrap .ai-chat .btn-wrap a + a{margin-left:8px}
header .ai-wrap .ai-chat .alert{
    animation-name: alertShow, alertHide;
    animation-delay: 0s, 3s;
    animation-duration: 0.8s, 0.8s;
    animation-fill-mode:forwards;
    position:absolute;bottom:20px;left:50%;transform:translateX(-50%);padding:0 12px 0 8px;width:max-content;font-size:14px;color:#222426;font-weight:500;;line-height:36px;border:1px solid #00a88b;background:#fff;border-radius:8px;box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.08);
}
header .ai-wrap .ai-bottom-area{position:absolute;bottom:0;left:0;right:0;padding:16px;box-shadow: 0 -4px 8px 0 rgba(0, 0, 0, 0.04);}
header .ai-wrap .ai-text-wrap{position:relative;padding:12px 12px 12px 24px;height:56px;border-radius:32px;background-color: #f7f7f7;}
header .ai-wrap .ai-text-wrap .file-add{width:32px;height:32px;border-radius:50%;background:url('../img/common/ico_plus.png') no-repeat 50% 50% #fff;background-size:12px auto}
header .ai-wrap .ai-text-wrap input{height:32px;line-height:32px;width:calc(100% - 80px);border:0;font-size:16px;font-weight:500;color:#222426}
header .ai-wrap .ai-text-wrap input:placeholder-shown + button{background-image:url('../img/common/ico_enter.png')}
header .ai-wrap .ai-text-wrap input:placeholder-shown + button + button{display:none}
header .ai-wrap .ai-text-wrap input::placeholder{color:#919191;font-weight:400;font-size:16px;}
header .ai-wrap .ai-text-wrap .btn-enter{right:12px;width:32px;height:32px;background:url('../img/common/ico_enter_on.png') no-repeat;background-size:32px auto}
header .ai-wrap .ai-text-wrap .btn-remove{right:56px;width:24px;height:24px;background:url('../img/common/ico_close02.png') no-repeat;background-size:24px auto}
header .ai-wrap .ai-text-wrap .btn-enter,
header .ai-wrap .ai-text-wrap .btn-remove{position:absolute;overflow:hidden;top:50%;transform:translateY(-50%);text-indent:-9999px;}
@keyframes alertShow {
    from{opacity:0;display:none}
    to{opacity:1;display:block}
}
@keyframes alertHide {
    from{opacity:1;display:block}
    to{opacity:0;display:none}
}

