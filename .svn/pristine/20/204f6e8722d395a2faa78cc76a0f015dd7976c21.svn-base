header .ai-wrap{display:none;position:absolute;top:67px;right:-40px;border-radius:16px;width:480px;height:calc(100vh - 180px);box-shadow: 0 4px 12px 0 rgba(0, 0, 0, 0.12);border: solid 1px #dfe3e8;background-color: #fff;transition:all 0.3s;}

header .h-search-ai.on .ai-wrap{display:block;}
header .ai-wrap:after{border-bottom: 10px solid #fff;border-left: 6px solid transparent;border-right: 6px solid transparent;height: 0;right:57px;position: absolute;top: -9px;width: 0; content:'';}
header .ai-wrap:before{border-bottom: 10px solid #dfe3e8;border-left: 6px solid #fff;border-right: 6px solid #fff;height: 0;right:57px;position: absolute;top: -11px;width: 0; content:'';}
