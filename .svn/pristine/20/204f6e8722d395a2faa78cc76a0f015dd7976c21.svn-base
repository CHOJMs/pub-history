/* 프로세스단계 */
.sub_process {}
.sub_process .contents {display: flex; flex-wrap: wrap; flex-direction: column; justify-content: space-between; min-height: calc(100vh - 100px);}
.sub_process .process_wrap + .btn_fixed {margin-left: 460px; margin-top: 80px; max-width: 520px; width: 100%; height: auto; position:relative; bottom: 0; border-top:0 solid #6d6e70; /*background: #fff;*/ transition: all 0.5s;}
.sub_process .process_wrap + .btn_fixed.mt0 { margin-top: 80px !important;}
.sub_process .process_wrap {padding: 15.6rem 0 0 46rem; position: relative; top: 0; bottom: 0; width: 100%; transition: all .5s;}

.sub_process .process_wrap::after {content: ""; display: block; position: fixed; right: 60px; bottom: 60px; width: 420px; height: 420px; background:url('../img/img_progress.png') center center / cover no-repeat;}
.sub_process .process_wrap.fin_cen_box::after {background: none;}
.sub_process .process_wrap.fin_cen_box1::after {background: none;}

.sub_process .process_wrap.no_btn {bottom: 0; height: inherit;}
.pro_box {width: 52rem;}
.pro_txt_box {padding-bottom: 4.0rem;}
.pro_txt_box .tit_info {display: flex; justify-content: space-between; align-items: center; font-size: 3.2rem; line-height: 4.2rem; font-weight: 700; width: 80rem; letter-spacing: -0.05rem;}
