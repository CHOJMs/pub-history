/* PC1366 View */
@media only screen and  (max-width: 1366px) {
    #header.active {height: 930px; max-height: 100%; overflow-y: scroll;}
    .header-inner .flex-list {flex-direction: column;}
    .header-inner .flex-list > div + div {margin-top: 15px;}
