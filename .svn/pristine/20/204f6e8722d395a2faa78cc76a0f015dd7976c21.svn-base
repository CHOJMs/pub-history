    /* 모바일 스크롤 화살표 */
    .sub_process .scroll-arrow {display: unset; opacity: 0; visibility: hidden; position: fixed; left: 50%; bottom: 0; width: 100%; height: 48px; transform: translateX(-50%); background: #fff; font-size: 0;}
    .sub_process.more .scroll-arrow {opacity: 1; visibility: visible; transition: all 0.3s;}
    .scroll-arrow {}
    .scroll-arrow .arrow {display: flex; justify-content: center; position:absolute; top: 50%; left: 50%; transform: translate(-50%, -50%); width:38px;height:22px;overflow: hidden}
    .scroll-arrow .arrow-img{position:absolute;opacity: 0;width:19px;height: 8px;background: url("../img/scroll_arrow.png") no-repeat center top / cover;}
    .scroll-arrow .arrow-img:nth-child(1){
        animation: arrow 1.4s 0s linear infinite ;
    }
    .scroll-arrow .arrow-img:nth-child(2) {
        animation: arrow 1.4s 0.8s linear infinite;
    }
    @keyframes arrow {
        0% {transform: translate(0px, 1px);opacity: 0;}
        50% {transform: translate(0px, 9px);opacity: 1;}
        100% {transform: translate(0px, 18px);opacity: 0;}
    }

