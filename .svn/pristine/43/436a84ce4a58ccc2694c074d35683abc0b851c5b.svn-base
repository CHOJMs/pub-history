/* 토글 체크박스 */
.toggle-box{position: relative;}
.toggle-box input{position: absolute;top: 0;left: 0;}
.toggle-box label{display: block;position: relative;width: 46px;height: 28px;background: #dfe3e8;border-radius: 60px;transition: .4s;cursor: pointer;}
.toggle-box label:after{position: absolute;left: 2px;top: 50%;width: 24px;height: 24px;border-radius: 100%;background-color: #fff;transform: translateY(-50%);transition: all 0.4s;box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.16);content:''}
/* .toggle-box label:before{content: "거부";position: absolute;left: 34px;top: 50%;transform: translateY(-50%);transition: all 0.4s;font-size: 12px;font-weight:600;color:#5c6773;} */
.toggle-box input:checked + label{background:#00a88b}
.toggle-box input:checked + label:after{left: calc(100% - 26.5px);}
/* .toggle-box input:checked + label:before{content: "동의";color: #fff;left: 10px;} */

