/* 주소 : 2줄인 경우 */
.com-inp .add-tit {font-size: 1.6rem; color: rgb(92, 103, 115); width: 10rem; text-align: left; position: absolute; left: 3.2rem; top: 21px; z-index: 2;}
.com-inp .add-txt {width: 100%; height: inherit; font-size: 1.8rem; font-weight: 500; padding: 1.8rem 5.5rem 1.7rem 16.8rem; border: 1px solid rgb(223, 227, 232); border-radius: 0.8rem; line-height: 22px; min-height: 64px; color: #222426; word-break: break-all; word-wrap: break-word;}
.com-inp .add-txt:focus, .com-inp .add-txt:hover {color: rgb(34, 36, 38); border: 1px solid rgb(34, 36, 38); transition: all .5s;}
.com-inp .add-txt.ico-arrow::after, .com-inp .hei-inh.ico-arrow::after {background-image: none;}
.com-inp textarea.add-txt { color: #222426; padding: 15px 3rem 2px 11rem; line-height: 20px; min-height: inherit; overflow: hidden;}

.com-inp button.btn-arrow {font-size: 0; width: 2.4rem; height: 2.4rem; position: absolute; right: 3rem; top: 50%; background: url('../img/ui_common/ico_arrow02_pc.png') no-repeat; background-size: 2.4rem; transform: translateY(-50%);}

.dash {position: relative !important; flex: none !important; width: 24px; text-align: center; color: #fff; margin-right: -23px;}
.dash::before {content: ''; display: block; position: absolute; left: 50%; top: 50%; width: 5px; height: 1px; margin: 1px 0 0 -2px; background: #444;}
.text_data .dash::before {display: none;}

/* 폼객체 : 셀렉트박스 추가 */
.select-area {position: relative; width: 100%;}
.select-area a.sel-btn {display: block; height: 48px; margin: 0; padding: 0; border: none;}
.select-area a.sel-btn input {cursor: pointer;}
.select-area a.sel-btn::after {content: '선택'; font-size: 0; width: 2.4rem; height: 2.4rem; position: absolute; right: .8rem; top: 1rem; background: url('../img/ui_common/ico_select_pc.png') no-repeat; background-size: 2.4rem; transition: 0.3s;}
.select-area a.active::after {transform: rotate(-180deg);}
.select-area a.sel-btn.active + .option-list {display: block; z-index: 3;}
.select-area a::after {content: '선택'; font-size: 0; width: 2.4rem; height: 2.4rem; position: absolute; right: .8rem; top: 1rem; background: url('../img/ui_common/ico_select_pc.png') no-repeat; background-size: 2.4rem; transition: 0.3s;}

/* 폼객체 : selectbox 리스트형 */
.option-list {position: absolute; width: 100%; border-radius: .8rem; background: #fff; border: 1px solid #dfe3e8; overflow-y: auto; margin-top: 1.7rem; z-index: 2; max-height: 180px; display: none;}
.option-list li {padding-top:0;}
.option-list li a {display: block; position: relative; height: 4rem; line-height: 4rem; padding: 0 3.2rem; font-size: 1.6rem;}
.option-list li a:hover, .option-list li.active a {background: rgb(241, 245, 250); color: rgb(21, 113, 235);}
.option-list li.active a::after {content: ''; width: 1.6rem; height: 1.6rem; position: absolute; right: 2rem; top: 1rem; background: url('../img/ui_common/ico_select_chk.png') no-repeat; background-size: 1.6rem;}
.option-list li a::after {background-image: none;}


/* 폼객체 : 역순정렬 */
.form-list {position: relative; display: table; width: 100%; transition: all .4s; visibility: visible;}

/* 검색영역 */
.search-box {background: rgb(247, 247, 247); border-radius: .8rem; padding: 0 2.4rem; margin-bottom: 4.8rem;}
.search-box .sch-cnt {display: flex; align-items: center;}
.search-box .sch-cnt input {border-bottom: none; font-size: 1.8rem !important; height: 6.4rem !important; font-weight: 500; color: rgb(34, 36, 38);}
.search-box .sch-cnt .btn-del {font-size: 0; width: 2.9rem; height: 2.4rem; margin-right: .7rem; background: url('../img/ui_common/ico_del.png') no-repeat; background-size: 24px auto;}

.sch-info {margin-top: -24px;}
.sch-info .s-txt {margin-bottom: 1.6rem; font-weight: 700; color: rgb(34, 36, 38); font-size: 1.6rem;}
.sch-info li {margin-bottom: .8rem; font-size: 1.6rem; line-height: 20px;}
.sch-info li span {color: rgb(145, 145, 145); padding-left: .8rem;}

/* 검색결과 */
.search-res-box .add-tit {font-size: 1.8rem; font-weight: 700;  margin-bottom: 1.6rem;}
.search-res-box .r-tit {font-size: 1.6rem; margin-bottom: 1.6rem;}
.search-res-box li {margin-bottom: 1.2rem;}
.search-res-box li a {display: block; font-size: 1.4rem; line-height: 2.2rem; color: rgb(34, 36, 38); font-weight: 500; background: #fff; border-radius: .8rem; padding: 1.6rem; word-break: break-all;}
.search-res-box li a p {font-weight: 400; color: rgb(112, 116, 120);}
.search-res-box .add-nodata {text-align: center; font-size: 1.6rem; color: rgb(92, 103, 115); padding-top: 2.4rem; line-height: 2.4rem;}

/* 상세주소 */
.add-info {background: rgb(245, 245, 245); padding: 1.6rem; border-radius: .8rem; font-size: 1.4rem; font-weight: 500; margin-bottom: 2.4rem; line-height: 2.2rem;}
.add-info p {font-weight: 400; color: rgb(112, 116, 120);}

/* 약관동의-팝업 테이블 */
.viewType.tbl-view {margin-bottom: 1.6rem;}
.viewType.tbl-view th {text-align: left;}
.viewType td .desc {font-size: 1.2rem; padding-top: .8rem;}
.viewType tr.sub th {font-size: 1.3rem;}

.viewType th {border-bottom:.1rem solid rgb(235, 236, 238); font-size: 1.4rem;}
.viewType td {border-bottom:.1rem solid rgb(235, 236, 238); line-height: 1.57; font-size: 1.4rem;}

.dataType td {border-bottom: .1rem solid rgb(235, 236, 238);}
.dataType th {font-size: 1.4rem;}

/* 주소검색 */
.post-box {padding-top: 10px;}
.post-box li {position: relative; margin-bottom: 3.2rem;}
.post-box li .q {position: relative;}
.post-box li .q a {display: block;}
.post-box li .q a .post-tit {font-size: 1.8rem; font-weight: 700; margin-bottom: 1.6rem; color: rgb(34, 36, 38);}
.post-box li .q a .post-tit span {font-size: 1.4rem; font-weight: 400; color: rgb(112, 116, 120); margin-left: .8rem;}
.post-box li .q a .ico-arrow {position: absolute; right: 0; top: .2rem; width: 2.4rem; height: 2.4rem; font-size: 0; background: url('../img/ui_common/ico_post_dropdown.png') no-repeat; background-size: 2.4rem; transition: 0.3s;}
.post-box li.show .q a .ico-arrow {background-size: 2.4rem; transform: rotate(-180deg);}
.post-box li .a {display: none;}
.post-box .com-inp {margin-top: .4rem;}
.post-box .com-inp label {left: 2.4rem;}
.post-box .com-inp input {padding: 1.7rem 2.4rem 1.6rem 11.8rem !important; font-weight: 400;}
.post-box .btn-cont button {width: 100%; height: 5.6rem; color: #fff; font-size: 1.8rem; font-weight: 500; border-radius: 0.8rem; margin-top: .8rem; background: rgb(21, 113, 235);}
.post-box .btn-cont.gray button {background: rgb(188, 192, 197);}

/* 소비자구분 약관 */
.form-chk-list li {margin-bottom: 1.2rem;}
.form-chk-list li:last-child {margin-bottom: 0rem;}
.form-chk-list li.active .agree-box { border: 1px solid #00a88b}
.form-chk-list li .agree-box { border: 1px solid rgb(223, 227, 232); border-radius: .8rem;}
.form-chk-list li .agree-box .chk:checked+.label{color:#00a88b !important;}
/* .form-chk-list li .agree-box .chk:checked+.label:after{border: 1px solid rgb(21, 113, 235); background-color: rgb(21, 113, 235);} */
.form-chk-list li .agree-box .chk.chk_1:checked+.label:after{border: none; background-color: transparent;}
.form-chk-list li .agree-total-box.chk-line {border-bottom: 1px solid rgb(223, 227, 232); border-radius: 0; background: transparent; margin-bottom: 1.5rem;}
.form-chk-list li .agree-total-box {background: #fff; margin-bottom: 0;}
.form-chk-list li .agree-box .checkbox label {font-size: 1.8rem; color:rgb(92, 103, 115) !important; padding-right: 0; line-height: 2.8rem; word-break: keep-all; word-wrap: break-word;}
.form-chk-list li .agree-box .checkbox {margin-right: 1.6rem;}
.form-chk-list li .tooltip {margin-top: -5px;}
.form-chk-list li .agree-box .agree-total-box .checkbox:last-child {margin-bottom: 0;}
.form-chk-list li .agree-box .checkbox:last-child {margin-bottom: 3.2rem;}

