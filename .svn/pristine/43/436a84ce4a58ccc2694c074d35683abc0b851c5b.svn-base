@charset "utf-8";

/* ----------------------------------------------------------
2022-09-30 신규
---------------------------------------------------------- */

/* etc */
.mob {display: none;}
.pc {display: block;}

/* ----------------------------------------------------------
	버튼
---------------------------------------------------------- */
/* 아이콘 + 버튼 */
 .btn-clear {position: absolute; width: 2.4rem; height: 2.4rem; right: 0; top: 1.2rem; background: url('../img/ui_common/ico_del.png') no-repeat; background-size: 24px auto; font-size: 0;}

/* ----------------------------------------------------------
	폼객체
---------------------------------------------------------- */

/* 패딩 */
.pb140 {padding-bottom: 10rem !important;}

/* 마진 */
.m0 {margin: 0rem !important;}
.mt4 {margin-top: .4rem !important;}

/* 폼객체 : 공통 */
.inp-data input, .select-area input {font-size: 2rem !important; color: rgb(34, 36, 38) !important; font-weight: 500; height: 4.8rem !important; font-family: "pretendard";}
/*.inp-data input[type="password"] {border-bottom: transparent; font-size: 3rem !important; padding-bottom: 10px;}
.inp-data input[type="password"]::placeholder {font-size: 2rem;}*/
button {font-family: "pretendard";}

input::-ms-input-placeholder,
input::-webkit-input-placeholder,
input::-moz-placeholder,
input::-moz-placeholder {color: #717171; font-weight: 500; font-family: "pretendard";}
input::placeholder {color: #717171 !important; font-weight: 500; font-family: "pretendard";}

/* 자동완성배경 */
input:-webkit-autofill { -webkit-box-shadow: 0 0 0 31px #fff inset ; -webkit-text-fill-color: #000; }
input:-webkit-autofill, input:-webkit-autofill:hover, input:-webkit-autofill:focus, input:-webkit-autofill:active { transition: background-color 5000s ease-in-out 0s; }

textarea {font-family: "pretendard"; resize: none; overflow-y: auto; vertical-align: top;}
textarea::placeholder {color: #919191;}
textarea::-webkit-scrollbar {width: 12px;}
textarea::-webkit-scrollbar-track {border-radius: 0px; background: transparent;}
textarea::-webkit-scrollbar-thumb {border-radius: 30px; background: #d3d7dd; border: 3px solid #ffffff;}

 /* 검색 - 아이콘 */
 .btn-search {font-size: 0; width: 2.4rem; height: 2.4rem; background: url('../img/ui_common/ico_search.png') no-repeat; background-size: 2.4rem;}

/* 폼객체 : selectbox 박스형 */
.form-iem h2 {font-size: 1.8rem; font-weight: 600; line-height: 1.5; padding-bottom: 1.6rem; margin-top: 4rem;}
.form-iem .com-inp:first-child {margin-top: 0rem;}
.form-iem.mrg-t { margin-top: 4rem;}
.com-inp {position: relative; margin-top: .8rem;}
h2 + .com-inp {margin-top: 0;}
.com-inp label {font-size: 1.6rem; color: rgb(92, 103, 115); width: 11rem; text-align: left; position: absolute; left: 3.2rem; top: 50%; transform: translateY(-50%); z-index: 1; padding-right: 6px; word-break: keep-all; word-wrap: break-word; font-weight: 400; line-height: 18px;}
.com-inp label em {color: #f75248; margin-left: 4px; font-size: 1.6rem;}
.com-inp label em span {font-size: 0;}
.com-inp input {font-size: 1.8rem !important; height: 64px !important; line-height: 64px; cursor: pointer; border: 1px solid rgb(223, 227, 232); border-radius: 0.8rem; padding: 0 3.2rem 0 16.8rem; background: #fff; font-family: "pretendard"; color: rgb(34, 36, 38); font-weight: 500;letter-spacing:-0.16px;}
.com-inp input:focus, .com-inp input:hover {border: 1px solid rgb(34, 36, 38) !important;}
.com-inp input::placeholder{color:#919191 !important}
.com-inp input.a_r {text-align: right;}
.com-inp input.a_r_txt {text-align: right; padding-right: 65px !important;}
.com-inp input.a_r_txt1 {text-align: right; padding-right: 50px !important;}
.com-inp input.disabled {border: 1px solid rgb(223, 227, 232) !important; background: rgb(245, 245, 245); font-weight: 500;}
.com-inp input.disabled:focus, .com-inp input.disabled:hover {border: 1px solid rgb(223, 227, 232) !important; cursor: default;}
.com-inp a.disabled {border-bottom: 0 !important;}
.com-inp a.ico-arrow.disabled::after,
.com-inp a.ico-select.disabled::after {opacity: .3;}
.com-inp a {position: relative; display: block; width: 100%; height: 6.4rem; margin: 0; padding: 0; border: none;}
.com-inp a::after,
.com-inp a.ico-select::after {content: '선택'; font-size: 0; width: 2.4rem; height: 2.4rem; position: absolute; right: 3.2rem; top: 1.9rem; background: url('../img/ui_common/ico_select_pc.png') no-repeat; background-size: 2.4rem; pointer-events: none;}
.com-inp a.ico-select.calendar::after {background-image:url('../img/ui_common/ico_calendar03.png');}
.com-inp a input {padding: 0 5.5rem 0 16.8rem; line-height: 64px;}
.com-inp a.ico-arrow::after {content: '검색'; font-size: 0; width: 2.4rem; height: 2.4rem; position: absolute; right: 3rem; top: 50%; transform: translateY(-50%);  background: url('../img/ui_common/ico_arrow02_pc.png') no-repeat; background-size: 2.4rem; pointer-events: none;}
.com-inp a.hei-inh {height: auto; color: rgb(34, 36, 38); border: 1px solid rgb(223, 227, 232); border-radius: 0.8rem; padding: 1.8rem 5.5rem 1.7rem 16.8rem; font-size: 18px; font-weight: 500; line-height: 22px;}
.com-inp a.hei-inh:focus, .com-inp a.hei-inh:hover {color: rgb(34, 36, 38); outline: none;}
.com-inp a.hei-inh::after {top: 50% !important; transform: translateY(-50%) !important;}
.com-inp textarea {font-size: 1.8rem !important; height: 160px; width: 100%; cursor: pointer; border: 1px solid rgb(223, 227, 232); border-radius: 0.8rem; padding: 1.9rem 3.2rem 1.9rem 16.8rem; background: #fff; letter-spacing: -0.05em; color: #222426; font-weight: 500;}
.com-inp textarea:focus {border: 1px solid rgb(34, 36, 38) !important; outline: 0;}
.com-inp textarea:focus, .com-inp textarea:hover {border: 1px solid rgb(34, 36, 38) !important;}
.label-textarea {top: 19px !important; transform: inherit !important;}
.label-textarea p {font-size: 12px; color: #919191;}
.popup-contents .com-inp textarea {padding: 19px 32px 19px 128px; min-height: 320px;}

.com-inp span {display:flex;align-items:center;position: absolute; right: 32px; top: 50%; font-size: 1.8rem; font-weight: 500; transform: translateY(-50%); line-height: 64px;}
.com-inp .ico-bank, .ico-bank {position: absolute; left: 16.8rem; top: 2rem; width: 2.4rem; height: 2.4rem; display: inline-block; z-index: 2;}
.com-inp .ico-bank + input {padding-left: 20rem !important;}
.com-inp .btn-search {position: absolute; right: 3.2rem; top: 50%; transform: translateY(-50%);}

.com-inp .inp-area {position: relative; border: 1px solid rgb(223, 227, 232);  border-radius: 0.8rem; display: flex;align-items:center; padding: 0 3.2rem 0 16.8rem;}
.com-inp .inp-area:focus, .com-inp .inp-area:hover {border: 1px solid rgb(34, 36, 38) !important;}
.com-inp .inp-area input {border: none; flex: 1; padding: 1.9rem 0; margin-left: 2.4rem; text-align: center;}
.com-inp .inp-area input:focus, .com-inp .inp-area input:hover {border: none !important;}
.com-inp .inp-area input:first-child {margin-left: 0; text-align: left;}
.com-inp .inp-area .dash {position: relative; top: 0; right: 0; transform: inherit;}
.com-inp .inp-area a {width: inherit; flex: 1;}
.com-inp .inp-area a::after,
.com-inp .inp-area a.ico-select::after {right: 0;}
.com-inp .inp-area .ph-color::placeholder{color:#00a88b !important;background:rgb(240, 249, 244)}

/* 주민번호 */
.com-inp .inp-area.col2 input {flex: initial; width: 63px;}
.com-inp .inp-area.col2 input:nth-child(3) {width: 74px; text-align: left;}

/* 운전면허 */
.com-inp .inp-area.car-num input {flex: inherit; width: 60px;}
.com-inp .inp-area.car-num input:nth-child(3) {width: 25px;}
.com-inp .inp-area.car-num input:nth-child(5) {width: 75px;}
.com-inp .inp-area.car-num input:nth-child(7) {width: 25px;}
.com-inp .inp-area.car-num input.ico-select {background-position: right -4px center;}
.com-inp .inp-area.car-num a {flex: inherit; width: 52px; padding: 0 !important; border: none;}
.com-inp .inp-area.car-num a.ico-select::after {right: -7px !important;}

/* 발급년도 */
.com-inp .inp-area.col3 input {flex: initial;}
.com-inp .inp-area.col3 input:nth-child(1) {width: 42px;}
.com-inp .inp-area.col3 input:nth-child(3) {width: 25px;}
.com-inp .inp-area.col3 input:nth-child(5) {width: 25px;}
.com-inp .inp-area.col3 em{font-size:1.8rem;line-height:24px}

