@charset "UTF-8";

/*subMian.css*/
#header.sub { border-bottom: none;}

.sub-top {padding: 84px 0 60px; display: flex; justify-content: space-between; align-items: center;}
.sub-top.device-pc {display: flex;}
.sub-top .sub-title {display: inline-flex; align-items: center; padding-bottom: 0; margin-top: 0; line-height: 1; font-size: 38px; letter-spacing: -0.02em;}
.sub-top .sub-title span {margin-left: 16px; display: inline-flex; width: 18px; height: 10px; background: url(../img/main/arrow_title.png) center center / 100% no-repeat;}
.sub-top ul {display: flex;}
.sub-top ul li {display: inline-flex; align-items: center;}
.sub-top ul li a {line-height: 1; font-size: 20px; letter-spacing: -0.01em; color: rgb(34, 36, 38);}
.sub-top ul li + li::before {content: ""; display: inline-flex; margin: 0 24px; width: 1px; height: 11px; background: rgb(211, 213, 216);}

.sub-list-wrap {display: flex; flex-wrap: wrap; padding-bottom: 100px;}
.sub-list-wrap div h3 {line-height: 1; margin: 80px 0 24px; font-size: 24px; font-weight: 500; letter-spacing: -0.012em;}
.sub-list-wrap div ul + h3 {margin-top: 80px !important;}
.sub-list-wrap div.first h3, .sub-list-wrap div.left h3 {margin-top: 0px;}
.sub-list-wrap div > div {display: flex; justify-content: space-between; align-items: center; width: 30.33%;}
.sub-list-wrap div > div > ul {margin-bottom: 24px; margin-top: 80px; display: flex;}
.sub-list-wrap div > div > ul li + li::before {content: ""; display: inline-flex; margin: 0 16px; width: 1px; height: 11px; background: rgb(211, 213, 216);}
.sub-list-wrap div > div > ul li a {font-size: 16px; letter-spacing: -0.008em; color: rgb(34, 36, 38);}
.sub-list-wrap > div {width: 100%;}

.sub-list-wrap > div.full {max-width: 100%; width: 100%;}
.sub-list-wrap > div.full > ul {display: flex; flex-wrap: wrap; justify-content: flex-start; align-items: flex-start; margin: 0 0 -30px 0;}
.sub-list-wrap > div.full > ul > li { max-width: 560px; height: 180px;}
.sub-list-wrap > div.full > ul > li div {margin-top: 12px;}
.sub-list-wrap > div.full > ul > li.first {background: rgb(255, 243, 165);}
.sub-list-wrap > div.full > ul > li.first p {line-height: 1.5; font-size: 22px; color: rgb(34, 36, 38);}
.sub-list-wrap > div.full > ul > li:nth-child(n+4) { margin-top: 30px;}

.sub-list-wrap > div > ul > li {position: relative; display: inline-block; vertical-align: top; height: auto; min-height: 240px;margin-right: 60px; width: calc(33.33% - 43px); padding: 30px 40px; background: #fff; box-shadow: 0 0 12px rgba(0, 0, 0, 0.05); border-radius: 8px;}
.sub-list-wrap > div > ul > li:nth-of-type(3), .sub-list-wrap > div > ul > li:nth-of-type(6) { margin-right: 0;}
.sub-list-wrap > div > ul > li:nth-child(n+4) { margin-top: 30px;}
.sub-list-wrap > div > ul > li .box-tit {line-height: 1.5; font-size: 28px; font-weight: bold; letter-spacing: -0.014em; color: rgb(34, 36, 38);}
.sub-list-wrap > div > ul > li .box-txt {margin-top: 8px; line-height: 1.5; font-size: 20px; letter-spacing: -0.01em; color: rgb(112, 116, 120);}
.sub-list-wrap > div > ul > li ul {margin-top: 20px; display: flex;}
.sub-list-wrap > div > ul > li ul li {line-height: 1.5; font-size: 20px; letter-spacing: -0.01em; color: rgb(112, 116, 120);}
.sub-list-wrap > div > ul > li ul li + li {margin-left: 40px;flex-wrap: wrap;}
.sub-list-wrap > div > ul > li ul li strong {margin-left: 5px; margin-right: 5px;font-weight: bold; font-size: 24px; color: rgb(34, 36, 38);}
.sub-list-wrap > div > ul > li ul li span {font-size: 20px;}
.sub-list-wrap > div > ul > li > div {margin-top: 23px; text-align: right; position: absolute; bottom: 30px; right: 40px;}
.sub-list-wrap > div > ul > li > div > a { position: relative; display: block; line-height: 1.5; font-size: 18px; letter-spacing: -0.009em; color: rgb(18, 151, 239); font-weight: 500;}
.sub-list-wrap > div > ul > li > div > a:after { content: ''; width: 1.6rem; height: 1.6rem; display: inline-block; position: absolute; top: 0.5rem; background:url('../img/ico_arrow01.png') no-repeat right 0 / 1.6rem 1.6rem;}
.sub-list-wrap > div > ul > li > div.btn { position: absolute; right: 0; top: 0; text-align: center; margin-top: 0; bottom: initial;}
.sub-list-wrap > div > ul > li > div.btn a { display: flex; width: 88px; height: 56px; font-size: 16px; color: rgb(255, 243, 165); align-items: center; justify-content: center; border-radius: 0 8px 0 8px; background: rgb(34, 36, 38);}
.sub-list-wrap > div > ul > li > div.btn a::after {display: none;}

.device-pc {display: block;}
.device-mobile {display: none;}

@media screen and (max-width: 1600px){

}

@media screen and (max-width: 1520px){
    .sub-list-wrap > div > ul > li { width: calc(50% - 32px);}
    .sub-list-wrap > div > ul > li:nth-child(even) { margin-right: 0;}
    .sub-list-wrap > div > ul > li:nth-of-type(3) { margin-right: 60px;}
    .sub-list-wrap > div > ul > li:nth-child(n+3) { margin-top: 30px;}
    /*.sub-list-wrap > div > ul > li:nth-child(-n+2) { margin-top: 16px;}
    .sub-list-wrap > div > ul > li:nth-child(n+2) {margin-top: 30px;}*/
    .sub-list-wrap div > div { width: calc(50% - 30px);}

    .sub-list-wrap > div > ul > li:first-child { margin-top: 0;}

}

@media screen and (max-width: 1280px){
    .wrap > div > ul > li:nth-child(even) { margin-right: 0;}
}

@media screen and (max-width: 1024px){
    .device-pc {display: none;}
    .device-mobile {display: block;}
    .sub-line {display: none;}

    .container.bg_gray {padding: 70px 10px 100px; min-height: 100vh;}

    .sub-top {padding: 0 0;}
    .sub-top ul.device-pc {display: none;}
    .sub-top h2.sub-title {position: fixed; left: 50%; top: 17px; font-size: 16px; transform: translateX(-50%); z-index: 99; display: none;}
    .sub-top h2.sub-title span {display: none;}

    .sub-top + .swipe_tab_box {padding: 0 10px;}

    .sub-list-wrap {padding-bottom: 0;}
    .sub-list-wrap div h3 {display: none;}
    .sub-list-wrap div:first-of-type h3 + ul { margin-top: 0px;}
    .sub-list-wrap > div {max-width: 100%; width: 100%;}
    .sub-list-wrap > div.right {margin-top: 0;}
    .sub-list-wrap > div + div {margin-left: 0;}
    .sub-list-wrap > div > ul {display: none;}
    .sub-list-wrap > div > ul.active {display: block;}
    .sub-list-wrap > div > ul + div {margin-top: 0;}
    .sub-list-wrap > div > div > ul {display: none;}
    .sub-list-wrap > div > ul > li {position: relative; padding: 24px 30px; width: 100%; height: auto; min-height: auto;margin-top: 16px; margin-right: 0px !important;}
    .sub-list-wrap > div > ul > li .box-tit {font-size: 18px;}
    .sub-list-wrap > div > ul > li .box-txt {margin-top: 6px; font-size: 15px;}
    .sub-list-wrap > div > ul > li ul {margin-top: 16px;margin-bottom: 25px;}
    .sub-list-wrap > div > ul > li ul li {display: flex; align-items: flex-start; font-size: 13px;line-height: 29px;}
    .sub-list-wrap > div > ul > li ul li strong {font-size: 18px;line-height: 1.5;}
    .sub-list-wrap > div > ul > li ul li strong span {font-size: 14px;}
    .sub-list-wrap > div > ul > li > div {margin-top: 0; position: absolute; right: 30px; bottom: 25px;}
    .sub-list-wrap > div > ul > li > div a {font-size: 13px;}
    .sub-list-wrap > div > ul > li > div > a:after { top: 0.2rem;}
    .sub-list-wrap > div > ul > li > div.btn a { width: 72px; height: 40px; font-size: 13px;}
    .sub-list-wrap > div > ul > li:nth-child(n+2) { margin-top: 16px;}

    .sub-list-wrap > div.full > ul {margin-bottom: -16px;}
    .sub-list-wrap > div.full > ul > li {max-width: 100%; width: 100%; height: auto; min-height: 130px;}
    .sub-list-wrap > div.full > ul > li {margin-bottom: 16px; margin-left: 0 !important; margin-top: 0px !important;}
    .sub-list-wrap > div.full > ul > li.first { height: auto;}
    .sub-list-wrap > div.full > ul > li.first p {font-size: 16px; text-align: center;}
    .sub-list-wrap > div.full > ul > li div {margin-top: 0;}

    .util-list {margin-top: 16px; background: #fff; box-shadow: 0 0 12px rgba(0, 0, 0, 0.05); border-radius: 8px;}
    .util-list li + li {border-top: 1px solid rgb(235, 236, 238)}
    .util-list li a {padding: 0 20px; width: 100%; height: 56px; display: inline-flex; justify-content: space-between; align-items: center; line-height: 1; font-size: 16px; font-weight: bold; color: rgb(34, 36, 38);}
    .util-list li a::after {content: ""; display: inline-flex; width: 16px; height: 16px; transform: rotate(-90deg); background: url(../img/ico_menu_close.png) center center / cover no-repeat;}

}