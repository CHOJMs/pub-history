/* 정렬 */
.ac {text-align:center;}
.ar {text-align:right;}

/* icon */
.dot {position:relative; margin-bottom:10px; padding-left:10px; line-height: 22px}
.dot:before {content:''; position:absolute; left:0px; top:10px; display:block; width:3px; height:3px; margin-top:-2px; background:#666; border-radius:100%; }

.caution {position:relative; padding-left:15px !important; color:#333; line-height:18px; font-size:14px; word-wrap:break-word; word-break:keep-all;}
.caution:before { content:'※'; position:absolute; left:0; top:0;}

/* bg */
.bg_gray_box {background:#f6f6f6; padding:20px 20px 20px 20px;}

.readonly {background:#f2f2f2 !important; color:#000 !important;}