<script type="text/javascript">

	$(document).ready(function(){

	});


	function goInsert(){
		if(validate()){

			var frm  = document.frm;

			frm.METHOD.value = "AdminMtCodeIns";
			frm.action = "/admin/AdminCodeAction.do";
			EncForm(frm);
			frm.submit();
		}
	}


	function validate(){
		var CODE1 = $("input[name='CODE1']").val();
		var CODE_NAME1 = $("input[name='CODE_NAME1']").val();
		var CODE_NAME_ENG1 = $("input[name='CODE_NAME_ENG1']").val();
		var UP_CODE1 = $("input[name='UP_CODE1']").val();
		var UP_CODE_NAME1 = $("input[name='UP_CODE_NAME1']").val();
		var DIVISION1 = $("input[name='DIVISION1']").val();

		if( CODE1 == '' ){
			alert("코드를 입력하세요.");
			$("input[name='CODE1']").focus();
			return false;
		}

		if(CODE_NAME1 == ''){
			alert("코드명을 입력하세요.");
			$("input[name='CODE_NAME1']").focus();
			return false;
		}

		if( CODE_NAME_ENG1 == '' ){
			alert("코드영문명을 입력하세요.");
			$("input[name='CODE_NAME_ENG1']").focus();
			return false;
		}

		if( UP_CODE1 == '' ){
			alert("상위코드를 입력하세요.");
			$("input[name='UP_CODE1']").focus();
			return false;
		}

		if( UP_CODE_NAME1 == '' ){
			alert("상위코드명을 입력하세요.");
			$("input[name='UP_CODE_NAME1']").focus();
			return false;
		}

		if( DIVISION1 == '' ){
			alert("구분코드를 입력하세요.");
			$("input[name='DIVISION1']").focus();
			return false;
		}

		return true;	
	}

	// 목록
	function goSearch(){

		var f = document.frm;
		f.METHOD.value = "AdminMtCodeList";
		f.action = "/admin/AdminCodeAction.do";
		EncForm(f);
		f.submit();
	}

</script>
</head>




