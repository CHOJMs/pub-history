<script>
    $(document).ready(function(){
        $('footer').load('/web/renewal/footer_inc.html');

		//pc 메인 상단 배너 play, pause 버튼
		$('.main-rolling-wrap .controler .pause').click(function(){
			$(this).siblings('.play').css('visibility', 'visible');
			$(this).css('visibility', 'hidden');
		});
		$('.main-rolling-wrap .controler .play').click(function(){
			$(this).siblings('.pause').css('visibility', 'visible');
			$(this).css('visibility', 'hidden');
		});

		//mobile 메인 상단 카드배너 play, pause 버튼
		$('.stack-slider-wrap .controler .pause').click(function(){
			$(this).siblings('.play').css('visibility', 'visible');
			$(this).css('visibility', 'hidden');
		});
		$('.stack-slider-wrap .controler .play').click(function(){
			$(this).siblings('.pause').css('visibility', 'visible');
			$(this).css('visibility', 'hidden');
		});
    }); 
</script>
