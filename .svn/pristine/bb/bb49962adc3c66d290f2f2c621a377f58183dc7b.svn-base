@charset'utf-8';


/* === list === */
.list-dot {}
.list-dot li {position:relative; padding-bottom:10px; padding-left:10px; background-image:none !important; text-indent:0px !important; line-height: 22px}
.list-dot li:last-child {padding-bottom:0;}
.list-dot li:before {content:''; position:absolute; left:0px; top:10px; display:block; width:3px; height:3px; margin-top:-2px; background:#666; border-radius:100%; }
.list-dot li.none:before {content:''; position:absolute; left:0px; top:10px; display:block; width:0; height:0; margin-top:0px; background:none; border-radius:0; }
.list-dot.inline li {display:block; padding-bottom:5px; width:100% !important;}
.list-dot.inline li:last-child {padding-bottom:0;}

.list-dot dd {position:relative; padding-bottom:10px; padding-left:10px; line-height: 22px;}
.list-dot dd:last-child {padding-bottom:0;}
.list-dot dd:before {content:''; position:absolute; left:0px; top:10px; display:block; width:3px; height:3px; margin-top:-2px; background:#666; border-radius:100%; }
.list-dot dd.none:before {content:''; position:absolute; left:0px; top:10px; display:block; width:0; height:0; margin-top:0px; background:none; border-radius:0; }
.list-dot dd ul {margin-left:-10px;}

.list-dash {}
.list-dash li {position:relative; padding-bottom:10px; padding-left:9px !important; text-indent:0px !important; line-height: 22px; background-image:none !important;}
.list-dash li:last-child {padding-bottom:0;}
.list-dash li:before {content:''; display:block; position:absolute; left:0; top:10px; width:5px; height:1px; margin:0; padding:0; font-size:0; line-height:0; background:#666; }

.list-normal li {padding-bottom:10px; padding-left:0 !important; background-image:none !important;}
.list-normal li:first-child{padding-top:5px;}
.list-normal li:last-child {padding-bottom:0;}
.list-normal li:before {width:0; height:0;}

.list-number li {font-size:14px; color:#000; font-weight:normal; padding-bottom:10px; line-height:22px; padding-left:18px; text-indent:-18px;}
.scroll_box .box_form .list-number li {background-image:none; padding-left:18px;}

.dot {position:relative; margin-bottom:10px; padding-left:10px; line-height: 22px}
.dot:before {content:''; position:absolute; left:0px; top:10px; display:block; width:3px; height:3px; margin-top:-2px; background:#666; border-radius:100%; }

.dot_gray {position:relative; margin-bottom:10px; padding-left:12px; line-height: 22px}
.dot_gray:before {content:''; position:absolute; left:0px; top:8px; display:block; width:5px; height:5px; margin-top:0px; background:#c8c8c8; border-radius:100%; }

.dash_bar {position:relative; padding-bottom:10px; padding-left:9px !important; text-indent:0px !important; line-height: 22px}
.dash_bar:before {content:''; display:block; position:absolute; left:0; top:11px; width:5px; height:1px; margin:0; padding:0; font-size:0; line-height:0; background:#666; }

.icon-info {padding-left:20px; padding-bottom:3px; color:#333; background:url("../img/common/ico_caution_gray.png") 0 2px no-repeat; background-size:16px 16px; word-break:keep-all; word-wrap:break-word;}
.icon-info-box {padding-left:5px;}
/*.icon-info-box .icon-info {background-position:0 0;}*/
h2 .icon-info-box {padding-left:8px; position:inherit; top:4px; left:inherit; right:inherit; color:inherit; font-size:inherit;}
h2 .icon-info-box .icon-info {width:17px; height:20px; display:inline-block; padding:0;}

.scroll_box .box_form .list-dot li {background-image:none !important;}
.agree_box dd.dash_bar {margin-top:0px;}
.agree_box dd ul.list-dash, .agree_box dd ul.list-dot {margin-left:-5px;}

.agree_box p.info_txt {font-size:14px; color:#000; padding-bottom:45px; line-height:20px; word-wrap:break-word; word-break:keep-all;}


/* === box-color === */
.red_box {border-top:1px solid #87a2ba; border-bottom:1px solid #87a2ba; padding:15px; color:#000;}
.red_box .tl {font-size:14px; color:#000; padding-bottom:15px; line-height:18px; font-weight:bold;}
.red_box li {line-height:22px; font-size:13px;}
.red_box li:last-child {padding-bottom:0;}
.black_box {border:1px solid #666; background:#fff; padding:15px; color:#000;}
.agree_box .box1 {border:1px solid #b3b3b3; padding:15px 15px 10px 15px;}

.bg_blue {background:#ebf0f6; padding:15px; display:table; width:100%; margin-bottom:10px;}
.bg_blue span {display:table-cell; font-weight:bold; font-size:14px;}
.bg_blue span + span {text-align:right;}


/* === font-size === */
.font10 {font-size:10px;}
.font12 {font-size:12px;}
.font13 {font-size:13px !important;}
.font14 {font-size:14px;}
.font15 {font-size:15px !important;}
.font16 {font-size:16px !important;}
.font17 {font-size:17px !important;}

/* === 정렬 === */
.fl_l {float:left !important;}
.fl_r {float:right !important;}
.a_l {text-align:left !important; padding-left:10px !important;}
.a_r {text-align:right !important; padding-right:10px !important;}
.a_c {text-align:center !important;}

.border_top_line {border-top:1px solid #e8e8e8;}
.border_bottom_line {border-bottom:1px solid #e8e8e8;}
.border_top_none {border-top:none !important;}
.border_bottom_none {border-bottom:none !important;}
.border_color {border-color:#ccc !important;}
.dash_bottom_line {border-bottom:1px dashed #e8e8e8;}

/* === 파일찾기 버튼 ===*/
.file_wrap {width:100%; position:relative; display:flex; margin-top:10px;}
.file_input_textbox {float:left; height:45px !important; border:1px solid #666 !important;}
.file_input_div {float:left; position:relative; width:200px; height:45px; overflow:hidden;}
.file_input_div  a.file_input_btn {margin:0 5px 0 0; background:#000; color:#fff; text-align:center; height:45px; line-height:45px; display:block;}

.file_wrap .form {float:left; flex:1; height:45px !important; border:1px solid #666 !important; margin-right:6px;}
.file_wrap .txt {float:left; flex:1; height:45px !important; border:1px solid #666 !important; margin-right:6px; padding:0px 15px; border-radius:4px; line-height:45px;}
.file_wrap .filebox .btn {padding:13px 0; width:100px; cursor:pointer; border-radius:5px; background:#575c66; border:none; color:#fff !important; font-size:14px;}
.file_wrap .filebox .btn.blue {background:#007bc8;}
.file_wrap input.ui_photo {position: absolute; width:1px; height:1px; padding:0; margin:-1px; overflow:hidden; clip:rect(0,0,0,0); border:0;} /* 파일 필드 숨기기 */
.file_wrap .fileName {float:left; flex:1; height:45px !important; line-height:45px; color:#000; padding:0 10px; border:1px solid #666 !important; margin-right:6px; border-radius:4px; position:relative;}
.file_wrap a.file_delete {width:25px; height:25px; cursor:pointer; background:#fafafa; color:#777; border-radius:4px; border:1px solid #e3e3e3; position:absolute; top:50%; right:8px; top:calc(50% - 12px); text-align:center;}
.file_wrap a.file_delete img {width:14px; height:17px; margin-top:3px; vertical-align:top;}
.box_form dt .filebox {text-align:right; margin-bottom:-4px; margin-top:-32px;}
.box_form dt .filebox .btn {padding:9px 0; width:100px; cursor:pointer; border-radius:5px; background:#575c66; border:none; color:#fff !important; font-size:14px;}
.box_form dt .filebox .btn.blue {background:#007bc8;}

/* === 새로고침버튼 ===*/
.refresh_btn {clear:both; float:right; display:inline-block;}
.refresh_btn a {display:block; text-align:center; font-size:12px; padding:3px 15px 1px; line-height:20px; border-radius:15px; border:1px solid #007bc8;}
.refresh_btn a span {padding-left:20px; background: url(../img/common/ico_refresh01.png) no-repeat 0 50% / 13px auto;}

/* === checkbox === */
.form_checkbox_img {margin-bottom:10px;}
.form_checkbox_img span {display:inline-block; padding-left:25px; padding-top:0px; margin-left:15px; color:#000; font-size:12px; padding-bottom:5px; background:url("../img/common/bg_check_off1.png") no-repeat left top / 16px 16px; line-height:18px;}


.check_txt_list li {font-size:14px; line-height:22px; margin-bottom:14px;}
.check_txt_list li:last-child {padding-bottom:0; margin-bottom:0;}
.check_gray_box ul {width:100%;}
.check_gray_box ul li {font-size:14px; line-height:22px; margin-bottom:7px;}
.check_gray_box ul li:last-child {margin-bottom:0;}
.check_gray_box ul li input:not(.cssbrowser) + label {background-position:left 3px;}
.check_gray_box ul li input:not(.cssbrowser):checked + label {background-position:left 3px;}


/* === 약관팝업 === */
.list_sign_box {}
.list_sign_box dt {font-size:12px; width:100px; color:#000; padding-bottom:7px; display:inline-block;}
.list_sign_box dd {font-size:12px; width: calc(100% - 105px); color:#333; padding-left:0px; padding-bottom:7px; text-indent:0; display:inline-block;}
