	.rent_m_box li:nth-child(3) a {background:url(../img/common/ico_partner_i03.png) no-repeat center 0 / 46px 42px;}
	.rent_m_box li:nth-child(4) a {background:url(../img/common/ico_partner_i04.png) no-repeat center 0 / 46px 42px;}
