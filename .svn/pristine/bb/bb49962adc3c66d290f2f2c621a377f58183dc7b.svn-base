/* 약관 */
.box_txt_list dt {font-size:13px; color:#333; width:110px; display:inline-block; vertical-align:top; padding-top:0px; line-height:18px;}
.box_txt_list dd {font-size:13px; padding-bottom:10px; width: calc(100% - 115px); display:inline-block; margin:0; line-height:18px; text-indent:0;}
.box_txt_list dt.block, .box_txt_list dd.block {display:block; width:100%;}
.box_txt_list dt.dash_bar:before {top:9px;}
