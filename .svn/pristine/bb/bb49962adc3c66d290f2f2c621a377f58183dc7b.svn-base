.box_txt_list1 dd {font-size:13px; width: calc(100% - 115px); color:#333; padding-bottom:10px; display:inline-block; text-align:right; margin:0; line-height:18px; text-indent:0; vertical-align:top;}
.box_txt_list1 dt.block, .box_txt_list1 dd.block {display:block; width:100%;}
