	.land_top .banner_list li:nth-child(3) div {background:url("../img/2020/icon_banner03.png") no-repeat center 63px / 48px 48px #fff;}
	.land_top .btn_box .btn {display:inline-block; padding-left:20px; flex:1 1 0; width:220px; margin:0;}
	.land_top .btn_box .btn:first-child  {padding-left:0px;}
	.land_top .info {color:#b8d1ea; font-size:14px; margin-top:17px; text-align:center;}
