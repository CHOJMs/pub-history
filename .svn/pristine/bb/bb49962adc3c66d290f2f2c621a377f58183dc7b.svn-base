.box_txt_list3 dd:first-of-type {font-size:12px; padding-bottom:0; width: calc(100% - 210px); vertical-align:middle; display:inline-block; margin:0; line-height:22px; text-indent:0;}
.box_txt_list3 dd:last-of-type {width:100px; vertical-align:middle; display:inline-block;}
.box_txt_list3 .btn_wrap button {font-size:12px;}
.box_txt_list.font dt, .box_txt_list.font dd {font-size:14px;}
.box_txt_list dd p.caution {font-size:12px;}

.set_line_height dd {line-height:30px !important;}
.set_line_height dd .btn_wrap.blue1 a {line-height:19px !important; height:29px;}

.box_txt_list dd:last-child, .box_txt_list dt:nth-last-child(2),
.box_txt_list1 dd:last-child, .box_txt_list1 dt:nth-last-child(2),
.box_txt_list2 dd:last-child, .box_txt_list2 dt:nth-last-child(2) {padding-bottom:0; margin-bottom:0;}






