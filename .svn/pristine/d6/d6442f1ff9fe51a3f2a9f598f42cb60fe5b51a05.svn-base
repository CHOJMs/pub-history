/* 로그인 */
/* .wrap.login {background: linear-gradient(205deg, #244b4d 100%, #081c31 -1%); min-height: 100%;} */
.wrap.login {background:linear-gradient(205deg, #244b4d 25%, #081c31 96%); min-height: 100%;}

@media only screen and (max-width: 1280px){

    /* gnb */
    #header.login .logo {font-size: 0; width: 100%;}
    #header.sub h1.logo {display: block;}

    .mobile-btn { right: 78px; left: initial; top: 18px;}
    .mobile-btn ul li.alram a { display: block; position: relative;}
    .mobile-btn ul li.back a { display: none;}

    .wrap.bg_2 { min-height: calc(100% - 117px); background: linear-gradient(to bottom, rgba(255, 255, 255, 0.3), rgba(226, 244, 255, 1));}
    .wrap.bg_1 { min-height: calc(100% - 117px); background: linear-gradient(to bottom, rgba(255, 255, 255, 0.3), rgba(228, 255, 249, 1));}
    .main_wrap { min-height: calc(100% - 64px);}
    .main_wrap .contents { padding: 24px 20px 98px; display: block !important;}

    /* 로그인 */
    .main_wrap p.txt { font-size: 3rem; font-weight: 700; letter-spacing: -0.09rem; transition: all 0.3s;}
    .main_wrap p.txt span {font-size: 2rem;}
    .login_box::before { display: block; content: ""; width: 100%; height: 705px; background:url('../../img/mobile_office/main_img1.png') no-repeat center 50%  / 984px auto; transition: all 0.3s;}
    .login_box .login {margin-top: 2rem;}
    .login_box .login .text_data { margin-top: 0rem;}
    .login_box .login .text_data + .text_data {margin-top: -1px;}
    .login_box .login .btn_box { }
    .login_box .login .btn_box .btn a {border-radius: .4rem; margin-top: 1.6rem;}
    input[type="text"], input[type="password"] { padding-left: 24px !important; color: rgb(153, 153, 153) !important; border: 1px solid rgb(235, 236, 238) !important; border-radius: 1px !important; height: 56px !important;}
    input::-moz-placeholder,
    input::-ms-input-placeholder,
    input::-webkit-input-placeholder,
    input::-moz-placeholder { color: rgb(153, 153, 153) !important; }

    /* idpw */
    .idpw { display: flex; justify-content: space-between; margin-top: 16px;}
    .idpw li:nth-of-type(1) {font-size: 13px; color: rgb(34, 36, 38);}
    .idpw li > .checkbox > .chk+.label { height: 15px; padding-left: 19px; font-size: 13px; line-height: 16px;}
    .idpw li > .checkbox > .chk+.label:after { border-radius: 2px; width: 15px; height: 15px; background-size: 7px auto;}
    .idpw li > .checkbox > .chk:checked+.label:after { background: url('../../img/ico_check01_on.png') no-repeat center center #fff; background-size: 7px auto;}
    .idpw li + li a { font-size: 13px; color: rgb(112, 116, 120);}

    /* 메인 */
    .tag1 { width: 130px; height: 26px; line-height: 26px; padding-left: 10px; margin-bottom: 12px; font-size: 13px; color: #fff; background:url('../../img/mobile_office/bg_tag1.png') no-repeat 0 0  / 130px auto;}
    .tag2 { width: 68.7px; height: 26px; line-height: 26px; padding-left: 10px; margin-bottom: 12px; font-size: 13px; color: #fff; background:url('../../img/mobile_office/bg_tag2.png') no-repeat 0 0  / 68.7px auto;}
    .name {font-size: 30px; letter-spacing: -0.9px; margin-bottom: 24px;}
    .name span {font-weight: 700;}
    .main_box.img1::before { display: block; content: ""; width: 100%; height: 705px; background:url('../../img/mobile_office/main_img2.png') no-repeat center 50%  / 984px auto;}
    .main_box.img2::before { display: block; content: ""; width: 100%; height: 705px; background:url('../../img/mobile_office/main_img3.png') no-repeat center 50%  / 984px auto;}
    .main_box ul { display: flex; flex-wrap: wrap; justify-content: space-between; margin-top: 2.4rem;}
    .main_box li { position: relative; width: 100%; margin-top: 16px; background: #fff; border-radius: 8px; box-shadow: 0 -0.05rem 1rem rgb(0 0 0 / 10%);}
    .main_box li:first-child {margin-top: 0;}
    .main_box li a::after {display: block; content: ""; width: 32px; height: 32px; position: absolute; right: 24px; top: calc(50% - 17px); background:url('../../img/mobile_office/ico_btn.png') no-repeat right 50%  / 32px auto;}
    .main_box li a {display: block; padding: 20px 24px;}
    .main_box li a p.tit { font-size: 16px; font-weight: 700; color: rgb(34, 36, 38); margin-bottom: 4px;}
    .main_box li a p.txt { font-size: 14px; font-weight: 400; padding: 0; margin: 0;}


    #footer {padding: 24px 20px; font-size: 12px; color: rgba(188, 192, 197, 1); background: rgba(34, 36, 38, 1);}
    #footer .footer-logo {padding: 0; margin: 0; margin-bottom: 10px; line-height: 1;}
    #footer address {font-style: normal;}
    #footer p {margin-top: 2px;}
    .footer-logo {width: 120px;}

    /* 
    ------------------------------------------------
    ************** 2024 리뉴얼 *****************
    ------------------------------------------------
    */
    .main-zone {}
    .main-zone .contents {padding: 1.6rem 2rem 4rem;}

    /* 로그인 */
    .login-zone .contents {padding: 16.4rem 3rem 0;}/* 11.6rem */
    .logo-wrap {margin-bottom: 5rem; text-align: center; }
    .logo-target {height: 3.6rem}
    .logo-target img {display: block; width: auto; max-height: 100%; margin: 0 auto;}
    .logo-basic {height: 1.5rem; margin-top: 1.2rem;}
    .logo-basic img {display: block; width: auto; max-height: 100%; margin: 0 auto;}
    .login-area {display: flex; flex-direction: column;}
    .login-area::before {content: ""; display: inline-block; overflow: hidden; aspect-ratio: 315/147; width: 31.5rem; margin: 0 auto; background: url('../../img/mobile_office/img_login@2x.png') no-repeat 0 0;background-size: cover;}
    .login-area .login .text_data { margin-top: 0rem; border-radius: 0.8rem 0.8rem 0 0; overflow: hidden;}
    .login-area .login .text_data + .text_data {border-radius: 0 0 0.8rem 0.8rem;}
    .login-area .login .btn_box .btn a {border-radius: .4rem; margin-top: 1.6rem;}
    .login-area .login .text_data { margin-top: 0rem; border-radius: 0.8rem 0.8rem 0 0;}
    .login-area .login .text_data + .text_data {border-radius: 0 0 0.8rem 0.8rem; border-top: 0.1rem solid rgb(217 217 217);}
    .login-area input[type="text"], 
    .login-area input[type="password"] {background-color: transparent; font-size: 1.6rem; letter-spacing: -0.016rem; padding-left: 2.4rem !important; color: rgb(34 36 38) !important; height: 56px !important; border: none !important;}
    .login-area input::-webkit-input-placeholder,
    .login-area input::placeholder {color: rgb(163 163 163) !important;}
    .login-area .btn_box .btn a {color: #fff; background: linear-gradient(103deg, #43c17d 10%, #12d3a6 95%); border: none; padding: 1.8rem 0; font-size: 1.8rem; letter-spacing: -0.018rem;}

    /* idpw */
    .login-area .login-idpw-list { display: flex; justify-content: space-between; margin: 1.8rem 0 2.6rem;}
    .login-area .login-idpw-list .pw-change {font-size: 1.4rem; color: #fff; letter-spacing: -0.014rem;}
    .login-area .login-chk {overflow:hidden; position:absolute; border:0; background:#fff; margin:0 0 0 0rem; width:1px; height:1px; margin-left: -1px; clip: rect(0,0,0,0);}
    .login-area .login-chk + .label{display:inline-block; position:relative; padding-left: 2.6rem; font-size: 1.4rem; border: none; letter-spacing: -0.014rem; color: #fff; cursor:pointer;}
    .login-area .login-chk +.label:after {content: ""; position: absolute; top:0; left:0; width:2rem; height:2rem; margin-top:0rem; transition: .3s; background:url('../../img/mobile_office/login_chk_off.svg') no-repeat 0 0; background-size: cover; }
    .login-area .login-chk:checked +.label:after {margin-top:0; background: url('../../img/mobile_office/login_chk_on.svg') no-repeat 0 0;}
    
}

@media only screen and (max-width: 1024px){

    /* 아이폰 - 노치 */
    body.ios #header h1 {padding-top: var(--ios); height: calc(var(--ios) + 50px);}

    /* gnb */
    .mobile-btn { right: 68px; left: initial; top: 10px;}
    .main_wrap .contents { padding: 66px 20px 37px; display: block !important;}

    /* 로그인 */
    .main_wrap p.txt { font-size: 1.8rem;}
    .login_box::before { height: 240px; background-size: 335px auto;}

    /* 메인 */
    .name {font-size: 18px; margin-bottom: 24px;}
    .main_box.img1::before { height: 240px; background-size: 335px auto;}
    .main_box.img2::before { height: 240px; background-size: 335px auto;}

    .rent_gnb .mobile-btn{ left: initial;}

    /* 아이폰 - 노치 */
    body.ios #header::after { height: 0;}

}



@media only screen and (min-width:360px){

    .wrap { min-height: calc(100% - 35px);}

}

@media only screen and (min-width:414px){

    .wrap { min-height: calc(100% - 117px);}

}

@media only screen and (max-width:359px){

    .wrap { min-height: calc(100% - -46px);}
    .login_box::before {background-size: 95% auto;}
    .login_box .login { margin-top: 0;}

}