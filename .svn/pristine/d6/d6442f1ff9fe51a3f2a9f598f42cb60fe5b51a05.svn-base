@charset "UTF-8";

.wrap { height: auto;}
img {max-width: 100%;}

/* gnb */
.mobile-btn ul li.back, .mobile-btn ul li.home {display: none;}
.mobile-btn ul li.alram {display: block;}


/* 팝업 - 차량 실사촬영 */
.text-color-box {height: 60px; display: flex; justify-content: center; align-items: center; border-radius: 8px; text-align: center; background: rgba(242, 248, 251, 1);}
.text-color-box p {font-family: 'KOHO', 'Noto Sans KR'; font-size: 18px; font-weight: bold; letter-spacing: -0.09em;}

.tab-box {margin-top: 16px;}
.tab-box ul {display: flex;}
.tab-box ul li {flex: 1;}
.tab-box ul li + li {margin-left: 15px;}
.tab-box ul li a {display: inline-flex; justify-content: center; align-items: center; width: 100%; height: 56px; font-size: 16px; font-weight: 500; color: rgba(34, 36, 38, 1); text-align: center; border-radius: 8px; background: rgba(235, 236, 238, 1);}
.tab-box ul li.active a {color: #fff; background: rgba(18, 151, 239, 1) url(../../img/mobile_office/ico_check.svg) 10% center / 16px no-repeat;}

.photo-wrap {padding-bottom: 16px;} /* 하단 여백 + 버튼 영역 */
.photo-wrap h2 {font-size: 18px;}
.photo-wrap > div > p {font-size: 16px; font-weight: bold; color: rgba(34, 36, 38, 1);}
.photo-wrap > div ul {margin-top: 16px; display: flex;}
.photo-wrap > div ul li {flex: 1;}
.photo-wrap > div ul li + li {margin-left: 15px;}
.photo-wrap > div ul li figure {margin: 0; padding-bottom: 75%; display: flex; justify-content: center; align-items: center; position: relative; width: 100%; height: 0; background: #fff;}
.photo-wrap > div ul li figure > img {position: absolute; left: 50%; top: 50%; transform: translate(-50%, -50%); max-width: calc(100% - 13.8%);}
.photo-wrap > .photo-back ul li:nth-of-type(1) > figure > img {max-width: calc(100% - 27.6%);}
.photo-wrap > .photo-back ul li figure {padding-bottom: 75%;}
.photo-wrap > div ul li figure span {position: absolute; left: 50%; top: 50%; width: 40%; transform: translate(-50%, -50%);}
.photo-wrap > div div {margin-top: 10px;}
.photo-wrap > div div p {font-size: 13px; font-weight: 500; color: rgba(34, 36, 38, 1); letter-spacing: -0.065em;}
.photo-wrap > div div span {font-size: 12px; font-weight: 400; color: rgba(112, 116, 120, 1); letter-spacing: -0.065em;}

.photo-front {margin-top: 24px;}
.photo-back {padding-top: 24px; margin-top: 24px; border-top: 1px solid rgba(221, 221, 221, 1);}

.btn_wrap .reg-btn button {background: rgba(235, 236, 238, 1); border: 1px solid rgba(235, 236, 238, 1); color: rgba(153, 153, 153, 1);}
.btn_wrap .reg-btn.active button {background: rgba(34, 36, 38, 1); border: 1px solid rgba(34, 36, 38, 1);}



/* 2023-11-20 휴대폰본인인증 */
.agree-box .agree-total-box {background: rgb(241 , 245, 250); border-radius: 1.6rem; padding: 0 32px; margin-bottom: 1.2rem;}
.agree-box .agree-total-box .checkbox {margin: 0; }
.agree-box .agree-total-box label {font-size: 1.8rem !important; font-weight: 500 !important; color: rgb(34, 36, 38) !important; padding: 20px 0 20px 32px; height: initial !important;}
.agree-box .agree-total-box .checkbox .chk+.label:after, .agree-box .agree-total-box .checkbox .chk {top:20px;}
.agree-box .agree-total-box + .checkbox {padding-top: 1.2rem;}
.agree-box .agree-total-box .checkbox a.arrow {top: 20px;}
.agree-box .checkbox {position: relative; margin-bottom: 1.6rem; margin-left: 3.2rem; margin-right: 3.2rem;}
.agree-box .checkbox label {font-size: 1.6rem; color: rgb(112, 116, 120); font-weight: 400; width: calc(100% - 30px);}
.agree-box .checkbox label em {color: #f75248; margin-left: 4px; font-size: 1.6rem; display: inline-flex;}
.agree-box .checkbox label em span {font-size: 0;}
.agree-box .checkbox a.arrow {font-size: 0; position: absolute; right: 0; width: 2.4rem; height: 2.4rem;}
.agree-box .checkbox a.arrow::before {content: ''; display: block; width: 2.4rem; height: 2.4rem; background: url('../img/ui_common/ico_arrow.png') no-repeat right center; background-size:2.4rem auto; position: absolute; right: 0; top: 0px;}
.agree-box .agree-total-box .checkbox .wa-arrow {top: 50%; transform: translateY(-50%);}
.agree-box .checkbox .wa-arrow {position: absolute; right: 0; width: 24px; height: 24px;}
.agree-box .checkbox .wa-arrow img.agr-pc {width: 24px; height: 24px; display: block;}
.agree-box .checkbox .wa-arrow img.agr-mob {display: none;}
.agree-box .checkbox ul {margin-top: 1.2rem; margin-left: 3rem;}
.agree-box .checkbox ul li {table-layout: fixed; display: flex; justify-content: space-between; flex-flow: row wrap;}
.agree-box .checkbox ul li .checkbox {width: 50%; margin-left: 0rem; margin-bottom: .6rem; margin-right: 0;}
.agree-box .checkbox ul li .checkbox:last-child {margin-bottom: 0;}
.popup-half .agree-box .agree-total-box {border-radius: 1.6rem;}
.agree-box .checkbox ul li .checkbox .chk.chk_1+.label{color: rgb(92, 103, 115); font-size: 1.4rem;}


.agree-box .list-dot {margin-left: 3.2rem;}
.agree-box .checkbox .chk:checked+.label:after {border: 0.1rem solid #1571eb; background: url(../img/ico_check_on.png) no-repeat center center #1571eb; background-size: 13px 9px;}
.agree-box .checkbox .chk+.label:after{position:absolute; top:0; left:0; width:2.4rem; height:2.4rem; margin-top:0rem; transition: .3s; background:url('../../img/ui_common/ico_check_off.png') no-repeat center center; border:none; background-size:24px 24px;; content: ''}
.agree-box .checkbox .chk:checked+.label:after{margin-top:0; background: url('../../img/ui_common/ico_check_on.png') no-repeat center center; border:none; border-radius: 50%; background-size:24px 24px;}
.agree-box .checkbox .chk.chk_1+.label:after{border:none; background:url('../../img/ui_common/ico_check1_off.png') no-repeat center center; background-color: transparent; background-size: 24px 24px;}
.agree-box .checkbox .chk.chk_1:checked+.label:after{border:none; background: url('../../img/ui_common/ico_check1_on.png') no-repeat center center; background-color: transparent;; background-size:24px 24px;}
.agree-box .checkbox:last-child {margin-bottom: 0;}
.agree-box .checkbox a.arrow:focus {outline: 1px dotted #222426;}

/* 2023-07-20 휴대폰본인인증 */
.popup .agree-box + .h-tit {margin-top: 32px; margin-bottom: 8px; font-size: 15px; color: rgb(34,36,38);}
.time-box {position: relative; width: 100%;}
.time-box .btn-cont {position: absolute; right: 0; top: 3px; display: flex; align-items: center;}
.time-box .btn-cont button {height: 32px; color: rgb(21, 113, 235); font-size: 14px; line-height: 30px; padding: 0 12px; font-weight: 500; border-radius: 4px; border: 1px solid rgb(21, 113, 235);}
.time-box input[type="tel"] {border-bottom: 1px solid #bcc3ce; border-left: 0; border-top: 0; border-right: 0; border-radius: 0;}
.inp-data .time {color: rgb(0, 168, 139); font-size: 16px; margin-right: 12px;}


