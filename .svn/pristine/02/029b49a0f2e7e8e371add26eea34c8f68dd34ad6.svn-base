/* title */
h2 { position: relative; font-size: 2.2rem; line-height: 3rem; text-align: left; padding-bottom: 1rem; margin-top: 5.6rem; color: rgb(34, 36, 38); letter-spacing: -0.05em; font-weight: 700;}
h2 span.font_red {font-size: 2.2rem; padding-left: .4rem;}
h2:first-child {padding-top: 0; margin-top: 0;}
h3 {font-size: 1.8rem; line-height: 2.7rem; margin-top: 2.4rem; margin-bottom: 1rem; color: rgb(34, 36, 38); letter-spacing: -0.01em; font-weight: 700;}
h3 span.font_red {padding-left: .4rem;}
h2 em {position: relative; color:#f75248; font-size:1.8rem; padding-left:.2rem;}
h2 em span {opacity:0; width:1.5rem; height:1.5rem; position:absolute; top:.5rem; left:.5rem;}
h2.h2_line {border-bottom: 1px solid rgb(235, 236, 238); padding-bottom: 1.6rem; margin-bottom: 1.6rem;}
h2 + .h_tit, h2 + h3 {margin-top: 1.4rem;}
.h_tit {position: relative; font-size: 1.8rem; line-height: 2.7rem; margin-bottom: 1rem; color: rgb(34, 36, 38); letter-spacing: -0.05em; font-weight: 700; text-align: left;}
.h_tit em, h3 em {position: relative; color:#f75248; font-size:1.8rem; padding-left:.2rem;}
.h_tit em span, h3 em span {opacity:0; width:1.5rem; height:1.5rem; position:absolute; top:.5rem; left:.5rem;}
.text_data + .h_tit, .textarea_box + .h_tit, .icon_bullet + .h_tit, .btn_box + .h_tit, .text_data + div > .h_tit, .text_data + div + .h_tit, .inline_rchk + .h_tit, .text_data + div, .text_data + div + div  {margin-top: 4rem;}
div + div > .h_tit {margin-top: 4rem;}
.tit_info {display: flex; justify-content: space-between; line-height: 28px; align-items: center;}
.tit_info .txt_num {font-size: 1.3rem; font-weight: 700; color: rgb(72, 94, 116); width: 130px; text-align: right; font-family: Koho; flex-basis: 130px; text-align: right;}
.tit_info .txt_num span {font-size: 3rem; font-weight: 700; color: rgb(34, 36, 38); padding-right: 6px;}
.txt {font-size: 1.7rem; letter-spacing: -0.05em; padding-bottom: 1.6rem; margin-top: -0.4rem; color: rgb(112, 116, 120);}
.txt1 {font-size: 1.4rem; letter-spacing: -0.05em; padding-bottom: 1.6rem; margin-top: -0.4rem; color: rgb(112, 116, 120);}
.txt1.pb40 {padding-bottom: 5.6rem !important;}
.txt1 + .h_tit {margin-top: 0;}
.txt_small {font-size: 1.4rem; letter-spacing: -0.05em; padding-bottom: 3.2rem; margin-top: -0.4rem; color: rgb(112, 116, 120);}
.call {font-size: 1.4rem; margin-top: 1rem;}
.h_tit + .radio_box, .h_tit + .radio_box.text_data {margin-top: 1.6rem;}
.h_tit + .radiobox {margin-top: 1.6rem;}
.h_tit + .txt {margin-bottom: 4rem; padding-bottom: 0;}
.line + h2 {margin-top: 1.6rem;}
.inline_rchk.radio_box.text_data  {display: block;}
.inline_rchk.radio_box.text_data.pc_block li { margin-bottom: 1.6rem; width: 20%;}
.inline_rchk.radio_box.text_data.pc_block li:last-of-type {margin-bottom: 0;}
.text_data + .text_data { margin-top: 1rem;}

/* 블릿 */
.icon_bullet {display: block; position: relative; text-align: left; color:rgb(112, 116, 120); line-height:1.8rem; font-size:1.4rem; letter-spacing: -0.05em; padding-left: 1.4rem; margin-bottom: 1rem; margin-top: 1.6rem; word-wrap:break-word; word-break:keep-all;}
.icon_bullet + .icon_bullet {margin-top: -4px;}
.icon_bullet:before {content: ''; position: absolute; display: inline-block; left: 0; top: .5rem; width: .8rem; height: .8rem; margin-right: .8rem; border: .1rem solid rgb(112,116,120); border-radius: 50%;}
.icon_bullet a {color: rgb(112,116,120); font-size: 1.4rem;}
.icon_bullet.mt-24 {margin-top:-2.8rem; margin-bottom: 8rem;}
.icon_bullet.mt-28 {margin-top:-2.8rem; margin-bottom: 8rem;}

.icon_bullet.font_red:before {border: .1rem solid rgb(255, 36, 36);}
.icon_bullet.font_red a {color: rgb(255, 36, 36);}
.icon_bullet.font_blue:before {border: .1rem solid rgb(71, 179, 251);}

.icon_bullet2 {display: block;position: relative;text-align: left;color:rgb(112, 116, 120);line-height: 2.2rem;font-size: 1.8rem;letter-spacing: -0.1rem;padding-left: 2rem;margin-bottom: 1rem;/* margin-top: 1.6rem; */word-wrap:break-word;word-break:keep-all;}
.icon_bullet2:before {content: '※';position: absolute;left: 0;top: 0;}
.icon_bullet2 + .icon_bullet2 {margin-top: -4px;}
.icon_bullet2 a {color: rgb(112,116,120); font-size: 1.4rem;}
.icon_bullet2.mt-24 {margin-top:-2.8rem; margin-bottom: 8rem;}

.icon_bullet2.font_red:before {}
.icon_bullet2.font_red a {color: rgb(255, 36, 36);}
.icon_bullet2.font_blue:before {border: .1rem solid rgb(71, 179, 251);}

/*달력*/
.calender {position:absolute; right:0; top:calc(50% - 13px); cursor:pointer; text-indent:-5000px; border:none; width:27px; height:27px; background:url("../img/icon_calender.png") no-repeat 0 0 / 100% auto;}


/* icon */
.caution {position:relative; padding-left:1.5rem; color:#333; line-height:1.8rem; font-size:1.4rem; word-wrap:break-word; word-break:keep-all; text-align:left;}
.caution:before { content:'※'; position:absolute; left:0; top:0;}
.icon_acco {display:inline-block; width: 2.4rem; height: 2.4rem; vertical-align: middle; background:url('../img/ico_arrow_off.png') right 0 no-repeat; background-size:2.4rem; text-indent:-5000px; line-height:2.4rem;}
.ico_banner2 {display:inline-block; width: 2.4rem; height: 2.4rem; vertical-align: middle; background:url('../img/ico_banner2.png') right 0 no-repeat; background-size:2.4rem; text-indent:-5000px; line-height:2.4rem;}
.icon_qu {display: inline-block; vertical-align: middle; padding-right: 2.5rem; line-height: 2rem; color:#333; background:url("../img/ico-question.png") right 0 no-repeat; background-size:2rem 2rem; word-break:keep-all; word-wrap:break-word;}
.icon_open {display:inline-block; padding-right:2.5rem; vertical-align:middle; background:url('../img/ico_openwindow.png') right 0 no-repeat; background-size:2.4rem 2.4rem; text-indent:0; line-height:2.4rem;}
.icon_info {padding-left:20px; padding-bottom:3px; color:#333; background:url("../img/ico_caution_gray.png") 0 2px no-repeat; background-size:16px 16px; word-break:keep-all; word-wrap:break-word;}
.icon_info_box {padding-left:5px;}
h2 .icon_info_box {padding-left:8px; position:inherit; top:4px; left:inherit; right:inherit; color:inherit; font-size:inherit;}
h2 .icon_info_box .icon_info {width:17px; height:20px; display:inline-block; padding:0;}

/* box-color */
.black_box {background: rgb(60, 64, 68); border-radius: 1.3rem; padding: 2.4rem; position:relative;}
.blue_box {background: #E9F7FE; border-radius: .8rem; padding: 2rem 2.4rem;}
.blue_box p {font-size: 1.3rem;}
.blue_box p.a_c {font-size: 1.4rem; text-align: center;}
.gray_box {border: .1rem solid #ebecee; border-radius: .8rem; padding: 1.8rem 2rem;}
.white_box {background: #fff; border-radius: .8rem; padding: 3.3rem 4rem; letter-spacing: -0.05em; box-shadow: 0 0 1.2rem rgba(0, 0, 0, 0.05);}
.white_linebox {border-top: 4px solid #fff;}
.t_line_box {border-top: 1px solid rgb(34, 36, 38); padding: 2.4rem 0 4.2rem; margin-top: 1.7rem;}
.t_line_box1 {border-top: 1px solid rgb(211, 213, 216); padding-top: 1.6rem;}
.t_line_radiobox {position: relative;}
.t_line_radiobox span {position: absolute; padding-left: 3.6rem; top: 0; line-height: 2.8rem; font-size: 1.8rem; letter-spacing: -0.05em;}
.popup-contents .white_box {background: #fff; border-radius: 1.3rem; padding: 2.2rem 2rem; margin-top: .8rem; font-size: 1.4rem; color: rgb(112, 116, 120); letter-spacing: -0.05em;}
.line_box {border: 1px solid rgb(34, 36, 38); padding: 2rem 2.4rem;}

/* 공통 */
button {cursor: pointer;border: none;background-color: transparent;}
input.chk {color: #1e1e1e; font-size: 1.4rem; }
input[type="radio"] {width:1px; height:1px; margin-left: -1px; appearance:none;-webkit-appearance:none;-moz-appearance:none; opacity: 0;}
input[type="checkbox"] {width:1px; height:1px; margin-left: -1px; appearance:none;-webkit-appearance:none;-moz-appearance:none; opacity: 0;}
input[type="text"],
input[type="number"],
input[type="tel"],
input[type="email"],
input[type="password"] {width:100%; height:4.8rem; padding:0; font-size:1.7rem; border-radius: 0px; letter-spacing: -0.05em; color: rgb(112, 116, 120); border-bottom:2px solid rgb(211, 213, 216); border-left: none; border-top: none; border-right: none; background-color: transparent; border-radius: 0; box-sizing: border-box;vertical-align: top; appearance: none;}
input[type="text"]:hover,
input[type="number"]:hover,
input[type="tel"]:hover,
input[type="email"]:hover,
input[type="password"]:hover {border-bottom:2px solid #222426; transition: all .5s; color: rgb(34, 36, 38);}
input[type="text"]:focus,
input[type="number"]:focus,
input[type="tel"]:focus,
input[type="email"]:focus,
input[type="password"]:focus {border-bottom:2px solid #222426; outline:none; color: rgb(34, 36, 38);}
input::-ms-input-placeholder,
input::-webkit-input-placeholder,
input::-moz-placeholder,
input::-moz-placeholder {color: #919191;}
input::placeholder {color: #919191}

select {-webkit-appearance:none; -moz-appearance:none; appearance:none; }
select::-ms-expand {display:none; }
select { border-bottom:1px solid rgb(211, 213, 216); border-left: none; border-top: none; border-right: none;}
select em {color: rgb(255, 36, 36); font-size: 1.4rem;}
select:focus, select:hover {border-bottom:2px solid #222426; transition: all .5s; color: rgb(34, 36, 38);}

input:-internal-autofill-selected  { background-color: #fff !important;}
input:-webkit-autofill,
input:-webkit-autofill:hover,
input:-webkit-autofill:focus { background-color: transparent !important;}

.textarea_box { border-bottom: 2px solid rgb(211, 213, 216); box-sizing:border-box; display:block; overflow:hidden; position:relative; word-break: break-all; box-sizing: border-box;}
.textarea_box textarea {box-sizing:border-box; width:100%; height:130px; background: transparent; vertical-align:top; border:none; line-height:20px; font-size:15px; color:rgb(112, 116, 120); word-break:break-all;}

input.chk + .disabled { border-bottom: none;}
.disabled {color:#919191 !important; background-image: none; background-color: transparent;}
.readonly {background-color: transparent; color: rgb(34, 36, 38); border-bottom:2px solid #eeeff0 !important;}

/* inpui타입 - 동의서보기 아이콘*/
.form_box, .checkbox, .agr_sub_check {position: relative;}
.agr_sub_check span.img {position:absolute; right: 1rem; top: .4rem;}
.form_box span.img a img, .checkbox span.img a img, .agr_sub_check span.img a img, h2 a.open_img img { width: 2.4rem; height: 2.4rem;}
h2 a.open_img {position:absolute; right: 0; top: .5rem;}
.form_box span.img, .checkbox span.img { position: absolute; right: 0; top: 0;}
.form_box span.img a h2 a.open_img {display: inline-block; width: 24px; height: 24px; font-size: 14px; /*text-indent: -5000px;*/ position:absolute; right: 0; top: 0; text-decoration:underline; color: rgb(145, 145, 145);}
.checkbox span.img a {display: inline-block; padding:0; width: 24px; height: 24px; font-size: 14px; letter-spacing: -0.07em; /*text-indent: -5000px;*/ position:absolute; right: 0; top: 0; text-decoration:underline; color: rgb(145, 145, 145); line-height: inherit;}
a.open_img {display: inline-block; width: 24px; height: 24px; font-size: 14px; font-weight: 400; /*text-indent: -5000px;*/ position: absolute; right: 0; top: 0; text-decoration:underline; color: rgb(145, 145, 145);}
h2 .open_img::before {top: -0.3rem;}
.form_box span.img a:focus, .checkbox span.img a:focus, h2 .open_img:focus, .open_img:focus {outline: 1px dotted #333;}
.form_box span.img img, .checkbox span.img img, h2 a.open_img img, a.open_img img  {display: none;}

/* 인풋박스 */
.input_label input::placeholder {color: #2e2e2e !important; font-size: 1.4rem !important;}
.input_label input[type="text"] {padding-left: 10rem;}
.input_label .label {font-size: 1.3rem; color:#929292; position:absolute; left:2rem; top: 2.1rem;}

/* 체크박스 */
.checkbox {margin-bottom: 1.6rem;}
.checkbox:last-child {margin-bottom: 0rem;}
.chk{overflow:hidden;position:absolute;border:0;background:#fff;margin:0 0 0 0rem;width:2.4rem;height:2.4rem;}
.chk+.label{display:inline-block; position:relative; padding-left:3.4rem; font-size:1.7rem; height:2.4rem; line-height: 2.4rem; font-weight: 500; border: none; color: rgb(34, 36, 38); cursor:pointer; box-sizing:border-box;}
.chk+.label:after{position:absolute; top:0; left:0; width:2.4rem; height:2.4rem; margin-top:0rem; transition: .3s; background:url('../img/ico_check_off.png') no-repeat center center #fff; border:.1rem solid #d3d3d3; border-radius: 50%; background-size:12px 8px; content: ''}
.chk:checked+.label{color:#222426;}
.chk:focus+.label { outline: 1px dotted #333;}
.chk:checked+.label:after{margin-top:0; background: url('../img/ico_check_on.png') no-repeat center center #1297ef; border:.1rem solid #1297ef; border-radius: 50%; background-size:13px 9px;}
.check .label:hover span{display:inline-block; padding-bottom:.1rem; border-bottom:.1rem solid #1428A0; color:#1428A0;}
.chk.chk_1+.label {padding-left: 3.2rem;}
.chk.chk_1+.label:after{border:none; background:url('../img/ico_check01_off.png') no-repeat center center; background-color: transparent; background-size: 12px 8px;}
.chk.chk_1:checked+.label:after{border:none; background: url('../img/ico_check01_on.png') no-repeat center center; background-color: transparent;; background-size:13px 9px;}
.chk+.label .font_blue {color: rgb(18, 151, 239) !important; margin-right: .3rem; font-weight: 500;}
.chk+.label .font_gray {color: rgb(112, 116, 120); margin-right: .3rem; font-weight: 500;}

.checkbox.line-02 {margin-top: 24px; padding-top: 24px; border-top: 1px solid rgb(235, 236, 238);}

.chk+.label.font22 {font-size: 1.8rem; font-weight: 700 !important;}
.chk.disabled+.label:after {background:url('../img/ico_check_dis.png') no-repeat center center #ebecee; border:.1rem solid #d3d3d3; border-radius: 50%; background-size:1.2rem auto;}
.chk.disabled+.label { color: rgb(188, 192, 197);}
.chk.chk_1.disabled+.label:after {background:url('../img/ico_check_dis.png') no-repeat center center; border: none; background-color: transparent; background-size:1.5rem auto;}
.chk.chk_1.disabled+.label { color: rgb(188, 192, 197);}

/* 주소복사하기 */
.ch_addcopy {margin-top: 24px; margin-bottom: 40px;}
.ch_addcopy .chk {width: 20px; height: 20px; font-size: 14px;}
.ch_addcopy .chk:checked+.label {font-size: 14px; color:rgb(18, 151, 239); padding-left: 2.4rem;}
.ch_addcopy .chk:checked+.label:after, .ch_addcopy .chk+.label:after {width: 20px;height: 20px;background-size:.9rem auto;}
.ch_addcopy .chk+.label {line-height: 20px;height: 20px;font-size: 14px;padding-left: 2.4rem;}

.checkbox .ch_addcopy { display: inline-block; margin: 0 0 0 4px;}

/* 적합성 체크 */
.ch_suit {margin-top: 24px; margin-bottom: 40px;}
.ch_suit .chk {/* width: 20px; *//* height: 20px; *//* font-size: 14px; */}
.ch_suit .chk:checked+.label {/* font-size: 14px; */color:rgb(18, 151, 239);/* padding-left: 2.4rem; */}
.ch_suit .chk:checked+.label:after, .ch_suit .chk+.label:after {/* width: 20px; *//* height: 20px; *//* background-size:.9rem auto; */}
.ch_suit .chk+.label {/* line-height: 20px; *//* height: 20px; *//* font-size: 14px; *//* padding-left: 2.4rem; */}

.checkbox .ch_suit { display: inline-block; margin: 0 0 0 4px;}

/*라디오버튼*/
.rchk{overflow:hidden; position:absolute; border:0; background:#fff; margin:0 0 0 0px; width:2.4rem; height:2.4rem;}
.rchk+.rchk-label{display:inline-block; position:relative; height:2.4rem; padding-left:3.2rem; font-size:1.7rem; font-weight: 500; line-height:2.4rem; color: rgb(34, 36, 38); cursor:pointer}
.rchk+.rchk-label:after{position:absolute;top:0; left:0; width:2.4rem; height:2.4rem; margin-top:0px; background:#fff; border:.1rem solid #d3d3d3; border-radius: 50%; background-size:1.6rem;content: ''}
.rchk+.rchk-label:before {background:#bcc0c5; width: .8rem; height: .8rem; border-radius: 50%; display: block; content: ''; position: absolute; top: .8rem; left: .8rem; z-index: 1; /*transform: scale(2.5, 2.5); opacity: 0;*/}
.rchk:checked+.rchk-label:after{background:rgb(18, 151, 239); background-size:1.6rem; border:.1rem solid rgb(18, 151, 239);}
.rchk:checked+.rchk-label:before {background:#fff; width: .8rem; height: .8rem; display: block; content: ''; position: absolute; top: .8rem; left: .8rem; transition: .3s; transform: scale(1.3, 1.3); opacity: 1;}
.inline_rchk li {display: inline-block !important; width: 45%;}
.inline_rchk li.mt10 {margin-top: 1.7rem !important;}
.inline_rchk li:nth-child(n+3) { margin-top: 10px;}
.inline_rchk_1 li {display: inline-block !important; width: 32.6%;}
.inline_rchk_2 li {display: inline-block !important; width: 19%;}
.rchk.disabled+.rchk-label {color: rgb(188, 192, 197);}
.rchk.disabled+.rchk-label:after {background:#ebecee; border:.1rem solid #d3d3d3;}
.rchk.disabled+.rchk-label:before {background: #bcc0c5;}
.rchk+.rchk-label.pl28, .chk.chk_1 + .label.pl28, .chk + .label.pl28 {padding-left: 0; width: 2.4rem;}

.flex_rchk {margin-top: .9rem; display: flex; flex-wrap: wrap;}
.flex_rchk li {position: relative; display: inline-flex; margin: 0 12px 8px 0; width: 45%;}
